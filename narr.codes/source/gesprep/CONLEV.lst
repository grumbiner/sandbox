          1 |      SUBROUTINE CONLEV (ICT,ZX,ZN)
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .                                       .
          4 |C SUBPROGRAM:    CONLEV      DETERMINES NUMBER OF CONTOURS FOR PLOT
          5 |C   PRGMMR: DIMEGO           ORG: W/NMC22    DATE: 86-07-18
          6 |C
          7 |C ABSTRACT: DETERMINES NUMBER OF CONTOURS FOR PLOT
          8 |C
          9 |C PROGRAM HISTORY LOG:
         10 |C   86-07-18  G DIMEGO
         11 |C   88-09-22  B SCHMIDT ADDED THE DOCBLOCK
         12 |C
         13 |C USAGE:    CALL CONLEV (ICT, ZX, ZN)
         14 |C   INPUT ARGUMENT LIST:
         15 |C     ICT      - LOGICAL VARIABLE
         16 |C     ZX       - MAXIMUM VALUE
         17 |C     ZN       - MINIMUM VALUE
         18 |C
         19 |C   SUBPROGRAMS CALLED:
         20 |C     LIBRARY:
         21 |C       COMMON   - BLK3
         22 |C
         23 |C ATTRIBUTES:
         24 |C   LANGUAGE: STANDARD FORTRAN
         25 |C   MACHINE:
         26 |C
         27 |C$$$
         28 |      IMPLICIT REAL   (A-H,O-Z)
         29 |      DIMENSION CON(24),XLIM(4),YLIM(4)
         30 |      CHARACTER*4 YTITLE(20)
         31 |      CHARACTER*1 XTITLE(30)
         32 |      LOGICAL IACROS,XREV,YREV,TICK,ALLCON
         33 |      COMMON /BLK3/ LI,LJ,LID,IACROS,XREV,YREV,TICK,ALLCON,XLIM,YLIM,
         34 |     + XRG,YRG,IXD,IYD,NPRL,NPRC,NLV,CON,XTITLE,YTITLE
         35 |      DIF = ABS(ZX-ZN)
         36 |C WAS XX = HMAX1(ABS(ZX),ABS(ZN))
         37 |      XX = AMAX1(ABS(ZX),ABS(ZN))
         38 |      CONT = CON(2)
         39 |      IEVODD = 0
         40 |      NLV2 = NLV/2
         41 |      ENLV = NLV
         42 |      ENMN = 0.5 * ENLV
         43 |      ENMX = 1.25 * ENLV
         44 |      GO TO (10,25,30,40,50,60),ICT
         45 |   10 IF(IEVODD .EQ. 1) GO TO 41
         46 |      CON(1) = -CON(2) * 10. * ENLV
         47 |      RETURN
         48 |   20 CON(2) = CON(2) + CONT
         49 |   25 IF(DIF/CON(2) .GT. ENMX - 2.) GO TO 20
         50 |      GO TO 10
         51 |   30 IF(DIF/CON(2) - ENLV) 36,10,33
         52 |   33 IF(DIF/CON(2) .LE. ENMX - 2.) GO TO 10
         53 |      CON(2) = CON(2) * 2.
         54 |      IF(DIF/CON(2) .LE.  ENMX   ) GO TO 10
         55 |      CON(2) = CON(2) * 2.5
         56 |      IF(DIF/CON(2) .LE. ENMX-2.) GO TO 10
         57 |      CON(2) = CON(2) * 2.
         58 |      GO TO 33
         59 |   36 IF(DIF/CON(2) .GE.ENMN + 1.) GO TO 10
         60 |      CON(2) = CON(2) * .5
         61 |      IF(DIF/CON(2) .GE. ENMN) GO TO 10
         62 |      CON(2) = CON(2) * .4
         63 |      IF(DIF/CON(2) .GE.ENMN + 1.) GO TO 10
         64 |      CON(2) = CON(2) * .5
         65 |      GO TO 36
         66 |   40 NLV = NLV + 4
         67 |      NLV2 = NLV2 + 2
         68 |      CON(NLV2 + 1) = 1. E-21
         69 |      CON(NLV2 + 2) = .1 * CONT
         70 |      CON(NLV2 + 3) =.25 * CONT
         71 |      CON(NLV2 + 4) = .5 * CONT
         72 |      CON(NLV2 + 5) =      CONT
         73 |      NST = NLV2 + 6
         74 |      GO TO 43
         75 |   41 CONT = CON(2)
         76 |      CON(NLV2 + 1) = 1. E-21
         77 |      CON(NLV2 + 2) = .5 * CONT
         78 |      CON(NLV2 + 3) =      CONT
         79 |      NST = NLV2 + 4
         80 |   43 DO 45 II = NST,NLV
         81 |      CON(II) = CON(II-1) + CONT
         82 |   45 CONTINUE
         83 |      DO 47 II = 1,NLV2
         84 |      III = NLV - II + 1
         85 |      CON(II) = -CON(III)
         86 |   47 CONTINUE
         87 |      ALLCON = .TRUE.
         88 |      RETURN
         89 |   50 RAT = ABS( ZX/ZN )
         90 |      IF( RAT .GT. 100.  .OR.  RAT .LT. .01 )  GO TO 30
         91 |   60 IEVODD = 1
         92 |      DIF = 2. * XX
         93 |      ENMX = ENLV - 1.
         94 |      ENMN = FLOAT(NLV2 - 3)
         95 |      GO TO 30
         96 |      END
 
