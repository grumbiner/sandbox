        824 |88106 FORMAT(' radicl,I,J,A,B,ZS,K ',E15.5,2I4,3E15.5,I5)
        825 |      WRITE(6,1109)PLEV(K),DA(I,K,1),TLEV(K)
        826 |      WRITE(6,1109)PLO,ZLO,TLO
        827 |1109  FORMAT(3E12.5)
        828 |      IF(RADICL.GE.0.0) GO TO 2400
        829 |      GO TO 2500
        830 | 2400 ALNPS = ALNP3 - 2. * (G * ZS(I,J,2) - PHI3) /
