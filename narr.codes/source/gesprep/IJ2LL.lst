          1 |      SUBROUTINE IJ2LL (XI,YJ,YLAT,WLON)
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .                                       .
          4 |C SUBPROGRAM:    IJ2LL       CONVERT GRID XI,YJ TO LAT-LONG
          5 |C   PRGMMR: DIMEGO           ORG: W/NMC22    DATE: 88-03-31
          6 |C
          7 |C ABSTRACT: CONVERT GRID INDEX XI,YJ TO LATITUDE-LONGITUDE
          8 |C   USING GRID-DEFINITION INFO. FROM COMMON BLOCK /GRID/
          9 |C
         10 |C PROGRAM HISTORY LOG:
         11 |C   83-03-31  DIMEGO
         12 |C   88-06-30  DIMEGO      DOCBLOCK HALF / PL1 / FTN200
         13 |C
         14 |C USAGE:    CALL IJ2LL(XI,YJ,YLAT,WLON)
         15 |C   INPUT ARGUMENT LIST:
         16 |C     XI       - FIRST INDEX VALUE OF GRID POINT
         17 |C     YJ       - SECOND INDEX VALUE OF GRID POINT
         18 |C
         19 |C   OUTPUT ARGUMENT LIST:
         20 |C     YLAT     - LATITUDE OF GRID POINT (XI,YJ) N POS
         21 |C     WLON     - LONGITUDE OF GRID POINT (XI,YJ)W POS
         22 |C
         23 |C   INPUT FILES:
         24 |C     /GRID/   - COMMON BLOCK CONTAINING GRID DEFINITION
         25 |C
         26 |C ATTRIBUTES:
         27 |C   LANGUAGE: standard FORTRAN
         28 |C   MACHINE:
         29 |C
         30 |C$$$
         31 |      IMPLICIT REAL (A-H,O-Z)
         32 |C  CONVERT POINT (XI,YJ) IN GRID SPACE TO LATITUDE/LONGITUDE
         33 |      LOGICAL POLA,NORTH,SIGERR
         34 |      COMMON /GRID/ POLA,NORTH,ALONVT,POLEI,POLEJ,XMESHL,SIGERR
         35 |      EQUIVALENCE (YLATS,ALONVT) , (DLAT,POLEI)
         36 |      EQUIVALENCE (XLONW,POLEJ) , (DDLON,XMESHL)
         37 |      DATA EARTHR / 6371.2 /, CONV2D / 57.2957795 /
         38 |      IF( POLA )  GO TO 10
         39 |C  GRID IS LAT/LONG
         40 |      YLAT = YLATS + (YJ-1.) * DLAT
         41 |      WLON = XLONW - (XI-1.) * DDLON
         42 |      IF( WLON .GT. 360. )WLON = WLON - 360.
         43 |      IF( WLON .LT. 0.0) WLON = WLON + 360.
         44 |      RETURN
         45 |   10 CONTINUE
         46 |C  GRID IS POLAR STEREOGRAPHIC
         47 |      RE = (EARTHR * 1.86603) / XMESHL
         48 |      GI2 = RE * RE
         49 |      DELX = XI - POLEI
         50 |      DELY = YJ - POLEJ
         51 |      R2 = DELX*DELX + DELY*DELY
         52 |      IF( R2 .EQ. 0.0 )  GO TO 40
         53 |      TLON = CONV2D * ATAN2(DELY,DELX)
         54 |      IF(  NORTH  )  WLON = ALONVT - 90. - TLON
         55 |      IF(.NOT.NORTH) WLON = ALONVT + 90. + TLON
         56 |      IF( WLON .GT. 360. )WLON = WLON - 360.
         57 |      IF( WLON .LT. 0.0) WLON = WLON + 360.
         58 |      YLAT = ASIN ((GI2 - R2) / (GI2 + R2)) * CONV2D
         59 |      IF(.NOT.NORTH) YLAT = -YLAT
         60 |      RETURN
         61 |   40 YLAT = 90.
         62 |      IF(.NOT.NORTH) YLAT = -YLAT
         63 |      WLON = ALONVT
         64 |      RETURN
         65 |      END
 
