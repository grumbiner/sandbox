          1 |                             SUBROUTINE BC6432(INFIL)
          2 |C     ******************************************************************
          3 |C     *                                                                *
          4 |C     *  CONVERTS THE 64-BIT BOUNDARY VALUES FROM PTETABC TO 32-BIT    *
          5 |C     *  WORDS AND DOES THE REFORMATING OF DATA                        *
          6 |C     *                                                                *
          7 |C     ******************************************************************
          8 |         include "parmeta.res"
  1       1 |      PARAMETER (IM=237,JM=387,LM=45,LSM=39)
          9 |                             P A R A M E T E R
         10 |     & (IMT=IM*2-1,JMT=JM/2+1)
         11 |                             P A R A M E T E R
         12 |     & (LB=2*JMT+IMT-3
         13 |     &, LP1=LM+1,LM1=LM-1
         14 |     &, IMJM=IM*JM-JM/2)
         15 |C-----------------------------------------------------------------------
         16 |                             R E A L
         17 |     & PDB2  (LB,2)
         18 |     &,TB2   (LB,LM,2),QB2   (LB,LM,2),UB2   (LB,LM,2),VB2   (LB,LM,2)
         19 |C-----------------------------------------------------------------------
         20 |                             D I M E N S I O N
         21 |     & PDBA  (LB)
         22 |     &,TBA   (LB,LM),QBA   (LB,LM),UBA   (LB,LM),VBA   (LB,LM)
         23 |C-----------------------------------------------------------------------
         24 |                             C O M M O N /BCHFLDS/
         25 |     & PDBA  ,TBA   ,QBA   ,UBA   ,VBA
         26 |C-----------------------------------------------------------------------
         27 |      READ (INFIL,end=99131) PDB2,TB2,QB2,UB2,VB2
         28 |C-----------------------------------------------------------------------
         29 |      DO N=1,LB
         30 |         PDBA(N)=PDB2(N,1)
         31 |      ENDDO
         32 |C-----------------------------------------------------------------------
         33 |      DO L=1,LM
         34 |        DO N=1,LB
         35 |         TBA(N,L)=TB2(N,L,1)
         36 |         QBA(N,L)=QB2(N,L,1)
         37 |         UBA(N,L)=UB2(N,L,1)
         38 |         VBA(N,L)=VB2(N,L,1)
         39 |        ENDDO
         40 |      ENDDO
         41 |C-----------------------------------------------------------------------
         42 |99131 continue
         43 |      RETURN
         44 |      END
 
