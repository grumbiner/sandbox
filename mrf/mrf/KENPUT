##KEN SVDATA( 21,IGPT,ITNUM)= DVSFC1(IGRD(IGPT))
##KEN SVDATA( 28,IGPT,ITNUM)= DUSFCG(IGRD(IGPT))
##KEN SVDATA( 29,IGPT,ITNUM)= DVSFCG(IGRD(IGPT))
##KEN SVDATA(  9,IGPT,ITNUM)= SMSOIL (IGRD(IGPT),1)
##KEN SVDATA( 61,IGPT,ITNUM)= SMSOIL (IGRD(IGPT),2)
CCKEN SVDATA( 65,IGPT,ITNUM)= RUNOF (IGRD(IGPT))
##KEN SVDATA( 63,IGPT,ITNUM)= HPBL(IGRD(IGPT))
##KEN SVDATA( 64,IGPT,ITNUM)= GAMT(IGRD(IGPT))
##KEN SVDATA( 65,IGPT,ITNUM)= GAMQ(IGRD(IGPT))
