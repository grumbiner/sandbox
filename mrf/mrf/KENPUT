##KEN SVDATA( 23,IGPT,ITNUM)= CD  (IGRD(IGPT))
##KEN SVDATA( 24,IGPT,ITNUM)= CDQ (IGRD(IGPT))
##KEN SVDATA( 62,IGPT,ITNUM)= RNET (IGRD(IGPT))
CCKEN SVDATA( 63,IGPT,ITNUM)= EVAP (IGRD(IGPT))
##KEN SVDATA( 64,IGPT,ITNUM)= HFLX (IGRD(IGPT))
##KEN SVDATA(  6,IGPT,ITNUM)= STSOIL (IGRD(IGPT),1)
##KEN SVDATA(  7,IGPT,ITNUM)= STSOIL (IGRD(IGPT),2)
##KEN SVDATA( 34,IGPT,ITNUM)= U10 (IGRD(IGPT))
##KEN SVDATA( 35,IGPT,ITNUM)= V10 (IGRD(IGPT))
##KEN SVDATA( 30,IGPT,ITNUM)= T2  (IGRD(IGPT))
##KEN SVDATA( 31,IGPT,ITNUM)= Q2  (IGRD(IGPT))
##KEN SVDATA( 32,IGPT,ITNUM)= CANOPY(IGRD(IGPT))
