 %INCLUDE COMGPD;
      PARAMETER(IM=#LONB2,IX=#LONB22,KM=#LEVS)
      REAL SLMSK(IM),PSEXP(IM),TG3(IM),SHELEG(IM),RADSL(IM),DLWSF1(IM),
