##KEN IF(ILSHRT.LT.2) THEN
##KEN R=SQRT(RCL)
##KEN DO 331 K=1,#LEVS
##KEN SVDATA(K+#SLVARK+0*#LEVS,IGPT,ITNUM)=U(IGRD(IGPT),K)*R
##KEN SVDATA(K+#SLVARK+1*#LEVS,IGPT,ITNUM)=V(IGRD(IGPT),K)*R
