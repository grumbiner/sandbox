        433 |     1      ts(ier,jer),qs(ier,jer)
        434 |1234      format(2i4,3x,4(f7.1,1x))
        435 |           DO LER = 1, NUMLEV
        436 |            write(6,1235)ler,levs(ler),
        437 |     1       z(ier,jer,ler),t(ier,jer,ler),
        438 |     2       u(ier,jer,ler),v(ier,jer,ler),
        439 |     3       q(ier,jer,ler)
        440 |1235        format(1x,i2,2x,i5,2x,f7.1,1x,3f8.2,2x,f8.6)
        441 |           enddo
        442 |c      DO J = 1, JLIM
        443 |c       DO I = 1, ILIM
        444 |c         IF(MASKE(I,J).EQ.0) print *,'maske=0 at ',i,j
        445 |c         IF(MASKV(I,J).EQ.0) print *,'maskv=0 at ',i,j
        446 |c       ENDDO
        447 |c      ENDDO
