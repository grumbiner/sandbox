        375 |     X               KPDS,KGDS,MASK,GRID,IRET)
        376 |          IF(IRET.EQ.0) THEN
        377 |            II = 1
        378 |            JJ = JJ1
        379 |            DO KK = 1, ITOT
        380 |              IF(IV.EQ.1) THEN
        381 |                Z(II,JJ,L) = GRID(KK)
        382 |              ELSEIF(IV.EQ.2) THEN
        383 |                T(II,JJ,L) = GRID(KK)
        384 |              ELSEIF(IV.EQ.3) THEN
        385 |                U(II,JJ,L) = GRID(KK)
        386 |                IF(MASK(KK)) THEN
        387 |                  MASKV(II,JJ) = 1
        388 |                ENDIF
        389 |              ELSEIF(IV.EQ.4) THEN
        390 |                V(II,JJ,L) = GRID(KK)
        391 |              ELSEIF(IV.EQ.5) THEN
        392 |                Q(II,JJ,L) = GRID(KK)
        393 |              ENDIF
        394 |              IPREV=II
        395 |              II = II + 1
        396 |              IF(MOD(IPREV,IMAX).EQ.0) THEN
        397 |                II = 1
        398 |                JJ = JJ + JJINC
        399 |                IF(JJ.GT.JMAX.OR.JJ.LT.1) GO TO 100
        400 |              ENDIF
        401 |            ENDDO
