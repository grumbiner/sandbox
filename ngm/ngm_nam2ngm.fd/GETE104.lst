        155 |          WRITE(6,10) (KPDS(KER),KER=1,22)
        156 |10        FORMAT(22I8)
        157 |20        FORMAT(14I8)
        158 |          GO TO 1001
        159 |        ENDIF
        160 |      ENDDO
        161 |1001  CONTINUE
        162 |C
        163 |C    FILL IN GRIDEF COMMON BLOCK
        164 |C    THE FOLLOWING DEFINED REGARDLESS OF GRID PROJECTION
        165 |C
        166 |      IMAX = KGDS(2)
        167 |      JMAX = KGDS(3)
        168 |C
        169 |      ALAT1 = KGDS(4) * 0.001
        170 |      ELON1 = KGDS(5) * 0.001 + 360.0
        171 |      ELONV = KGDS(7) * 0.001 + 360.0
        172 |      ALATAN = 0.0
        173 |      DXX = KGDS(8) * 0.001
        174 |      DYY = KGDS(9) * 0.001
        175 |C
        176 |      print *,'gridspecs ',ALAT1,ELON1,ELONV,ALATAN,DXX,DYY
        177 |C
        178 |C    ADD RINC FORECAST HOURS TO START DATE TO GET VALID DATE
        179 |C    IN CASE WE HAVE TO USE A ETA FORECAST TO START THE NGM
        180 |C    USE W3MOVDAT
        181 |C
        182 |      CALL W3MOVDAT(RINC,INCDAT,JNCDAT)
        183 |      WRITE(6,*)' INPUT DATE FROM ETA ',INCDAT,RINC
        184 |      WRITE(6,*)' VALID DATE FROM ETA ',JNCDAT
        185 |C
        186 |C    USE W3DIFDAT TO ENSURE THAT BACKGROUND FROM GDAS HAS
        187 |C    SAME VALID DATE AS THE ETA ANALYSIS
        188 |C
        189 |      CALL W3DIFDAT(JNCDAT,IAODAT,2,DIFF)
        190 |      IF(DIFF(2) .EQ. 0.0) THEN
        191 |       WRITE(6,*)' GDAS AND ETA VALID DATES MATCH SO CONTINUE '
        192 |      ELSE
        193 |       WRITE(6,*)' VALID DATE FROM ETA ',JNCDAT
        194 |       WRITE(6,*)' VALID DATE FROM GDAS ',IAODAT
        195 |       WRITE(6,*)' GDAS AND ETA VALID DATES DO NOT MATCH SO ABORT '
        196 |       ISTAT = -1
        197 |       RETURN
        198 |      ENDIF
        199 |C
        200 |C    GET GRID NUMBER FROM PDS
        201 |C
        202 |      IGDNUM = KPDS(3)
        203 |C
        204 |C   PROCESS THE GRIB FILE
        205 |C
        206 |      NUMVAL = IMAX*JMAX
        207 |      KMAX = MAXLEV
        208 |C
        209 |C   INITIALIZE MASK ARRAY
        210 |C
        211 |      DO J = 1, JLIM
        212 |       DO I = 1, ILIM
        213 |        MASKE(I,J) = 0
        214 |        MASKV(I,J) = 0
        215 |       ENDDO
        216 |      ENDDO
        217 |C
        218 |      WRITE(6,280) IMAX,JMAX,NUMLEV,KMAX
        219 |  280 FORMAT(' IMAX,JMAX,NUMLEV,KMAX ',5I4)
        220 |  285 FORMAT(' IV, IVAR, L, IRET:  ',4I5)
        221 |
        222 |C -== GET SURFACE FIELDS ==-
        223 |      L = 0
        224 |      IV= 0
        225 |C   SURFACE PRESSURE (MB)
        226 |
        227 |c   to start each new file with its index, set J=-1 for sfc pressure
        228 |C
        229 |C   USE THIS FIELD TO SET UP MASK ARRAY
        230 |C
        231 |      J = -1
        232 |      JPDS = -1
        233 |      JPDS(3) = IGDNUM
        234 |      JPDS(5) = 001
        235 |      JPDS(6) = 001
        236 |      JPDS(13) = 1
        237 |      CALL GETGB(LUGB,LUGI,NUMVAL,J,JPDS,JGDS,KF,K,
        238 |     X           KPDS,KGDS,MASK,GRID,IRET)
        239 |      IF(IRET.EQ.0) THEN
        240 |        II = 1
        241 |        JJ = JJ1
        242 |        DO KK = 1, ITOT
        243 |          PS(II,JJ) = GRID(KK) * 0.01
        244 |          IF(MASK(KK)) THEN
        245 |            MASKE(II,JJ) = 1
        246 |          ENDIF
        247 |c         print *,ii,jj,maske(ii,jj),ps(ii,jj)
        248 |          IPREV=II
        249 |          II = II + 1
        250 |          IF(MOD(IPREV,IMAX).EQ.0) THEN
        251 |            II = 1
        252 |            JJ = JJ + JJINC
        253 |            IF(JJ.GT.JMAX.OR.JJ.LT.1) GO TO 110
        254 |          ENDIF
        255 |        ENDDO
        256 |  110 CONTINUE
        257 |      ELSE
        258 |        WRITE(6,285)IV,JPDS(5),L,IRET
        259 |        WRITE(6,*)' COULD NOT UNPACK GRID 104 FILE FROM
        260 |     1   ETA RUN SO ABORT '
        261 |         ISTAT = IRET
        262 |        RETURN
        263 |      ENDIF
        264 |
        265 |C   SURFACE GEOPOTENTIAL HEIGHT (M)
        266 |
        267 |      J = 0
        268 |      JPDS = -1
        269 |      JPDS(3) = IGDNUM
        270 |      JPDS(5) = 007
        271 |      JPDS(6) = 001
        272 |      JPDS(13) = 1
        273 |      CALL GETGB(LUGB,LUGI,NUMVAL,J,JPDS,JGDS,KF,K,
        274 |     X           KPDS,KGDS,MASK,GRID,IRET)
        275 |      IF(IRET.EQ.0) THEN
        276 |        II = 1
        277 |        JJ = JJ1
        278 |        DO KK = 1, ITOT
        279 |          ZS(II,JJ) = GRID(KK)
        280 |          IPREV=II
        281 |          II = II + 1
        282 |          IF(MOD(IPREV,IMAX).EQ.0) THEN
        283 |            II = 1
        284 |            JJ = JJ + JJINC
        285 |            IF(JJ.GT.JMAX.OR.JJ.LT.1) GO TO 120
        286 |          ENDIF
        287 |        ENDDO
        288 |  120 CONTINUE
        289 |      ELSE
        290 |        WRITE(6,285)IV,JPDS(5),L,IRET
        291 |        WRITE(6,*)' COULD NOT UNPACK GRID 104 FILE FROM
        292 |     1   ETA RUN SO ABORT '
        293 |         ISTAT = IRET
        294 |         RETURN
        295 |      ENDIF
        296 |
        297 |C   2-M TEMPERATURE (K)
        298 |
        299 |      J = 0
        300 |      JPDS = -1
        301 |      JPDS(3) = IGDNUM
        302 |      JPDS(5) = 011
        303 |      JPDS(6) = 105
        304 |      JPDS(7) = 2
        305 |      JPDS(13) = 1
        306 |      CALL GETGB(LUGB,LUGI,NUMVAL,J,JPDS,JGDS,KF,K,
        307 |     X           KPDS,KGDS,MASK,GRID,IRET)
        308 |      IF(IRET.EQ.0) THEN
        309 |        II = 1
        310 |        JJ = JJ1
        311 |        DO KK = 1, ITOT
        312 |          TS(II,JJ) = GRID(KK)
        313 |          IPREV=II
        314 |          II = II + 1
        315 |          IF(MOD(IPREV,IMAX).EQ.0) THEN
        316 |            II = 1
        317 |            JJ = JJ + JJINC
        318 |            IF(JJ.GT.JMAX.OR.JJ.LT.1) GO TO 130
        319 |          ENDIF
        320 |        ENDDO
        321 |  130 CONTINUE
        322 |      ELSE
        323 |        WRITE(6,285)IV,JPDS(5),L,IRET
        324 |        WRITE(6,*)' COULD NOT UNPACK GRID 104 FILE FROM
        325 |     1   ETA RUN SO ABORT '
        326 |         ISTAT = IRET
        327 |         RETURN
        328 |      ENDIF
        329 |
        330 |C   2-M SPECIFIC HUMIDITY
        331 |
        332 |      J = 0
        333 |      JPDS = -1
        334 |      JPDS(3) = IGDNUM
        335 |      JPDS(5) = 051
        336 |      JPDS(6) = 105
        337 |      JPDS(7) = 2
        338 |      JPDS(13) = 1
        339 |      CALL GETGB(LUGB,LUGI,NUMVAL,J,JPDS,JGDS,KF,K,
        340 |     X           KPDS,KGDS,MASK,GRID,IRET)
        341 |      IF(IRET.EQ.0) THEN
        342 |        II = 1
        343 |        JJ = JJ1
        344 |        DO KK = 1, ITOT
        345 |          QS(II,JJ) = GRID(KK)
        346 |          IPREV=II
        347 |          II = II + 1
        348 |          IF(MOD(IPREV,IMAX).EQ.0) THEN
        349 |            II = 1
        350 |            JJ = JJ + JJINC
        351 |            IF(JJ.GT.JMAX.OR.JJ.LT.1) GO TO 140
        352 |          ENDIF
        353 |        ENDDO
        354 |  140 CONTINUE
        355 |      ELSE
        356 |        WRITE(6,285)IV,JPDS(5),L,IRET
