          1 |c     PROGRAM GETE104
          2 |      SUBROUTINE GETE104(LUGB,LUGI,JNCDAT,ISTAT)
          3 |C                .      .    .                                       .
          4 |C SUBPROGRAM:    GETE104     RETRIEVE ETA MODEL ANALYSIS
          5 |C   PRGMMR: ROGERS           ORG: W/NP22     DATE: 98-11-05
          6 |C
          7 |C ABSTRACT: RETRIEVES THE ON-TIME ETA ANALYSIS (POSTED ON THE NGM
          8 |C           SUPER C-GRID #104)
          9 |C
         10 |C PROGRAM HISTORY LOG:
         11 |C   98-11-05  Eric Rogers
         12 |C
         13 |C USAGE:    CALL GETE104( LUGB, LUGI, JNCDAT, ISTAT)
         14 |C
         15 |C   INPUT FILES:
         16 |C     LUGB     - INTEGER unit for GRIB data file
         17 |C     LUGI     - INTEGER unit for GRIB Index file
         18 |C
         19 |C   OUTPUT ARGUMENT LIST:
         20 |C     JNCDAT   - DATE OF ETA ANALYSIS
         21 |C     ISTAT    - INTEGER =0  MEANS SUCCESSFUL COMPLETION
         22 |C                        =-1 MEANS GRID COULD NOT BE RETURNED
         23 |C   OUTPUT VIA COMMON BLOCK /GRID /
         24 |C     COMMON /EGRID /PS(ILIM,JLIM),ZS(ILIM,JLIM)
         25 |C    1 Z(ILIM,JLIM,MAXLEV),
         26 |C    2 T(ILIM,JLIM,MAXLEV),
         27 |C    3 U(ILIM,JLIM,MAXLEV),
         28 |C    4 V(ILIM,JLIM,MAXLEV),
         29 |C    5 Q(ILIM,JLIM,MAXLEV), MASKE(ILIM,JLIM),
         30 |C
         31 |C             REAL & INTEGER ARRAYS dimensioned (ILIM,JLIM,MAXLEV)
         32 |C             Containing 3-D fields on arrays of (IMAX,JMAX,NUMLEV)
         33 |C             And the MASKE INTEGER ARRAY(IMAX,JMAX)
         34 |C             MASKE=0 INDICATES A GRIDPOINT WITH MISSING DATA
         35 |C                PRESENT CONTENTS:
         36 |C                PS  SURFACE PRESSURE FROM GDAS GUESS (PA)
         37 |C                ZS  SURFACE HEIGHT (K)
         38 |C                Z   GEOPOTENTIAL HEIGHT (M)
         39 |C                T   TEMPERATURE (K)
         40 |C                U   WIND U-COMPONENT (M/S)
         41 |C                V   WIND V-COMPONENT (M/S)
         42 |C                Q   Specific HUMIDITY (G/G)
         43 |C
         44 |C REMARKS:
         45 |
         46 |C ATTRIBUTES:
         47 |C   LANGUAGE: FORTRAN-90
         48 |C   MACHINE:  CRAY C-90
         49 |C$$$
         50 |      INCLUDE "parmg104"
  1       1 |      PARAMETER(ILIM=147,JLIM=110,MAXLEV=39)
         51 |      PARAMETER(ITOT=ILIM*JLIM)
         52 |      COMMON /GBLDAT/ IAODAT(8)
         53 |      COMMON /GRIDEF/ IMAX,JMAX,KMAX,ALAT1,ELON1,DXX,DYY,ELONV,
         54 |     1    ALATAN
         55 |C
         56 |      COMMON /EGRID /PS(ILIM,JLIM),ZS(ILIM,JLIM),
         57 |     1 Z(ILIM,JLIM,MAXLEV),TS(ILIM,JLIM),
         58 |     2 T(ILIM,JLIM,MAXLEV),QS(ILIM,JLIM),
         59 |     3 U(ILIM,JLIM,MAXLEV),
         60 |     4 V(ILIM,JLIM,MAXLEV),
         61 |     5 Q(ILIM,JLIM,MAXLEV),MASKE(ILIM,JLIM),
         62 |     6 MASKV(ILIM,JLIM)
         63 |C
         64 |      COMMON /IPOINT/ IPT,JPT
         65 |C
         66 |      DIMENSION GRID(ITOT),DIFF(5)
         67 |      DIMENSION RINC(5),INCDAT(8),JNCDAT(8)
         68 |      INTEGER JPDS(200),JGDS(200),KPDS(200),KGDS(200)
         69 |      INTEGER LEVS(MAXLEV),IVAR(5)
         70 |      LOGICAL*1 MASK(ITOT)
         71 |C
         72 |      DATA LEVS/1000,975,950,925,900,875,850,825,800,775,750,725,
         73 |     X           700,675,650,625,600,575,550,525,500,475,450,425,
         74 |     X           400,375,350,325,300,275,250,225,200,175,150,125,
         75 |     X           100,75,50/
         76 |C
         77 |      DATA IVAR/007,011,033,034,051/
         78 |C
         79 |      PARAMETER(MBUF=2000000,JF=1000000)
         80 |      CHARACTER CBUF(MBUF)
         81 |      LOGICAL*1 LB(JF)
         82 |      REAL F(JF)
         83 |      PARAMETER(MSK1=32000,MSK2=4000)
         84 |      INTEGER JENS(200),KENS(200)
         85 |C
         86 |      NUMLEV=MAXLEV
         87 |C
         88 |      IPT=67
         89 |      JPT=16
         90 |C
         91 |      JJ1 = 1
         92 |      JJINC = 1
         93 |C
         94 |      DO K = 1, 8
         95 |        INCDAT(K) = 0
         96 |        JNCDAT(K) = 0
         97 |        IF(K .LE. 5) THEN
         98 |          RINC(K) = 0.0
         99 |        ENDIF
        100 |      ENDDO
        101 |C
        102 |      ISTAT = 0
        103 |C
        104 |C  READ INDEX FILE TO GET GRID SPECS
        105 |C
        106 |      IRGI = 1
        107 |      IRGS = 1
        108 |      KMAX = 0
        109 |      JR=0
        110 |      KSKIP = 0
        111 |      CALL BAOPEN(13,'fort.13',IRETGB)
        112 |      CALL BAOPEN(14,'fort.14',IRETGI)
        113 |      print *,'after baopen ',iretgb, iretgi
        114 |      CALL GETGI(LUGI,KSKIP,MBUF,CBUF,NLEN,NNUM,IRGI)
        115 |      write(6,*)' IRET FROM GETGI ',IRGI
        116 |      IF(IRGI .NE. 0) THEN
        117 |        WRITE(6,*)' PROBLEMS READING GRIB INDEX FILE SO ABORT'
        118 |        ISTAT = IRGI
        119 |        RETURN
        120 |      ENDIF
        121 |      REWIND LUGI
        122 |C
        123 |      DO K = 1, NNUM
        124 |        JR = K - 1
        125 |        JPDS = -1
        126 |        JGDS = -1
        127 |        CALL GETGB1S(CBUF,NLEN,NNUM,JR,JPDS,JGDS,JENS,
        128 |     &               KR,KPDS,KGDS,KENS,LSKIP,LGRIB,IRGS)
        129 |        write(6,*)' IRET FROM GETGB1S ',IRGS
        130 |        IF(IRGI .NE. 0) THEN
        131 |          WRITE(6,*)' PROBLEMS ON 1ST READ OF GRIB FILE SO ABORT'
        132 |          ISTAT = IRGS
        133 |          RETURN
        134 |        ENDIF
        135 |C
        136 |C    USE  F I R S T  PDS TO GET START DATE & FORECAST LENGTH
        137 |C
        138 |        IF(K.EQ.1) THEN
        139 |          MODGEN = KPDS(2)
        140 |          IYS = KPDS(8)
        141 |          INCDAT(2) = KPDS(9)
        142 |          INCDAT(3) = KPDS(10)
        143 |          INCDAT(5) = KPDS(11)
        144 |          RINC(2) = FLOAT(KPDS(14))
        145 |          IGCENT = KPDS(21)
        146 |          IF(IYS .EQ. 100) THEN
        147 |            INCDAT(1) = IGCENT * 100
        148 |          ELSE
        149 |            INCDAT(1) = (IGCENT - 1) * 100 + IYS
        150 |          ENDIF
        151 |        ENDIF
        152 |
        153 |        IF(KPDS(3).EQ.104) THEN
