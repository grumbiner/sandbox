        408 |          ENDIF
        409 |  100   CONTINUE
        410 |  200 CONTINUE
        411 |C
        412 |C  CONVERT SENSIBLE TEMP TO VIRTUAL TEMP
        413 |C
        414 |      DO K = 1, MAXLEV
        415 |       DO J = 1, JLIM
        416 |        DO I = 1, ILIM
        417 |         IF(MASKE(I,J).EQ.1) THEN
        418 |          TV = T(I,J,K)*(1.0+(0.61*Q(I,J,K)))
        419 |          T(I,J,K) = TV
        420 |          IF(K.EQ.1) THEN
        421 |           TVS = TS(I,J)*(1.0+(0.61*QS(I,J)))
        422 |           TS(I,J) = TVS
        423 |          ENDIF
        424 |         ENDIF
        425 |        ENDDO
        426 |       ENDDO
        427 |      ENDDO
