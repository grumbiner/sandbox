          1 |      SUBROUTINE xintrp(Y,X,XBAR,N,AINTRP)
          2 |c     FUNCTION xintrp(Y,X,XBAR,N)
          3 |      IMPLICIT REAL (A-H,O-Z)
          4 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          5 |C                .      .    .                                       .
          6 |C SUBPROGRAM:    XINTRP      VERTICAL INTERPOLATION FUNCTION (AIKEN)
          7 |C   PRGMMR: DEAVEN           ORG: W/NMC22    DATE: 85-01-31
          8 |C
          9 |C ABSTRACT: PROGRAM TO PERFORM VERTICAL INTERPOLATION FROM A PROFILE
         10 |C   OF VALUES (Y) AT LOG PRESSURES (X) TO A PARTICULAR POINT (XBAR)
         11 |C   USING AN ITERATIVE AIKEN SCHEME WHICH IS VARIABLE ORDER IN LN P.
         12 |C
         13 |C PROGRAM HISTORY LOG:
         14 |C   85-01-31  DEAVEN
         15 |C   88-07-28  DIMEGO      DOCBLOCK & HALF PRECISION
         16 |C   89-03-28  DEAVEN      MODIFIED TO BE AT MOST QUADRATIC
         17 |C
         18 |C USAGE:    WXYZ = XINTRP(Y,X,XBAR,N)
         19 |C   INPUT ARGUMENT LIST:
         20 |C     Y        - ARRAY CONTAINING SOURCE VALUES
         21 |C     X        - ARRAY OF LEVELS AT WHICH WE HAVE Y
         22 |C     XBAR     - LEVEL VALUE AT WHICH WE WANT A VALUE
         23 |C     N        - NUMBER OF LEVELS IN ARRAYS X AND Y
         24 |C
         25 |C   OUTPUT ARGUMENT LIST:
         26 |C     XINTRP   - FUNCTION VALUE IS INTERPOLATED VALUE OF Y AT XBAR
         27 |C
         28 |C REMARKS: SENSITIVE TO UNEQUAL SPACING IN X
         29 |C
         30 |C ATTRIBUTES:
         31 |C   LANGUAGE: STANDARD FORTRAN
         32 |C   MACHINE:
         33 |C
         34 |C$$$
         35 |        PARAMETER (K15=SELECTED_REAL_KIND(15))
         36 |        DIMENSION Y(40),P(50,50)
         37 |        REAL(KIND=K15) X(51),XBAR,R(50),RMIN
         38 |C
         39 |        DO 333 I = 1,N
         40 |          R(I) = X(I) - XBAR
         41 |          P(I,1) = Y(I)
         42 |c         print *,'in xintrp ',i,n,r(i),x(i),y(i),xbar
         43 |333     CONTINUE
         44 |        RMIN = 9.E+7
         45 |        DO 10 I = 1,N
         46 |          IF(ABS(R(I)).GT.RMIN)GO TO 10
         47 |            RMIN = ABS(R(I))
         48 |            IS = I
         49 |10      CONTINUE
         50 |        IF(RMIN.EQ.0.0)GO TO 99
         51 |        IF(IS.LT.2) IS = 2
         52 |        IF(IS.GE.N) IS = N-1
         53 |        JS = IS
         54 |        JE = IS +1
         55 |c       IF(JE.GT.N) JE = N
         56 |        IF(JS.LT.3) GO TO 55
         57 |        DO 334 I = 1,N
         58 |          P(I,JS-1) = Y(I)
         59 |334     CONTINUE
         60 |55      CONTINUE
         61 |c       DO 1 J = JS,JE
         62 |c       DO 1 I = J,JE
         63 |c1      P(I,J) = (P(J-1,J-1)*R(I) - R(J-1)*P(I,J-1))/(X(I)-X(J-1))
         64 |c   J=JS and I=JS
         65 |        J=JS
         66 |        I=JS
         67 |c       print *,'in xintrp1 ',X(I),X(J-1),I,J-1
         68 |        P(I,J) = (P(J-1,J-1)*R(I) - R(J-1)*P(I,J-1))/(X(I)-X(J-1))
         69 |c   J=JS and I=JE
         70 |        J=JS
         71 |        I=JE
         72 |c       print *,'in xintrp2 ',X(I),X(J-1),I,J-1
         73 |        P(I,J) = (P(J-1,J-1)*R(I) - R(J-1)*P(I,J-1))/(X(I)-X(J-1))
         74 |c   J=JE and I=JE
         75 |        J=JE
         76 |        I=JE
         77 |c       print *,'in xintrp3 ',X(I),X(J-1),I,J-1
         78 |        P(I,J) = (P(J-1,J-1)*R(I) - R(J-1)*P(I,J-1))/(X(I)-X(J-1))
         79 |        AINTRP = P(JE,JE)
         80 |        RETURN
         81 |99      AINTRP = Y(IS)
         82 |        RETURN
         83 |        END
 
