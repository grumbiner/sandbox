        411 |C
        412 |C     PROCESS THE SURFACE MOISTURE.
        413 |      DO 9200 NG=1,NGRDUSE
        414 |C
        415 |      CALL GRDSET(NG, INDEXSQ, MADDRS, IM, JM, DUM, DUM, DUM, DUM)
        416 |C
        417 |C     SPECIFY THE POINTER FOR SURFACE MOISTURE IN ARRAY VBL.
        418 |      MADDSQ   = MADDRS -1 -IM
        419 |C
        420 |      DO 9100 J=1,JM
        421 |C
        422 |      DO 9000 I=1,IM
        423 |      FIELD(I + (J-1)*IM) = VBL(MADDSQ + IM*J + I)
        424 | 9000 CONTINUE
        425 |C
        426 | 9100 CONTINUE
        427 |C
        428 |      LABEL(1) = LVAR(9)
        429 |      LABEL(2) = LGRID(NG)
        430 |      CALL STATS(FIELD, 1,IM,IM, 1,JM,JM, 1,1,KM, LABEL)
        431 |C
        432 | 9200 CONTINUE
        433 |C
        434 |C
        435 |C     PROCESS THE ALBEDO.
        436 |      DO 10200 NG=1,NGRDUSE
        437 |C
        438 |      CALL GRDSET(NG, INDEXALB, MADDRS, IM, JM, DUM, DUM, DUM, DUM)
        439 |C
        440 |C     SPECIFY THE POINTER FOR THE ALBEDO IN ARRAY VBL.
        441 |      MADDALB  = MADDRS -1 -IM
        442 |C
        443 |      DO 10100 J=1,JM
        444 |C
        445 |      DO 10000 I=1,IM
        446 |      FIELD(I + (J-1)*IM) = VBL(MADDALB +IM*J + I)
        447 |10000 CONTINUE
        448 |C
        449 |10100 CONTINUE
        450 |C
        451 |      LABEL(1) = LVAR(10)
        452 |      LABEL(2) = LGRID(NG)
        453 |      CALL STATS(FIELD, 1,IM,IM, 1,JM,JM, 1,1,KM, LABEL)
        454 |C
        455 |10200 CONTINUE
        456 |C
        457 |      RETURN
