        305 |C     DO 5300 I=1,IM
        306 |C     FIELD(I + (J-1)*IM) = VBL(MADDSLP + IM*J + I)
        307 |C5300 CONTINUE
        308 |C
        309 |C5500 CONTINUE
        310 |C
        311 |C     LABEL(1) = LVAR(11)
        312 |C     LABEL(2) = LGRID(NG)
        313 |C     CALL STATS(FIELD, 1,IM,IM, 1,JM,JM, 1,1,KM, LABEL)
        314 |C
        315 |C5900 CONTINUE
