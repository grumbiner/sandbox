        100 |     2            ' -GRID 9'/
        101 |C
        102 |C     SPECIFY THE UNIT NUMBER OF THE PRINTER.
        103 |      IOUTUPRT = 6
        104 |C
        105 |C     ENSURE THAT THE SIZE OF ARRAY FIELD IS LESS THAN THE SIZE OF
        106 |C     ARRAY ANAL.
        107 |      IJKMAX0 =  IIJKMAX
        108 |      LONFP1  = LONF + 1
