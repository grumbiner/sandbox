        141 |   60 CONTINUE
        142 |C
        143 |C     NOW THE U COMPONENT IS PROCESSED.
        144 |C
        145 |      DO 1200 NG=1,NGRDUSE
        146 |      CALL GRDSET (NG, INDEXHU, DUM, IM, JM, DUM, DUM, DUM, DUM)
        147 |C
        148 |C     SPECIFY THE POINTERS FOR VARIOUS VARIABLES IN ARRAY VBL.
        149 |      IJ     = IM * JM
        150 |      MADDHU = IADDRG(INDEXHU, NG) -1 -IM -IJ
        151 |      MADDH  = IADDRG(INDEXH , NG) -1 -IM
        152 |C
        153 |      DO 1100 J=1,JM
        154 |C
        155 |      DO 1000 K=1,KM
        156 |C
        157 |      DO 900 I=1,IM
        158 |         IF (J .LT. JM) THEN
        159 |            FIELD(I + (J-1)*IM + (K-1)*IJ)
        160 |     1               = VBL(MADDHU + IM*J +IJ*K + I) * 2.0E0
        161 |     2               / (VBL(MADDH+I+IM*J) + VBL(MADDH+I+IM+IM*J))
        162 |         ELSE
        163 |C
        164 |C     IF THIS IS THE LAST ROW, WE ASSUME THAT
        165 |C     THE SURFACE PRESSURE OF THE NONEXISTENT NEXT ROW IS THE
        166 |C     SAME AS THE CURRENT ROW.
        167 |            FIELD(I + (J-1)*IM + (K-1)*IJ)
        168 |     1                   = VBL(MADDHU + IM*J +IJ*K + I)
        169 |     2                   / VBL(MADDH+I+IM*J)
        170 |         END IF
        171 |  900 CONTINUE
        172 |C
        173 | 1000 CONTINUE
        174 |C
        175 | 1100 CONTINUE
        176 |C
        177 |      LABEL(1) = LVAR(1)
        178 |      LABEL(2) = LGRID(NG)
        179 |      CALL STATS(FIELD, 1,IM,IM, 1,JM,JM, 1,KM,KM, LABEL)
        180 |C
        181 | 1200 CONTINUE
        182 |C
        183 |C
        184 |C     PROCESS THE V COMPONENT.
        185 |      DO 2200 NG=1,NGRDUSE
        186 |C
        187 |      CALL GRDSET(NG, INDEXHV, DUM, IM, JM, DUM, DUM, DUM, DUM)
        188 |C
        189 |C     SPECIFY THE POINTERS FOR VARIOUS VARIABLES IN ARRAY VBL.
        190 |      IJ     = IM * JM
        191 |      MADDHV = IADDRG(INDEXHV, NG) -1 -IM -IJ
        192 |      MADDH  = IADDRG(INDEXH , NG) -1 -IM
        193 |C
        194 |      DO 2100 J=1,JM
        195 |C
        196 |      DO 2050 K=1,KM
        197 |      DO 2000 I=1,IM-1
        198 |      FIELD(I + (J-1)*IM +(K-1)*IJ)
        199 |     1             = VBL(MADDHV + IM*J+IJ*K +I) * 2.0E0
        200 |     2             /(VBL(MADDH + I + IM*J) + VBL(MADDH + I + 1 + IM*J))
        201 | 2000 CONTINUE
        202 |C
        203 |      FIELD(IM +(J-1)*IM +(K-1)*IJ)
        204 |     1              = VBL(MADDHV +IM*J +IJ*K+IM) / VBL(MADDH+IM+IM*J)
        205 |C
        206 | 2050 CONTINUE
        207 |C
        208 | 2100 CONTINUE
        209 |C
        210 |      LABEL(1) = LVAR(2)
        211 |      LABEL(2) = LGRID(NG)
        212 |      CALL STATS(FIELD, 1,IM,IM, 1,JM,JM, 1,KM,KM, LABEL)
