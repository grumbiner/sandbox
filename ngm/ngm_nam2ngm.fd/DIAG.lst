        131 |     1            'SUBROUTINE DIAG.  EXECUTION TERMINATED.')
        132 |C
        133 |C              TERMINATE THE CONTROLLEE WITH A RETURN CODE OF 8.
        134 |      ERRMSG(1) = 'IN SUBROUTINE DIAG     NEAR STATEME'
        135 |      ERRMSG(2) = 'NT NUMBER 30:                      '
        136 |      ERRMSG(3) = 'THE VALUE OF NGRDUSE IS TOO LARGE F'
        137 |      ERRMSG(4) = 'OR THE SUBROUTINE.                 '
