        251 |      MADDHQ = IADDRG(INDEXHQ, NG) -1 -IM -IJ
        252 |      MADDH  = IADDRG(INDEXH , NG) -1 -IM
        253 |C
        254 |      DO 4100 J=1,JM
        255 |C
        256 |      DO 4000 K=1,KM
        257 |      DO 4000 I=1,IM
        258 |      FIELD(I + (J-1)*IM +(K-1)*IJ)
        259 |     1             = VBL(MADDHQ +IM*J + IJ*K +I) / VBL(MADDH+IM*J + I)
        260 | 4000 CONTINUE
        261 |C
        262 | 4100 CONTINUE
        263 |C
        264 |      LABEL(1) = LVAR(4)
        265 |      LABEL(2) = LGRID(NG)
        266 |      CALL STATS(FIELD, 1,IM,IM, 1,JM,JM, 1,KM,KM, LABEL)
        267 |C
        268 | 4200 CONTINUE
        269 |C
        270 |C
        271 |C     PROCESS THE SURFACE PRESSURE.
        272 |      DO 5200 NG=1,NGRDUSE
        273 |C
        274 |      CALL GRDSET(NG, INDEXH, MADDRS, IM, JM, DUM, DUM, DUM, DUM)
        275 |C
        276 |C     SPECIFY THE POINTER FOR SURFACE PRESSURE IN ARRAY VBL.
