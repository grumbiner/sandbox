        877 |           ENDIF
        878 |           VBL(ICOUNT) = QNEW(I,J,K)
        879 |          ENDIF
        880 |         ENDIF
        881 |         ICOUNT = ICOUNT + 1
        882 |        ENDDO
        883 |       ENDDO
        884 |      ENDDO
        885 |C
        886 |C   SURFACE PRESSURE
        887 |C
        888 |      ICOUNT=IADDRG(5,NG)
        889 |      print *,'start of ps on grid 2 at ',icount
        890 |      DO J = 1, JGOUT
        891 |       DO I = 1, IGOUT
        892 |        IF(J.LE.JLIM) THEN
        893 |         IF(MASKE(I,J).EQ.1) THEN
        894 |          IF(mod(i,5).eq.0 .and. mod(j,5).eq.0) THEN
        895 |           psgbl=vbl(icount)*1000.
        896 |           pseta=psnew(i,j)*1000.
        897 |           print *,'ps update ',i,j,icount,psgbl,pseta
        898 |          ENDIF
        899 |          VBL(ICOUNT) = PSNEW(I,J)
        900 |         ENDIF
        901 |        ENDIF
