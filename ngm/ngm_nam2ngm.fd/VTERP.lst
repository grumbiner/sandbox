        829 |      print *,'start of v on grid 2 at ',icount
        830 |      DO K = 1, IKM
        831 |       DO J = 1, JGOUT
        832 |        DO I = 1, IGOUT
        833 |         IF(J.LE.JLIM) THEN
        834 |          IF(MASKE(I,J).EQ.1 .AND. MASKV(I,J).EQ.1) THEN
        835 |           IF(I.EQ.IPT .AND. J.EQ.JPT .AND. K.LT.IKM) THEN
        836 |            print *,'v update ',i,j,k,icount,vbl(icount),vnew(i,j,k)
        837 |           ENDIF
        838 |           IF(K.LT.IKM) VBL(ICOUNT) = VNEW(I,J,K)
        839 |          ENDIF
        840 |         ENDIF
        841 |         ICOUNT = ICOUNT + 1
        842 |        ENDDO
        843 |       ENDDO
        844 |      ENDDO
        845 |C
        846 |C   POTENTIAL TEMPERATURE
        847 |C
        848 |      ICOUNT=IADDRG(3,NG)
        849 |      print *,'start of theta on grid 2 at ',icount
        850 |      DO K = 1, IKM
        851 |       DO J = 1, JGOUT
        852 |        DO I = 1, IGOUT
        853 |         IF(J.LE.JLIM) THEN
        854 |          IF(MASKE(I,J).EQ.1) THEN
        855 |           IF(I.EQ.IPT .AND. J.EQ.JPT .AND. K.LT.IKM) THEN
        856 |            print *,'theta update ',i,j,k,icount,vbl(icount),tnew(i,j,k)
        857 |           ENDIF
        858 |           IF(K.LT.IKM) VBL(ICOUNT) = TNEW(I,J,K)
        859 |          ENDIF
        860 |         ENDIF
        861 |         ICOUNT = ICOUNT + 1
        862 |        ENDDO
        863 |       ENDDO
        864 |      ENDDO
        865 |C
        866 |C   SPECIFIC HUMIDITY
        867 |C
        868 |      ICOUNT=IADDRG(4,NG)
        869 |      print *,'start of q on grid 2 at ',icount
        870 |      DO K = 1, IKMQ
        871 |       DO J = 1, JGOUT
        872 |        DO I = 1, IGOUT
        873 |         IF(J.LE.JLIM) THEN
