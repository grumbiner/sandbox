        312 |C
        313 |C  RECOMPUTE LEVATM BASED ON NEW SURFACE PRESSURE
        314 |C  ONLY IF PSNEW < PSOLD
        315 |C
        316 |      DO J = 1, JLIM
        317 |       DO I = 1, ILIM
        318 |        IF(MASKE(I,J) .EQ. 1) THEN
