        111 |C
        112 |C  COMPUTE LOGS OF PRESSURE LEVELS
        113 |C
        114 |      DO K = 1, MAXLEV
        115 |        ALPLEV(K) = LOG(EPLEV(K))
        116 |      ENDDO
        117 |C
        118 |C  EXTRACT SURFACE TERRAIN HEIGHT ON SUPER C-GRID FROM
        119 |C  VBL
        120 |C
        121 |      IM = IMG(2)
        122 |      JM = JMG(2)
        123 |      ISTART = IADDRG(10,2)
        124 |      DO J = 1, JM
        125 |        DO I = 1, IM
        126 |          NGMZ(I,J)  = VBL(ISTART) * FACTOR
        127 |          IF(I.EQ.IPT .AND. J.EQ.JPT) THEN
        128 |            print *,'NGMZ AT ',I,J,' = ',NGMZ(I,J)
        129 |          ENDIF
        130 |          ISTART = ISTART + 1
        131 |        ENDDO
        132 |      ENDDO
        133 |C
        134 |C     CALL PRINTNGM(NGMZ)
        135 |C
        136 |C  GET INDEX OF FIRST PRESSURE LEVEL IN THE ETA
        137 |C  ANALYSIS ABOVE GROUND (1=1000 MB,39=50 MB)
        138 |C  IF SURFACE IS < 7.5 MB FROM PRESSURE SURFACE
        139 |C  USE THE NEXT HIGHEST ONE TO PREVENT EXTRAPOLATION
        140 |C
        141 |      DO J = 1, JLIM
        142 |       DO I = 1, ILIM
        143 |        IF(MASKE(I,J) .EQ. 1) THEN
        144 |         DO K = 1,MAXLEV
        145 |          IF(PS(I,J).GE.EPLEV(K)) THEN
        146 |            PDIFF = PS(I,J) - EPLEV(K)
        147 |            LEVATM(I,J) = K
        148 |            LEVSAV(I,J) = K
        149 |            IF(PDIFF.LE.7.5) LEVATM(I,J) = K + 1
        150 |            IF(I.EQ.IPT .AND. J.EQ.JPT) THEN
        151 |             write(6,95)i,j,ps(i,j),k,eplev(k),levatm(i,j)
        152 | 95          format(1x,2i4,1x,f9.2,1x,i2,1x,f9.2,1x,i3)
        153 |            ENDIF
        154 |            GO TO 100
        155 |          ENDIF
        156 |         ENDDO
        157 |100      CONTINUE
        158 |        ENDIF
        159 |       ENDDO
        160 |      ENDDO
        161 |C
        162 |CTEST
        163 |C
        164 |C  RECOMPUTE HEIGHTS FROM ETA ANALYSIS
        165 |C
        166 |      DO J = 1, JLIM
        167 |       DO I = 1, ILIM
        168 |        IF(MASKE(I,J).EQ.1) THEN
        169 |         LEVA = LEVSAV(I,J)
        170 |         DO K = LEVA, MAXLEV
        171 |          IF(K.EQ.LEVA) THEN
        172 |            ZNEWE(I,J,K-1) = ZS(I,J)
        173 |            T2 = T(I,J,K)
        174 |            T3 = T(I,J,K+1)
        175 |            Q2 = Q(I,J,K)
        176 |            Q3 = Q(I,J,K+1)
        177 |            DELT = (LOG(PS(I,J))-ALPLEV(K))*
        178 |     1               ((T2-T3)/(ALPLEV(K)-ALPLEV(K+1)))
        179 |            DELQ = (LOG(PS(I,J))-ALPLEV(K))*
        180 |     1               ((Q2-Q3)/(ALPLEV(K)-ALPLEV(K+1)))
        181 |            T1 = T(I,J,K) + DELT
        182 |            Q1 = Q(I,J,K) + DELQ
        183 |            TSOLD(I,J) = TS(I,J)
        184 |            TS(I,J) = T1
        185 |            TBAR(I,J,K) = (TS(I,J) + T2) * 0.5
        186 |            PTOPT = ALPLEV(K)
        187 |            PBOT = LOG(PS(I,J))
        188 |          ELSE
        189 |            T1 = T(I,J,K-1)
        190 |            T2 = T(I,J,K)
        191 |            TBAR(I,J,K) = (T1 + T2) * 0.5
        192 |            IF(K.EQ.MAXLEV) TBAR(I,J,K+1) = (227.0 + T2) * 0.5
        193 |            PTOPT = ALPLEV(K)
        194 |            PBOT = ALPLEV(K-1)
        195 |          ENDIF
        196 |            ZNEWE(I,J,K) = ZNEWE(I,J,K-1)+(ROG*TBAR(I,J,K)*
        197 |     1         (PBOT-PTOPT))
        198 |            IF(I.EQ.IPT .AND. J.EQ.JPT) THEN
        199 |             print *,i,j,k,znewe(i,j,k),z(i,j,k),eplev(k),
        200 |     1          tbar(i,j,k),PTOPT,pbot,zs(i,j)
        201 |            ENDIF
        202 |          Z(I,J,K) = ZNEWE(I,J,K)
        203 |         ENDDO
        204 |        ENDIF
        205 |       ENDDO
        206 |      ENDDO
        207 |C
        208 |C  DO VERTICAL INTERPOLATION USING LOGIC FROM GESPREP
        209 |C
        210 |C  RECOMPUTE SURFACE PRESSURE BASED ON NGM TERRAIN
        211 |C
        212 |      DO J = 1, JLIM
        213 |       DO I = 1, ILIM
        214 |        IF(MASKE(I,J).EQ.1) THEN
        215 |c        if(j.eq.1) write(0,*)i,j
        216 |         PSTAR = PS(I,J)
        217 |         KSTART = LEVATM(I,J)
        218 |         DO K = KSTART, MAXLEV
        219 |           TLEV(K) = T(I,J,K)
        220 |           IF(I.EQ.IPT.AND.J.EQ.JPT) THEN
        221 |             WRITE(6,2350)I,J,K,LEVATM(I,J),TLEV(K),PSTAR
        222 | 2350        FORMAT(1X,4I4,2(1X,F12.3))
        223 |           ENDIF
        224 |         ENDDO
        225 | 2360    CONTINUE
        226 |         K = LEVATM(I,J)
        227 | 2370    CONTINUE
        228 |         PLO = EPLEV(K)
        229 |         ZLO = Z(I,J,K)
        230 |         TLO = TLEV(K)
        231 |         K = K + 1
        232 | 2380    CONTINUE
        233 |         IF(NGMZ(I,J) .GT. Z(I,J,K)) GO TO 2370
        234 |         ALNPB = ALPLEV(K-1)
        235 |         ALNPT = ALPLEV(K)
        236 |         DLNP = ALNPB - ALNPT
        237 |         ALNP3 = 0.5*(ALNPB+ALNPT)
        238 |         A = G * (Z(I,J,K) - ZLO) / DLNP
        239 |         B = R * (TLO - TLEV(K)) / DLNP
        240 |         PHI3 = G*0.5*(ZLO+Z(I,J,K)) + 0.125*B*DLNP*DLNP
        241 |         RADICL = A*A - 2.*B*(G*NGMZ(I,J) - PHI3)
        242 |         IF(RADICL.GE.0.0.AND.I.EQ.IPT.AND.J.EQ.JPT) THEN
        243 |           WRITE(6,110) RADICL,I,J,A,B,NGMZ(I,J),K
        244 |  110      FORMAT(' radicl,I,J,A,B,ZS,K ',G15.5,2I4,3G15.5,I5)
        245 |           WRITE(6,115)EPLEV(K),Z(I,J,K),TLEV(K)
        246 |           WRITE(6,115)PLO,ZLO,TLO
        247 |  115      FORMAT(1X,3(G12.5))
        248 |         ENDIF
        249 |         IF(RADICL.LE.0.0) THEN
        250 |           WRITE(6,120) RADICL,I,J,A,B,NGMZ(I,J),K
        251 |  120      FORMAT(' radicl<0,I,J,A,B,ZS,K ',G15.5,2I4,3G15.5,I5)
        252 |           WRITE(6,115)EPLEV(K),Z(I,J,K),TLEV(K)
        253 |           WRITE(6,115)PLO,ZLO,TLO
        254 |           GO TO 2500
        255 |         ENDIF
        256 |         ALNPS = ALNP3-2.*(G*NGMZ(I,J)-PHI3) /
        257 |     1      (A + SQRT(RADICL))
        258 |         PSNEW(I,J) = EXP(ALNPS)
        259 |         TSNEW(I,J) = TLO - (ALNPB-ALNPS)*B/R
        260 |         GO TO 2600
        261 |C
        262 |C   NGM TERRAIN IS LOWER THAN ETA TERRAIN
        263 |C
        264 | 2500    print *,'ngm is lower than eta ',i,j,zs(i,j),ngmz(i,j),
        265 |     1     ps(i,j),ts(i,j),tsold(i,j)
        266 |         PSNEW(I,J) = PS(I,J)
        267 |         TSNEW(I,J) = TS(I,J)
        268 | 2600    CONTINUE
        269 |         IF(NGMZ(I,J).GE.1000. .OR.
        270 |     1      I.EQ.IPT .AND.J.EQ.JPT) THEN
        271 |c        IF(MOD(I,5).EQ.0 .AND. MOD(J,5).EQ.0) THEN
        272 |           WRITE(6,160)I,J,PS(I,J),PSNEW(I,J),TS(I,J),
        273 |     1       TSNEW(I,J),NGMZ(I,J)
        274 |  160      FORMAT(1X,2I4,5(1X,E12.5))
        275 |         ENDIF
        276 |C
        277 |C   ENDIF ON MASKE
        278 |C
        279 |        ENDIF
        280 |       ENDDO
        281 |      ENDDO
        282 |      write(6,*)' done with surface pressure computation'
        283 |C
        284 |C  COMPUTE PRESSURE/LOG OF PRESSURE ON NGM SIGMA INTERFACES/MIDLAYERS
        285 |C  USING RECOMPUTED SURFACE PRESSURE
        286 |C
        287 |      DO K = 1, IKM + 1
        288 |       DO J = 1, JLIM
        289 |        DO I = 1, ILIM
        290 |         IF(MASKE(I,J).EQ.1) THEN
        291 |          IF(K .LE. IKM) THEN
        292 |           PINT(I,J,K) = SIGINT(K) * PSNEW(I,J)
