        337 |CTEST
        338 |C
        339 |C  GET HEIGHTS/TEMPS ON NGM MID-LAYERS
        340 |C
        341 |      DO J = 1, JLIM
        342 |       DO I = 1, ILIM
        343 |        IF(MASKE(I,J).EQ.1) THEN
        344 |c        print *,'start of hgt/temp loop ',i,j
