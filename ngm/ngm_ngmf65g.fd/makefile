# Threaded object files
OBJST=	LWCRUNCH.o ONEGRID.o

#After definitions, dependencies:

$(CMD):	$(OBJS) $(OBJST)
	$(FC) -o $@ $(OBJS) $(OBJST) $(LIBS)

$(OBJS): parmodel 

$(OBJST): parmodel

## The following two are equivalent (?) and 2 will take precedence over 1 if both present
%.o : %.f
	$(FC) $(FFLAGS) -c $<
.f.o:
	$(FC) $(FFLAGS) -c $<
