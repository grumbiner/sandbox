         33 |         XMIN=1.E30
         34 |         YMAX=-1.E30
         35 |         YMIN=1.E30
         36 |         DO 100 I=1,N
         37 |           XMAX=MAX(X(I),XMAX)
         38 |           XMIN=MIN(X(I),XMIN)
         39 |           YMAX=MAX(Y(I),YMAX)
         40 |           YMIN=MIN(Y(I),YMIN)
         41 |100      CONTINUE
         42 |         XYMAX=MAX(ABS(XMAX),ABS(XMIN),ABS(YMAX),ABS(YMIN))
         43 |         EPS=1.E-10*XYMAX
         44 |         DO 200 I=1,N
         45 |           IF(X(I).EQ.0.) X(I)=EPS
         46 |           ALPHA(I)=ATAN2(Y(I),X(I))
         47 |200      CONTINUE
         48 |       RETURN
         49 |       END
 
