  1      13 |     *     IDPFILLR=(4*IKM+1)*INSUMG+       3*(IIJKMAX*4+IIJMAX) )
  1      14 |C    *     IDPFILLR=(4*IKM+1)*INSUMG+INGRDUSE*(IIJKMAX*4+IIJMAX) )
         32 |         REAL RFOP(1),RCINVOP(1)
         33 |         real SCALZD(1),UNSCLZD(1),FOP(1),CINVOP(1)
         34 |         real BOP(1),COP(1)
         35 |         real BSHAT(1),BVHAT(1),BVIHAT(1)
         36 |C--------
         37 |         COMMON/DAVEBUFF/ COF(INCOEFS,INUMCOEF),GRD(INLOLA,INUMGRID)
         38 |         real COF,GRD
         39 |         REAL RCF(INCOEFS,2)
         40 |         EQUIVALENCE (COF,RCF)
         41 |C--------
         42 |         CALL getcoef(IARL,1)
         43 |         CALL getcoef(IARN,1)
         44 |         CALL getcoef(IRF,1)
         45 |         IAD=1
         46 |         DO 200 M=0,JCAP
         47 |           DO 100 L=0,JCAP-M
         48 |             N=M+L
         49 |             RCF(IAD,IARL)=L
         50 |             RCF(IAD+1,IARL)=L
         51 |             RCF(IAD,IARN)=N
         52 |             RCF(IAD+1,IARN)=N
         53 |             IAD=IAD+2
         54 |100        CONTINUE
         55 |200      CONTINUE
         56 |         ITOT=IAD-1
         57 |         NV=(JCAP+1)*(JCAP+2)
         58 |         N3=NV-2*JCAP-2
         59 |         I3=2*JCAP+3
         60 |         do 10002 i=1,nv
         61 |         RCF(i,IRF)=RCF(i,IARN)*(RCF(i,IARN)+1.)
         62 |         COP(i)=SQRT(RCF(i,IRF))
         63 |         UNSCLZD(i)=SQRT(RCF(i,IRF))/6.3712E6
         64 |10002    continue
         65 |         RCF(1,IRF)=0.
         66 |         RCF(2,IRF)=0.
         67 |         do 10004 i=3,nv
         68 |         RCF(i,IRF)=1./RCF(i,IRF)
         69 |10004    continue
         70 |         do 10006 i=1,nv
         71 |         RCINVOP(i)=SQRT(RCF(i,IRF))
         72 |         CINVOP(i)=RCINVOP(i)
         73 |         BOP(i)=2.*RCF(i,IARL)*RCF(i,IRF)
         74 |         SCALZD(i)=6.3712E6*CINVOP(i)
         75 |10006    continue
         76 |        do 10008 i=1,n3
         77 |         RCF(i3+i-1,IRF)=(RCF(i3+i-1,IARN)-RCF(i3+i-1,IARL))
         78 |     *            *(RCF(i3+i-1,IARN)+RCF(i3+i-1,IARL))/
         79 |     *        ((2.*RCF(i3+i-1,IARN)-1.)
         80 |     *        *(2.*RCF(i3+i-1,IARN)+1.))
         81 |         RCF(i3+i-1,IRF)=(RCF(i3+i-1,IARN)+1.)
         82 |     *       *(RCF(i3+i-1,IARN)-1.)*RCF(i3+i-1,IRF)/
         83 |     *           (RCF(i3+i-1,IARN)*RCF(i3+i-1,IARN))
         84 |         RCF(i3+i-1,IRF)=SQRT(RCF(i3+i-1,IRF))
         85 |10008    continue
         86 |         do 10010 i=1,2*jcap+2
         87 |         RFOP(i)=0.
         88 |10010    continue
         89 |         do 10012 i=1,n3
         90 |         RFOP(I3+i-1)=2.*RCF(I3+i-1,IRF)
         91 |10012    continue
         92 |         do 10014 i=1,nv
         93 |         FOP(i)=RFOP(i)
         94 |10014    continue
         95 |         CALL freecoef(IARL,1)
         96 |         CALL freecoef(IARN,1)
         97 |         CALL freecoef(IRF,1)
         98 |         do 10016 i=1,nv
         99 |         BSHAT(i)=0.
        100 |         BVHAT(i)=0.
        101 |         BVIHAT(i)=0.
        102 |10016    continue
        103 |         IAD=1
        104 |         DO 900 MP1=1,JCAP+1
        105 |           BSHAT(IAD)=1.
        106 |           BSHAT(IAD+1)=0.
        107 |           BVHAT(IAD)=1.
        108 |           BVHAT(IAD+1)=0.
        109 |           BVIHAT(IAD)=0.
        110 |           BVIHAT(IAD+1)=1.
        111 |           IAD=IAD+2
        112 |           IF(MP1.LE.JCAP) THEN
        113 |             DO 800 LP1=2,JCAP+2-MP1
        114 |               BSHAT(IAD)=1.
        115 |               BSHAT(IAD+1)=1.
        116 |               BVHAT(IAD)=1.
        117 |               BVHAT(IAD+1)=1.
        118 |               BVIHAT(IAD)=1.
        119 |               BVIHAT(IAD+1)=1.
        120 |               IAD=IAD+2
        121 |800          CONTINUE
        122 |           END IF
        123 |900      CONTINUE
        124 |         BVHAT(1)=0.
        125 |         BVIHAT(2)=0.
