  1       7 |     *        IIORBPL=3,IIORDPL=3,IIORBLP=3,IIORDLP=3,IMCUBIC=1,
  1       8 |     *        INUMVERT=2,INSIG=IKM,INMODES=18,INGMAX=3000,
  1       9 |     *        INKSIG=2,INSIGP1=INSIG+1,INLONEX=INLON+INKLON,
  1      10 |     *        INLATDP=2*INLATH,INLOLA=INLATDP*INLONEX,
  1      11 |     *        IJCAP1DP=IJCAPDP+1,IJCAP12=2*IJCAP1DP,
  1      12 |     *        INCOEFS=IJCAP1DP*(IJCAPDP+2),
  1      13 |     *     IDPFILLR=(4*IKM+1)*INSUMG+       3*(IIJKMAX*4+IIJMAX) )
  1      14 |C    *     IDPFILLR=(4*IKM+1)*INSUMG+INGRDUSE*(IIJKMAX*4+IIJMAX) )
         52 |         REAL UIO(IIJMAX,INUMVERT,INGRDUSE)
         53 |         REAL VIO(IIJMAX,INUMVERT,INGRDUSE)
         54 |         REAL PIO(IIJMAX,INUMVERT,INGRDUSE)
         55 |         double precision RLATH(INLATDP),RLONH(INLONEX)
         56 |         real rlathxy(inlatdp)
         57 |         double precision wgts(inlatdp)
         58 |         REAL XPOLE(1),YPOLE(1)
         59 |         REAL DX(INGRDUSE)
         60 |         REAL RKPO(IIJMAX,16)
         61 |         REAL RKLO(INLATDP,INLONEX,16)
         62 |         REAL RKPOWORK(IIJMAX,4)
         63 |         REAL RKLOWORK(INLATDP,INLONEX,4)
         64 |         REAL XPOLA(IIJMAX),YPOLA(IIJMAX)
         65 |         REAL XLOLA(INLONEX,INLATDP)
         66 |         REAL YLOLA(INLONEX,INLATDP)
         67 |         REAL WPOLA1(IIJMAX),WPOLA2(IIJMAX)
         68 |         REAL WLOLA1(INLATDP,INLONEX)
         69 |         REAL WLOLA2(INLATDP,INLONEX)
         70 |         REAL WLOLA12(INLATDP,INLONEX,2)
         71 |         EQUIVALENCE (WLOLA1(1,1),WLOLA12(1,1,1))
         72 |         EQUIVALENCE (WLOLA2(1,1),WLOLA12(1,1,2))
         73 |         REAL WLOLA34(INLATDP,INLONEX,2)
         74 |         REAL ULALO(INLATDP,INLONEX)
         75 |         REAL VLALO(INLATDP,INLONEX)
         76 |         REAL HLALO(INLATDP,INLONEX)
         77 |         REAL UPOLA(IIJMAX),VPOLA(IIJMAX),HPOLA(IIJMAX)
         78 |         REAL ZHAT(incoefs+1),ZHAT2(incoefs+1)
         79 |         REAL ZHAT3(incoefs+1),ZHAT4(incoefs+1)
         80 |         REAL DHAT(incoefs+1),DHAT2(incoefs+1)
         81 |         REAL DHAT3(incoefs+1),DHAT4(incoefs+1)
         82 |         REAL HHAT(incoefs+1),HHAT2(incoefs+1)
         83 |         REAL HHAT3(incoefs+1),HHAT4(incoefs+1)
         84 |         double precision ap(0:ijcapdp,0:ijcapdp)
         85 |         double precision bp(0:ijcapdp,0:ijcapdp)
         86 |         double precision aqr(0:ijcapdp,0:ijcapdp)
         87 |         double precision bqr(0:ijcapdp,0:ijcapdp)
         88 |         double precision gr(0:ijcapdp,0:ijcapdp)
         89 |         double precision slat(inlath),clat(inlath)
         90 |         double precision pe0(inlath,0:ijcapdp)
         91 |         double precision qe0(inlath,0:ijcapdp)
         92 |         double precision ro0(inlath,0:ijcapdp)
         93 |         real trigs(2*inlonex)
         94 |C--------
         95 |         INTEGER IMG(1),JMG(1)
         96 |         INTEGER IKPO(IIJMAX)
         97 |         INTEGER IKPOWORK(IIJMAX)
         98 |         INTEGER IKLO(INLATDP,INLONEX)
         99 |         INTEGER IKLOWORK(INLATDP,INLONEX),ifax(10)
        100 |         integer irs(0:ijcapdp,0:ijcapdp),iis(0:ijcapdp,0:ijcapdp)
        101 |         LOGICAL BKPO(IIJMAX)
        102 |         LOGICAL BKLO(INLATDP,INLONEX)
        103 |C--------
        104 |         ERAD=6371.22E3
        105 |         OMEGA=7.292116E-5
        106 |         OMEGAINV=1./OMEGA
        107 |         G=9.8
        108 |         GINV=1./G
        109 |         NGRDUSE=INGRDUSE
        110 |         NLON=INLON
        111 |         NLONEX=INLONEX
        112 |         NLAT=INLATDP
        113 |         NLATH=INLATH
        114 |         JCAP=IJCAPDP
        115 |         NCOEFS=INCOEFS
        116 |         NLOLA=NLAT*NLONEX
        117 |         DXA=.5*DXA2
        118 |         DLON=8.*ATAN(1.)/NLON
        119 |         DO 10 NG=1,NGRDUSE
        120 |           DX(NG)=DXA
        121 |           DXA=.5*DXA
        122 |10       CONTINUE
        123 |C--------
        124 |C-------- PICK UP GAUSSIAN LATITUDES FROM SPECTRAL PACKAGE
        125 |         CALL GETLALO(RLATH,RLONH,wgts,jcap,nlath,nlon,nlonex,
        126 |     *      irs,iis,ap,bp,aqr,bqr,gr,slat,clat,pe0,qe0,ro0,trigs,ifax)
        127 |         rlathxy=rlath
        128 |         rlathxy(1)=.5*(rlath(1)+rlath(2))
        129 |C--------
        130 |         DO 9000 K=1,NUMVERT
        131 |C----------
        132 |C---------- 1.  INTERPOLATE FROM POLA GRIDS TO LOLA GRID
        133 |C----------
        134 |           IORDPL = IIORDPL
        135 |           IORBPL = IIORBPL
        136 |           DO 110 NG=1,NGRDUSE
        137 |            IORB = IORDPL
        138 |            IF (NG.EQ.1) IORB = IORBPL
        139 |            DO 110 IORD=IORB,IORDPL
        140 |             CALL HLPO2LOL(UIO(1,K,NG),IMG(NG),JMG(NG),DX(NG),
        141 |     *              XPOLE(NG),YPOLE(NG)-.5,
        142 |     *              ULALO,NLONEX,NLAT,
        143 |     *                     RKLO,IKLO,BKLO,WLOLA1,
        144 |     *         XPOLA,YPOLA,XLOLA,YLOLA,RKLOWORK,IKLOWORK,RLATHxy,DLON,
        145 |     *         IORD)
        146 |             CALL HLPO2LOL(VIO(1,K,NG),IMG(NG),JMG(NG),DX(NG),
        147 |     *              XPOLE(NG)-.5,YPOLE(NG),
        148 |     *              VLALO,NLONEX,NLAT,
        149 |     *                     RKLO,IKLO,BKLO,WLOLA1,
        150 |     *         XPOLA,YPOLA,XLOLA,YLOLA,RKLOWORK,IKLOWORK,RLATHxy,DLON,
        151 |     *         IORD)
        152 |             CALL HLPO2LOL(PIO(1,K,NG),IMG(NG),JMG(NG),DX(NG),
        153 |     *              XPOLE(NG),YPOLE(NG),
        154 |     *              HLALO,NLONEX,NLAT,
        155 |     *                     RKLO,IKLO,BKLO,WLOLA1,
        156 |     *      XPOLA,YPOLA,XLOLA,YLOLA,RKLOWORK,IKLOWORK,RLATHxy,DLON,
        157 |     *         IORD)
        158 |110        CONTINUE
        159 |C----------
        160 |C---------- 2.  ROTATE WINDS
        161 |C----------
        162 |           DO 220 I=1,NLONEX
        163 |             ANGLE=(I-1.)*DLON
        164 |             COSLON=COS(ANGLE)
        165 |             SINLON=SIN(ANGLE)
        166 |             DO 218 J=1,NLAT
        167 |               WLOLA1(J,1)=-ULALO(J,I)*SINLON
        168 |     *                        +VLALO(J,I)*COSLON
        169 |               WLOLA2(J,1)=-ULALO(J,I)*COSLON
        170 |     *                        -VLALO(J,I)*SINLON
        171 |               ULALO(J,I)=WLOLA1(J,1)
        172 |               VLALO(J,I)=WLOLA2(J,1)
        173 |218          CONTINUE
        174 |220        CONTINUE
        175 |C----------
        176 |C---------- 3.  REFLECT ABOUT EQUATOR (U,H SYM, V ASYM)
        177 |C----------
        178 |           DO 320 I=1,NLONEX
        179 |             DO 310 J=NLATH+1,NLAT
        180 |               JS=NLAT+2-J
        181 |               HLALO(JS,I)=HLALO(J,I)
        182 |               ULALO(JS,I)=ULALO(J,I)
        183 |               VLALO(JS,I)=-VLALO(J,I)
        184 |310          CONTINUE
        185 |320        CONTINUE
        186 |C----------     DIVIDE BY G TO GET HEIGHT TENDENCY
        187 |           DO 322 I=1,NLOLA
        188 |             HLALO(I,1)=GINV*HLALO(I,1)
        189 |322        CONTINUE
        190 |C----------
        191 |C---------- 4.  TRANSFORM FROM GRID TO SPECTRAL, AND SCALE
        192 |C----------
        193 |           CALL GETDEPTH(DEPTH,K)
        194 |           call g2s1(zhat2,dhat2,hhat2,zhat,dhat,hhat,
        195 |     *       ulalo,vlalo,hlalo,rklowork,rklowork(1,1,2),rklowork(1,1,3),
        196 |     *      1,jcap,nlon,nlath,wgts,irs,iis,ap,bp,aqr,bqr,gr,slat,clat,
        197 |     *         pe0,qe0,ro0,trigs,ifax)
        198 |c          CALL SGRD2COF(HLALO,HHAT2,ITRANS,JCAP,NLATH,NLON)
        199 |c         CALL VGRD2COF(ULALO,VLALO,DHAT2,ZHAT2,ITRANS,JCAP,NLATH,NLON)
        200 |           CALL SCALCOFS(ZHAT2,DHAT2,HHAT2,ZHAT,DHAT,HHAT,DEPTH,JCAP)
        201 |C----------
        202 |C---------- 5.  EXTRACT FAST MODE TENDENCIES (ONLY FOR DIAGNOSTICS)
        203 |C----------
        204 |           CALL SANDF(ZHAT,DHAT,HHAT,ZHAT2,DHAT2,HHAT2,DEPTH,JCAP)
        205 |C----------  SLOW MODES FIRST
        206 |           DO 502 I=1,NCOEFS
        207 |             ZHAT3(I)=ZHAT2(I)
        208 |             DHAT3(I)=0.
        209 |             HHAT3(I)=0.
        210 |502        CONTINUE
        211 |           TRANS=1.
        212 |           CALL SPLUSF(ZHAT3,DHAT3,HHAT3,ZHAT4,DHAT4,HHAT4,DEPTH,TRANS,
        213 |     *                     JCAP)
        214 |           ISLOW=1
        215 |           CALL BALDIAG(ZHAT4,DHAT4,HHAT4,K,LOOP,ISLOW,JCAP)
        216 |C----------  NOW FAST MODES
        217 |           DO 504 I=1,NCOEFS
        218 |             ZHAT3(I)=0.
        219 |             DHAT3(I)=DHAT2(I)
        220 |             HHAT3(I)=HHAT2(I)
        221 |504        CONTINUE
        222 |           CALL SPLUSF(ZHAT3,DHAT3,HHAT3,ZHAT4,DHAT4,HHAT4,DEPTH,TRANS,
        223 |     *                     JCAP)
        224 |           ISLOW=2
        225 |           CALL BALDIAG(ZHAT4,DHAT4,HHAT4,K,LOOP,ISLOW,JCAP)
        226 |C----------
        227 |C---------- OBTAIN BALANCE INCREMENT
        228 |C----------
        229 |           CALL BALTEND(ZHAT,DHAT,HHAT,ZHAT2,DHAT2,HHAT2,JCAP,DEPTH)
        230 |           CALL USCLCOFS(ZHAT2,DHAT2,HHAT2,ZHAT,DHAT,HHAT,DEPTH,JCAP)
        231 |C----------
        232 |C---------- 8.  TRANSFORM FROM SPECTRAL TO GRID
        233 |C----------
        234 |           call s2g1(zhat,dhat,hhat,zhat2,dhat2,hhat2,
        235 |     *       ulalo,vlalo,hlalo,rklowork,rklowork(1,1,2),rklowork(1,1,3),
        236 |     *        1,jcap,nlon,nlath,irs,iis,ap,bp,aqr,bqr,gr,slat,clat,
        237 |     *         pe0,qe0,ro0,trigs,ifax)
        238 |           do 820 i=nlon+1,nlonex
        239 |             do 810 j=1,nlat
        240 |               ulalo(j,i)=ulalo(j,i-nlon)
        241 |               vlalo(j,i)=vlalo(j,i-nlon)
        242 |               hlalo(j,i)=hlalo(j,i-nlon)
        243 |810          continue
        244 |820        continue
        245 |c          CALL SCOF2GRD(HHAT,HLALO,JCAP,NLATH,NLON)
        246 |c          CALL VCOF2GRD(DHAT,ZHAT,ULALO,VLALO,JCAP,NLATH,NLON)
        247 |C----------
        248 |C---------- 9.  ROTATE WINDS FROM LOLA BACK TO POLA
        249 |C----------
        250 |           DO 920 I=1,NLONEX
        251 |             ANGLE=(I-1.)*DLON
        252 |             COSLON=COS(ANGLE)
        253 |             SINLON=SIN(ANGLE)
        254 |             DO 918 J=1,NLAT
        255 |               WLOLA1(J,1)=-ULALO(J,I)*SINLON
        256 |     *                        -VLALO(J,I)*COSLON
        257 |               WLOLA2(J,1)=ULALO(J,I)*COSLON
        258 |     *                        -VLALO(J,I)*SINLON
        259 |               ULALO(J,I)=WLOLA1(J,1)
        260 |               VLALO(J,I)=WLOLA2(J,1)
        261 |918          CONTINUE
        262 |920        CONTINUE
        263 |C---------- MULTIPLY H BY G TO GET GEOPOT INCREMENT
        264 |           DO 922 I=1,NLOLA
        265 |             HLALO(I,1)=G*HLALO(I,1)
        266 |922        CONTINUE
        267 |C----------
        268 |C---------- 10. INTERPOLATE LOLA GRID TO POLA GRIDS
        269 |C----------
        270 |           IORDLP = IIORDLP
        271 |           IORBLP = IIORBLP
        272 |           DO 1010 NG=1,NGRDUSE
        273 |            DO 1010 IORD=IORBLP,IORDLP
        274 |             CALL HLLO2POL(ULALO,NLONEX,NLAT,
        275 |     *           UIO(1,K,NG),IMG(NG),JMG(NG),DX(NG),
        276 |     *           XPOLE(NG),YPOLE(NG)-.5,RKPO,IKPO,BKPO,WLOLA1,
        277 |     *         XLOLA,YLOLA,XPOLA,YPOLA,RKPOWORK,IKPOWORK,rlathxy,DLON,
        278 |     *           IORD)
        279 |             CALL HLLO2POL(VLALO,NLONEX,NLAT,
        280 |     *           VIO(1,K,NG),IMG(NG),JMG(NG),DX(NG),
        281 |     *           XPOLE(NG)-.5,YPOLE(NG),RKPO,IKPO,BKPO,WLOLA1,
        282 |     *         XLOLA,YLOLA,XPOLA,YPOLA,RKPOWORK,IKPOWORK,rlathxy,DLON,
        283 |     *           IORD)
        284 |             CALL HLLO2POL(HLALO,NLONEX,NLAT,
        285 |     *           PIO(1,K,NG),IMG(NG),JMG(NG),DX(NG),
        286 |     *           XPOLE(NG),YPOLE(NG),RKPO,IKPO,BKPO,WLOLA1,
        287 |     *         XLOLA,YLOLA,XPOLA,YPOLA,RKPOWORK,IKPOWORK,rlathxy,DLON,
        288 |     *           IORD)
        289 |1010       CONTINUE
        290 |9000     CONTINUE
        291 |C        CALL BALPRNT(LOOP,NUMVERT)
        292 |       RETURN
        293 |       END
 
