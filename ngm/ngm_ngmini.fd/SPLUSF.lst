          1 |       SUBROUTINE SPLUSF(ZS,DF,HF,Z,D,H,DEPTH,TRANS,JCAPIN)
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .                                       .
          4 |C SUBPROGRAM:    SPLUSF      SLOW-AND-FAST TO SLOW+FAST TRANSFORM
          5 |C   PRGMMR: PARRISH          ORG: W/NMC22    DATE: 88-06-13
          6 |C
          7 |C ABSTRACT: GO FROM TEMPERTON SLOW-AND-FAST DECOMPOSITION TO FULL
          8 |C   VARIABLES (OR APPLY TRANSPOSE OF THIS OPERATION IF TRANS=-1).
          9 |C
         10 |C PROGRAM HISTORY LOG:
         11 |C   88-06-13  PARRISH
         12 |C
         13 |C USAGE:    CALL SPLUSF(ZS,DF,HF,Z,D,H,DEPTH,TRANS,JCAPIN)
         14 |C   INPUT ARGUMENT LIST:
         15 |C     ZS       - SLOW COMPONENT OF VORTICITY.
         16 |C     DF       - FAST COMPONENT OF DIVERGENCE.
         17 |C     HF       - FAST HEIGHT COMPONENT
         18 |C     DEPTH    - SCALE HEIGHT.
         19 |C     TRANS    - =1: SLOW-AND-FAST TRANSFORMED TO SLOW + FAST
         20 |C              - =-1: TRANSPOSE OPERATOR.
         21 |C     JCAPIN   - TRIANGULAR TRUNCATION
         22 |C
         23 |C   OUTPUT ARGUMENT LIST:
         24 |C     Z        - SLOW + FAST VORTICITY
         25 |C     D        - FAST DIVERGENCE (NOT CHANGED FROM INPUT--SLOW DIV=0)
         26 |C     H        - SLOW + FAST HEIGHT
         27 |C
         28 |C REMARKS: MUST INCLUDE BLOCK DATA BLOCKSP02 AND DEFINITION OF
         29 |C           PL1 VARIABLES BEFORE COMPILING.
         30 |C
         31 |C ATTRIBUTES:
         32 |C   LANGUAGE: FORTRAN200
         33 |C   MACHINE:  CYBER
         34 |C
         35 |C$$$
         36 |         include "myparam"
  1       1 |        INCLUDE 'parmodel'
  2       1 |C     PARAMETER FILE
  2       2 |C
  2       3 |C     SETUP FOR IBM SINGLE PRECISION REPRESENTATION
  2       4 |C     ON OUTPUT
  2       5 |C                      THIS VERSION 21 February 1991 (DiMego)
  2       6 |C
  2       7 |      PARAMETER (
  2       8 |     *      IBPW      =      4,
  2       9 |     *      IBPOR     = 120000,
  2      10 |     *      INUMREC   = IBPOR / IBPW,
  2      11 |     *      INSPECS   =    700,
  2      12 |     *      ILATG2    =    120,
  2      13 |     *      ILEVH     =     12,
  2      14 |     *      ILEVS     =     16,
  2      15 |     *      ILONF     =    360,
  2      16 |     *      INLATSH   =      5,
  2      17 |     *      IIJMAX    =  23668,
  2      18 |     *      IIJMAXB   =  25536,
  2      19 |     *      IIJRAD    =    892,
  2      20 |     *      IJCAP     =     80,
  2      21 |     *      IWPCPL    =     80,
  2      22 |     *      IKM       =     16 )
  2      23 |      PARAMETER (
  2      24 |     *      ILBCH     =   7*(147+111-7),
  2      25 |     *      ILBHI     =    (147+111-26),
  2      26 |     *      ILBUI     =    (2*147+111-39),
  2      27 |     *      ILBUO     =   (8*147+7*111-56),
  2      28 |     *      ILBVI     =    (147+2*111-39),
  2      29 |     *      ILBVO     =   (7*147+8*111-56),
  2      30 |     *      ILMOUT    =     20 )
  2      31 |      PARAMETER (
  2      32 |     *      IMAXOUTF  =     17,
  2      33 |     *      IMAXOUTT  =      3,
  2      34 |     *      IMAXOUTV  =    100,
  2      35 |     *      IMAXPROF  =      6,
  2      36 |     *      INFILLER  =  10000,
  2      37 |     *      INH       =     65,
  2      38 |     *      INGRDUSE  =      2,
  2      39 |     *      INGRDUS1  =      1,
  2      40 |     *      INIADDRS  =     44,
  2      41 |     *      INSCR     =     23,
  2      42 |     *      INSCRGEO  =     12,
  2      43 |     *      INSCR3    =     10,
  2      44 |     *      INSOIL    =      2,
  2      45 |     *      INSUMG    =  42164,
  2      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  2      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  2      48 |     *      INSHEM    = 13 * INH + 23,
  2      49 |     *      IIJKMAX   = IKM * IIJMAX )
  2      50 |      PARAMETER (
  2      51 |     *  IIWORDSB  = IIJMAXB / 64,
  2      52 |     *  IKMP1     = IKM + 1,
  2      53 |     *  ILVLTOT   = 7 * IKM + 37,
  2      54 |     *  ILONFP1   = ILONF + 1,
  2      55 |     *  INVBL     = ILVLTOT * INSUMG,
  2      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  2      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  2      58 |     *  IBLK1D    =  2 * IKM + 14,
  2      59 |     *  IBLK2D    = 16 * IKM + 36,
  2      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  2      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  2      62 |C
  1       2 |C-------
  1       3 |C------- FOLLOWING IS EXTRA STUFF FOR INITIALIZATION
  1       4 |C-------
  1       5 |         PARAMETER (INLATH=101,INUMCOEF=3*INLATH+35,
  1       6 |     *        INUMGRID=4,IJCAPDP=80,INLON=300,INKLON=2,INKLAT=2,
  1       7 |     *        IIORBPL=3,IIORDPL=3,IIORBLP=3,IIORDLP=3,IMCUBIC=1,
  1       8 |     *        INUMVERT=2,INSIG=IKM,INMODES=18,INGMAX=3000,
  1       9 |     *        INKSIG=2,INSIGP1=INSIG+1,INLONEX=INLON+INKLON,
  1      10 |     *        INLATDP=2*INLATH,INLOLA=INLATDP*INLONEX,
  1      11 |     *        IJCAP1DP=IJCAPDP+1,IJCAP12=2*IJCAP1DP,
  1      12 |     *        INCOEFS=IJCAP1DP*(IJCAPDP+2),
  1      13 |     *     IDPFILLR=(4*IKM+1)*INSUMG+       3*(IIJKMAX*4+IIJMAX) )
  1      14 |C    *     IDPFILLR=(4*IKM+1)*INSUMG+INGRDUSE*(IIJKMAX*4+IIJMAX) )
         37 |C--------
         38 |         real ZS(1),DF(1),HF(1),Z(1),D(1),H(1)
         39 |C--------
         40 |         LOGICAL BWORK(INCOEFS)
         41 |C--------
         42 |         COMMON/DAVEBUFF/ COF(INCOEFS,INUMCOEF),GRD(INLOLA,INUMGRID)
         43 |         real COF,GRD
         44 |C--------
         45 |         COMMON/FIXSP02/JCAP,NCOEFS,NFOP,JFOP,
         46 |     *      ISCALZD,IUNSCLZD,
         47 |     *      IBSHAT,IBVHAT,IBVIHAT,
         48 |     *      IBOP,ICOP,IFOP,ICINVOP,
         49 |     *      IRFOP,IRCINVOP,
         50 |     *      BFOP(INCOEFS),BONES(INCOEFS)
         51 |         LOGICAL BFOP,BONES
         52 |C--------
         53 |C-------- CHECK TO SEE IF INITIALIZATION OF INTERNAL VARIABLES NEEDED
         54 |         IF(JCAP.NE.JCAPIN) CALL SETSP02(JCAPIN)
         55 |C--------
         56 |C-------- MAKE SURE ZONAL PART OF COEFS HAS PROPER FORM
         57 |C--------
         58 |         do 10002 i=1,ncoefs
         59 |         ZS(i)=COF(i,IBVHAT)*ZS(i)
         60 |         DF(i)=COF(i,IBVIHAT)*DF(i)
         61 |         HF(i)=COF(i,IBSHAT)*HF(i)
         62 |10002    continue
         63 |         C0INV=6.3712E6*7.2921E-5/SQRT(9.8062*DEPTH)
         64 |C--------
         65 |C-------- VORTICITY FIRST
         66 |C--------
         67 |         do 10004 i=1,ncoefs
         68 |         D(i)=COF(i,ICINVOP)*HF(i)
         69 |         D(i)=C0INV*D(i)
         70 |10004    continue
         71 |         CALL GETCOEF(IWORK,1)
         72 |         CALL FOPMULT(D,Z,COF(1,IFOP),NCOEFS,NFOP,JFOP,COF(1,IWORK),
         73 |     *                BFOP,BONES,BWORK)
         74 |         do 10006 i=1,ncoefs
         75 |         Z(i)=ZS(i)+TRANS*Z(i)
         76 |10006    continue
         77 |C--------
         78 |C-------- HEIGHT NEXT
         79 |C--------
         80 |         CALL FOPMULT(ZS,D,COF(1,IFOP),NCOEFS,NFOP,JFOP,COF(1,IWORK),
         81 |     *                BFOP,BONES,BWORK)
         82 |         do 10008 i=1,ncoefs
         83 |         H(i)=COF(i,ICINVOP)*D(i)
         84 |         H(i)=C0INV*H(i)
         85 |         H(i)=HF(i)-TRANS*H(i)
         86 |10008    continue
         87 |C--------
         88 |C-------- FINALLY DIVERGENCE
         89 |C--------
         90 |         do 10010 i=1,ncoefs
         91 |         D(i)=DF(i)
         92 |10010    continue
         93 |         CALL FREECOEF(IWORK,1)
         94 |C--------
         95 |C-------- MAKE SURE ZONAL PART OF COEFS HAS PROPER FORM
         96 |C--------
         97 |         do 10012 i=1,ncoefs
         98 |         Z(i)=COF(i,IBVHAT)*Z(i)
         99 |         D(i)=COF(i,IBVIHAT)*D(i)
        100 |         H(i)=COF(i,IBSHAT)*H(i)
        101 |10012    continue
        102 |       RETURN
        103 |       END
 
