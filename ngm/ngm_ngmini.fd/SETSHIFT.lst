  1      10 |     *        INLATDP=2*INLATH,INLOLA=INLATDP*INLONEX,
  1      11 |     *        IJCAP1DP=IJCAPDP+1,IJCAP12=2*IJCAP1DP,
  1      12 |     *        INCOEFS=IJCAP1DP*(IJCAPDP+2),
  1      13 |     *     IDPFILLR=(4*IKM+1)*INSUMG+       3*(IIJKMAX*4+IIJMAX) )
  1      14 |C    *     IDPFILLR=(4*IKM+1)*INSUMG+INGRDUSE*(IIJKMAX*4+IIJMAX) )
         52 |         LOGICAL BITA(1)
         53 |C--------
         54 |         IMIN=MAX(1,1-L1,1-L2,1-L3,1-L4,1-L5)
         55 |         JMIN=MAX(1,1-M1,1-M2,1-M3,1-M4,1-M5)
         56 |         IMAX=MIN(IBIG,IBIG-L1,IBIG-L2,IBIG-L3,IBIG-L4,IBIG-L5)
         57 |         JMAX=MIN(JBIG,JBIG-M1,JBIG-M2,JBIG-M3,JBIG-M4,JBIG-M5)
         58 |         ILEN=IMAX-IMIN+1
         59 |         LEN=ILEN+IBIG*(JMAX-JMIN)
         60 |         IS1=IMIN+L1+IBIG*(JMIN+M1-1)
         61 |         IS2=IMIN+L2+IBIG*(JMIN+M2-1)
         62 |         IS3=IMIN+L3+IBIG*(JMIN+M3-1)
         63 |         IS4=IMIN+L4+IBIG*(JMIN+M4-1)
         64 |         IS5=IMIN+L5+IBIG*(JMIN+M5-1)
         65 |         DO 50 I=1,LEN
         66 |           BITA(I)=.FALSE.
         67 |50       CONTINUE
         68 |         II=0
         69 |         DO 100 I=1,LEN
         70 |           II=MOD(II,IBIG)+1
         71 |           IF(II.LE.ILEN) BITA(I)=.TRUE.
         72 |100      CONTINUE
         73 |       RETURN
         74 |       END
 
