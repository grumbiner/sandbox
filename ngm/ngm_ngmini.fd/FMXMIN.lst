          1 |       SUBROUTINE FMXMIN(F,N,MESAGE)
          2 |C--------
          3 |         CHARACTER*1 MESAGE(80),DOLLAR
          4 |         DATA DOLLAR/'$'/
          5 |C--------
          6 |         DIMENSION F(N)
          7 |C--------
          8 |         IFMAX=MIN(5,N)
          9 |         FMAX=-1.E15
         10 |         FMIN=1.E15
         11 |         FBAR=0.
         12 |         FRMS=0.
         13 |         RN=MAX(1,N)
         14 |         DO 100 I=1,N
         15 |           FMAX=MAX(F(I),FMAX)
         16 |           FMIN=MIN(F(I),FMIN)
         17 |           FBAR=FBAR+F(I)
         18 |           FRMS=FRMS+F(I)**2
         19 |100      CONTINUE
         20 |         FBAR=FBAR/RN
         21 |         FRMS=SQRT(FRMS/RN)
         22 |         IP=0
         23 |         DO 200 I=1,80
         24 |           IP=IP+1
         25 |           IF(MESAGE(I).EQ.DOLLAR) GO TO 210
         26 |200      CONTINUE
         27 |210      CONTINUE
         28 |         IP=IP-1
         29 |         IP=MAX(IP,1)
         30 |         WRITE(6,300)(MESAGE(I),I=1,IP)
         31 |300      FORMAT(1H ,80A1)
         32 |         WRITE(6,400)FMAX,FMIN,FBAR,FRMS,N
         33 |400      FORMAT(' FMX,MN,BR,RMS=',4E12.4, 'NUM PTS=',I6)
         34 |         IF(IFMAX.GT.0)
         35 |     *       WRITE(6,500)(F(I),I=1,IFMAX)
         36 |500      FORMAT(1H ,5E15.5)
         37 |       RETURN
         38 |       END
 
