  1      14 |C    *     IDPFILLR=(4*IKM+1)*INSUMG+INGRDUSE*(IIJKMAX*4+IIJMAX) )
         26 |C     COMMON BLOCK /COMCONST/ CONTAINS GRID-RELATED PARAMETERS,
         27 |C          AND A FEW OTHER COMMON CONSTANTS.
         28 |      COMMON /COMCONST/ IJMAX, KM, LVLTOT, IJRAD, LOFCLDS(2,4),
         29 |     1                  ICALLRAD, IPHYSPL, NGRDUSE, NH,
         30 |     2                  NTIME, ITIME, NSTEPS,
         31 |     3                  IMG(INGRDUSE), JMG(INGRDUSE),
         32 |     4                  IAG(INGRDUS1), JAG(INGRDUS1),
         33 |     5                  IBG(INGRDUS1), JBG(INGRDUS1),
         34 |     6                  IADDRG(INIADDRS, INGRDUSE),
         35 |     7                  NPTSFH(2, INGRDUSE),
         36 |     8                  KUMULUS, LGRIDPPT, KLIFT1, KLIFT2, IBUCKET,
         37 |     9                  XPOLEH(INGRDUSE), YPOLEH(INGRDUSE), RADIUS,
         38 |     1                  DELSIG(IKM), PR(IKM), PRESS(IKM),
         39 |     2                  SIGINT(IKMP1),
         40 |     3                  DTOVDX, ANGVEL,
         41 |     4                  SIGMACC, SIGMAGSP, SIGMADHQ, CRITCONV,
         42 |     5                  SATDEL, RHFACTOR, QBOUND,
         43 |     6                  ANEM, BLKDR, CHARN, CONAUST, DDORF, PKATO,
         44 |     7                  SCALEHT, SIGDOT, DLAMNGM
         45 |C
         46 |      COMMON            SCR     (IIJMAX,  INSCR),
         47 |     1                  SCRGEOG (IIJMAX,  INSCRGEO),
         48 |     2                  SCR3    (IIJKMAX, INSCR3),
         49 |     3                  FILLER  (INFILLER),
         50 |     4                  VBL     (INVBL),
         51 |     5                  BITGRDH (IIJMAX, 2, INGRDUSE),
         52 |     6                  BITGRDU (IIJMAX, 2, INGRDUSE),
         53 |     7                  BITGRDV (IIJMAX, 2, INGRDUSE),
         54 |     8                  BITSEA  (IIJMAX, INGRDUSE),
         55 |     9                  BITSNO  (IIJMAX, INGRDUSE),
         56 |     1                  BITWVL  (IIJMAX, INGRDUSE)
         57 |C
         58 |      LOGICAL BITGRDH, BITGRDU, BITGRDV, BITSEA, BITSNO, BITWVL
         59 |C
         60 |C     COMMON BLOCK /COMDIAG/ CONTAINS QUANTITIES SAVED FOR
         61 |C          DIAGNOSTIC PURPOSES.
         62 |      COMMON /COMDIAG/  KADIAB( IKM, INGRDUSE ),
         63 |     1                  NPTSDHQ (2, INGRDUSE),
         64 |     2                  KKCLOUD2(2, INGRDUSE), NPTSBUOY(2, INGRDUSE),
         65 |     3                  NPTSWATR(2, INGRDUSE), NPTSCC  (2, INGRDUSE),
         66 |     4                  KKGSP2  (2, INGRDUSE),
         67 |     5                  KKGSE2  (2, INGRDUSE),
         68 |     6                  NPTSSAT (IKM, 2, INGRDUSE),
         69 |     7                  NPTSEVAP(IKM, 2, INGRDUSE),
         70 |     8                  NPTSGSP (2, INGRDUSE), NUMPROF,
         71 |     9                  PPTCC   (2, INGRDUSE), PPTGSP  (2, INGRDUSE),
         72 |     1                  ALATPR  (IMAXPROF),    ALONPR  (IMAXPROF),
         73 |     2                  DESCRP  (IMAXPROF)
         74 |C
         75 |      CHARACTER*24 DESCRP
         76 |C
         77 |C     COMMON BLOCK /COMOUT/ CONTAINS OUTPUT CODE PARAMETERS
         78 |C               AND ARRAYS.
         79 |C
         80 |      CHARACTER*20 DESCRIPT
         81 |C          DESCRIPTORS USED IN SR  OUTHORIZ
         82 |C
         83 |      COMMON /COMOUT/
         84 |C
         85 |C          THE FOLLOWING ARE STORED BY OUT2FILE FROM DATA CARDS.
         86 |C          (THE LAST 6 WILL BE OVERWRITTEN BY  OUTPUT  IF KGTYPE
         87 |C          IS 5 OR 26 (I.E. AN LFM GRID DEFINED IN TABLE 7 OF
         88 |C          OFFICE NOTE 84.))
         89 |C
         90 |     1     INUNIT, IOUTUNIT,
         91 |     2     KGTYPE, IMOUT, JMOUT, DNPEQOUT, DLAMOUT, XIPOUT, YJPOUT,
         92 |C
         93 |C          THE FOLLOWING COME FROM  MAIN  READING THE DATA CARDS.
         94 |     2   LQTYPE(IMAXOUTV),   LSTYPE(IMAXOUTV),
         95 |     3   DESCRIPT(IMAXOUTV), LEVELS(ILMOUT,IMAXOUTV),  NOUTVBLS,
         96 |C
         97 |C          THE FOLLOWING ARE DERIVED BY   OUTPUT  .
         98 |     4       IJOUT, LMOUT, NFILLER, NS2, NS3NGM, NS3OUT,
         99 |     5       COSOUT,   SINOUT,    DXOUT,    ZSIGLYR(IKM),
        100 |C
        101 |C           ARRAYS  PLOUT  AND  HTFD  ARE SET BY DATA STATEMENTS IN
        102 |C           THE SR  OUTPUT  .  'PADDER' FILLS OUT TO FULL WORD BNDRY.
        103 |     6      PLOUT(ILMOUT),   HTFD( 6),  PADDER,
        104 |C
        105 |C          THE FOLLOWING ADDRESSES AND ADDRESS ARRAYS ARE SET UP
        106 |C          BY   OUTPREP  WHEN IT IS CALLED BY   OUTPUT  .
        107 |     7  J1(INGRDUSE),          J2(INGRDUSE),   JADDK2L(INGRDUSE,2),
        108 |     8  JADDFNGM(INGRDUSE), JHREC(INGRDUSE),   JHKAP(INGRDUSE),
        109 |     9  JSCR2(12),    JSCR3NGM(INGRDUSE,12),   JSCR3OUT(12),
        110 |     A  LENGD(INGRDUSE), LTYPE(ILMOUT,INGRDUSE), MB1, MB2, MINT,
        111 |     B   MSOUT1,  MSOUT2,  MZGNDOUT,  MNGM3,  MHOUT, MZFR, MZTRO,
        112 |     C      MSZGND(INGRDUSE), NGRIDATA(INGRDUSE),
        113 |C
        114 |C            NUMGDREQ, THE NUMBER OF NGM GRIDS NEEDED TO COVER THE
        115 |C            OUTPUT GRID, AND ZLOUT, ARE SET BY  OUTPREP  .
        116 |     D                NUMGDREQ,    ZLOUT(ILMOUT),
        117 |C
        118 |C          OUTPREP ALSO ASSIGNS THE FOLLOWING DESCRIPTORS TO BE
        119 |C          USED BY  OUTHORIZ  FOR HORIZONTAL INTERPOLATION.
        120 |     E    AHORD,BHORD,CHORD,DHORD, ILLD,IULD,  SCRHORD,  OUTHORD,
        121 |C
        122 |C
        123 |C          THE FOLLOWING ARRAY CONTAINS THE INTEGERS NEEDED TO
        124 |C          FORM THE LABEL THAT GOES WITH EACH OUTPUT ARRAY.
        125 |     F               LABEL84( 27 ),
        126 |C
        127 |C          THE FOLLOWING SYMBOLS REPRESENT TEMPORARY VALUES OF
        128 |C          INTEGERS TO GO INTO  LABEL84 WHEN THE OUTPUT ARRAY
        129 |C          HAS BEEN PREPARED FOR PACKING.
        130 |C
        131 |     G  LABQTYPE, LABSSUB1, LABTMARK,  S1VALUE,  S2VALUE,
        132 |     H  LABMLDIF, LABSSUB2, LABFSUB2, LABKGRID, LABNUMPT
        133 |C
        134 |C
        135 |      COMMON /COM1WAY/ ITIMSTEP, ITBOUND, INGLB
        136 |         REAL W0U(IIJMAX,1),W0V(IIJMAX,1)
        137 |         REAL W0TH(IIJMAX,1)
        138 |C--------
        139 |         IJ=IMG(NG)*JMG(NG)
        140 |         MADDU=IADDRG(1,NG)-1
        141 |         MADDV=IADDRG(2,NG)-1
        142 |         MADDT=IADDRG(3,NG)-1
        143 |         DO 100 K=1,INUMVERT
        144 |           DO 50 I=1,IJ
        145 |             VBL(MADDU+I)=W0U(I,K)
        146 |             VBL(MADDV+I)=W0V(I,K)
        147 |             VBL(MADDT+I)=W0TH(I,K)
        148 |50         CONTINUE
        149 |           MADDU=MADDU+IJ
        150 |           MADDV=MADDV+IJ
        151 |           MADDT=MADDT+IJ
        152 |100      CONTINUE
        153 |       RETURN
        154 |       END
 
