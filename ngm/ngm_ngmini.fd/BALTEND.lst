          1 |       SUBROUTINE BALTEND(ZT,DT,HT,ZCOR,DCOR,HCOR,JCAPIN,DEPTH)
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .                                       .
          4 |C SUBPROGRAM:    BALTEND     FIND CORRECTION TO ZERO GRAV TEND
          5 |C   PRGMMR: PARRISH          ORG: W/NMC22    DATE: 88-06-13
          6 |C
          7 |C ABSTRACT: GIVEN TENDENCIES, OBTAIN CORRECTION TO STATE VECTOR
          8 |C   WHICH ZEROES GRAVITY COMPONENT OF TENDENCY.
          9 |C
         10 |C PROGRAM HISTORY LOG:
         11 |C   88-06-13  PARRISH
         12 |C
         13 |C USAGE:    CALL BALTEND(ZT,DT,HT,ZCOR,DCOR,HCOR,JCAPIN,DEPTH)
         14 |C   INPUT ARGUMENT LIST:
         15 |C     ZT       - VORTICITY TEND COEFFICIENTS.
         16 |C     DT       - DIVERGENCE TEND COEFFICIENTS.
         17 |C     HT       - HEIGHT TEND COEFFICIENTS.
         18 |C     JCAPIN   - TRIANGULAR TRUNCATION
         19 |C     DEPTH    - SCALE HEIGHT.
         20 |C
         21 |C   OUTPUT ARGUMENT LIST:      (INCLUDING WORK ARRAYS)
         22 |C     ZCOR     - CORRECTION VORTICITY COEFFICIENTS.
         23 |C     DCOR     - CORRECTION DIVERGENCE COEFFICIENTS.
         24 |C     HCOR     - CORRECTION HEIGHT COEFFICIENTS.
         25 |C
         26 |C REMARKS: MUST INCLUDE BLOCK DATA BLOCKSP02 AND DEFINITION OF
         27 |C           PL1 VARIABLES BEFORE COMPILING.
         28 |C
         29 |C ATTRIBUTES:
         30 |C   LANGUAGE: FORTRAN200
         31 |C   MACHINE:  CYBER
         32 |C
         33 |C$$$
         34 |         include "myparam"
  1       1 |        INCLUDE 'parmodel'
  2       1 |C     PARAMETER FILE
  2       2 |C
  2       3 |C     SETUP FOR IBM SINGLE PRECISION REPRESENTATION
  2       4 |C     ON OUTPUT
  2       5 |C                      THIS VERSION 21 February 1991 (DiMego)
  2       6 |C
  2       7 |      PARAMETER (
  2       8 |     *      IBPW      =      4,
  2       9 |     *      IBPOR     = 120000,
  2      10 |     *      INUMREC   = IBPOR / IBPW,
  2      11 |     *      INSPECS   =    700,
  2      12 |     *      ILATG2    =    120,
  2      13 |     *      ILEVH     =     12,
  2      14 |     *      ILEVS     =     16,
  2      15 |     *      ILONF     =    360,
  2      16 |     *      INLATSH   =      5,
  2      17 |     *      IIJMAX    =  23668,
  2      18 |     *      IIJMAXB   =  25536,
  2      19 |     *      IIJRAD    =    892,
  2      20 |     *      IJCAP     =     80,
  2      21 |     *      IWPCPL    =     80,
  2      22 |     *      IKM       =     16 )
  2      23 |      PARAMETER (
  2      24 |     *      ILBCH     =   7*(147+111-7),
  2      25 |     *      ILBHI     =    (147+111-26),
  2      26 |     *      ILBUI     =    (2*147+111-39),
  2      27 |     *      ILBUO     =   (8*147+7*111-56),
  2      28 |     *      ILBVI     =    (147+2*111-39),
  2      29 |     *      ILBVO     =   (7*147+8*111-56),
  2      30 |     *      ILMOUT    =     20 )
  2      31 |      PARAMETER (
  2      32 |     *      IMAXOUTF  =     17,
  2      33 |     *      IMAXOUTT  =      3,
  2      34 |     *      IMAXOUTV  =    100,
  2      35 |     *      IMAXPROF  =      6,
  2      36 |     *      INFILLER  =  10000,
  2      37 |     *      INH       =     65,
  2      38 |     *      INGRDUSE  =      2,
  2      39 |     *      INGRDUS1  =      1,
  2      40 |     *      INIADDRS  =     44,
  2      41 |     *      INSCR     =     23,
  2      42 |     *      INSCRGEO  =     12,
  2      43 |     *      INSCR3    =     10,
  2      44 |     *      INSOIL    =      2,
  2      45 |     *      INSUMG    =  42164,
  2      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  2      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  2      48 |     *      INSHEM    = 13 * INH + 23,
  2      49 |     *      IIJKMAX   = IKM * IIJMAX )
  2      50 |      PARAMETER (
  2      51 |     *  IIWORDSB  = IIJMAXB / 64,
  2      52 |     *  IKMP1     = IKM + 1,
  2      53 |     *  ILVLTOT   = 7 * IKM + 37,
  2      54 |     *  ILONFP1   = ILONF + 1,
  2      55 |     *  INVBL     = ILVLTOT * INSUMG,
  2      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  2      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  2      58 |     *  IBLK1D    =  2 * IKM + 14,
  2      59 |     *  IBLK2D    = 16 * IKM + 36,
  2      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  2      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  2      62 |C
  1       2 |C-------
  1       3 |C------- FOLLOWING IS EXTRA STUFF FOR INITIALIZATION
  1       4 |C-------
  1       5 |         PARAMETER (INLATH=101,INUMCOEF=3*INLATH+35,
  1       6 |     *        INUMGRID=4,IJCAPDP=80,INLON=300,INKLON=2,INKLAT=2,
  1       7 |     *        IIORBPL=3,IIORDPL=3,IIORBLP=3,IIORDLP=3,IMCUBIC=1,
  1       8 |     *        INUMVERT=2,INSIG=IKM,INMODES=18,INGMAX=3000,
  1       9 |     *        INKSIG=2,INSIGP1=INSIG+1,INLONEX=INLON+INKLON,
  1      10 |     *        INLATDP=2*INLATH,INLOLA=INLATDP*INLONEX,
  1      11 |     *        IJCAP1DP=IJCAPDP+1,IJCAP12=2*IJCAP1DP,
  1      12 |     *        INCOEFS=IJCAP1DP*(IJCAPDP+2),
  1      13 |     *     IDPFILLR=(4*IKM+1)*INSUMG+       3*(IIJKMAX*4+IIJMAX) )
  1      14 |C    *     IDPFILLR=(4*IKM+1)*INSUMG+INGRDUSE*(IIJKMAX*4+IIJMAX) )
         35 |C--------
         36 |         INTEGER D1,H1,Z2,D2,H2
         37 |         real ZT(1),DT(1),HT(1),ZCOR(1),DCOR(1),HCOR(1)
         38 |C--------
         39 |         COMMON/DAVEBUFF/ COF(INCOEFS,INUMCOEF),GRD(INLOLA,INUMGRID)
         40 |         real COF,GRD
         41 |         REAL RCF(INCOEFS,2)
         42 |         EQUIVALENCE (COF,RCF)
         43 |         REAL RC0INV
         44 |C--------
         45 |         COMMON/FIXSP02/JCAP,NCOEFS,NFOP,JFOP,
         46 |     *      ISCALZD,IUNSCLZD,
         47 |     *      IBSHAT,IBVHAT,IBVIHAT,
         48 |     *      IBOP,ICOP,IFOP,ICINVOP,
         49 |     *      IRFOP,IRCINVOP,
         50 |     *      BFOP(INCOEFS),BONES(INCOEFS)
         51 |         LOGICAL BFOP,BONES
         52 |C--------
         53 |C-------- CHECK TO SEE IF INITIALIZATION OF INTERNAL VARIABLES NEEDED
         54 |         IF(JCAP.NE.JCAPIN) CALL SETSP02(JCAPIN)
         55 |         CALL GETCOEF(D1,1)
         56 |         CALL GETCOEF(H1,1)
         57 |         CALL GETCOEF(Z2,1)
         58 |         CALL GETCOEF(D2,1)
         59 |         CALL GETCOEF(H2,1)
         60 |         CALL getcoef(IRF,1)
         61 |         CALL getcoef(IRZ,1)
         62 |         CALL getcoef(IRWORK,1)
         63 |         CALL getcoef(IRC,1)
         64 |         CALL getcoef(IRB,1)
         65 |         CALL getcoef(IRR,1)
         66 |         RC0INV=6.3712E6*7.2921E-5/SQRT(9.8062*DEPTH)
         67 |         OMEGAINV=1./7.2921E-5
         68 |         JCAP1=JCAP+1
         69 |C--------
         70 |C-------- COMPUTE RHS OF INCREMENT EQUATIONS
         71 |C--------
         72 |         TRANS=-1.
         73 |         CALL SPLUSF(ZT,DT,HT,
         74 |     *                COF(1,Z2),COF(1,D1),COF(1,H1),DEPTH,TRANS,JCAP)
         75 |         DO 100 I=1,NCOEFS,2
         76 |           IP=I+1
         77 |           COF(IP,D2)=OMEGAINV*COF(I,D1)
         78 |           COF(I,D2)=-OMEGAINV*COF(IP,D1)
         79 |           COF(IP,H2)=OMEGAINV*COF(I,H1)
         80 |           COF(I,H2)=-OMEGAINV*COF(IP,H1)
         81 |100      CONTINUE
         82 |         do 10002 i=1,ncoefs
         83 |         RCF(i,IRF)=COF(i,H2)
         84 |10002    continue
         85 |C--------
         86 |C-------- SOLVE TRIDIAGONAL MATRIX EQUATION TO GET DIV INCREMENT.
         87 |C--------
         88 |         CALL SOLVTRID(IRF,IRZ,IRWORK,IRC,IRB,IRR,RC0INV)
         89 |         do 10004 i=1,ncoefs
         90 |         RCF(i,IRF)=RCF(i,IRF)*RC0INV
         91 |     *                 *RCF(i,IRCINVOP)
         92 |         COF(i,D1)=RCF(i,IRF)
         93 |10004    continue
         94 |C--------
         95 |C-------- COMPUTE RHS FOR HEIGHT INCREMENT
         96 |C--------
         97 |         do 10006 i=1,ncoefs
         98 |         RCF(i,IRF)=RC0INV*RCF(i,IRCINVOP)
         99 |     *      *(COF(i,D2)-COF(i,IBOP)*RCF(i,IRF))
        100 |10006    continue
        101 |C--------
        102 |C-------- SOLVE TRIDIAGONAL MATRIX EQUATION TO GET HEIGHT INCREMENT.
        103 |C--------
        104 |         CALL SOLVTRID(IRF,IRZ,IRWORK,IRC,IRB,IRR,RC0INV)
        105 |         do 10008 i=1,ncoefs
        106 |         COF(i,H1)=RCF(i,IRF)
        107 |         COF(i,Z2)=0.
        108 |10008    continue
        109 |         COF(1,H1)=0.
        110 |         COF(2,H1)=0.
        111 |         COF(1,D1)=0.
        112 |         COF(2,D1)=0.
        113 |         TRANS=1.
        114 |         CALL SPLUSF(COF(1,Z2),COF(1,D1),COF(1,H1),
        115 |     *        ZCOR,DCOR,HCOR,DEPTH,TRANS,JCAP)
        116 |         CALL FREECOEF(D1,1)
        117 |         CALL FREECOEF(H1,1)
        118 |         CALL FREECOEF(Z2,1)
        119 |         CALL FREECOEF(D2,1)
        120 |         CALL FREECOEF(H2,1)
        121 |         CALL freecoef(IRF,1)
        122 |         CALL freecoef(IRZ,1)
        123 |         CALL freecoef(IRWORK,1)
        124 |         CALL freecoef(IRC,1)
        125 |         CALL freecoef(IRB,1)
        126 |         CALL freecoef(IRR,1)
        127 |       RETURN
        128 |       END
 
