          1 |       FUNCTION CONMC(CNAME)
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .                                       .
          4 |C SUBPROGRAM:    CONMC       PROVIDE NMC*S VALUE FOR PHYSICAL CONST.
          5 |C   PRGMMR: PARRISH          ORG: W/NMC22    DATE: 88-09-09
          6 |C
          7 |C ABSTRACT: RETURN NMC HANDBOOK VALUE FOR REQUESTED PHYSICAL CONST.
          8 |C   NOTE THAT SOME VALUES HAVE BEEN MODIFIED BASED ON BILL COLLINS
          9 |C   SUGGESTION.  (G, RD, CP)
         10 |C
         11 |C PROGRAM HISTORY LOG:
         12 |C   88-09-09  PARRISH
         13 |C
         14 |C USAGE:    CONST=CONMC(CNAME)
         15 |C   INPUT ARGUMENT LIST:
         16 |C     CNAME    - CHARACTER ARRAY CONTAINING NAME OF CONSTANT AS
         17 |C              - SUGGESTED IN NMC HANDBOOK, SEC. 3.4.2.
         18 |C              - MUST END IN A "$" SIGN.
         19 |C
         20 |C ATTRIBUTES:
         21 |C   LANGUAGE: FORTRAN77
         22 |C   MACHINE:  CYBER
         23 |C
         24 |C$$$
         25 |         CHARACTER*1 CNAME(1),DOLLAR
         26 |         DATA DOLLAR/'$'/
         27 |         CHARACTER*8  TABONE(15)
         28 |         DATA  TABONE/
         29 |     *    'RERTH   ','G       ','OMEGA   ','RD      ','CP      ',
         30 |     *    'CV      ','RV      ','CVAP    ','CLIQ    ','HVAP    ',
         31 |     *    'HFUS    ','PSAT    ','SBC     ','SOLR    ','PI      '/
         32 |         CHARACTER*1 TABNAM(8,15)
         33 |         INTEGER LNAM(15)
         34 |         DATA LNAM/5,1,5,2,2,2,2,4,4,4,4,4,3,4,2/
         35 |         EQUIVALENCE (TABONE,TABNAM)
         36 |C******
         37 |C****** VALUES FROM NMC HANDBOOK
         38 |C******
         39 |C        DOUBLE PRECISION CONSTS(15)
         40 |C        DATA CONSTS/
         41 |C    *     6.3712E6,    9.8062, 7.2921E-5,  2.8704E2,  1.0046E3,
         42 |C    *     7.1760E2,  4.6150E2,  1.8460E3,  4.1855E3,  2.5000E6,
         43 |C    *     3.3358E5,  6.1078E2, 5.6730E-8,  1.3533E3,    3.1416/
         44 |C******
         45 |C****** MODIFIED VALUES, BASED ON BILL COLLINS SUGGESTIONS.
         46 |C******
         47 |         DIMENSION CONSTS(15)
         48 |         DATA CONSTS/
         49 |     *     6.3712E6,    9.8000, 7.2921E-5,  2.8705E2,  1.0045E3,
         50 |     *     7.1760E2,  4.6150E2,  1.8460E3,  4.1855E3,  2.5000E6,
         51 |     *     3.3358E5,  6.1078E2, 5.6730E-8,  1.3533E3,    3.1416/
         52 |C--------
         53 |C-------- FIRST FIND NUMBER OF CHARACTERS IN CNAME
         54 |C--------
         55 |         II=0
         56 |         DO 100 I=2,9
         57 |           II=II+1
         58 |           IF(CNAME(I).EQ.DOLLAR) GO TO 200
         59 |100      CONTINUE
         60 |         GO TO 500
         61 |200      CONTINUE
         62 |C--------
         63 |C-------- NOW FIND A MATCH
         64 |C--------
         65 |         DO 400 K=1,15
         66 |           JJ=LNAM(K)
         67 |           IF(II.EQ.JJ) THEN
         68 |             MATCH=0
         69 |             DO 300 I=1,II
         70 |               IF(CNAME(I).EQ.TABNAM(I,K)) MATCH=MATCH+1
         71 |300          CONTINUE
         72 |             IF(MATCH.EQ.II) THEN
         73 |               CONMC=CONSTS(K)
         74 |               RETURN
         75 |             END IF
         76 |           END IF
         77 |400      CONTINUE
         78 |500      CONTINUE
         79 |C--------
         80 |C-------- HERE FOR TROUBLE ONLY
         81 |C--------
         82 |         PRINT *,'TROUBLE IN CONMC'
         83 |         STOP
         84 |       END
 
