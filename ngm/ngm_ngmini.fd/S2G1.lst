          1 |      SUBROUTINE S2G1(ZS,DS,TS,PS,RS,QS,U,V,T,P,R,Q,
          2 |     *                  NSIG,JCAP,NLON,NLATH,
          3 |     *       IRS,IIS,AP,BP,AQR,BQR,GR,SLAT,CLAT,PE0,QE0,RO0,TRIGS,IFAX)
          4 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          5 |C                .      .    .                                       .
          6 |C SUBPROGRAM:    S2G1       SPECTRAL TO GRID TRANSFORM
          7 |C   PRGMMR: PARRISH          ORG: W/NMC22    DATE: 90-09-21
          8 |C
          9 |C ABSTRACT: SUM COEFFICIENTS OF SPHERICAL HARMONIC SERIES
         10 |C   FOR ALL SPECTRAL MODEL VARIABLES.
         11 |C
         12 |C PROGRAM HISTORY LOG:
         13 |C   90-09-21  PARRISH
         14 |C
         15 |C USAGE:    CALL S2G1(ZS,DS,TS,PS,RS,QS,U,V,T,P,R,Q,
         16 |C    *                  NSIG,JCAP,NLON,NLATH,
         17 |C    *       IRS,IIS,AP,BP,AQR,BQR,GR,SLAT,CLAT,PE0,QE0,RO0,TRIGS,IFAX)
         18 |C   INPUT ARGUMENT LIST:
         19 |C     ZS       - ZS((JCAP+1)*(JCAP+2)+1,NSIG): VORTICITY COEFFICIENTS
         20 |C     DS       - DS((JCAP+1)*(JCAP+2)+1,NSIG): DIVERGENCE COEFFICIENTS
         21 |C     TS       - TS((JCAP+1)*(JCAP+2)+1,NSIG): VIRTUAL TEMP COEFS (K)
         22 |C     PS       - PS((JCAP+1)*(JCAP+2)+1): LOG(PSF) COEFS  (P IN CB)
         23 |C     RS       - RS((JCAP+1)*(JCAP+2)+1): TERRAIN COEFS (M)
         24 |C     QS       - QS((JCAP+1)*(JCAP+2)+1,NSIG): SPECIFIC HUMITITY COEFS
         25 |C     NSIG     - NUMBER OF SIGMA LEVELS
         26 |C     JCAP     - TRIANGULAR TRUNCATION
         27 |C     NLON     - NUMBER OF LONGITUDES
         28 |C     NLATH    - NUMBER OF GAUSSIAN LATS IN ONE HEMISPHERE
         29 |C     IRS      - IRS(0:JCAP,0:JCAP): ADDR OF RE(ZS(N,L)), ETC.
         30 |C     IIS      - IIS(0:JCAP,0:JCAP): SAME FOR IM PART
         31 |C     AP       - AP(0:JCAP,0:JCAP):  RECURSION
         32 |C     BP       - BP(0:JCAP,0:JCAP):    CONSTANTS
         33 |C     AQR      - AQR(0:JCAP,0:JCAP):     FOR GENERATING
         34 |C     BQR      - BQR(0:JCAP,0:JCAP):       VECTOR SPHERICAL
         35 |C     GR       - GR(0:JCAP,0:JCAP):          HARMONICS
         36 |C     SLAT     - SLAT(NLATH):  SIN(GAUSSIAN LATITUDES)
         37 |C     CLAT     - CLAT(NLATH):  COS(GAUSSIAN LATS)
         38 |C     PE0      - PE0(NLATH,0:JCAP): P(L,L)
         39 |C     QE0      - QE0(NLATH,0:JCAP): Q(L,L)
         40 |C     RO0      - RO0(NLATH,0:JCAP): R(L,L)
         41 |C     TRIGS    - TRIGS(NLON*2)
         42 |C     IFAX     - IFAX(10)
         43 |C
         44 |C   OUTPUT ARGUMENT LIST:
         45 |C     U        - LONGITUDE COMPONENT OF WINDS
         46 |C     V        - LATITUDE COMPONENT OF WINDS
         47 |C     T        - TEMPS
         48 |C     P        - LOG(PSFC)
         49 |C     R        - TERRAIN
         50 |C     Q        - MOISTURE VARIABLES
         51 |C
         52 |C ATTRIBUTES:
         53 |C   LANGUAGE: CFT77
         54 |C   MACHINE:  CRAY YMP
         55 |C
         56 |C$$$
         57 |         DIMENSION ZS((JCAP+1)*(JCAP+2)+1,NSIG)
         58 |         DIMENSION DS((JCAP+1)*(JCAP+2)+1,NSIG)
         59 |         DIMENSION TS((JCAP+1)*(JCAP+2)+1,NSIG)
         60 |         DIMENSION PS((JCAP+1)*(JCAP+2)+1)
         61 |         DIMENSION RS((JCAP+1)*(JCAP+2)+1)
         62 |         DIMENSION QS((JCAP+1)*(JCAP+2)+1,NSIG)
         63 |         DIMENSION U(2*NLATH,NLON+1,NSIG),V(2*NLATH,NLON+1,NSIG)
         64 |         DIMENSION T(2*NLATH,NLON+1,NSIG),Q(2*NLATH,NLON+1,NSIG)
         65 |         DIMENSION P(2*NLATH,NLON+1),R(2*NLATH,NLON+1)
         66 |         DIMENSION IRS(0:JCAP,0:JCAP),IIS(0:JCAP,0:JCAP)
         67 |         double precision AP(0:JCAP,0:JCAP),BP(0:JCAP,0:JCAP)
         68 |         double precision AQR(0:JCAP,0:JCAP),BQR(0:JCAP,0:JCAP)
         69 |         double precision GR(0:JCAP,0:JCAP)
         70 |         double precision SLAT(NLATH),CLAT(NLATH)
         71 |         DIMENSION TRIGS(NLON*2),IFAX(10)
         72 |         double precision PE0(NLATH,0:JCAP),QE0(NLATH,0:JCAP)
         73 |         double precision RO0(NLATH,0:JCAP)
         74 |C--------
         75 |C-------- INTERNAL SCRATCH DYNAMIC SPACE FOLLOWS:
         76 |C--------
         77 |         double precision PE(NLATH,0:JCAP),QE(NLATH,0:JCAP)
         78 |         double precision RO(NLATH,0:JCAP)
         79 |         double precision PO(NLATH,0:JCAP),QO(NLATH,0:JCAP)
         80 |         double precision RE(NLATH,0:JCAP)
         81 |         DIMENSION WORK(2*NLATH*NLON)
         82 |C--------
         83 |C-------- MAKE SURE THAT ZONAL IMAGINARY COEFS ARE ZERO, AND
         84 |C-------- (0,0) COMPONENT OF Z,D ALSO
         85 |C--------
         86 |         DO 20 K=1,NSIG
         87 |CDIR$ IVDEP
         88 |           DO 10 N=0,JCAP
         89 |             ZS(IIS(N,0),K)=0.
         90 |             DS(IIS(N,0),K)=0.
         91 |             TS(IIS(N,0),K)=0.
         92 |             QS(IIS(N,0),K)=0.
         93 |10         CONTINUE
         94 |           ZS(IRS(0,0),K)=0.
         95 |           DS(IRS(0,0),K)=0.
         96 |20       CONTINUE
         97 |CDIR$ IVDEP
         98 |         DO 30 N=0,JCAP
         99 |           PS(IIS(N,0))=0.
        100 |           RS(IIS(N,0))=0.
        101 |30       CONTINUE
        102 |         PO=0.d0
        103 |         QO=0.d0
        104 |         RE=0.d0
        105 |         PE=PE0
        106 |         QE=QE0
        107 |         RO=RO0
        108 |C--------
        109 |C-------- NOW SUM IN LATITUDE (AFTER ZEROING OUTPUT ARRAYS)
        110 |C--------
        111 |         U=0.
        112 |         V=0.
        113 |         T=0.
        114 |         P=0.
        115 |         R=0.
        116 |         Q=0.
        117 |C--------
        118 |         DO 1300 L=0,JCAP
        119 |           LR=2*L+1
        120 |           LI=2*L+2
        121 |           JSTART=1
        122 |           IF(L.GT.1) JSTART=2
        123 |           DO 900 N=L,JCAP,2
        124 |C------------
        125 |C------------ FIRST SUM EVEN/ODD TERMS (N=L,L+2,...)
        126 |C------------
        127 |             DO 200 K=1,NSIG
        128 |CDIR$ IVDEP
        129 |               DO 100 J=JSTART,NLATH
        130 |                 JR=2*NLATH+1-J
        131 |                 T(J,LR,K)=T(J,LR,K)+PE(J,L)*TS(IRS(N,L),K)
        132 |                 T(J,LI,K)=T(J,LI,K)+PE(J,L)*TS(IIS(N,L),K)
        133 |                 Q(J,LR,K)=Q(J,LR,K)+PE(J,L)*QS(IRS(N,L),K)
        134 |                 Q(J,LI,K)=Q(J,LI,K)+PE(J,L)*QS(IIS(N,L),K)
        135 |                 U(JR,LR,K)=U(JR,LR,K)+RO(J,L)*ZS(IRS(N,L),K)
        136 |                 U(J,LR,K)=U(J,LR,K)+QE(J,L)*DS(IIS(N,L),K)
        137 |                 U(JR,LI,K)=U(JR,LI,K)+RO(J,L)*ZS(IIS(N,L),K)
        138 |                 U(J,LI,K)=U(J,LI,K)-QE(J,L)*DS(IRS(N,L),K)
        139 |                 V(J,LR,K)=V(J,LR,K)+QE(J,L)*ZS(IIS(N,L),K)
        140 |                 V(JR,LR,K)=V(JR,LR,K)-RO(J,L)*DS(IRS(N,L),K)
        141 |                 V(J,LI,K)=V(J,LI,K)-QE(J,L)*ZS(IRS(N,L),K)
        142 |                 V(JR,LI,K)=V(JR,LI,K)-RO(J,L)*DS(IIS(N,L),K)
        143 | 100           CONTINUE
        144 | 200         CONTINUE
        145 |CDIR$ IVDEP
        146 |             DO 300 J=JSTART,NLATH
        147 |               P(J,LR)=P(J,LR)+PE(J,L)*PS(IRS(N,L))
        148 |               P(J,LI)=P(J,LI)+PE(J,L)*PS(IIS(N,L))
        149 |               R(J,LR)=R(J,LR)+PE(J,L)*RS(IRS(N,L))
        150 |               R(J,LI)=R(J,LI)+PE(J,L)*RS(IIS(N,L))
        151 |300          CONTINUE
        152 |C------------
        153 |C------------ NOW DO ODD/EVEN  (N=L+1,L+3,...)
        154 |C------------
        155 |             IF(N+1.LE.JCAP) THEN
        156 |CDIR$ IVDEP
        157 |               DO 400 J=JSTART,NLATH
        158 |                 PO(J,L)=AP(N,L)*SLAT(J)*PE(J,L)+BP(N,L)*PO(J,L)
        159 |                 QO(J,L)=AQR(N,L)*SLAT(J)*QE(J,L)
        160 |     *                     +BQR(N,L)*QO(J,L)
        161 |                 RE(J,L)=AQR(N,L)*SLAT(J)*RO(J,L)
        162 |     *                 +BQR(N,L)*RE(J,L)+GR(N,L)*PE(J,L)*CLAT(J)
        163 |400            CONTINUE
        164 |               DO 600 K=1,NSIG
        165 |CDIR$ IVDEP
        166 |                 DO 500 J=JSTART,NLATH
        167 |                   JR=2*NLATH+1-J
        168 |                   T(JR,LR,K)=T(JR,LR,K)+PO(J,L)*TS(IRS(N+1,L),K)
        169 |                   T(JR,LI,K)=T(JR,LI,K)+PO(J,L)*TS(IIS(N+1,L),K)
        170 |                   Q(JR,LR,K)=Q(JR,LR,K)+PO(J,L)*QS(IRS(N+1,L),K)
        171 |                   Q(JR,LI,K)=Q(JR,LI,K)+PO(J,L)*QS(IIS(N+1,L),K)
        172 |                   U(J,LR,K)=U(J,LR,K)+RE(J,L)*ZS(IRS(N+1,L),K)
        173 |                   U(JR,LR,K)=U(JR,LR,K)+QO(J,L)*DS(IIS(N+1,L),K)
        174 |                   U(J,LI,K)=U(J,LI,K)+RE(J,L)*ZS(IIS(N+1,L),K)
        175 |                   U(JR,LI,K)=U(JR,LI,K)-QO(J,L)*DS(IRS(N+1,L),K)
        176 |                   V(JR,LR,K)=V(JR,LR,K)+QO(J,L)*ZS(IIS(N+1,L),K)
        177 |                   V(J,LR,K)=V(J,LR,K)-RE(J,L)*DS(IRS(N+1,L),K)
        178 |                   V(JR,LI,K)=V(JR,LI,K)-QO(J,L)*ZS(IRS(N+1,L),K)
        179 |                   V(J,LI,K)=V(J,LI,K)-RE(J,L)*DS(IIS(N+1,L),K)
        180 | 500             CONTINUE
        181 | 600           CONTINUE
        182 |CDIR$ IVDEP
        183 |               DO 700 J=JSTART,NLATH
        184 |                 JR=2*NLATH+1-J
        185 |                 P(JR,LR)=P(JR,LR)+PO(J,L)*PS(IRS(N+1,L))
        186 |                 P(JR,LI)=P(JR,LI)+PO(J,L)*PS(IIS(N+1,L))
        187 |                 R(JR,LR)=R(JR,LR)+PO(J,L)*RS(IRS(N+1,L))
        188 |                 R(JR,LI)=R(JR,LI)+PO(J,L)*RS(IIS(N+1,L))
        189 |700            CONTINUE
        190 |C--------------
        191 |C-------------- GET NEXT PE,QE,RO
        192 |C--------------
        193 |CDIR$ IVDEP
        194 |               DO 800 J=JSTART,NLATH
        195 |                 PE(J,L)=AP(N+1,L)*SLAT(J)*PO(J,L)+BP(N+1,L)*PE(J,L)
        196 |                 QE(J,L)=AQR(N+1,L)*SLAT(J)*QO(J,L)
        197 |     *                     +BQR(N+1,L)*QE(J,L)
        198 |                 RO(J,L)=AQR(N+1,L)*SLAT(J)*RE(J,L)
        199 |     *                 +BQR(N+1,L)*RO(J,L)+GR(N+1,L)*PO(J,L)*CLAT(J)
        200 |800            CONTINUE
        201 |             END IF
        202 |900        CONTINUE
        203 |C----------
        204 |C---------- NOW COMBINE EVEN AND ODD PARTS
        205 |C----------
        206 |           DO 1100 K=1,NSIG
        207 |CDIR$ IVDEP
        208 |             DO 1000 J=JSTART,NLATH
        209 |               JR=2*NLATH+1-J
        210 |               TRE=T(J,LR,K)
        211 |               TRO=T(JR,LR,K)
        212 |               T(J,LR,K)=TRE+TRO
        213 |               T(JR,LR,K)=TRE-TRO
        214 |               TIE=T(J,LI,K)
        215 |               TIO=T(JR,LI,K)
        216 |               T(J,LI,K)=TIE+TIO
        217 |               T(JR,LI,K)=TIE-TIO
        218 |               QRE=Q(J,LR,K)
        219 |               QRO=Q(JR,LR,K)
        220 |               Q(J,LR,K)=QRE+QRO
        221 |               Q(JR,LR,K)=QRE-QRO
        222 |               QIE=Q(J,LI,K)
        223 |               QIO=Q(JR,LI,K)
        224 |               Q(J,LI,K)=QIE+QIO
        225 |               Q(JR,LI,K)=QIE-QIO
        226 |               URE=U(J,LR,K)
        227 |               URO=U(JR,LR,K)
        228 |               U(J,LR,K)=URE+URO
        229 |               U(JR,LR,K)=URE-URO
        230 |               UIE=U(J,LI,K)
        231 |               UIO=U(JR,LI,K)
        232 |               U(J,LI,K)=UIE+UIO
        233 |               U(JR,LI,K)=UIE-UIO
        234 |               VRE=V(J,LR,K)
        235 |               VRO=V(JR,LR,K)
        236 |               V(J,LR,K)=VRE+VRO
        237 |               V(JR,LR,K)=VRE-VRO
        238 |               VIE=V(J,LI,K)
        239 |               VIO=V(JR,LI,K)
        240 |               V(J,LI,K)=VIE+VIO
        241 |               V(JR,LI,K)=VIE-VIO
        242 | 1000        CONTINUE
        243 | 1100      CONTINUE
        244 |CDIR$ IVDEP
        245 |           DO 1200 J=JSTART,NLATH
        246 |             JR=2*NLATH+1-J
        247 |             PRE=P(J,LR)
        248 |             PRO=P(JR,LR)
        249 |             P(J,LR)=PRE+PRO
        250 |             P(JR,LR)=PRE-PRO
        251 |             PIE=P(J,LI)
        252 |             PIO=P(JR,LI)
        253 |             P(J,LI)=PIE+PIO
        254 |             P(JR,LI)=PIE-PIO
        255 |             RRE=R(J,LR)
        256 |             RRO=R(JR,LR)
        257 |             R(J,LR)=RRE+RRO
        258 |             R(JR,LR)=RRE-RRO
        259 |             RIE=R(J,LI)
        260 |             RIO=R(JR,LI)
        261 |             R(J,LI)=RIE+RIO
        262 |             R(JR,LI)=RIE-RIO
        263 |1200       CONTINUE
        264 |1300     CONTINUE
        265 |C--------
        266 |C-------- FINALLY DO FOURIER SUMS IN LONGITUDE
        267 |C--------
        268 |         LOT=NLATH*2
        269 |         DO 1400 K=1,NSIG
        270 |           CALL M1FFTM(U(1,1,K),WORK,TRIGS,IFAX,LOT,1,NLON,LOT,1)
        271 |           CALL M1FFTM(V(1,1,K),WORK,TRIGS,IFAX,LOT,1,NLON,LOT,1)
        272 |           CALL M1FFTM(T(1,1,K),WORK,TRIGS,IFAX,LOT,1,NLON,LOT,1)
        273 |           CALL M1FFTM(Q(1,1,K),WORK,TRIGS,IFAX,LOT,1,NLON,LOT,1)
        274 | 1400    CONTINUE
        275 |         CALL M1FFTM(P,WORK,TRIGS,IFAX,LOT,1,NLON,LOT,1)
        276 |         CALL M1FFTM(R,WORK,TRIGS,IFAX,LOT,1,NLON,LOT,1)
        277 |       RETURN
        278 |       END
 
