          1 |      SUBROUTINE M1FFTA(A,WORK,TRIGS,INC,JUMP,N,LOT)
          2 |C     SUBROUTINE M1FFTA - PREPROCESSING STEP FOR FFT99, ISIGN=+1
          3 |C     (SPECTRAL TO GRIDPOINT TRANSFORM)
          4 |C
          5 |      DIMENSION A(N),WORK(N),TRIGS(N)
          6 |      NH=N/2
          7 |      NX=N
          8 |      INK=INC+INC
          9 |C
         10 |C     A(0)   A(N/2)
         11 |      IA=1
         12 |      IB=N*INC+1
         13 |      JA=1
         14 |      JB=2
         15 |CDIR$ IVDEP
         16 |      DO 10 L=1,LOT
         17 |      WORK(JA)=A(IA)
         18 |      WORK(JB)=A(IA)
         19 |      IA=IA+JUMP
         20 |      IB=IB+JUMP
         21 |      JA=JA+NX
         22 |      JB=JB+NX
         23 |   10 CONTINUE
         24 |C
         25 |C     REMAINING WAVENUMBERS
         26 |      IABASE=2*INC+1
         27 |      IBBASE=(N-2)*INC+1
         28 |      JABASE=3
         29 |      JBBASE=N-1
         30 |C
         31 |      DO 30 K=3,NH,2
         32 |      IA=IABASE
         33 |      IB=IBBASE
         34 |      JA=JABASE
         35 |      JB=JBBASE
         36 |      C=TRIGS(N+K)
         37 |      S=TRIGS(N+K+1)
         38 |CDIR$ IVDEP
         39 |      DO 20 L=1,LOT
         40 |      WORK(JA)=(A(IA)+A(IB))-
         41 |     *    (S*(A(IA)-A(IB))+C*(A(IA+INC)+A(IB+INC)))
         42 |      WORK(JB)=(A(IA)+A(IB))+
         43 |     *    (S*(A(IA)-A(IB))+C*(A(IA+INC)+A(IB+INC)))
         44 |      WORK(JA+1)=(C*(A(IA)-A(IB))-S*(A(IA+INC)+A(IB+INC)))+
         45 |     *    (A(IA+INC)-A(IB+INC))
         46 |      WORK(JB+1)=(C*(A(IA)-A(IB))-S*(A(IA+INC)+A(IB+INC)))-
         47 |     *    (A(IA+INC)-A(IB+INC))
         48 |      IA=IA+JUMP
         49 |      IB=IB+JUMP
         50 |      JA=JA+NX
         51 |      JB=JB+NX
         52 |   20 CONTINUE
         53 |      IABASE=IABASE+INK
         54 |      IBBASE=IBBASE-INK
         55 |      JABASE=JABASE+2
         56 |      JBBASE=JBBASE-2
         57 |   30 CONTINUE
         58 |C
         59 |      IF (IABASE.NE.IBBASE) GO TO 50
         60 |C     WAVENUMBER N/4 (IF IT EXISTS)
         61 |      IA=IABASE
         62 |      JA=JABASE
         63 |CDIR$ IVDEP
         64 |      DO 40 L=1,LOT
         65 |      WORK(JA)=2.0 E0*A(IA)
         66 |      WORK(JA+1)=-2.0 E0*A(IA+INC)
         67 |      IA=IA+JUMP
         68 |      JA=JA+NX
         69 |   40 CONTINUE
         70 |C
         71 |   50 CONTINUE
         72 |      RETURN
         73 |      END
 
