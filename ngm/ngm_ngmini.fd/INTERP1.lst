          1 |       SUBROUTINE INTERP1(F,G,RK,IK,BK,NF,NX,NY,NKX,NKY,NKZ,IZERO)
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .                                       .
          4 |C SUBPROGRAM:    INTERP1     INTERPOLATE FROM FIELD G TO FIELD F
          5 |C   PRGMMR: PARRISH          ORG: W/NMC22    DATE: 88-08-08
          6 |C
          7 |C ABSTRACT: INTERPOLATE FROM FIELD G TO FIELD F, USING PREVIOUSLY
          8 |C   DETERMINED INTERPOLATION CONSTANTS AND GATHER INDICES.
          9 |C
         10 |C PROGRAM HISTORY LOG:
         11 |C   88-08-08  PARRISH
         12 |C
         13 |C USAGE:    CALL INTERP1(F,G,RK,IK,BK,NF,NX,NY,NKX,NKY,NKZ,IZERO)
         14 |C   INPUT ARGUMENT LIST:
         15 |C     F        - INPUT INTERPOLATEES (RESULT INTERPOLATEES MAY BE
         16 |C              - ADDED ON IF DESIRED)
         17 |C     G        - INPUT INTERPOLATORS
         18 |C     RK       - INTERPOLATOR WEIGHTS
         19 |C     IK       - INTERPOLATOR BASE GATHER INDEX
         20 |C              -                          NK=NXK*NKY*NKZ
         21 |C              -             F(IOBS)=SUM(RK(IOBS,K)*G(IK(IOBS)+IS(K)))
         22 |C              -                         K=1
         23 |C     BK       - =0, POINT IS EXTRAPOLATEE (DO NOTHING).
         24 |C              - =1, POINT IS INTERPOLATEE (SO INTERPOLATE).
         25 |C     NF       - TOTAL NUMBER OF INTERPOLATEES.
         26 |C     NX       - X DIMENSION OF INTERPOLATOR GRID
         27 |C     NY       - Y DIMENSION OF INTERPOLATOR GRID
         28 |C     NKX      - ORDER OF INTERPOLATION IN X-DIRECTION.
         29 |C     NKY      - ORDER OF INTERPOLATION IN Y-DIRECTION.
         30 |C     NKZ      - ORDER OF INTERPOLATION IN Z-DIRECTION.
         31 |C     IZERO    - =0, THEN ZERO F BEFORE ACCUMULATING RESULT.
         32 |C              - =1, THEN ACCUMULATE INTERPOLATEE TO CURRENT
         33 |C              -                CONTENTS OF F.
         34 |C
         35 |C   OUTPUT ARGUMENT LIST:      (INCLUDING WORK ARRAYS)
         36 |C     F        - OUTPUT INTERPOLATEES
         37 |C
         38 |C ATTRIBUTES:
         39 |C   LANGUAGE: FORTRAN200
         40 |C   MACHINE:  CYBER
         41 |C
         42 |C$$$
         43 |C--------
         44 |         include "myparam"
  1       1 |        INCLUDE 'parmodel'
  2       1 |C     PARAMETER FILE
  2       2 |C
  2       3 |C     SETUP FOR IBM SINGLE PRECISION REPRESENTATION
  2       4 |C     ON OUTPUT
  2       5 |C                      THIS VERSION 21 February 1991 (DiMego)
  2       6 |C
  2       7 |      PARAMETER (
  2       8 |     *      IBPW      =      4,
  2       9 |     *      IBPOR     = 120000,
  2      10 |     *      INUMREC   = IBPOR / IBPW,
  2      11 |     *      INSPECS   =    700,
  2      12 |     *      ILATG2    =    120,
  2      13 |     *      ILEVH     =     12,
  2      14 |     *      ILEVS     =     16,
  2      15 |     *      ILONF     =    360,
  2      16 |     *      INLATSH   =      5,
  2      17 |     *      IIJMAX    =  23668,
  2      18 |     *      IIJMAXB   =  25536,
  2      19 |     *      IIJRAD    =    892,
  2      20 |     *      IJCAP     =     80,
  2      21 |     *      IWPCPL    =     80,
  2      22 |     *      IKM       =     16 )
  2      23 |      PARAMETER (
  2      24 |     *      ILBCH     =   7*(147+111-7),
  2      25 |     *      ILBHI     =    (147+111-26),
  2      26 |     *      ILBUI     =    (2*147+111-39),
  2      27 |     *      ILBUO     =   (8*147+7*111-56),
  2      28 |     *      ILBVI     =    (147+2*111-39),
  2      29 |     *      ILBVO     =   (7*147+8*111-56),
  2      30 |     *      ILMOUT    =     20 )
  2      31 |      PARAMETER (
  2      32 |     *      IMAXOUTF  =     17,
  2      33 |     *      IMAXOUTT  =      3,
  2      34 |     *      IMAXOUTV  =    100,
  2      35 |     *      IMAXPROF  =      6,
  2      36 |     *      INFILLER  =  10000,
  2      37 |     *      INH       =     65,
  2      38 |     *      INGRDUSE  =      2,
  2      39 |     *      INGRDUS1  =      1,
  2      40 |     *      INIADDRS  =     44,
  2      41 |     *      INSCR     =     23,
  2      42 |     *      INSCRGEO  =     12,
  2      43 |     *      INSCR3    =     10,
  2      44 |     *      INSOIL    =      2,
  2      45 |     *      INSUMG    =  42164,
  2      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  2      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  2      48 |     *      INSHEM    = 13 * INH + 23,
  2      49 |     *      IIJKMAX   = IKM * IIJMAX )
  2      50 |      PARAMETER (
  2      51 |     *  IIWORDSB  = IIJMAXB / 64,
  2      52 |     *  IKMP1     = IKM + 1,
  2      53 |     *  ILVLTOT   = 7 * IKM + 37,
  2      54 |     *  ILONFP1   = ILONF + 1,
  2      55 |     *  INVBL     = ILVLTOT * INSUMG,
  2      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  2      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  2      58 |     *  IBLK1D    =  2 * IKM + 14,
  2      59 |     *  IBLK2D    = 16 * IKM + 36,
  2      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  2      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  2      62 |C
  1       2 |C-------
  1       3 |C------- FOLLOWING IS EXTRA STUFF FOR INITIALIZATION
  1       4 |C-------
  1       5 |         PARAMETER (INLATH=101,INUMCOEF=3*INLATH+35,
  1       6 |     *        INUMGRID=4,IJCAPDP=80,INLON=300,INKLON=2,INKLAT=2,
  1       7 |     *        IIORBPL=3,IIORDPL=3,IIORBLP=3,IIORDLP=3,IMCUBIC=1,
  1       8 |     *        INUMVERT=2,INSIG=IKM,INMODES=18,INGMAX=3000,
  1       9 |     *        INKSIG=2,INSIGP1=INSIG+1,INLONEX=INLON+INKLON,
  1      10 |     *        INLATDP=2*INLATH,INLOLA=INLATDP*INLONEX,
  1      11 |     *        IJCAP1DP=IJCAPDP+1,IJCAP12=2*IJCAP1DP,
  1      12 |     *        INCOEFS=IJCAP1DP*(IJCAPDP+2),
  1      13 |     *     IDPFILLR=(4*IKM+1)*INSUMG+       3*(IIJKMAX*4+IIJMAX) )
  1      14 |C    *     IDPFILLR=(4*IKM+1)*INSUMG+INGRDUSE*(IIJKMAX*4+IIJMAX) )
         45 |         REAL F(NF),G(1)
         46 |         REAL RK(NF,1)
         47 |C--------
         48 |         INTEGER IK(NF)
         49 |         LOGICAL BK(NF)
         50 |C--------
         51 |         NXY=NX*NY
         52 |         IF(IZERO.EQ.0) F=0.
         53 |         K=0
         54 |         DO 130 IKZ=1,NKZ
         55 |           DO 120 IKY=1,NKY
         56 |             DO 110 IKX=1,NKX
         57 |               K=K+1
         58 |               IS=IKX-1+(IKY-1)*NX+(IKZ-1)*NXY
         59 |               DO 11002 I=1,NF
         60 |                 IF(BK(I)) F(I)=F(I)+RK(I,K)*G(IK(I)+IS)
         61 |11002          CONTINUE
         62 |110          CONTINUE
         63 |120        CONTINUE
         64 |130      CONTINUE
         65 |       RETURN
         66 |       END
 
