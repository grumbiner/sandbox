          1 |       SUBROUTINE POLORXY(RLON,RLAT,MC,NC,XPOLET,YPOLET,DX,DY,ERAD,
          2 |     *          XOBS,YOBS)
          3 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          4 |C                .      .    .                                       .
          5 |C SUBPROGRAM:    POLORXY     GET LOLA COORDINATES OF POLA GRID POINTS
          6 |C   PRGMMR: PARRISH          ORG: W/NMC22    DATE: 88-08-08
          7 |C
          8 |C ABSTRACT: OBTAIN LOLA COORDINATES OF POLA GRID POINTS.
          9 |C
         10 |C PROGRAM HISTORY LOG:
         11 |C   88-08-08  PARRISH
         12 |C
         13 |C USAGE:    CALL POLORXY(RLON,RLAT,MC,NC,XPOLET,YPOLET,DX,DY,ERAD,
         14 |C                         XOBS,YOBS)
         15 |C   INPUT ARGUMENT LIST:
         16 |C     MC       - X-DIMENSION OF POLA GRID
         17 |C     NC       - Y-DIMENSION OF POLA GRID
         18 |C     XPOLET   - X-COORDINATE OF POLE
         19 |C     YPOLET   - Y-COORDINATE OF POLE
         20 |C     DX       - X-GRID INCREMENT
         21 |C     DY       - Y-GRID INCREMENT
         22 |C     ERAD     - RADIUS OF EARTH
         23 |C
         24 |C   OUTPUT ARGUMENT LIST:      (INCLUDING WORK ARRAYS)
         25 |C     RLON     - LONGITUDE COORDINATES OF POLA GRID POINTS (RADIANS)
         26 |C     RLAT     - LATITUDE COORDINATES OF POLA GRID POINTS
         27 |C     XOBS     - WORK ARRAY
         28 |C     YOBS     - WORK ARRAY
         29 |C
         30 |C   SUBPROGRAMS CALLED: MYVATAN2
         31 |C
         32 |C ATTRIBUTES:
         33 |C   LANGUAGE: FORTRAN200
         34 |C   MACHINE:  CYBER
         35 |C
         36 |C$$$
         37 |         include "myparam"
  1       1 |        INCLUDE 'parmodel'
  2       1 |C     PARAMETER FILE
  2       2 |C
  2       3 |C     SETUP FOR IBM SINGLE PRECISION REPRESENTATION
  2       4 |C     ON OUTPUT
  2       5 |C                      THIS VERSION 21 February 1991 (DiMego)
  2       6 |C
  2       7 |      PARAMETER (
  2       8 |     *      IBPW      =      4,
  2       9 |     *      IBPOR     = 120000,
  2      10 |     *      INUMREC   = IBPOR / IBPW,
  2      11 |     *      INSPECS   =    700,
  2      12 |     *      ILATG2    =    120,
  2      13 |     *      ILEVH     =     12,
  2      14 |     *      ILEVS     =     16,
  2      15 |     *      ILONF     =    360,
  2      16 |     *      INLATSH   =      5,
  2      17 |     *      IIJMAX    =  23668,
  2      18 |     *      IIJMAXB   =  25536,
  2      19 |     *      IIJRAD    =    892,
  2      20 |     *      IJCAP     =     80,
  2      21 |     *      IWPCPL    =     80,
  2      22 |     *      IKM       =     16 )
  2      23 |      PARAMETER (
  2      24 |     *      ILBCH     =   7*(147+111-7),
  2      25 |     *      ILBHI     =    (147+111-26),
  2      26 |     *      ILBUI     =    (2*147+111-39),
  2      27 |     *      ILBUO     =   (8*147+7*111-56),
  2      28 |     *      ILBVI     =    (147+2*111-39),
  2      29 |     *      ILBVO     =   (7*147+8*111-56),
  2      30 |     *      ILMOUT    =     20 )
  2      31 |      PARAMETER (
  2      32 |     *      IMAXOUTF  =     17,
  2      33 |     *      IMAXOUTT  =      3,
  2      34 |     *      IMAXOUTV  =    100,
  2      35 |     *      IMAXPROF  =      6,
  2      36 |     *      INFILLER  =  10000,
  2      37 |     *      INH       =     65,
  2      38 |     *      INGRDUSE  =      2,
  2      39 |     *      INGRDUS1  =      1,
  2      40 |     *      INIADDRS  =     44,
  2      41 |     *      INSCR     =     23,
  2      42 |     *      INSCRGEO  =     12,
  2      43 |     *      INSCR3    =     10,
  2      44 |     *      INSOIL    =      2,
  2      45 |     *      INSUMG    =  42164,
  2      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  2      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  2      48 |     *      INSHEM    = 13 * INH + 23,
  2      49 |     *      IIJKMAX   = IKM * IIJMAX )
  2      50 |      PARAMETER (
  2      51 |     *  IIWORDSB  = IIJMAXB / 64,
  2      52 |     *  IKMP1     = IKM + 1,
  2      53 |     *  ILVLTOT   = 7 * IKM + 37,
  2      54 |     *  ILONFP1   = ILONF + 1,
  2      55 |     *  INVBL     = ILVLTOT * INSUMG,
  2      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  2      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  2      58 |     *  IBLK1D    =  2 * IKM + 14,
  2      59 |     *  IBLK2D    = 16 * IKM + 36,
  2      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  2      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  2      62 |C
  1       2 |C-------
  1       3 |C------- FOLLOWING IS EXTRA STUFF FOR INITIALIZATION
  1       4 |C-------
  1       5 |         PARAMETER (INLATH=101,INUMCOEF=3*INLATH+35,
  1       6 |     *        INUMGRID=4,IJCAPDP=80,INLON=300,INKLON=2,INKLAT=2,
  1       7 |     *        IIORBPL=3,IIORDPL=3,IIORBLP=3,IIORDLP=3,IMCUBIC=1,
  1       8 |     *        INUMVERT=2,INSIG=IKM,INMODES=18,INGMAX=3000,
  1       9 |     *        INKSIG=2,INSIGP1=INSIG+1,INLONEX=INLON+INKLON,
  1      10 |     *        INLATDP=2*INLATH,INLOLA=INLATDP*INLONEX,
  1      11 |     *        IJCAP1DP=IJCAPDP+1,IJCAP12=2*IJCAP1DP,
  1      12 |     *        INCOEFS=IJCAP1DP*(IJCAPDP+2),
  1      13 |     *     IDPFILLR=(4*IKM+1)*INSUMG+       3*(IIJKMAX*4+IIJMAX) )
  1      14 |C    *     IDPFILLR=(4*IKM+1)*INSUMG+INGRDUSE*(IIJKMAX*4+IIJMAX) )
         38 |C--------
         39 |         REAL XOBS(1),YOBS(1),RLAT(1),RLON(1)
         40 |C--------
         41 |         CALL POLARXY(XOBS,YOBS,MC,NC,XPOLET,YPOLET,DX,DY)
         42 |         MNC=MC*NC
         43 |         ERAD2I=1./(4.*ERAD*ERAD)
         44 |         TPI=8.*ATAN(1.)
         45 |         DO 100 I=1,MNC
         46 |           RLAT(I)=ERAD2I*(XOBS(I)**2+YOBS(I)**2)
         47 |           RLAT(I)=(1.-RLAT(I))/(1.+RLAT(I))
         48 |           RLAT(I)=ASIN(RLAT(I))
         49 |100      CONTINUE
         50 |         CALL MYVATAN2(RLON,YOBS,XOBS,MNC)
         51 |C SAMEAS--> RLON(1;MNC)=VATAN2(YOBS(1;MNC),XOBS(1;MNC);MNC)
         52 |C------- BUT MYVATAN2 TAKES CARE OF STUPID RULE ABOUT X=Y=0 NOT ALLOWED
         53 |C--------
         54 |C-------- CHANGE RANGE OF LONGITUDE TO 0-2*PI.
         55 |C--------
         56 |         DO 200 I=1,MNC
         57 |           IF(RLON(I).LT.0.) RLON(I)=RLON(I)+TPI
         58 |200      CONTINUE
         59 |       RETURN
         60 |       END
 
