          1 |       SUBROUTINE SOLVTRID(IRF,IRZ,IRWORK,IRC,IRB,IRR,RC0INV)
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .                                       .
          4 |C SUBPROGRAM:    SOLVTRID    SOLVE TRIDIAG TEMPERTON MATRIX
          5 |C   PRGMMR: PARRISH          ORG: W/NMC22    DATE: 88-06-13
          6 |C
          7 |C ABSTRACT: GO FROM FULL VARIABLES TO TEMPERTON SLOW-AND-FAST
          8 |C   DECOMPOSITION.
          9 |C
         10 |C PROGRAM HISTORY LOG:
         11 |C   88-06-13  PARRISH
         12 |C
         13 |C USAGE:    CALL SANDF(Z,D,H,ZS,DF,HF,DEPTH)
         14 |C   INPUT ARGUMENT LIST:
         15 |C     Z        - SLOW + FAST VORTICITY
         16 |C     D        - DIVERGENCE.
         17 |C     H        - SLOW + FAST HEIGHT
         18 |C     DEPTH    - SCALE HEIGHT.
         19 |C
         20 |C   OUTPUT ARGUMENT LIST:
         21 |C     ZS       - SLOW COMPONENT OF VORTICITY.
         22 |C     DF       - FAST DIVERGENCE (NOT CHANGED FROM INPUT--SLOW DIV=0)
         23 |C     HF       - FAST HEIGHT COMPONENT
         24 |C
         25 |C REMARKS: MUST INCLUDE BLOCK DATA BLOCKSP02 AND DEFINITION OF
         26 |C           PL1 VARIABLES BEFORE COMPILING.
         27 |C
         28 |C ATTRIBUTES:
         29 |C   LANGUAGE: FORTRAN200
         30 |C   MACHINE:  CYBER
         31 |C
         32 |C$$$
         33 |         include "myparam"
  1       1 |        INCLUDE 'parmodel'
  2       1 |C     PARAMETER FILE
  2       2 |C
  2       3 |C     SETUP FOR IBM SINGLE PRECISION REPRESENTATION
  2       4 |C     ON OUTPUT
  2       5 |C                      THIS VERSION 21 February 1991 (DiMego)
  2       6 |C
  2       7 |      PARAMETER (
  2       8 |     *      IBPW      =      4,
  2       9 |     *      IBPOR     = 120000,
  2      10 |     *      INUMREC   = IBPOR / IBPW,
  2      11 |     *      INSPECS   =    700,
  2      12 |     *      ILATG2    =    120,
  2      13 |     *      ILEVH     =     12,
  2      14 |     *      ILEVS     =     16,
  2      15 |     *      ILONF     =    360,
  2      16 |     *      INLATSH   =      5,
  2      17 |     *      IIJMAX    =  23668,
  2      18 |     *      IIJMAXB   =  25536,
  2      19 |     *      IIJRAD    =    892,
  2      20 |     *      IJCAP     =     80,
  2      21 |     *      IWPCPL    =     80,
  2      22 |     *      IKM       =     16 )
  2      23 |      PARAMETER (
  2      24 |     *      ILBCH     =   7*(147+111-7),
  2      25 |     *      ILBHI     =    (147+111-26),
  2      26 |     *      ILBUI     =    (2*147+111-39),
  2      27 |     *      ILBUO     =   (8*147+7*111-56),
  2      28 |     *      ILBVI     =    (147+2*111-39),
  2      29 |     *      ILBVO     =   (7*147+8*111-56),
  2      30 |     *      ILMOUT    =     20 )
  2      31 |      PARAMETER (
  2      32 |     *      IMAXOUTF  =     17,
  2      33 |     *      IMAXOUTT  =      3,
  2      34 |     *      IMAXOUTV  =    100,
  2      35 |     *      IMAXPROF  =      6,
  2      36 |     *      INFILLER  =  10000,
  2      37 |     *      INH       =     65,
  2      38 |     *      INGRDUSE  =      2,
  2      39 |     *      INGRDUS1  =      1,
  2      40 |     *      INIADDRS  =     44,
  2      41 |     *      INSCR     =     23,
  2      42 |     *      INSCRGEO  =     12,
  2      43 |     *      INSCR3    =     10,
  2      44 |     *      INSOIL    =      2,
  2      45 |     *      INSUMG    =  42164,
  2      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  2      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  2      48 |     *      INSHEM    = 13 * INH + 23,
  2      49 |     *      IIJKMAX   = IKM * IIJMAX )
  2      50 |      PARAMETER (
  2      51 |     *  IIWORDSB  = IIJMAXB / 64,
  2      52 |     *  IKMP1     = IKM + 1,
  2      53 |     *  ILVLTOT   = 7 * IKM + 37,
  2      54 |     *  ILONFP1   = ILONF + 1,
  2      55 |     *  INVBL     = ILVLTOT * INSUMG,
  2      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  2      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  2      58 |     *  IBLK1D    =  2 * IKM + 14,
  2      59 |     *  IBLK2D    = 16 * IKM + 36,
  2      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  2      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  2      62 |C
  1       2 |C-------
  1       3 |C------- FOLLOWING IS EXTRA STUFF FOR INITIALIZATION
  1       4 |C-------
  1       5 |         PARAMETER (INLATH=101,INUMCOEF=3*INLATH+35,
  1       6 |     *        INUMGRID=4,IJCAPDP=80,INLON=300,INKLON=2,INKLAT=2,
  1       7 |     *        IIORBPL=3,IIORDPL=3,IIORBLP=3,IIORDLP=3,IMCUBIC=1,
  1       8 |     *        INUMVERT=2,INSIG=IKM,INMODES=18,INGMAX=3000,
  1       9 |     *        INKSIG=2,INSIGP1=INSIG+1,INLONEX=INLON+INKLON,
  1      10 |     *        INLATDP=2*INLATH,INLOLA=INLATDP*INLONEX,
  1      11 |     *        IJCAP1DP=IJCAPDP+1,IJCAP12=2*IJCAP1DP,
  1      12 |     *        INCOEFS=IJCAP1DP*(IJCAPDP+2),
  1      13 |     *     IDPFILLR=(4*IKM+1)*INSUMG+       3*(IIJKMAX*4+IIJMAX) )
  1      14 |C    *     IDPFILLR=(4*IKM+1)*INSUMG+INGRDUSE*(IIJKMAX*4+IIJMAX) )
         34 |C--------
         35 |         COMMON/DAVEBUFF/ COF(INCOEFS,INUMCOEF),GRD(INLOLA,INUMGRID)
         36 |         real COF,GRD
         37 |         REAL RCF(INCOEFS,2)
         38 |         EQUIVALENCE (COF,RCF)
         39 |C--------
         40 |         COMMON/FIXSP02/JCAP,NCOEFS,NFOP,JFOP,
         41 |     *      ISCALZD,IUNSCLZD,
         42 |     *      IBSHAT,IBVHAT,IBVIHAT,
         43 |     *      IBOP,ICOP,IFOP,ICINVOP,
         44 |     *      IRFOP,IRCINVOP,
         45 |     *      BFOP(INCOEFS),BONES(INCOEFS)
         46 |         LOGICAL BFOP,BONES
         47 |C--------
         48 |C-------- SET UP TRIDIAGONAL MATRIX TO INVERT
         49 |C--------
         50 |         JCAP1=JCAP+1
         51 |         do 10002 i=1,ncoefs
         52 |         RCF(i,IRC)=0.
         53 |10002    continue
         54 |         NM1VECT=2*JCAP1
         55 |         NVECT=NM1VECT-2
         56 |         NM1=1
         57 |         N=NM1+NM1VECT
         58 |         do 10004 i=1,ncoefs
         59 |         RCF(i,IRB)=RCF(i,IRFOP)*RCF(i,IRFOP)
         60 |10004    continue
         61 |         DO 100 M=2,JCAP1
         62 |         do 10006 i=1,nvect
         63 |           RCF(N+i-1,IRC)=RCF(N+i-1,IRFOP)*RC0INV*RC0INV
         64 |     *                      *RCF(NM1+i-1,IRCINVOP)
         65 |10006    continue
         66 |           NM1=N
         67 |           N=N+NVECT
         68 |           NVECT=NVECT-2
         69 |100      CONTINUE
         70 |         NVECT=2*JCAP1
         71 |         NP1VECT=NVECT-2
         72 |         N=1
         73 |         NP1=N+NVECT
         74 |         DO 200 M=1,JCAP
         75 |         do 10008 i=1,np1vect
         76 |           RCF(N+i-1,IRC)=RCF(N+i-1,IRC)
         77 |     *          *RCF(NP1+i-1,IRCINVOP)*RCF(NP1+i-1,IRFOP)
         78 |           RCF(N+i-1,IRB)=RCF(N+i-1,IRB)
         79 |     *          +RCF(NP1+i-1,IRFOP)*RCF(NP1+i-1,IRFOP)
         80 |10008    continue
         81 |           N=NP1
         82 |           NP1=NP1+NP1VECT
         83 |           NP1VECT=NP1VECT-2
         84 |200      CONTINUE
         85 |         do 10010 i=1,ncoefs
         86 |         RCF(i,IRB)=RCF(i,IRCINVOP)
         87 |     *                    *RCF(i,IRCINVOP)*RC0INV*RC0INV
         88 |     *         *RCF(i,IRB)+1.
         89 |10010    continue
         90 |C--------
         91 |C-------- DOWNWARD SWEEP
         92 |C--------
         93 |         NM2VECT=2*JCAP1
         94 |         NM1VECT=NM2VECT-2
         95 |         NVECT=NM1VECT-2
         96 |         NM2=1
         97 |         NM1=NM2+NM2VECT
         98 |         N=NM1+NM1VECT
         99 |         DO 300 M=3,JCAP1
        100 |           do 10012 i=1,nvect
        101 |           RCF(i,IRR)=RCF(NM1+i-1,IRC)/RCF(NM2+i-1,IRB)
        102 |           RCF(N+i-1,IRF)=RCF(N+i-1,IRF)
        103 |     *                  -RCF(i,IRR)*RCF(NM2+i-1,IRF)
        104 |           RCF(N+i-1,IRB)=RCF(N+i-1,IRB)
        105 |     *                  -RCF(i,IRR)*RCF(NM1+i-1,IRC)
        106 |10012      continue
        107 |           NM2=NM1
        108 |           NM1=N
        109 |           N=N+NVECT
        110 |           NVECT=NVECT-2
        111 |300      CONTINUE
        112 |C--------
        113 |C-------- SOLUTION PHASE
        114 |C--------
        115 |         RCF(NCOEFS,IRF)=RCF(NCOEFS,IRF)/RCF(NCOEFS,IRB)
        116 |         RCF(NCOEFS-1,IRF)=RCF(NCOEFS-1,IRF)/RCF(NCOEFS-1,IRB)
        117 |         RCF(NCOEFS-2,IRF)=RCF(NCOEFS-2,IRF)/RCF(NCOEFS-2,IRB)
        118 |         RCF(NCOEFS-3,IRF)=RCF(NCOEFS-3,IRF)/RCF(NCOEFS-3,IRB)
        119 |         RCF(NCOEFS-4,IRF)=RCF(NCOEFS-4,IRF)/RCF(NCOEFS-4,IRB)
        120 |         RCF(NCOEFS-5,IRF)=RCF(NCOEFS-5,IRF)/RCF(NCOEFS-5,IRB)
        121 |C--------
        122 |         NP2=NCOEFS-1
        123 |         NP2VECT=2
        124 |         NP1VECT=NP2VECT+2
        125 |         NVECT=NP1VECT+2
        126 |         NP1=NP2-NP1VECT
        127 |         N=NP1-NVECT
        128 |         DO 400 M=JCAP1-2,1,0-1
        129 |           do 10014 i=1,np2vect
        130 |           RCF(N+i-1,IRF)=RCF(N+i-1,IRF)
        131 |     *            -RCF(NP1+i-1,IRC)*RCF(NP2+i-1,IRF)
        132 |10014      continue
        133 |           do 10016 i=1,nvect
        134 |           RCF(N+i-1,IRF)=RCF(N+i-1,IRF)/RCF(N+i-1,IRB)
        135 |10016      continue
        136 |           NP2=NP1
        137 |           NP1=N
        138 |           NP2VECT=NP1VECT
        139 |           NP1VECT=NVECT
        140 |           NVECT=NVECT+2
        141 |           N=N-NVECT
        142 |400      CONTINUE
        143 |       RETURN
        144 |       END
 
