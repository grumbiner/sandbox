          1 |       SUBROUTINE MODT2SIG(HMODE,TSIG,RLNPSFC,S,NV,NMODIN)
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .                                       .
          4 |C SUBPROGRAM:    MODT2SIG    VERT MODE TO SIGMA TRANSFORM
          5 |C   PRGMMR: PARRISH          ORG: W/NMC22    DATE: 88-09-08
          6 |C
          7 |C ABSTRACT: TRANSFORM FROM VERTICAL MODE AMPLITUDES TO SIGMA
          8 |C   COORDINATES FOR TEMP AND LOG(PSFC). USE ONLY WITH COEFFICIENTS!
          9 |C
         10 |C PROGRAM HISTORY LOG:
         11 |C   88-09-08  PARRISH
         12 |C
         13 |C USAGE:    CALL MODT2SIG(HMODE,TSIG,RLNPSFC,S,NV,NMODIN)
         14 |C   INPUT ARGUMENT LIST:
         15 |C     HMODE    - INTEGER POINTERS FOR INPUT MODE VARIABLES
         16 |C     TSIG     - INTEGER POINTERS FOR OUTPUT SIGMA VARIABLES
         17 |C     RLNPSFC  - DITTO
         18 |C     NV       - NUMBER OF POINTS TO TRANSFORM
         19 |C     NMODIN   - NUMBER OF VERTICAL MODES
         20 |C
         21 |C   OUTPUT ARGUMENT LIST:
         22 |C     S        - ARRAY CONTAINING ALL COEFFICIENT STUFF (POINTERS
         23 |C              - ARE USED TO IDENTIFY WHAT IS WHAT).
         24 |C
         25 |C ATTRIBUTES:
         26 |C   LANGUAGE: FORTRAN200
         27 |C   MACHINE:  CYBER
         28 |C
         29 |C$$$
         30 |         include "myparam"
  1       1 |        INCLUDE 'parmodel'
  2       1 |C     PARAMETER FILE
  2       2 |C
  2       3 |C     SETUP FOR IBM SINGLE PRECISION REPRESENTATION
  2       4 |C     ON OUTPUT
  2       5 |C                      THIS VERSION 21 February 1991 (DiMego)
  2       6 |C
  2       7 |      PARAMETER (
  2       8 |     *      IBPW      =      4,
  2       9 |     *      IBPOR     = 120000,
  2      10 |     *      INUMREC   = IBPOR / IBPW,
  2      11 |     *      INSPECS   =    700,
  2      12 |     *      ILATG2    =    120,
  2      13 |     *      ILEVH     =     12,
  2      14 |     *      ILEVS     =     16,
  2      15 |     *      ILONF     =    360,
  2      16 |     *      INLATSH   =      5,
  2      17 |     *      IIJMAX    =  23668,
  2      18 |     *      IIJMAXB   =  25536,
  2      19 |     *      IIJRAD    =    892,
  2      20 |     *      IJCAP     =     80,
  2      21 |     *      IWPCPL    =     80,
  2      22 |     *      IKM       =     16 )
  2      23 |      PARAMETER (
  2      24 |     *      ILBCH     =   7*(147+111-7),
  2      25 |     *      ILBHI     =    (147+111-26),
  2      26 |     *      ILBUI     =    (2*147+111-39),
  2      27 |     *      ILBUO     =   (8*147+7*111-56),
  2      28 |     *      ILBVI     =    (147+2*111-39),
  2      29 |     *      ILBVO     =   (7*147+8*111-56),
  2      30 |     *      ILMOUT    =     20 )
  2      31 |      PARAMETER (
  2      32 |     *      IMAXOUTF  =     17,
  2      33 |     *      IMAXOUTT  =      3,
  2      34 |     *      IMAXOUTV  =    100,
  2      35 |     *      IMAXPROF  =      6,
  2      36 |     *      INFILLER  =  10000,
  2      37 |     *      INH       =     65,
  2      38 |     *      INGRDUSE  =      2,
  2      39 |     *      INGRDUS1  =      1,
  2      40 |     *      INIADDRS  =     44,
  2      41 |     *      INSCR     =     23,
  2      42 |     *      INSCRGEO  =     12,
  2      43 |     *      INSCR3    =     10,
  2      44 |     *      INSOIL    =      2,
  2      45 |     *      INSUMG    =  42164,
  2      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  2      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  2      48 |     *      INSHEM    = 13 * INH + 23,
  2      49 |     *      IIJKMAX   = IKM * IIJMAX )
  2      50 |      PARAMETER (
  2      51 |     *  IIWORDSB  = IIJMAXB / 64,
  2      52 |     *  IKMP1     = IKM + 1,
  2      53 |     *  ILVLTOT   = 7 * IKM + 37,
  2      54 |     *  ILONFP1   = ILONF + 1,
  2      55 |     *  INVBL     = ILVLTOT * INSUMG,
  2      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  2      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  2      58 |     *  IBLK1D    =  2 * IKM + 14,
  2      59 |     *  IBLK2D    = 16 * IKM + 36,
  2      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  2      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  2      62 |C
  1       2 |C-------
  1       3 |C------- FOLLOWING IS EXTRA STUFF FOR INITIALIZATION
  1       4 |C-------
  1       5 |         PARAMETER (INLATH=101,INUMCOEF=3*INLATH+35,
  1       6 |     *        INUMGRID=4,IJCAPDP=80,INLON=300,INKLON=2,INKLAT=2,
  1       7 |     *        IIORBPL=3,IIORDPL=3,IIORBLP=3,IIORDLP=3,IMCUBIC=1,
  1       8 |     *        INUMVERT=2,INSIG=IKM,INMODES=18,INGMAX=3000,
  1       9 |     *        INKSIG=2,INSIGP1=INSIG+1,INLONEX=INLON+INKLON,
  1      10 |     *        INLATDP=2*INLATH,INLOLA=INLATDP*INLONEX,
  1      11 |     *        IJCAP1DP=IJCAPDP+1,IJCAP12=2*IJCAP1DP,
  1      12 |     *        INCOEFS=IJCAP1DP*(IJCAPDP+2),
  1      13 |     *     IDPFILLR=(4*IKM+1)*INSUMG+       3*(IIJKMAX*4+IIJMAX) )
  1      14 |C    *     IDPFILLR=(4*IKM+1)*INSUMG+INGRDUSE*(IIJKMAX*4+IIJMAX) )
         31 |C--------
         32 |         INTEGER HMODE(1),TSIG(1),RLNPSFC
         33 |         REAL S(INCOEFS,1)
         34 |C--------
         35 |         COMMON/FIXSP03/NSIG,NMODES,
         36 |     *      DELSIG(INSIG),
         37 |     *      HEINVT(INSIG),HEINV(INSIG,INSIG),
         38 |     *      HEINVG(INSIG,INSIG),
         39 |     *      HPIBINVE(INSIG),HTBINVE(INSIG,INSIG),
         40 |     *      HE(INSIG,INSIG),HDEPTHS(INMODES)
         41 |C--------
         42 |C-------- INITIALIZE ALL INTERNAL VARIABLES IF NOT ALREADY DONE
         43 |         IF(NSIG.NE.INSIG) CALL SETSP03
         44 |C--------
         45 |         G=9.8062
         46 |         DO 550 K=1,NSIG
         47 |           DO 548 I=1,NV
         48 |             S(I,TSIG(K))=0.
         49 |548        CONTINUE
         50 |550      CONTINUE
         51 |         DO 552 I=1,NV
         52 |           S(I,RLNPSFC)=0.
         53 |552      CONTINUE
         54 |         DO 575 K=1,NMODIN
         55 |           DO 558 I=1,NV
         56 |             S(I,RLNPSFC)=S(I,RLNPSFC)
         57 |     *           +HPIBINVE(K)*S(I,HMODE(K))*G
         58 |558        CONTINUE
         59 |           DO 570 KKR=1,NSIG
         60 |             KK=NSIG+1-KKR
         61 |             DO 568 I=1,NV
         62 |               S(I,TSIG(KKR))=S(I,TSIG(KKR))
         63 |     *             +HTBINVE(KK,K)*S(I,HMODE(K))*G
         64 |568          CONTINUE
         65 |570        CONTINUE
         66 |575      CONTINUE
         67 |C-------- DIVIDE TEMPS BY GASCON.
         68 |         GASCONI=1./287.04
         69 |         DO 600 K=1,NSIG
         70 |           DO 598 I=1,NV
         71 |             S(I,TSIG(K))=GASCONI*S(I,TSIG(K))
         72 |598        CONTINUE
         73 |600      CONTINUE
         74 |       RETURN
         75 |       END
 
