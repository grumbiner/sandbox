         74 |C
         75 |C     COMMON BLOCK /COMCONST/ CONTAINS GRID-RELATED PARAMETERS,
         76 |C          AND A FEW OTHER COMMON CONSTANTS.
         77 |      COMMON /COMCONST/ IJMAX, KM, LVLTOT, IJRAD, LOFCLDS(2,4),
         78 |     1                  ICALLRAD, IPHYSPL, NGRDUSE, NH,
         79 |     2                  NTIME, ITIME, NSTEPS,
         80 |     3                  IMG(INGRDUSE), JMG(INGRDUSE),
         81 |     4                  IAG(INGRDUS1), JAG(INGRDUS1),
         82 |     5                  IBG(INGRDUS1), JBG(INGRDUS1),
         83 |     6                  IADDRG(INIADDRS, INGRDUSE),
         84 |     7                  NPTSFH(2, INGRDUSE),
         85 |     8                  KUMULUS, LGRIDPPT, KLIFT1, KLIFT2, IBUCKET,
         86 |     9                  XPOLEH(INGRDUSE), YPOLEH(INGRDUSE), RADIUS,
         87 |     1                  DELSIG(IKM), PR(IKM), PRESS(IKM),
         88 |     2                  SIGINT(IKMP1),
         89 |     3                  DTOVDX, ANGVEL,
         90 |     4                  SIGMACC, SIGMAGSP, SIGMADHQ, CRITCONV,
         91 |     5                  SATDEL, RHFACTOR, QBOUND,
         92 |     6                  ANEM, BLKDR, CHARN, CONAUST, DDORF, PKATO,
         93 |     7                  SCALEHT, SIGDOT, DLAMNGM
         94 |C
         95 |      COMMON            SCR     (IIJMAX,  INSCR),
         96 |     1                  SCRGEOG (IIJMAX,  INSCRGEO),
         97 |     2                  SCR3    (IIJKMAX, INSCR3),
         98 |     3                  FILLER  (INFILLER),
         99 |     4                  VBL     (INVBL),
        100 |     5                  BITGRDH (IIJMAX, 2, INGRDUSE),
        101 |     6                  BITGRDU (IIJMAX, 2, INGRDUSE),
        102 |     7                  BITGRDV (IIJMAX, 2, INGRDUSE),
        103 |     8                  BITSEA  (IIJMAX, INGRDUSE),
        104 |     9                  BITSNO  (IIJMAX, INGRDUSE),
        105 |     1                  BITWVL  (IIJMAX, INGRDUSE)
        106 |C
        107 |      LOGICAL BITGRDH, BITGRDU, BITGRDV, BITSEA, BITSNO, BITWVL
        108 |C
        109 |C     COMMON BLOCK /COMSHEM/ CONTAINS VARIABLES NEEDED
        110 |C     FOR THE EQUATORIAL BOUNDARY CONDITIONS.
        111 |      COMMON /COMSHEM/  NSHEM, NSHEMH, NSHEMU, NSHEMV,
        112 |     1                  ISHEMH(INSHEM, 2), ISHEMU(INSHEM, 3),
        113 |     2                  ISHEMV(INSHEM, 3),
        114 |     3                  SHH4H (INSHEM, 4), SHU4U (INSHEM, 4),
        115 |     4                  SHV4U (INSHEM, 4), SHU4V (INSHEM, 4),
        116 |     5                  SHV4V (INSHEM, 4)
        117 |C
        118 |      DIMENSION X1 ( IIJMAX )
        119 |      EQUIVALENCE ( X1, SCR )
        120 |C
        121 |C          GRID A DIMENSIONS
        122 |      IM=2*NH+6
        123 |      IMJM=IM*IM
        124 |C          NUMBER OF H, HU, AND HV BNDRY POINTS.
        125 |      MH=NSHEMH
        126 |      MU=NSHEMU
        127 |      MV=NSHEMV
        128 |C
        129 |C                        DO THE H VARIABLE TYPE POINTS.
        130 |C          FIRST SET DESCRIPTORS USED FOR THE H COMPUTATIONS.
        131 |C          PRESET INITIAL ADDRESS FOR H VBL TYPES OF GRID A
        132 |      IAH=IADDRG(3,1)-IMJM-1
        133 |C
        134 |      DO 10 K=1,KHINTERP
        135 |      IAH=IAH+IMJM
        136 |C          GATHER H VALUES SURROUNDING IMAGE PT INTO X1,--,X4.
        137 |C          COMPUTE BILINEAR INTPD VALUE AT IMAGE POINT
        138 |C          SCATTER RESULT FROM X5 INTO H ARRAY IN VBL.
        139 |CMIC$ PARALLEL SHARED(MH, IAH, X1, ISHEMH, VBL, IM, SHH4H) PRIVATE(IQ2)
        140 |CMIC$ DO PARALLEL VECTOR
        141 |      DO 88890 IQ2=1,MH
        142 |         X1 ( IQ2 ) =
        143 |     *      SHH4H(IQ2,1)*VBL(ISHEMH(IQ2,2)+IAH)+
        144 |     *      SHH4H(IQ2,2)*VBL(ISHEMH(IQ2,2)+IAH+1)+
        145 |     *      SHH4H(IQ2,3)*VBL(ISHEMH(IQ2,2)+IAH+IM)+
        146 |     *      SHH4H(IQ2,4)*VBL(ISHEMH(IQ2,2)+IAH+1+IM)
        147 |88890 CONTINUE
        148 |CFPP$ PERMUTATION(ISHEMH)
        149 |CMIC$ DO PARALLEL VECTOR
        150 |      DO 88891 IQ2 = 1, MH
        151 |         VBL(ISHEMH(IQ2,1)+IAH) = X1 ( IQ2 )
        152 |88891 CONTINUE
        153 |CMIC$ END PARALLEL
        154 |C
        155 |   10 CONTINUE
        156 |C
        157 |C             COMPUTE HU(N=1) AND HV(N=2) BNDRY VALUES.
        158 |C          FIRST THE SPECIAL DESCRIPTORS FOR THE HU OPERATION.
        159 |C          PRESET ADDRESS FOR SCATTER INTO HU ARRAY.
        160 |      ISC=IADDRG(1,1)-IMJM-1
        161 |C          NUMBER OF HU BNDRY POINTS.
        162 |      MX=MU
        163 |C
        164 |      N=0
        165 |C          ENTER N LOOP(N=1 FOR HU PTS, N=2 FOR HV PTS).
        166 |   20 N=N+1
        167 |C          PRESET ADDRESSES FOR GATHERING HU AND HV VALUES  AROUND
        168 |C               THE IMAGE POINT.
        169 |      IGU=IADDRG(1,1)-IMJM-1
        170 |      IGV=IADDRG(2,1)-IMJM-1
        171 |C
        172 |C          ENTER THE K LOOP
        173 |      DO 30 K=1,KM
        174 |      IGU=IGU+IMJM
        175 |      IGV=IGV+IMJM
        176 |      ISC=ISC+IMJM
        177 |      IF ( N .EQ. 1 ) THEN
        178 |C          GATHER HU VALUES SURROUNDING THE IMAGE POINT.
        179 |C          FORM LINEAR COMBINATION OF THESE HU VALUES INTO X5.
        180 |CMIC$ PARALLEL SHARED(MX, ISC, X1, ISHEMU, VBL, IGU, IM, IGV, SHU4U,
        181 |CMIC$1   SHV4U) PRIVATE(IQ2)
        182 |CMIC$ DO PARALLEL VECTOR
        183 |      DO 88960 IQ2=1,MX
        184 |         X1 ( IQ2 ) =
        185 |     *      SHU4U(IQ2,1)*VBL(ISHEMU(IQ2,2)+IGU)+
        186 |     *      SHU4U(IQ2,2)*VBL(ISHEMU(IQ2,2)+IGU+1)+
        187 |     *      SHU4U(IQ2,3)*VBL(ISHEMU(IQ2,2)+IGU+IM)+
        188 |     *      SHU4U(IQ2,4)*VBL(ISHEMU(IQ2,2)+IGU+1+IM)+
        189 |     *      SHV4U(IQ2,1)*VBL(ISHEMU(IQ2,3)+IGV)+
        190 |     *      SHV4U(IQ2,2)*VBL(ISHEMU(IQ2,3)+IGV+1)+
        191 |     *      SHV4U(IQ2,3)*VBL(ISHEMU(IQ2,3)+IGV+IM)+
        192 |     *      SHV4U(IQ2,4)*VBL(ISHEMU(IQ2,3)+IGV+1+IM)
        193 |88960 CONTINUE
        194 |CFPP$ PERMUTATION(ISHEMU)
        195 |CMIC$ DO PARALLEL VECTOR
        196 |      DO 88961 IQ2 = 1, MX
        197 |         VBL(ISHEMU(IQ2,1)+ISC) = X1 ( IQ2 )
        198 |88961 CONTINUE
        199 |CMIC$ END PARALLEL
        200 |      ELSE
        201 |C          GATHER HU VALUES SURROUNDING THE IMAGE POINT.
        202 |C          FORM LINEAR COMBINATION OF THESE HU VALUES INTO X5.
        203 |CMIC$ PARALLEL SHARED(MX, ISC, X1, ISHEMV, VBL, IGU, IM, IGV, SHU4V,
        204 |CMIC$1   SHV4V) PRIVATE(IQ2)
        205 |CMIC$ DO PARALLEL VECTOR
        206 |      DO 88962 IQ2=1,MX
        207 |         X1 ( IQ2 ) =
        208 |     *      SHU4V(IQ2,1)*VBL(ISHEMV(IQ2,2)+IGU)+
        209 |     *      SHU4V(IQ2,2)*VBL(ISHEMV(IQ2,2)+IGU+1)+
        210 |     *      SHU4V(IQ2,3)*VBL(ISHEMV(IQ2,2)+IGU+IM)+
        211 |     *      SHU4V(IQ2,4)*VBL(ISHEMV(IQ2,2)+IGU+1+IM)+
        212 |     *      SHV4V(IQ2,1)*VBL(ISHEMV(IQ2,3)+IGV)+
        213 |     *      SHV4V(IQ2,2)*VBL(ISHEMV(IQ2,3)+IGV+1)+
        214 |     *      SHV4V(IQ2,3)*VBL(ISHEMV(IQ2,3)+IGV+IM)+
        215 |     *      SHV4V(IQ2,4)*VBL(ISHEMV(IQ2,3)+IGV+1+IM)
        216 |88962 CONTINUE
        217 |CFPP$ PERMUTATION(ISHEMV)
        218 |CMIC$ DO PARALLEL VECTOR
        219 |      DO 88963 IQ2 = 1, MX
        220 |         VBL(ISHEMV(IQ2,1)+ISC) = X1 ( IQ2 )
        221 |88963 CONTINUE
        222 |CMIC$ END PARALLEL
        223 |      END IF
        224 |C
        225 |   30 CONTINUE
        226 |C
        227 |C                    ARE WE DONE WITH HU AND HV,,,
        228 |      IF( N .EQ. 2 )  GO TO 40
        229 |C
        230 |C          N=1, MUST DO HV PTS.   SET SPECIAL DESCRIPTORS FOR HV.
        231 |C          PRESET ADDRESS FOR SCATTER INTO HV ARRAY
        232 |      ISC=IADDRG(2,1)-IMJM-1
        233 |C          NUMBER OF HV BNDRY POINTS.
        234 |      MX=MV
        235 |C          RE-ENTER THE N LOOP
        236 |      GO TO 20
        237 |C
        238 |   40 CONTINUE
        239 |C
        240 |C
        241 |      RETURN
        242 |      END
 
