         46 |         MN=M*N
         47 |         FMAX=-1.E30
         48 |         FMIN=1.E30
         49 |         FNEWMAX=-1.E30
         50 |         FNEWMIN=1.E30
         51 |         DIFRMS=0.
         52 |         DIFMEAN=0.
         53 |         DIFMAX=-1.E30
         54 |         DIFMIN=1.E30
         55 |         IDIFMAX=0
         56 |         IDIFMIN=0
         57 |         DO 50 I=1,MN
         58 |           FMAX=MAX(F(I),FMAX)
         59 |           FNEWMAX=MAX(FNEW(I),FNEWMAX)
         60 |           FMIN=MIN(F(I),FMAX)
         61 |           FNEWMIN=MIN(FNEW(I),FNEWMIN)
         62 |           DIFF=F(I)-FNEW(I)
         63 |           DIFRMS=DIFRMS+DIFF**2
         64 |           IF(DIFF.GT.DIFMAX) THEN
         65 |             DIFMAX=DIFF
         66 |             IDIFMAX=I
         67 |           END IF
         68 |           IF(DIFF.LT.DIFMIN) THEN
         69 |             DIFMIN=DIFF
         70 |             IDIFMIN=I
         71 |           END IF
         72 |           DIFMEAN=DIFMEAN+DIFF
         73 |50       CONTINUE
         74 |         DIFMEAN=DIFMEAN/MN
         75 |         DIFRMS=SQRT(DIFRMS/MN)
         76 |         JDMAX=IDIFMAX/M
         77 |         IDMAX=IDIFMAX-M*JDMAX
         78 |         JDMAX=JDMAX+1
         79 |         JDMIN=IDIFMIN/M
         80 |         IDMIN=IDIFMIN-M*JDMIN
         81 |         JDMIN=JDMIN+1
         82 |         WRITE(6,100)LABEL(K),GRID(NG),FMAX,FNEWMAX,FMIN,FNEWMIN,
         83 |     *      DIFRMS,DIFMAX,DIFMIN,DIFMEAN,IDMAX,JDMAX,IDMIN,JDMIN,
         84 |     *       Z(IDIFMAX),Z(IDIFMIN)
         85 | 100     FORMAT(1H ,A4,2X,A6,' OLD,NEW MAX=',2E12.5,' OLD,NEW MIN=',
         86 |     *      2E12.5,/,'          OLD-NEW RMS,MAX,MIN,MEAN=',4E12.5,/,
         87 |     *    '          I,JMAX=',2I5,'  I,JMIN=',2I5,
         88 |     *        '  TERRAIN AT MAX,MIN DIFF=',2E12.5)
         89 |       RETURN
