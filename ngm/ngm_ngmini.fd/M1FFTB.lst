          1 |      SUBROUTINE M1FFTB(WORK,A,TRIGS,INC,JUMP,N,LOT)
          2 |C     SUBROUTINE M1FFTB - POSTPROCESSING STEP FOR FFT99, ISIGN=-1
          3 |C     (GRIDPOINT TO SPECTRAL TRANSFORM)
          4 |C
          5 |      DIMENSION WORK(N),A(N),TRIGS(N)
          6 |C
          7 |      NH=N/2
          8 |      NX=N
          9 |      INK=INC+INC
         10 |C
         11 |C     A(0)   A(N/2)
         12 |      SCALE=1.0 E0/FLOAT(N)
         13 |      IA=1
         14 |      IB=2
         15 |      JA=1
         16 |      JB=N*INC+1
         17 |CDIR$ IVDEP
         18 |      DO 10 L=1,LOT
         19 |      A(JA)=SCALE*(WORK(IA)+WORK(IB))
         20 |      A(JA+INC)=0.0 E0
         21 |      IA=IA+NX
         22 |      IB=IB+NX
         23 |      JA=JA+JUMP
         24 |      JB=JB+JUMP
         25 |   10 CONTINUE
         26 |C
         27 |C     REMAINING WAVENUMBERS
         28 |      SCALE=0.5 E0*SCALE
         29 |      IABASE=3
         30 |      IBBASE=N-1
         31 |      JABASE=2*INC+1
         32 |      JBBASE=(N-2)*INC+1
         33 |C
         34 |      DO 30 K=3,NH,2
         35 |      IA=IABASE
         36 |      IB=IBBASE
         37 |      JA=JABASE
         38 |      JB=JBBASE
         39 |      C=TRIGS(N+K)
         40 |      S=TRIGS(N+K+1)
         41 |CDIR$ IVDEP
         42 |      DO 20 L=1,LOT
         43 |      A(JA)=SCALE*((WORK(IA)+WORK(IB))
         44 |     *   +(C*(WORK(IA+1)+WORK(IB+1))+S*(WORK(IA)-WORK(IB))))
         45 |      A(JB)=SCALE*((WORK(IA)+WORK(IB))
         46 |     *   -(C*(WORK(IA+1)+WORK(IB+1))+S*(WORK(IA)-WORK(IB))))
         47 |      A(JA+INC)=SCALE*((C*(WORK(IA)-WORK(IB))-S*(WORK(IA+1)+WORK(IB+1)))
         48 |     *    +(WORK(IB+1)-WORK(IA+1)))
         49 |      A(JB+INC)=SCALE*((C*(WORK(IA)-WORK(IB))-S*(WORK(IA+1)+WORK(IB+1)))
         50 |     *    -(WORK(IB+1)-WORK(IA+1)))
         51 |      IA=IA+NX
         52 |      IB=IB+NX
         53 |      JA=JA+JUMP
         54 |      JB=JB+JUMP
         55 |   20 CONTINUE
         56 |      IABASE=IABASE+2
         57 |      IBBASE=IBBASE-2
         58 |      JABASE=JABASE+INK
         59 |      JBBASE=JBBASE-INK
         60 |   30 CONTINUE
         61 |C
         62 |      IF (IABASE.NE.IBBASE) GO TO 50
         63 |C     WAVENUMBER N/4 (IF IT EXISTS)
         64 |      IA=IABASE
         65 |      JA=JABASE
         66 |      SCALE=2.0 E0*SCALE
         67 |CDIR$ IVDEP
         68 |      DO 40 L=1,LOT
         69 |      A(JA)=SCALE*WORK(IA)
         70 |      A(JA+INC)=-SCALE*WORK(IA+1)
         71 |      IA=IA+NX
         72 |      JA=JA+JUMP
         73 |   40 CONTINUE
         74 |C
         75 |   50 CONTINUE
         76 |      RETURN
         77 |      END
 
