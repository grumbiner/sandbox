  1      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  1      62 |C
         64 |C
         65 |C...TRANSLATED BY FPP 3.00Z36 11/09/90  14:44:53
         66 |C...SWITCHES: OPTON=I47,OPTOFF=VAE0
         67 |C
         68 |C              (BITS IS A SCRATCH BIT VECTOR FOR AN I-PATTERN.)
         69 |      LOGICAL BITH(IIJMAX,2), BITU(IIJMAX,2), BITV(IIJMAX,2)
         70 |      LOGICAL BITSTRUH(512), BITSTRV(512), BITSZERO(512)
         71 |C
         72 |C              SPECIFY THE UNIT NUMBER OF THE PRINTER.
         73 |      IOUTUPRT = 6
         74 |C
         75 |C              REFUSE TO DO GRID A ( DONE BY SHEMPRE)
         76 |      IF( NG .GT. 1 ) GO TO 10
         77 |      WRITE (IOUTUPRT, 5) NG
         78 |    5 FORMAT(1H1,2X,'*****SUBR. BITOUT WAS ASKED TO DO GRID A.NG=',I12)
         79 |C
         80 |      CALL EXIT(16)
         81 |C
         82 |   10 CONTINUE
         83 |C
         84 |C              ENSURE THAT NO MORE THAN 512 POINTS, THE DIMENSION OF
         85 |C              SEVERAL BIT STRINGS, IS NOT EXCEEDED.  IF IT IS,
         86 |C              SIMPLY INCREASE ALL REFERENCES TO 512 IN THIS
         87 |C              SUBROUTINE TO SOME SATISFACTORY VALUE.
         88 |        IF (IM .LE. 512) GO TO 18
         89 |        WRITE (IOUTUPRT, 14) IM
         90 |   14   FORMAT ('1', 2X,'***** SUBROUTINE BITOUT WAS ASKED ',
         91 |     1                  'TO HANDLE A FIELD OF', I4, ' POINTS IN ',
         92 |     2      'THE I DIRECTION.'/
         93 |     3          ' ', 8X,'THIS IS MORE THAN THE 512 LIMIT ',
         94 |     4                  'HARDWIRED TO SAVE SPACE IN THIS SUBROUTINE.')
         95 |C
         96 |        CALL EXIT(16)
         97 |C
         98 |   18   CONTINUE
         99 |C
        100 |C            INITIALIZE LOGICAL VECTORS
        101 |CMIC$ PARALLEL SHARED(BITSZERO, BITSTRUH, BITSTRV, BITH, BITU, BITV)
        102 |CMIC$1    PRIVATE(I, J)
        103 |C
        104 |C            INITIALIZE LOGICAL VECTORS
        105 |CMIC$ DO PARALLEL
        106 |      DO 57 J = 1, 20568
        107 |         BITH(J,1) = .FALSE.
        108 |         BITU(J,1) = .FALSE.
        109 |         BITV(J,1) = .FALSE.
        110 |   57 CONTINUE
        111 |CMIC$ DO PARALLEL VECTOR
        112 |        DO 58 I = 1, 512
        113 |           BITSZERO ( I ) = .FALSE.
        114 |           BITSTRUH ( I ) = .TRUE.
        115 |           BITSTRV ( I )  = .TRUE.
        116 |58      CONTINUE
        117 |CMIC$ END PARALLEL
        118 |C
        119 |C
        120 |      NPAIR = 1
        121 |C
        122 |C              SET BIT STRINGS OF NONFORECAST POINTS FOR U/H AND V
        123 |C              POINTS AFTER THE FIRST TIME STEP OF A PAIR.
        124 |      BITSTRUH(1)    = .FALSE.
        125 |      BITSTRUH(2)    = .FALSE.
        126 |      BITSTRUH(IM)   = .FALSE.
        127 |      BITSTRV (1)    = .FALSE.
        128 |      BITSTRV (2)    = .FALSE.
        129 |      BITSTRV (IM-1) = .FALSE.
        130 |      BITSTRV (IM)   = .FALSE.
        131 |C
        132 |      IADD  = IM + 1
        133 |      JM2   = JM - 2
        134 |C
        135 |      IF (JM2 - 2 .GT. 0) THEN
        136 |CMIC$    DO ALL SHARED(JM2, IADD, IM, BITSTRUH, BITU, BITH, BITSTRV,
        137 |CMIC$1      BITV) PRIVATE(J, IQ2W6E)
        138 |         DO 200 J = 1, JM2 - 2
        139 |            DO 77001 IQ2W6E = 1, IM
        140 |               BITU(IADD+IQ2W6E-1+J*IM,1) = BITSTRUH(IQ2W6E)
        141 |               BITH(IADD+IQ2W6E-1+J*IM,1) = BITSTRUH(IQ2W6E)
        142 |               BITV(IADD+IQ2W6E-1+J*IM,1) = BITSTRV(IQ2W6E)
        143 |77001       CONTINUE
        144 |  200    CONTINUE
        145 |         IADD = (JM2-2)*IM + IADD
        146 |      ENDIF
        147 |C
        148 |C              NOW JM-1 ROW.
        149 |      IADD = IADD + IM
        150 |CMIC$ DO ALL VECTOR SHARED(IM, IADD, BITSZERO, BITU, BITSTRUH, BITH,
        151 |CMIC$1   BITSTRV, BITV) PRIVATE(IQ2W6E)
        152 |      DO 88920 IQ2W6E = 1, IM
        153 |         BITU(IADD+IQ2W6E-1,1) = BITSZERO(IQ2W6E)
        154 |         BITH(IADD+IQ2W6E-1,1) = BITSTRUH(IQ2W6E)
        155 |         BITV(IADD+IQ2W6E-1,1) = BITSTRV(IQ2W6E)
        156 |88920 CONTINUE
        157 |C
        158 |C
        159 |      NPAIR = 2
        160 |C
        161 |C              ADD ADDITIONAL NONFORECAST POINTS FOR U/H AND V POINTS
        162 |C              AFTER THE SECOND TIME STEP OF A PAIR.
        163 |      BITSTRUH(3)    = .FALSE.
        164 |      BITSTRUH(4)    = .FALSE.
        165 |      BITSTRUH(IM-2) = .FALSE.
        166 |      BITSTRUH(IM-1) = .FALSE.
        167 |      BITSTRV (3)    = .FALSE.
        168 |      BITSTRV (4)    = .FALSE.
        169 |      BITSTRV (IM-3) = .FALSE.
        170 |      BITSTRV (IM-2) = .FALSE.
        171 |      IADD = 3 * IM + 1
        172 |      JM4  = JM - 4
        173 |C
        174 |      IF (JM4 - 4 .GT. 0) THEN
        175 |CMIC$    DO ALL SHARED(JM4, IADD, IM, BITSTRUH, BITU, BITH, BITSTRV,
        176 |CMIC$1      BITV) PRIVATE(J, IQ2W6E)
        177 |         DO 300 J = 1, JM4 - 4
        178 |            DO 77002 IQ2W6E = 1, IM
        179 |               BITU(IADD+IQ2W6E-1+J*IM,2) = BITSTRUH(IQ2W6E)
        180 |               BITH(IADD+IQ2W6E-1+J*IM,2) = BITSTRUH(IQ2W6E)
        181 |               BITV(IADD+IQ2W6E-1+J*IM,2) = BITSTRV(IQ2W6E)
        182 |77002       CONTINUE
        183 |  300    CONTINUE
        184 |         IADD = (JM4-4)*IM + IADD
        185 |      ENDIF
        186 |C
        187 |C              NOW THE JM-3 ROW.
        188 |      IADD = IADD + IM
        189 |CMIC$ DO ALL VECTOR SHARED(IM, IADD, BITSZERO, BITU, BITSTRUH, BITH,
        190 |CMIC$1   BITSTRV, BITV) PRIVATE(IQ2W6E)
        191 |      DO 88980 IQ2W6E = 1, IM
        192 |         BITU(IADD+IQ2W6E-1,2) = BITSZERO(IQ2W6E)
        193 |         BITH(IADD+IQ2W6E-1,2) = BITSTRUH(IQ2W6E)
        194 |         BITV(IADD+IQ2W6E-1,2) = BITSTRV(IQ2W6E)
        195 |88980 CONTINUE
        196 |C
        197 |      RETURN
        198 |C
        199 |      END
 
