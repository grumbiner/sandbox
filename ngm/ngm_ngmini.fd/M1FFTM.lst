          1 |      SUBROUTINE M1FFTM(A,WORK,TRIGS,IFAX,INC,JUMP,N,LOT,ISIGN)
          2 |      DIMENSION A(N),WORK(N),TRIGS(N),IFAX(1)
          3 |C
          4 |      NFAX=IFAX(1)
          5 |      NX=N
          6 |      NH=N/2
          7 |      INK=INC+INC
          8 |      IF (ISIGN.EQ.+1) GO TO 30
          9 |C
         10 |C     IF NECESSARY, TRANSFER DATA TO WORK AREA
         11 |      IGO=50
         12 |      IF (MOD(NFAX,2).EQ.1) GOTO 40
         13 |      IBASE=1
         14 |      JBASE=1
         15 |      DO 20 L=1,LOT
         16 |      I=IBASE
         17 |      J=JBASE
         18 |CDIR$ IVDEP
         19 |      DO 10 M=1,N
         20 |      WORK(J)=A(I)
         21 |      I=I+INC
         22 |      J=J+1
         23 |   10 CONTINUE
         24 |      IBASE=IBASE+JUMP
         25 |      JBASE=JBASE+NX
         26 |   20 CONTINUE
         27 |C
         28 |      IGO=60
         29 |      GO TO 40
         30 |C
         31 |C     PREPROCESSING (ISIGN=+1)
         32 |C     ------------------------
         33 |C
         34 |   30 CONTINUE
         35 |      CALL M1FFTA(A,WORK,TRIGS,INC,JUMP,N,LOT)
         36 |      IGO=60
         37 |C
         38 |C     COMPLEX TRANSFORM
         39 |C     -----------------
         40 |C
         41 |   40 CONTINUE
         42 |      IA=1
         43 |      LA=1
         44 |      DO 80 K=1,NFAX
         45 |      IF (IGO.EQ.60) GO TO 60
         46 |   50 CONTINUE
         47 |      CALL M1VPAS(A(IA),A(IA+INC),WORK(1),WORK(2),TRIGS,
         48 |     *   INK,2,JUMP,NX,LOT,NH,IFAX(K+1),LA)
         49 |      IGO=60
         50 |      GO TO 70
         51 |   60 CONTINUE
         52 |      CALL M1VPAS(WORK(1),WORK(2),A(IA),A(IA+INC),TRIGS,
         53 |     *    2,INK,NX,JUMP,LOT,NH,IFAX(K+1),LA)
         54 |      IGO=50
         55 |   70 CONTINUE
         56 |      LA=LA*IFAX(K+1)
         57 |   80 CONTINUE
         58 |C
         59 |      IF (ISIGN.EQ.-1) GO TO 130
         60 |C
         61 |C     IF NECESSARY, TRANSFER DATA FROM WORK AREA
         62 |      IF (MOD(NFAX,2).EQ.1) GO TO 110
         63 |      IBASE=1
         64 |      JBASE=1
         65 |      DO 100 L=1,LOT
         66 |      I=IBASE
         67 |      J=JBASE
         68 |CDIR$ IVDEP
         69 |      DO 90 M=1,N
         70 |      A(J)=WORK(I)
         71 |      I=I+1
         72 |      J=J+INC
         73 |   90 CONTINUE
         74 |      IBASE=IBASE+NX
         75 |      JBASE=JBASE+JUMP
         76 |  100 CONTINUE
         77 |C
         78 |C     FILL IN ZEROS AT END
         79 |  110 CONTINUE
         80 |      GO TO 140
         81 |C
         82 |C     POSTPROCESSING (ISIGN=-1):
         83 |C     --------------------------
         84 |C
         85 |  130 CONTINUE
         86 |      CALL M1FFTB(WORK,A,TRIGS,INC,JUMP,N,LOT)
         87 |C
         88 |  140 CONTINUE
         89 |      RETURN
         90 |      END
 
