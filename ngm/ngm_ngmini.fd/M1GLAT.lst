          1 |      SUBROUTINE M1GLAT(KHALF,COLRAD,WGT,WGTCS,RCS2)
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .                                       .
          4 |C SUBPROGRAM:    M1GLAT      COMPUTE GAUSSIAN LATS AND WEIGHTS
          5 |C   PRGMMR: SELA             ORG: W/NMC22    DATE: 79-03-03
          6 |C
          7 |C ABSTRACT: COMPUTE GAUSSIAN LATITUDES AND WEIGHTS.
          8 |C   SEE P 887, 25.4.29, HANDBOOK OF MATH FUNCTIONS, ABRAMOWITZ AND
          9 |C   STEGUN, FOR DETAILS.
         10 |C
         11 |C PROGRAM HISTORY LOG:
         12 |C   79-03-03  SELA
         13 |C   88-04-08  PARRISH   ADD DOCBLOCK
         14 |C
         15 |C USAGE:    CALL M1GLAT(KHALF,COLRAD,WGT,WGTCS,RCS2)
         16 |C   INPUT ARGUMENT LIST:
         17 |C     KHALF    - NUMBER OF GAUSSIAN LATITUDES TO COMPUTE (POLE TO
         18 |C              - EQUATOR)
         19 |C
         20 |C   OUTPUT ARGUMENT LIST:
         21 |C     COLRAD   - GAUSSIAN COLATITUDES IN RADIANS (FULL PRECISION)
         22 |C     WGT      - INTEGRATION WEIGHTS (FULL PRECISION)
         23 |C     WGTCS    - WGT/(COS(LAT))**2
         24 |C     RCS2     - 1/(COS(LAT))**2
         25 |C
         26 |C ATTRIBUTES:
         27 |C   LANGUAGE: FTN200
         28 |C   MACHINE:  CYBER
         29 |C
         30 |C$$$
         31 |      implicit double precision (a-h,o-z)
         32 |      double precision COLRAD(1),WGT(1),WGTCS(1),RCS2(1)
         33 |      EPS=1.d-12
         34 |      SI=1.d0
         35 |      K2=2*KHALF
         36 |      RK2=K2
         37 |      SCALE=2.d0  /(RK2*RK2)
         38 |      K1=K2-1
         39 |      PI=ATAN(SI)*4.d0
         40 |      DRADZ=PI/360.d0
         41 |      RAD=0.d0
         42 |      DO 1000 K=1,KHALF
         43 |      ITER=0
         44 |      DRAD=DRADZ
         45 |1     CALL M1POLY(K2,RAD,P2)
         46 |2     P1 =P2
         47 |      ITER=ITER+1
         48 |      RAD=RAD+DRAD
         49 |      CALL M1POLY(K2,RAD,P2)
         50 |      IF(SIGN(SI,P1).EQ.SIGN(SI,P2))GO TO 2
         51 |      IF(DRAD.LT.EPS)GO TO 3
         52 |      RAD=RAD-DRAD
         53 |      DRAD=DRAD*0.25d0
         54 |      GO TO 1
         55 |3     CONTINUE
         56 |      COLRAD(K)=RAD
         57 |      PHI=RAD   *180.d0/PI
         58 |      CALL M1POLY(K1,RAD,P1)
         59 |      X=  COS(RAD)
         60 |      W=SCALE*(1.d0-X*X)/(P1*P1)
         61 |      WGT(K)=    W
         62 |      SN=  SIN(RAD)
         63 |      W=W/(SN*SN)
         64 |      WGTCS(K)=    W
         65 |      RC=1.d0/(SN*SN)
         66 |      RCS2(K)=   RC
         67 |      CALL M1POLY(K2,RAD,P1)
         68 |      PRPHI =    PHI
         69 |      PRCOL =    COLRAD(K)
         70 |1000  CONTINUE
         71 |C     THIS LINE COMES FROM META FOR DEBUG
         72 |      RETURN
         73 |      END
 
