         47 |C
         48 |      COMMON            SCR     (IIJMAX,  INSCR),
         49 |     1                  SCRGEOG (IIJMAX,  INSCRGEO),
         50 |     2                  SCR3    (IIJKMAX, INSCR3),
         51 |     3                  FILLER  (INFILLER),
         52 |     4                  VBL     (INVBL),
         53 |     5                  BITGRDH (IIJMAX, 2, INGRDUSE),
         54 |     6                  BITGRDU (IIJMAX, 2, INGRDUSE),
         55 |     7                  BITGRDV (IIJMAX, 2, INGRDUSE),
         56 |     8                  BITSEA  (IIJMAX, INGRDUSE),
         57 |     9                  BITSNO  (IIJMAX, INGRDUSE),
         58 |     1                  BITWVL  (IIJMAX, INGRDUSE)
         59 |C
         60 |      LOGICAL BITGRDH, BITGRDU, BITGRDV, BITSEA, BITSNO, BITWVL
         61 |C
