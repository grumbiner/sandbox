         92 |     1     INUNIT, IOUTUNIT,
         93 |     2     KGTYPE, IMOUT, JMOUT, DNPEQOUT, DLAMOUT, XIPOUT, YJPOUT,
         94 |C
         95 |C          THE FOLLOWING COME FROM  MAIN  READING THE DATA CARDS.
         96 |     2   LQTYPE(IMAXOUTV),   LSTYPE(IMAXOUTV),
         97 |     3   DESCRIPT(IMAXOUTV), LEVELS(ILMOUT,IMAXOUTV),  NOUTVBLS,
         98 |C
         99 |C          THE FOLLOWING ARE DERIVED BY   OUTPUT  .
        100 |     4       IJOUT, LMOUT, NFILLER, NS2, NS3NGM, NS3OUT,
        101 |     5       COSOUT,   SINOUT,    DXOUT,    ZSIGLYR(IKM),
        102 |C
        103 |C           ARRAYS  PLOUT  AND  HTFD  ARE SET BY DATA STATEMENTS IN
        104 |C           THE SR  OUTPUT  .  'PADDER' FILLS OUT TO FULL WORD BNDRY.
        105 |     6      PLOUT(ILMOUT),   HTFD( 6),  PADDER,
        106 |C
        107 |C          THE FOLLOWING ADDRESSES AND ADDRESS ARRAYS ARE SET UP
        108 |C          BY   OUTPREP  WHEN IT IS CALLED BY   OUTPUT  .
        109 |     7  J1(INGRDUSE),          J2(INGRDUSE),   JADDK2L(INGRDUSE,2),
        110 |     8  JADDFNGM(INGRDUSE), JHREC(INGRDUSE),   JHKAP(INGRDUSE),
        111 |     9  JSCR2(12),    JSCR3NGM(INGRDUSE,12),   JSCR3OUT(12),
        112 |     A  LENGD(INGRDUSE), LTYPE(ILMOUT,INGRDUSE), MB1, MB2, MINT,
        113 |     B   MSOUT1,  MSOUT2,  MZGNDOUT,  MNGM3,  MHOUT, MZFR, MZTRO,
        114 |     C      MSZGND(INGRDUSE), NGRIDATA(INGRDUSE),
