        139 |         IJ=IMG(NG)*JMG(NG)
        140 |         MADDUC=IADDRGDP(1,NG)-1
        141 |         MADDVC=IADDRGDP(2,NG)-1
        142 |         MADDTC=IADDRGDP(3,NG)-1
        143 |         MADDQC=IADDRGDP(4,NG)-1
        144 |         MADDHC=IADDRGDP(5,NG)-1
        145 |         DO 100 K=1,KM
        146 |           DO 50 I=1,IJ
        147 |             VBLCOPY(MADDUC+I)=0.
        148 |             VBLCOPY(MADDVC+I)=0.
        149 |             VBLCOPY(MADDTC+I)=0.
        150 |             VBLCOPY(MADDQC+I)=0.
        151 |50         CONTINUE
        152 |           MADDTC=MADDTC+IJ
        153 |           MADDVC=MADDVC+IJ
        154 |           MADDUC=MADDUC+IJ
        155 |           MADDQC=MADDQC+IJ
        156 |100      CONTINUE
        157 |         DO 150 I=1,IJ
        158 |           VBLCOPY(MADDHC+I)=0.
        159 |150      CONTINUE
        160 |       RETURN
        161 |       END
 
