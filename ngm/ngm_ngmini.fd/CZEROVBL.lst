         31 |     1                  ICALLRAD, IPHYSPL, NGRDUSE, NH,
         32 |     2                  NTIME, ITIME, NSTEPS,
         33 |     3                  IMG(INGRDUSE), JMG(INGRDUSE),
         34 |     4                  IAG(INGRDUS1), JAG(INGRDUS1),
         35 |     5                  IBG(INGRDUS1), JBG(INGRDUS1),
         36 |     6                  IADDRG(INIADDRS, INGRDUSE),
         37 |     7                  NPTSFH(2, INGRDUSE),
         38 |     8                  KUMULUS, LGRIDPPT, KLIFT1, KLIFT2, IBUCKET,
         39 |     9                  XPOLEH(INGRDUSE), YPOLEH(INGRDUSE), RADIUS,
         40 |     1                  DELSIG(IKM), PR(IKM), PRESS(IKM),
