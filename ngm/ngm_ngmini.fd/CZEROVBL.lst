         67 |     3                  NPTSWATR(2, INGRDUSE), NPTSCC  (2, INGRDUSE),
         68 |     4                  KKGSP2  (2, INGRDUSE),
         69 |     5                  KKGSE2  (2, INGRDUSE),
         70 |     6                  NPTSSAT (IKM, 2, INGRDUSE),
         71 |     7                  NPTSEVAP(IKM, 2, INGRDUSE),
         72 |     8                  NPTSGSP (2, INGRDUSE), NUMPROF,
         73 |     9                  PPTCC   (2, INGRDUSE), PPTGSP  (2, INGRDUSE),
         74 |     1                  ALATPR  (IMAXPROF),    ALONPR  (IMAXPROF),
         75 |     2                  DESCRP  (IMAXPROF)
         76 |C
         77 |      CHARACTER*24 DESCRP
         78 |C
         79 |C     COMMON BLOCK /COMOUT/ CONTAINS OUTPUT CODE PARAMETERS
         80 |C               AND ARRAYS.
         81 |C
