        265 |             SIGOLD=SIGNEW
        266 |c            WRITE(6,111)DSIG(I),SIGHAT(I),SIGKAP(I)
        267 |c11          FORMAT(' DSIG,SIGHAT,SIGKAP=',3E12.4)
        268 |112        CONTINUE
        269 |           CALL CONVERTA(VBL(MADDU),VBL(MADDV),VBL(MADDT),VBL(MADDH),IJ,
