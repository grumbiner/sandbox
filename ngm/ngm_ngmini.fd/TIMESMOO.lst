        202 |        DO 9100 LOOP=1,2
        203 |C--------
        204 |C-------- ON FIRST TIME AROUND, GET TENDENCIES ON ALL GRIDS.
        205 |C-------- AT END OF FIRST TIME (LOOP=1,NG=NGRDUSE), COMPUTE TEMPERTON
        206 |C-------- CORRECTIONS.  ON SECOND TIME AROUND, ADD INCREMENTS
        207 |C-------- BACK TO FULL FIELDS.
        208 |C--------
        209 |        DO 9000 NG=1,INGRDUSE
        210 |         IBITNG2=MIN(NG,2)
        211 |         WRITE(6,10)NG
        212 | 10      FORMAT(' COMPUTING TENDENCY FOR NG=',I4)
        213 |         KHINTERP=2*KM+1
        214 |         IJ=IMG(NG)*JMG(NG)
        215 |         MADDU=IADDRG(1,NG)
        216 |         MADDV=IADDRG(2,NG)
        217 |         MADDT=IADDRG(3,NG)
        218 |         MADDQ=IADDRG(4,NG)
        219 |         MADDH=IADDRG(5,NG)
        220 |         MADDUC=IADDRGDP(1,NG)
        221 |         MADDVC=IADDRGDP(2,NG)
        222 |         MADDTC=IADDRGDP(3,NG)
        223 |         MADDQC=IADDRGDP(4,NG)
        224 |         MADDHC=IADDRGDP(5,NG)
