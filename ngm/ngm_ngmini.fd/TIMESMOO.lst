        243 |           CALL FROMVBL(W00U,W00V,W00TH,W00Q,W00H,NG)
        244 |           NPAIR=1
        245 |           LASTSTEP=0
        246 |C------------ MAKE SURE PHYSICS IS TURNED OFF (SET IPHYSPL=1)
        247 |           IPHYSPL=1
        248 |           CALL ONEGRID(NG,NPAIR,LASTSTEP)
        249 |             IF(NG.EQ.1) CALL SHEM(KHINTERP)
        250 |C----------
