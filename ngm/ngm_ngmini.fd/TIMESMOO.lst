        309 |     *           -VBLCOPY(MADDHC+I-1)
        310 |21004      CONTINUE
        311 |C--------
        312 |C-------- CONVERT TEMP TENDENCIES TO VIRTUAL TEMP TENDENCIES
        313 |C--------
        314 |         CALL GETVIRTT(R0TH,W00H,W00Q,IJ,IJMAX,KM)
        315 |C--------
        316 |C--------  OBTAIN EIGENVALUES, TRANSFORM MATRICES.
        317 |C--------
        318 |C        CALL VMATHS(TBAR,DSIG,PR,CPDP,RKAPPA,GASCON,
        319 |C    *                   SIGHAT,HDEPTHS,HEINVT,HEINV,HEINVG,
        320 |C    *                   HPIBINVE,HTBINVE,HE)
        321 |         CALL VTRANSFR(HDEPTHS,HEINVT,HEINV,HEINVG,HPIBINVE,HTBINVE,HE)
        322 |C--------
        323 |C-------- TRANSFORM TO VERTICAL MODE SPACE
        324 |C--------
        325 |         DO 225 KK=1,NUMVERT
        326 |           DO 22502 I=1,IJ
        327 |             W0TH(I,KK)=HEINVT(KK)*R0H(I)
        328 |             W0U(I,KK)=0.
        329 |             W0V(I,KK)=0.
        330 |22502      CONTINUE
        331 |           DO 220 KR=1,IKM
        332 |             K=IKM+1-KR
        333 |             DO 22002 I=1,IJ
        334 |               W0U(I,KK)=W0U(I,KK)+HEINV(KK,K)*
        335 |     *                       R0U(I,KR)
        336 |               W0V(I,KK)=W0V(I,KK)+HEINV(KK,K)*
        337 |     *                       R0V(I,KR)
        338 |               W0TH(I,KK)=W0TH(I,KK)+HEINVG(KK,K)*
        339 |     *                       R0TH(I,KR)
        340 |22002        CONTINUE
        341 |220        CONTINUE
        342 |225      CONTINUE
        343 |C--------SAVE ALL GRIDS IN SPECIAL HOLDING ARRAYS PTSAVE,UTSAVE,VTSAVE
        344 |         DO 235 KK=1,NUMVERT
        345 |           DO 23502 I=1,IJ
        346 |             PTSAVE(I,KK,NG)=W0TH(I,KK)
        347 |             UTSAVE(I,KK,NG)=W0U(I,KK)
        348 |             VTSAVE(I,KK,NG)=W0V(I,KK)
        349 |23502      CONTINUE
        350 |235      CONTINUE
        351 |C--------
        352 |C-------- RESTORE ORIGINAL VBL.
        353 |C--------
        354 |         CALL TOVBL(W00U,W00V,W00TH,W00Q,W00H,NG)
        355 |C--------
        356 |C-------- SAVE FIRST NUMVERT LEVELS OF U,V,T FROM VBL IN U0SAVE,ETC.
        357 |C--------
        358 |         CALL SFROMVBL(U0SAVE(1,1,NG),V0SAVE(1,1,NG),P0SAVE(1,1,NG),NG)
        359 |C--------
        360 |C-------- IF NG=NGRDUSE, THEN DO INITIALIZATION ON ALL GRIDS.
        361 |C--------
        362 |         IF(NG.EQ.INGRDUSE) THEN
        363 |C--------
        364 |C-------- 1. FIX UP BOUNDARIES FOR TENDENCIES
        365 |C--------
        366 |           DO 241 NGG=1,INGRDUSE
        367 |             CALL STOVBL(UTSAVE(1,1,NGG),VTSAVE(1,1,NGG),
        368 |     *          PTSAVE(1,1,NGG),NGG)
        369 |241        CONTINUE
        370 |           DO 242 NGG=1,INGRDUSE
        371 |             IF(NGG.LT.INGRDUSE) CALL LATBND(NGG,NUMVERT)
        372 |             IF(NGG.EQ.1) CALL SHEM(NUMVERT)
        373 |             CALL SFROMVBL(UTSAVE(1,1,NGG),VTSAVE(1,1,NGG),
        374 |     *          PTSAVE(1,1,NGG),NGG)
        375 |242        CONTINUE
        376 |C----------
        377 |C---------- 2. DO TEMPERTON INITIALIZATION ON ALL GRIDS
        378 |C----------
        379 |           CALL TEMPERTN(UTSAVE,VTSAVE,PTSAVE,
        380 |     *        IMG,JMG,XPOLEH,YPOLEH,DELTAXA0,LOOPBIG,NUMVERT)
        381 |C----------    LAST ITERATION IS FOR DIAGNOSTICS ONLY, DON*T UPDATE
        382 |           IF(LOOPBIG.EQ.NONLOOPB) THEN
        383 |             DO 260 NGG=1,INGRDUSE
        384 |               DO 250 KK=1,NUMVERT
        385 |                 DO 25002 I=1,IIJMAX
        386 |                   UTSAVE(I,KK,NGG)=0.
        387 |                   VTSAVE(I,KK,NGG)=0.
        388 |                   PTSAVE(I,KK,NGG)=0.
        389 |25002            CONTINUE
        390 |250            CONTINUE
        391 |260          CONTINUE
        392 |           END IF
        393 |C--------
        394 |C-------- 5. FINALLY EXCHANGE U0SAVE,ETC. WITH VBL(U,ETC.)
        395 |C--------   THUS RESTORING VBL TO ORIGINAL STATE.
        396 |C--------
        397 |           DO 325 NGG=1,INGRDUSE
        398 |             CALL STOVBL(U0SAVE(1,1,NGG),V0SAVE(1,1,NGG),
        399 |     *          P0SAVE(1,1,NGG),NGG)
        400 |325        CONTINUE
        401 |         END IF
        402 |C----------------------
        403 |C----------------HERE FOR LOOP=2:  ONE STEP OF INITIALIZATION
        404 |C----------------               COMPLETE.  NOW ADD INCREMENT TO FULL
        405 |C------------------                FIELDS.
        406 |        ELSE
        407 |C--------
        408 |C-------- RETURN INCREMENTS FROM VERTICAL MODE SPACE
        409 |C--------
        410 |         DO 535 KK=1,NUMVERT
        411 |           DO 53502 I=1,IJ
        412 |             W0TH(I,KK)=PTSAVE(I,KK,NG)
        413 |             W0U(I,KK)=UTSAVE(I,KK,NG)
        414 |             W0V(I,KK)=VTSAVE(I,KK,NG)
        415 |53502      CONTINUE
        416 |535      CONTINUE
        417 |         CALL FROMVBL(W00U,W00V,W00TH,W00Q,W00H,NG)
        418 |         CALL TOVBL(W0U,W0V,W0TH,W0Q,W0H,NG)
        419 |         DO 550 K=1,IKM
        420 |           DO 55002 I=1,IJ
        421 |             W0U(I,K)=0.
        422 |             W0V(I,K)=0.
        423 |             W0TH(I,K)=0.
        424 |55002      CONTINUE
        425 |550      CONTINUE
        426 |         DO 55004 I=1,IJ
        427 |           W0H(I)=0.
        428 |55004    CONTINUE
        429 |         DO 575 K=1,NUMVERT
        430 |           IADDT=MADDT+(K-1)*IJ
        431 |           IADDU=MADDU+(K-1)*IJ
        432 |           IADDV=MADDV+(K-1)*IJ
        433 |           DO 57502 I=1,IJ
        434 |             W0H(I)=W0H(I)+HPIBINVE(K)*VBL(IADDT+I-1)
        435 |57502      CONTINUE
        436 |           DO 570 KKR=1,IKM
        437 |             KK=IKM+1-KKR
        438 |             DO 57002 I=1,IJ
        439 |               W0TH(I,KKR)=W0TH(I,KKR)+HTBINVE(KK,K)*VBL(IADDT+I-1)
        440 |               W0U(I,KKR)=W0U(I,KKR)+HE(KK,K)*VBL(IADDU+I-1)
        441 |               W0V(I,KKR)=W0V(I,KKR)+HE(KK,K)*VBL(IADDV+I-1)
        442 |57002        CONTINUE
        443 |570        CONTINUE
        444 |575      CONTINUE
        445 |C--------
        446 |C-------- CONVERT VIRT TEMP CORRECTIONS TO ACTUAL TEMP CORRECTIONS
        447 |C--------
        448 |         CALL GETACTT(W0TH,W00H,W00Q,IJ,IJMAX,KM)
        449 |C--------
        450 |C-------- 6. CONVERT AND ADD INCREMENTS BACK TO ORIGINAL VARIABLES
        451 |C--------      (W0U,V,TH,H CONTAINS INCREMENTS, W00U,.. FULL FIELDS)
        452 |C--------
        453 |           CALL UNAVERTA(VBL(MADDU),VBL(MADDV),VBL(MADDT),VBL(MADDH),
        454 |     *           VBL(MADDQ),IJ,
        455 |     *                  IMG(NG),JMG(NG),PR,GASCON,
        456 |     *                  W0U,W0V,W0TH,W0H,
        457 |     *                  W00U,W00V,W00TH,W00H,W00Q,
        458 |     *                  R0Q(1,1),R0Q(1,2),R0Q(1,3),R0Q(1,4),
        459 |     *                  R0Q(1,5),R0Q(1,6),R0Q(1,7),R0Q(1,8))
        460 |        END IF
        461 |9000    CONTINUE
        462 |9100   CONTINUE
        463 |       RETURN
        464 |       END
 
