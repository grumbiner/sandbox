          1 |       SUBROUTINE SANDF(Z,D,H,ZS,DF,HF,DEPTH,JCAPIN)
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .                                       .
          4 |C SUBPROGRAM:    SANDF       SLOW+FAST TO SLOW-AND-FAST TRANSFORM
          5 |C   PRGMMR: PARRISH          ORG: W/NMC22    DATE: 88-06-13
          6 |C
          7 |C ABSTRACT: GO FROM FULL VARIABLES TO TEMPERTON SLOW-AND-FAST
          8 |C   DECOMPOSITION.
          9 |C
         10 |C PROGRAM HISTORY LOG:
         11 |C   88-06-13  PARRISH
         12 |C
         13 |C USAGE:    CALL SANDF(Z,D,H,ZS,DF,HF,DEPTH)
         14 |C   INPUT ARGUMENT LIST:
         15 |C     Z        - SLOW + FAST VORTICITY
         16 |C     D        - DIVERGENCE.
         17 |C     H        - SLOW + FAST HEIGHT
         18 |C     DEPTH    - SCALE HEIGHT.
         19 |C
         20 |C   OUTPUT ARGUMENT LIST:
         21 |C     ZS       - SLOW COMPONENT OF VORTICITY.
         22 |C     DF       - FAST DIVERGENCE (NOT CHANGED FROM INPUT--SLOW DIV=0)
         23 |C     HF       - FAST HEIGHT COMPONENT
         24 |C
         25 |C REMARKS: MUST INCLUDE BLOCK DATA BLOCKSP02 AND DEFINITION OF
         26 |C           PL1 VARIABLES BEFORE COMPILING.
         27 |C
         28 |C ATTRIBUTES:
         29 |C   LANGUAGE: FORTRAN200
         30 |C   MACHINE:  CYBER
         31 |C
         32 |C$$$
         33 |         include "myparam"
  1       1 |        INCLUDE 'parmodel'
  2       1 |C     PARAMETER FILE
  2       2 |C
  2       3 |C     SETUP FOR IBM SINGLE PRECISION REPRESENTATION
  2       4 |C     ON OUTPUT
  2       5 |C                      THIS VERSION 21 February 1991 (DiMego)
  2       6 |C
  2       7 |      PARAMETER (
  2       8 |     *      IBPW      =      4,
  2       9 |     *      IBPOR     = 120000,
  2      10 |     *      INUMREC   = IBPOR / IBPW,
  2      11 |     *      INSPECS   =    700,
  2      12 |     *      ILATG2    =    120,
  2      13 |     *      ILEVH     =     12,
  2      14 |     *      ILEVS     =     16,
  2      15 |     *      ILONF     =    360,
  2      16 |     *      INLATSH   =      5,
  2      17 |     *      IIJMAX    =  23668,
  2      18 |     *      IIJMAXB   =  25536,
  2      19 |     *      IIJRAD    =    892,
  2      20 |     *      IJCAP     =     80,
  2      21 |     *      IWPCPL    =     80,
  2      22 |     *      IKM       =     16 )
  2      23 |      PARAMETER (
  2      24 |     *      ILBCH     =   7*(147+111-7),
  2      25 |     *      ILBHI     =    (147+111-26),
  2      26 |     *      ILBUI     =    (2*147+111-39),
  2      27 |     *      ILBUO     =   (8*147+7*111-56),
  2      28 |     *      ILBVI     =    (147+2*111-39),
  2      29 |     *      ILBVO     =   (7*147+8*111-56),
  2      30 |     *      ILMOUT    =     20 )
  2      31 |      PARAMETER (
  2      32 |     *      IMAXOUTF  =     17,
  2      33 |     *      IMAXOUTT  =      3,
  2      34 |     *      IMAXOUTV  =    100,
  2      35 |     *      IMAXPROF  =      6,
  2      36 |     *      INFILLER  =  10000,
  2      37 |     *      INH       =     65,
  2      38 |     *      INGRDUSE  =      2,
  2      39 |     *      INGRDUS1  =      1,
  2      40 |     *      INIADDRS  =     44,
  2      41 |     *      INSCR     =     23,
  2      42 |     *      INSCRGEO  =     12,
  2      43 |     *      INSCR3    =     10,
  2      44 |     *      INSOIL    =      2,
  2      45 |     *      INSUMG    =  42164,
  2      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  2      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  2      48 |     *      INSHEM    = 13 * INH + 23,
  2      49 |     *      IIJKMAX   = IKM * IIJMAX )
  2      50 |      PARAMETER (
  2      51 |     *  IIWORDSB  = IIJMAXB / 64,
  2      52 |     *  IKMP1     = IKM + 1,
  2      53 |     *  ILVLTOT   = 7 * IKM + 37,
  2      54 |     *  ILONFP1   = ILONF + 1,
  2      55 |     *  INVBL     = ILVLTOT * INSUMG,
  2      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  2      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  2      58 |     *  IBLK1D    =  2 * IKM + 14,
  2      59 |     *  IBLK2D    = 16 * IKM + 36,
  2      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  2      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  2      62 |C
  1       2 |C-------
  1       3 |C------- FOLLOWING IS EXTRA STUFF FOR INITIALIZATION
  1       4 |C-------
  1       5 |         PARAMETER (INLATH=101,INUMCOEF=3*INLATH+35,
  1       6 |     *        INUMGRID=4,IJCAPDP=80,INLON=300,INKLON=2,INKLAT=2,
  1       7 |     *        IIORBPL=3,IIORDPL=3,IIORBLP=3,IIORDLP=3,IMCUBIC=1,
  1       8 |     *        INUMVERT=2,INSIG=IKM,INMODES=18,INGMAX=3000,
  1       9 |     *        INKSIG=2,INSIGP1=INSIG+1,INLONEX=INLON+INKLON,
  1      10 |     *        INLATDP=2*INLATH,INLOLA=INLATDP*INLONEX,
  1      11 |     *        IJCAP1DP=IJCAPDP+1,IJCAP12=2*IJCAP1DP,
  1      12 |     *        INCOEFS=IJCAP1DP*(IJCAPDP+2),
  1      13 |     *     IDPFILLR=(4*IKM+1)*INSUMG+       3*(IIJKMAX*4+IIJMAX) )
  1      14 |C    *     IDPFILLR=(4*IKM+1)*INSUMG+INGRDUSE*(IIJKMAX*4+IIJMAX) )
         34 |C--------
         35 |         real Z(1),D(1),H(1),ZS(1),DF(1),HF(1)
         36 |C--------
         37 |         REAL RC0INV
         38 |         LOGICAL BWORK(INCOEFS)
         39 |C--------
         40 |         COMMON/DAVEBUFF/ COF(INCOEFS,INUMCOEF),GRD(INLOLA,INUMGRID)
         41 |         real COF,GRD
         42 |         REAL RCF(INCOEFS,2)
         43 |         EQUIVALENCE (COF,RCF)
         44 |C--------
         45 |         COMMON/FIXSP02/JCAP,NCOEFS,NFOP,JFOP,
         46 |     *      ISCALZD,IUNSCLZD,
         47 |     *      IBSHAT,IBVHAT,IBVIHAT,
         48 |     *      IBOP,ICOP,IFOP,ICINVOP,
         49 |     *      IRFOP,IRCINVOP,
         50 |     *      BFOP(INCOEFS),BONES(INCOEFS)
         51 |         LOGICAL BFOP,BONES
         52 |C--------
         53 |C-------- CHECK TO SEE IF INITIALIZATION OF INTERNAL VARIABLES NEEDED
         54 |         IF(JCAP.NE.JCAPIN) CALL SETSP02(JCAPIN)
         55 |C--------
         56 |C-------- MAKE SURE ZONAL PART OF COEFS HAS PROPER FORM
         57 |C--------
         58 |         do 10002 i=1,ncoefs
         59 |         Z(i)=COF(i,IBVHAT)*Z(i)
         60 |         D(i)=COF(i,IBVIHAT)*D(i)
         61 |         H(i)=COF(i,IBSHAT)*H(i)
         62 |10002    continue
         63 |         CALL getcoef(IRF,1)
         64 |         CALL getcoef(IRZ,1)
         65 |         CALL getcoef(IRWORK,1)
         66 |         CALL getcoef(IRC,1)
         67 |         CALL getcoef(IRB,1)
         68 |         CALL getcoef(IRR,1)
         69 |         RC0INV=6.3712E6*7.2921E-5/SQRT(9.8062*DEPTH)
         70 |         JCAP1=JCAP+1
         71 |C--------
         72 |C-------- OBTAIN RHS OF EQUATION TO SOLVE FOR FAST HEIGHT
         73 |C--------
         74 |         do 10004 i=1,ncoefs
         75 |         RCF(i,IRZ)=Z(i)
         76 |10004    continue
         77 |         CALL FOPMULT(RCF(1,IRZ),RCF(1,IRF),RCF(1,IRFOP),NCOEFS,NFOP,
         78 |     *                 JFOP,RCF(1,IRWORK),BFOP,BONES,BWORK)
         79 |         do 10006 i=1,ncoefs
         80 |         RCF(i,IRF)=RC0INV*RCF(i,IRCINVOP)
         81 |     *              *RCF(i,IRF)+H(i)
         82 |10006    continue
         83 |C--------
         84 |C-------- SOLVE TRIDIAGONAL MATRIX EQUATION FOR FAST HEIGHT
         85 |C--------
         86 |         CALL SOLVTRID(IRF,IRZ,IRWORK,IRC,IRB,IRR,RC0INV)
         87 |         do 10008 i=1,ncoefs
         88 |         HF(i)=RCF(i,IRF)
         89 |10008    continue
         90 |C--------
         91 |C-------- COMPUTE FAST COMPONENT OF VORTICITY
         92 |C--------
         93 |         do 10010 i=1,ncoefs
         94 |         RCF(i,IRZ)=RC0INV
         95 |     *       *RCF(i,IRCINVOP)*RCF(i,IRF)
         96 |10010    continue
         97 |         CALL FOPMULT(RCF(1,IRZ),RCF(1,IRF),RCF(1,IRFOP),NCOEFS,NFOP,
         98 |     *        JFOP,RCF(1,IRWORK),BFOP,BONES,BWORK)
         99 |C-------- NOW GET SLOW COMPONENT OF VORTICITY
        100 |         do 10012 i=1,ncoefs
        101 |         ZS(i)=Z(i)-RCF(i,IRF)
        102 |10012    continue
        103 |C-------- FINALLY DIVERGENCE
        104 |         do 10014 i=1,ncoefs
        105 |         DF(i)=D(i)
        106 |10014    continue
        107 |         CALL freecoef(IRF,1)
        108 |         CALL freecoef(IRZ,1)
        109 |         CALL freecoef(IRWORK,1)
        110 |         CALL freecoef(IRC,1)
        111 |         CALL freecoef(IRB,1)
        112 |         CALL freecoef(IRR,1)
        113 |C--------
        114 |C-------- MAKE SURE ZONAL PART OF COEFS HAS PROPER FORM
        115 |C--------
        116 |         do 10016 i=1,ncoefs
        117 |         ZS(i)=COF(i,IBVHAT)*ZS(i)
        118 |         DF(i)=COF(i,IBVIHAT)*DF(i)
        119 |         HF(i)=COF(i,IBSHAT)*HF(i)
        120 |10016    continue
        121 |       RETURN
        122 |       END
 
