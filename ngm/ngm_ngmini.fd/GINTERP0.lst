  2      22 |     *      IKM       =     16 )
  2      23 |      PARAMETER (
  2      24 |     *      ILBCH     =   7*(147+111-7),
  2      25 |     *      ILBHI     =    (147+111-26),
  2      26 |     *      ILBUI     =    (2*147+111-39),
  2      27 |     *      ILBUO     =   (8*147+7*111-56),
  2      28 |     *      ILBVI     =    (147+2*111-39),
  2      29 |     *      ILBVO     =   (7*147+8*111-56),
  2      30 |     *      ILMOUT    =     20 )
  2      31 |      PARAMETER (
  2      32 |     *      IMAXOUTF  =     17,
  2      33 |     *      IMAXOUTT  =      3,
  2      34 |     *      IMAXOUTV  =    100,
  2      35 |     *      IMAXPROF  =      6,
  2      36 |     *      INFILLER  =  10000,
  2      37 |     *      INH       =     65,
  2      38 |     *      INGRDUSE  =      2,
  2      39 |     *      INGRDUS1  =      1,
  2      40 |     *      INIADDRS  =     44,
  2      41 |     *      INSCR     =     23,
  2      42 |     *      INSCRGEO  =     12,
  2      43 |     *      INSCR3    =     10,
  2      44 |     *      INSOIL    =      2,
  2      45 |     *      INSUMG    =  42164,
  2      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  2      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  2      48 |     *      INSHEM    = 13 * INH + 23,
  2      49 |     *      IIJKMAX   = IKM * IIJMAX )
  2      50 |      PARAMETER (
  2      51 |     *  IIWORDSB  = IIJMAXB / 64,
  2      52 |     *  IKMP1     = IKM + 1,
  2      53 |     *  ILVLTOT   = 7 * IKM + 37,
  2      54 |     *  ILONFP1   = ILONF + 1,
  2      55 |     *  INVBL     = ILVLTOT * INSUMG,
  2      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  2      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  2      58 |     *  IBLK1D    =  2 * IKM + 14,
  2      59 |     *  IBLK2D    = 16 * IKM + 36,
  2      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  2      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  2      62 |C
  1       2 |C-------
  1       3 |C------- FOLLOWING IS EXTRA STUFF FOR INITIALIZATION
  1       4 |C-------
  1       5 |         PARAMETER (INLATH=101,INUMCOEF=3*INLATH+35,
  1       6 |     *        INUMGRID=4,IJCAPDP=80,INLON=300,INKLON=2,INKLAT=2,
  1       7 |     *        IIORBPL=3,IIORDPL=3,IIORBLP=3,IIORDLP=3,IMCUBIC=1,
  1       8 |     *        INUMVERT=2,INSIG=IKM,INMODES=18,INGMAX=3000,
  1       9 |     *        INKSIG=2,INSIGP1=INSIG+1,INLONEX=INLON+INKLON,
  1      10 |     *        INLATDP=2*INLATH,INLOLA=INLATDP*INLONEX,
  1      11 |     *        IJCAP1DP=IJCAPDP+1,IJCAP12=2*IJCAP1DP,
  1      12 |     *        INCOEFS=IJCAP1DP*(IJCAPDP+2),
  1      13 |     *     IDPFILLR=(4*IKM+1)*INSUMG+       3*(IIJKMAX*4+IIJMAX) )
  1      14 |C    *     IDPFILLR=(4*IKM+1)*INSUMG+INGRDUSE*(IIJKMAX*4+IIJMAX) )
         68 |         REAL RK(NOBS,NKX,NKY,NKZ),RKWORK(NOBS,4)
         69 |         REAL XOBS(NOBS),YOBS(NOBS),ZOBS(NOBS),VOBS(NOBS)
         70 |         REAL XGRD(NX),YGRD(NY),ZGRD(NZ)
         71 |         LOGICAL BITK(NOBS)
         72 |C--------
         73 |         INTEGER IK(NOBS),IKWORK(NOBS)
         74 |C--------
         75 |         NXY=NX*NY
         76 |         NXYZ=NXY*NZ
         77 |C--------
         78 |C-------- OBTAIN INTERPOLATION CONSTS FOR EACH DIRECTION.
         79 |C--------
         80 |C-------- SET ALL BITS TO INDICATE ALL POINTS ARE INTERPOLATEES UNTIL
         81 |C-------- DETERMINED OTHERWISE
         82 |C--------
         83 |         BITK=.TRUE.
         84 |C--------------X-DIRECTION
         85 |         CALL INTERCON(IK,RK,BITK,XOBS,NOBS,XGRD,NX,NKX,EXTRAPX)
         86 |         IK=IK+1
         87 |C--------------Y-DIRECTION
         88 |         CALL INTERCON(IKWORK,RKWORK,BITK,YOBS,NOBS,YGRD,NY,NKY,EXTRAPY)
         89 |         IK=IK+IKWORK*NX
         90 |         IF(NKY.GT.1) THEN
         91 |           DO 220 IKY=2,NKY
         92 |             DO 210 IKX=1,NKX
         93 |               DO 21002 I=1,NOBS
         94 |                 RK(I,IKX,IKY,1)=RK(I,IKX,1,1)
         95 |21002          CONTINUE
         96 |210          CONTINUE
         97 |220        CONTINUE
         98 |           DO 240 IKY=1,NKY
         99 |             DO 230 IKX=1,NKX
        100 |               DO 23002 I=1,NOBS
        101 |                 RK(I,IKX,IKY,1)=RK(I,IKX,NKY,1)*RKWORK(I,IKY)
        102 |23002          CONTINUE
        103 |230          CONTINUE
        104 |240        CONTINUE
        105 |         END IF
        106 |C--------------Z-DIRECTION
        107 |         CALL INTERCON(IKWORK,RKWORK,BITK,ZOBS,NOBS,ZGRD,NZ,NKZ,EXTRAPZ)
        108 |         IK=IK+IKWORK*NXY
        109 |         IF(NKZ.GT.1) THEN
        110 |           DO 330 IKZ=2,NKZ
        111 |             DO 320 IKY=1,NKY
        112 |               DO 310 IKX=1,NKX
        113 |                 DO 31002 I=1,NOBS
        114 |                   RK(I,IKX,IKY,IKZ)=RK(I,IKX,IKY,1)
        115 |31002            CONTINUE
        116 |310            CONTINUE
        117 |320          CONTINUE
        118 |330        CONTINUE
        119 |           DO 360 IKZ=1,NKZ
        120 |             DO 350 IKY=1,NKY
        121 |               DO 340 IKX=1,NKX
        122 |                 DO 34002 I=1,NOBS
        123 |                   RK(I,IKX,IKY,IKZ)=RK(I,IKX,IKY,NKZ)*RKWORK(I,IKZ)
        124 |34002            CONTINUE
        125 |340            CONTINUE
        126 |350          CONTINUE
        127 |360        CONTINUE
        128 |         END IF
        129 |         IF(NVR.GT.1) THEN
        130 |           DO 36004 I=1,NOBS
        131 |             IK(I)=IK(I)+(NINT(VOBS(I))-1)*NXYZ
        132 |36004      CONTINUE
        133 |         END IF
        134 |       RETURN
        135 |       END
 
