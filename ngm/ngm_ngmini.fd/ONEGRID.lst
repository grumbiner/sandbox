        319 |         DO 77003 IQ2 = 1, IM
        320 |            EM2H(I+IQ2-1+J*IM) = C11U + S1(IQ2)
        321 |            EMV(I+IQ2-1+J*IM) = C11U + S2(IQ2)
        322 |77003    CONTINUE
        323 |         C11U = S4(J)
        324 |         DO 77004 IQ2 = 1, IM
        325 |            EMU(I+IQ2-1+J*IM) = C11U + S1(IQ2)
        326 |            EM2C(I+IQ2-1+J*IM) = C11U + S2(IQ2)
        327 |77004    CONTINUE
        328 |   13 CONTINUE
        329 |C
        330 |C--------------GET SINE OF THE LATITUDE AT H POINTS FROM THE
        331 |C              SCALE FACTOR.
        332 |CMIC$ DO PARALLEL VECTOR
        333 |      DO 89120 IQ2=1,IJ
        334 |         SINLAT(IQ2)=(TWO/EM2H(IQ2)) - ONE
        335 |C
        336 |C------------- GET EM SQUARED AT H, U, AND V POINTS.
        337 |         EM2H(IQ2)=EM2H(IQ2)*EM2H(IQ2)
        338 |         EM2U(IQ2)=EMU(IQ2)*EMU(IQ2)
        339 |         EM2V(IQ2)=EMV(IQ2)*EMV(IQ2)
        340 |C
        341 |C--------------GET RECIPROCAL OF EM AT U AND V POINTS
        342 |         EMUIN(IQ2)=ONE/EMU(IQ2)
        343 |         EMVIN(IQ2)=ONE/EMV(IQ2)
        344 |C
        345 |C------------- GET EM SQUARED AT THE H PRIME(C) POINT.
        346 |         EM2C(IQ2)=EM2C(IQ2)*EM2C(IQ2)
        347 |89120 CONTINUE
        348 |CMIC$ END PARALLEL
        349 |C
        350 |C--------------DTIME * (X - XP) / (2 * RADIUS**2) AT H PTS
        351 |C              IS PLACED INTO DICOR
        352 |C              AND  DTIME * (Y - YP) / (2 * RADIUS**2)
        353 |C              IS PLACED INTO DJCOR.
        354 |C
        355 |         C2 = TWO * DTOVDX * DXOV2R * DXOV2R
        356 |         C1 = C2 * (ONE - XIP)
        357 |CMIC$ DO ALL VECTOR SHARED(IM, C1, C2, S1) PRIVATE(IQ2)
        358 |         DO 2001 IQ2 = 1, IM
        359 |            S1(IQ2) = C1 + C2*FLOAT(IQ2-1)
        360 |2001     CONTINUE
        361 |         C1 = C2 * (ONE - YJP)
        362 |CMIC$ DO ALL VECTOR SHARED(JM, C1, C2, S3) PRIVATE(IQ2)
        363 |         DO 2002 IQ2 = 1, JM
        364 |            S3(IQ2) = C1 + C2*FLOAT(IQ2-1)
        365 |2002     CONTINUE
        366 |C
        367 |      I = 1 - IM
        368 |CMIC$ DO ALL SHARED(JM, I, IM, S3, DJCOR, S1, DICOR) PRIVATE(J, C1, IQ2)
        369 |      DO 14 J = 1, JM
        370 |         C1 = S3(J)
        371 |         DO 77005 IQ2 = 1, IM
        372 |            DJCOR(I+IQ2-1+J*IM) = C1
        373 |            DICOR(I+IQ2-1+J*IM) = S1(IQ2)
        374 |77005    CONTINUE
        375 |   14 CONTINUE
        376 |C
        377 |C--------------------- DETERMINE LIMITS ON J-ROWS
        378 |C
        379 |         J1 =  1
        380 |         J2 =  2
        381 |         J3 =  3
        382 |         JM0 = JM
        383 |         JM1 = JM - 1
        384 |         JM2 = JM - 2
        385 |C
        386 |C              IF THIS IS THE SECOND TIME STEP OF A PAIR, WE
        387 |C              MUST ADD (OR SUBTRACT) 2 .
        388 |      IF( NPAIR .LT. 2 ) GO TO 17
        389 |C
        390 |         J1  = J1 + 2
        391 |         J2  = J2 + 2
        392 |         J3  = J3 + 2
        393 |         JM0 = JM0 - 2
        394 |         JM1 = JM1 - 2
        395 |         JM2 = JM2 - 2
        396 |C
        397 |   17 CONTINUE
        398 |C
        399 |C------------- SET UP THE THREE STANDARD VECTOR INITIAL
        400 |C              ADDRESSES (RELATIVE TO A SINGLE HORIZONTAL
        401 |C              FIELD) THAT BEGIN WITH THE ROW.
        402 |C              J = J1,J2,J3
        403 |         LADD2 = IM * (J2 - 1) + 1
        404 |         LADD3 = IM * (J3 - 1) + 1
        405 |C
        406 |C------------- SET UP THE 6 POSSIBLE VECTOR LENGTHS FOR THE
        407 |C              BLOCKS OF J-ROWS J = JP THROUGH JMQ, WHERE
        408 |C              P STANDS FOR 1,2,OR 3 AND Q STANDS FOR
        409 |C              0,1, OR 2.
        410 |C
        411 |         LEN22 = IM * (JM2 - J2 + 1)
        412 |         LEN21 = IM * (JM1 - J2 + 1)
        413 |         LEN20 = IM * (JM0 - J2 + 1)
        414 |C
        415 |         LEN32 = IM * (JM2 - J3 + 1)
        416 |         LEN31 = IM * (JM1 - J3 + 1)
        417 |         LEN30 = IM * (JM0 - J3 + 1)
        418 |C
        419 |C================ CONVERGENCE FOR HALF TIME STEP =================
        420 |C
        421 |C              PRESET ADDRESSES FOR HU AND HV IN ARRAY VBL.
        422 |         JADDU = MADDU - 1 + LADD2 - IJ
        423 |         JADDV = MADDV - 1 + LADD2 - IJ
        424 |         JADD  = LADD2 - IJ
        425 |C
        426 |CFPP$ NODEPCHK L
        427 |CMIC$ DO ALL SHARED(JADDU, IJ, JADDV, JADD, LEN21, LADD2, LEN20, LADD3,
        428 |CMIC$1   DELSIG, VBL, EMUIN, EMVIN, EM2C, CONV) PRIVATE(S1, S2, K, C1,
        429 |CMIC$2   IQ2, C12U) SAVELAST
        430 |      DO 100 K = 1, 16
        431 |         C12U = -DELSIG(K)
        432 |C
        433 |         DO 77006 IQ2 = 1, LEN21
        434 |            S1(LADD2+IQ2-1) = VBL(JADDU+IJ*K+IQ2-1)*EMUIN(LADD2+IQ2-1)
        435 |77006    CONTINUE
        436 |C
        437 |         DO 77007 IQ2 = 1, LEN20
        438 |            S2(LADD2+IQ2-1) = VBL(JADDV+IJ*K+IQ2-1)*EMVIN(LADD2+IQ2-1)
        439 |77007    CONTINUE
        440 |C
        441 |C              PUT  - DELSIG TIMES FLUX DIFFS. * EM**2 INTO CONV
        442 |         DO 77008 IQ2 = 1, LEN21
        443 |            CONV(JADD+IJ*K+IQ2-1) = EM2C(LADD2+IQ2-1)*C12U*(S1(LADD2+IQ2
        444 |     1         )-S1(LADD2+IQ2-1)+S2(LADD3+IQ2-1)-S2(LADD2+IQ2-1))
        445 |77008    CONTINUE
        446 |  100 CONTINUE
        447 |C
        448 |C------------- COMPUTE THE VERTICAL SUM OF CONV, ACCUMULATING IT IN S1.
        449 |      JADD = LADD2 + IJ
        450 |CMIC$ DO ALL VECTOR SHARED(LEN21, LADD2, JADD, CONV, S1) PRIVATE(IQ2)
        451 |      DO 89320 IQ2=1,LEN21
        452 |         S1(LADD2+IQ2-1)=CONV(LADD2+IQ2-1)+CONV(JADD+IQ2-1)
        453 |89320 CONTINUE
        454 |C
        455 |      DO 105 K = 3,IKM
        456 |         JADD = JADD + IJ
        457 |CMIC$ DO ALL VECTOR SHARED(LEN21, LADD2, JADD, S1, CONV) PRIVATE(IQ2)
        458 |      DO 89330 IQ2=1,LEN21
        459 |         S1(LADD2+IQ2-1)=S1(LADD2+IQ2-1)+CONV(JADD+IQ2-1)
        460 |89330 CONTINUE
        461 |  105 CONTINUE
        462 |C
        463 |C              ARRAY S1 NOW HAS DDDH, WHICH EQUALS THE LOCAL
        464 |C              TIME DERIVATIVE OF SURFACE PRESSURE
        465 |C              AT C POINTS, J = J2,JM1.
        466 |C
        467 |      JADDH = LADD2 + MADDH - 1
        468 |CMIC$ PARALLEL SHARED(LEN21, DTOV2DX, LADD2, S1, S4, HP, JADDH, IM, VBL)
        469 |CMIC$1    PRIVATE(IQ2)
        470 |CMIC$ DO PARALLEL VECTOR
        471 |      DO 89340 IQ2=1,LEN21
        472 |         S4(LADD2+IQ2-1)=P5*(VBL(JADDH+IQ2-1)+VBL(JADDH+IM+IQ2-1))
        473 |89340 CONTINUE
        474 |CMIC$ DO PARALLEL VECTOR
        475 |      DO 89400 IQ2=1,LEN21
        476 |         HP(LADD2+IQ2-1)=DTOV2DX*S1(LADD2+IQ2-1)+
        477 |     *                   P5*(S4(LADD2+IQ2-1)+S4(LADD2+IQ2))
        478 |89400 CONTINUE
        479 |CMIC$ END PARALLEL
        480 |C
        481 |C------------- CONVERT CONV INTO W = H * DELX * D(SIGMA) / DT, J=J2,JM1.
        482 |      C1 = DELSIG(1)
        483 |CMIC$ DO ALL VECTOR SHARED(LEN21,LADD2,C1,IJ,CONV,S1,W)PRIVATE(IQ2)
        484 |      DO 89410 IQ2=1,LEN21
        485 |         W(LADD2+IJ+IQ2-1)=CONV(LADD2+IQ2-1)-C1*S1(LADD2+IQ2-1)
        486 |89410 CONTINUE
        487 |C
        488 |C              K = 2 ARRAY OF W NOW CONTAINS W FOR INTERFACE K = 2.
        489 |C              TOP NONZERO W,AT INTERFACE KM, WILL END UP AT LEVEL
        490 |C              KM OF ARRAY W.
        491 |      JADDW = LADD2 + IJ
        492 |      DO 110 K = 3,IKM
        493 |      C1    = DELSIG(K - 1)
        494 |      JADDW = JADDW + IJ
        495 |CMIC$ DO ALL VECTOR IF (ABS(IJ).GE.LEN21 .OR. IJ.EQ.0) SHARED(LEN21,
        496 |CMIC$1   JADDW, IJ, C1, LADD2, CONV, W, S1) PRIVATE(IQ2)
        497 |      DO 89430 IQ2=1,LEN21
        498 |         W(JADDW+IQ2-1)=
        499 |     *   CONV(JADDW-IJ+IQ2-1)+W(JADDW-IJ+IQ2-1)-C1*S1(LADD2+IQ2-1)
        500 |89430 CONTINUE
        501 |  110 CONTINUE
        502 |C
        503 |C--------------GET RECIPROCAL OF H INTO S11, J = J2,JM0.
        504 |C
        505 |CMIC$ PARALLEL SHARED(LEN21, LADD2, S4, S3, LADD3, S11, LEN20, S2, JADDH
        506 |CMIC$1   , VBL) PRIVATE(IQ2)
        507 |CMIC$ DO PARALLEL VECTOR
        508 |      DO 89440 IQ2=1,LEN20
        509 |         S11(LADD2+IQ2-1)=ONE/VBL(JADDH+IQ2-1)
        510 |89440 CONTINUE
        511 |C
        512 |C--------------AVERAGE IN I TO GET 1/H AT V PTS (S2, J = J2,JM0).
        513 |CMIC$ DO PARALLEL VECTOR
        514 |      DO 89450 IQ2=1,LEN20
        515 |         S2(LADD2+IQ2-1)=P5*(S11(LADD2+IQ2-1)+S11(LADD2+IQ2))
        516 |89450 CONTINUE
        517 |C
        518 |C--------------AVERAGE IN J FOR 1/H AT U  PTS (S4, J = J2,JM1).
        519 |CMIC$ DO PARALLEL VECTOR
        520 |      DO 89480 IQ2=1,LEN21
        521 |         S4(LADD2+IQ2-1)=P5*(S11(LADD2+IQ2-1)+S11(LADD3+IQ2-1))
        522 |89480 CONTINUE
        523 |C
        524 |C------------- GET I AVERAGE OF THESE FOR CENTER PT. (S3, J = J2,JM1).
        525 |CMIC$ DO PARALLEL VECTOR
        526 |      DO 89510 IQ2=1,LEN21
        527 |         S3(LADD2+IQ2-1)=P5*(S4(LADD2+IQ2-1)+S4(LADD2+IQ2))
        528 |89510 CONTINUE
        529 |CMIC$ END PARALLEL
        530 |C
        531 |C              WE CAN CHANGE HU,HV,HTH,AND HQ TO U,V,TH,AND Q WHERE
        532 |C              WE HAVE THE VALUES 1/H.
        533 |C
        534 |         JADDUH = MADDU - 1 + LADD2 - IJ
        535 |         JADDVH = MADDV - 1 + LADD2 - IJ
        536 |         JADDTH = MADDT - 1 + LADD2 - IJ
        537 |         JADDQH = MADDQ - 1 + LADD2 - IJ
        538 |         JADD   = LADD2 - IJ
        539 |C
        540 |CFPP$ NODEPCHK
        541 |CMIC$ DO ALL SHARED(JADDUH, IJ, JADDVH, JADDTH, JADDQH, JADD, LEN21,
        542 |CMIC$1   LADD2, LEN20, VBL, S4, U, S2, V, S11, TH, Q) PRIVATE(K, IQ2)
        543 |      DO 200 K = 1, 16
        544 |         DO 77009 IQ2 = 1, LEN21
        545 |            U(JADD+IJ*K+IQ2-1) = VBL(JADDUH+IJ*K+IQ2-1)*S4(LADD2+IQ2-1)
        546 |77009    CONTINUE
        547 |         DO 77010 IQ2 = 1, LEN20
        548 |            V(JADD+IJ*K+IQ2-1) = VBL(JADDVH+IJ*K+IQ2-1)*S2(LADD2+IQ2-1)
        549 |            TH(JADD+IJ*K+IQ2-1)=VBL(JADDTH+IJ*K+IQ2-1)*S11(LADD2+IQ2-1)
        550 |            Q(JADD+IJ*K+IQ2-1) = VBL(JADDQH+IJ*K+IQ2-1)*S11(LADD2+IQ2-1)
        551 |77010    CONTINUE
        552 |  200 CONTINUE
        553 |C
        554 |C--------------PUT AVERAGES OF TH, Q, AND V INTO HALF TIME ARRAYS,
        555 |C              FOR ROWS J2 THRU JM1.
        556 |      JADD  = LADD2 - IJ
        557 |CFPP$ NODEPCHK
        558 |CMIC$ DO ALL SHARED(JADD, IJ, LEN20, LADD2, LEN21, LADD3, TH, Q, V, THP
        559 |CMIC$1   , QP, VP) PRIVATE(S5, S6, S7, K, IQ2) SAVELAST
        560 |      DO 205 K = 1, 16
        561 |C            AVERAGE OF TH TO THP AT HP POINT
        562 |C              DITTO FOR Q.
        563 |C              FOR V WE NEED BACKWARD I AVERAGING.
        564 |         DO 77011 IQ2 = 1, LEN20
        565 |            S5(LADD2+IQ2-1)=0.5*(TH(JADD+IJ*K+IQ2-1)+TH(JADD+IJ*K+IQ2))
        566 |            S6(LADD2+IQ2-1) = 0.5*(Q(JADD+IJ*K+IQ2-1)+Q(JADD+IJ*K+IQ2))
        567 |            S7(LADD2+IQ2-1)=0.5*(V(JADD+IJ*K-2+IQ2)+V(JADD+IJ*K+IQ2-1))
        568 |77011    CONTINUE
        569 |         DO 77012 IQ2 = 1, LEN21
        570 |            THP(JADD+IJ*K+IQ2-1) = 0.5*(S5(LADD2+IQ2-1)+S5(LADD3+IQ2-1))
        571 |            QP(JADD+IJ*K+IQ2-1) = 0.5*(S6(LADD2+IQ2-1)+S6(LADD3+IQ2-1))
        572 |            VP(JADD+IJ*K+IQ2-1) = 0.5*(S7(LADD2+IQ2-1)+S7(LADD3+IQ2-1))
        573 |77012    CONTINUE
        574 |  205 CONTINUE
        575 |C
        576 |C------------- PUT HORIZONTAL AVG OF U INTO HALF TIME ARRAY,
        577 |C              FOR J3,JM1.
        578 |      JADD  = LADD2 - IJ
        579 |CFPP$ NODEPCHK
        580 |CMIC$ DO ALL SHARED(JADD, IJ, LEN21, LADD2, LEN31, LADD3, IM, U, UP)
        581 |CMIC$1    PRIVATE(S5, K, IQ2) SAVELAST
        582 |      DO 210 K = 1, 16
        583 |         DO 77013 IQ2 = 1, LEN21
        584 |            S5(LADD2+IQ2-1) = 0.5*(U(JADD+IJ*K+IQ2-1)+U(JADD+IJ*K+IQ2))
        585 |77013    CONTINUE
        586 |         DO 77014 IQ2 = 1, LEN31
        587 |            UP(JADD+IJ*K+IM+IQ2-1) = 0.5*(S5(LADD2+IQ2-1)+S5(LADD3+IQ2-1
        588 |     1         ))
        589 |77014    CONTINUE
        590 |  210 CONTINUE
        591 |C
        592 |C------------- CONVERT W INTO ZDIS USING 1/H AT CENTER POINT.
        593 |C              DO THIS FOR J2,JM1.  1/H IS IN S3.
        594 |C
        595 |      JADDW = LADD2
        596 |CFPP$ NODEPCHK
        597 |CMIC$ DO ALL SHARED(JADDW, IJ, DTOV2DX, LEN21, LADD2, DELSIG, W, S3,
        598 |CMIC$1   ZDIS) PRIVATE(K, C1, IQ2)
        599 |      DO 215 K = 1, 15
        600 |         C1 = -DTOV2DX/(DELSIG(1+K)+DELSIG(K))
        601 |         DO 77015 IQ2 = 1, LEN21
        602 |            ZDIS(JADDW+IJ*K+IQ2-1) = W(JADDW+IJ*K+IQ2-1)*C1*S3(LADD2+IQ2
        603 |     1         -1)
        604 |77015    CONTINUE
        605 |  215 CONTINUE
        606 |C
        607 |C              NONZERO VALUES OF ZDIS(K = 2,KM) ARE AT THE
        608 |C              CORRECT PLACE IN THE ZDIS ARRAY.
        609 |C
        610 |C---VER TH---- DO VERTICAL ADVECTION OF THETA.  FIRST
        611 |C              COMPUTE ZDIS(K) * (TH(K) - TH(K-1) ) FOR K=2,KM,
        612 |C              AND STORE IN W.
        613 |      JADD  = LADD2
        614 |CFPP$ NODEPCHK
        615 |CMIC$ DO ALL SHARED(JADD, IJ, LEN21, ZDIS, THP, W) PRIVATE(K, IQ2)
        616 |      DO 220 K = 1, 15
        617 |         DO 77016 IQ2 = 1, LEN21
        618 |            W(JADD+IJ*K+IQ2-1) = ZDIS(JADD+IJ*K+IQ2-1)*(THP(JADD+IJ*K+
        619 |     1         IQ2-1)-THP(JADD+IJ*(K-1)+IQ2-1))
        620 |77016    CONTINUE
        621 |  220 CONTINUE
        622 |      JADD = 15*IJ + JADD
        623 |CMIC$ PARALLEL SHARED(LEN21, JADD, THP, W, LADD2, IJ) PRIVATE(IQ2)
        624 |CMIC$ DO PARALLEL VECTOR
        625 |      DO 89860 IQ2=1,LEN21
        626 |         THP(LADD2+IQ2-1)=THP(LADD2+IQ2-1)+W(LADD2+IJ+IQ2-1)
        627 |89860 CONTINUE
        628 |CMIC$ DO PARALLEL VECTOR
        629 |      DO 89861 IQ2=1,LEN21
        630 |         THP(JADD+IQ2-1)=THP(JADD+IQ2-1)+W(JADD+IQ2-1)
        631 |89861 CONTINUE
        632 |CMIC$ END PARALLEL
        633 |C
        634 |      JADD  = LADD2
        635 |CFPP$ NODEPCHK
        636 |CMIC$ DO ALL SHARED(JADD, IJ, LEN21, THP, W) PRIVATE(K, IQ2)
        637 |      DO 225 K = 1, 14
        638 |         DO 77017 IQ2 = 1, LEN21
        639 |            THP(JADD+IJ*K+IQ2-1) = THP(JADD+IJ*K+IQ2-1) + W(JADD+IJ*K+
        640 |     1         IQ2-1) + W(JADD+IJ*(K+1)+IQ2-1)
        641 |77017    CONTINUE
        642 |  225 CONTINUE
        643 |C
        644 |C---VER Q------DO THE VERTICAL ADVECTION OF THE SPECIFIC HUMIDITY.
        645 |      JADD  = LADD2
        646 |CFPP$ NODEPCHK
        647 |CMIC$ DO ALL SHARED(JADD, IJ, LEN21, ZDIS, QP, W) PRIVATE(K, IQ2)
        648 |      DO 230 K = 1, 15
        649 |         DO 77018 IQ2 = 1, LEN21
        650 |            W(JADD+IJ*K+IQ2-1) = ZDIS(JADD+IJ*K+IQ2-1)*(QP(JADD+IJ*K+IQ2
        651 |     1         -1)-QP(JADD+IJ*(K-1)+IQ2-1))
        652 |77018    CONTINUE
        653 |  230 CONTINUE
        654 |      JADD = 15*IJ + JADD
        655 |CMIC$ PARALLEL SHARED(LEN21, JADD, QP, W, LADD2, IJ) PRIVATE(IQ2)
        656 |CMIC$ DO PARALLEL VECTOR
        657 |      DO 12920 IQ2=1,LEN21
        658 |         QP(LADD2+IQ2-1)=QP(LADD2+IQ2-1)+W(LADD2+IJ+IQ2-1)
        659 |12920 CONTINUE
        660 |CMIC$ DO PARALLEL VECTOR
        661 |      DO 12921 IQ2=1,LEN21
        662 |         QP(JADD+IQ2-1)=QP(JADD+IQ2-1)+W(JADD+IQ2-1)
        663 |12921 CONTINUE
        664 |CMIC$ END PARALLEL
        665 |C
        666 |      JADD  = LADD2
        667 |CFPP$ NODEPCHK
        668 |CMIC$ DO ALL SHARED(JADD, IJ, LEN21, QP, W) PRIVATE(K, IQ2)
        669 |      DO 235 K = 1, 14
        670 |         DO 77019 IQ2 = 1, LEN21
        671 |            QP(JADD+IJ*K+IQ2-1) = QP(JADD+IJ*K+IQ2-1) + W(JADD+IJ*K+IQ2-
        672 |     1         1) + W(JADD+IJ*(K+1)+IQ2-1)
        673 |77019    CONTINUE
        674 |  235 CONTINUE
        675 |C
        676 |C---VER U------THE VERTICAL ADVECTION OF U IS SIMILAR EXCEPT THAT A
        677 |C              BACKWARD J-AVERAGING OF ZDIS IS NECESSARY.
        678 |      JADD  = LADD3
        679 |CFPP$ NODEPCHK
        680 |CMIC$ DO ALL SHARED(JADD, IJ, LEN31, IM, UP, ZDIS, W) PRIVATE(K, IQ2)
        681 |      DO 240 K = 1, 15
        682 |         DO 77020 IQ2 = 1, LEN31
        683 |            W(JADD+IJ*K+IQ2-1) = (UP(JADD+IJ*K+IQ2-1)-UP(JADD+IJ*(K-1)+
        684 |     1         IQ2-1))*(0.5*(ZDIS(JADD+IJ*K-IM+IQ2-1)+ZDIS(JADD+IJ*K+IQ2
        685 |     2         -1)))
        686 |77020    CONTINUE
        687 |  240 CONTINUE
        688 |      JADD = 15*IJ + JADD
        689 |CMIC$ PARALLEL SHARED(LEN31, JADD, UP, W, LADD3, IJ) PRIVATE(IQ2)
        690 |CMIC$ DO PARALLEL VECTOR
        691 |      DO 89990 IQ2=1,LEN31
        692 |         UP(LADD3+IQ2-1)=UP(LADD3+IQ2-1)+W(LADD3+IJ+IQ2-1)
        693 |89990 CONTINUE
        694 |CMIC$ DO PARALLEL VECTOR
        695 |      DO 89991 IQ2=1,LEN31
        696 |         UP(JADD+IQ2-1)=UP(JADD+IQ2-1)+W(JADD+IQ2-1)
        697 |89991 CONTINUE
        698 |CMIC$ END PARALLEL
        699 |C
        700 |      JADD  = LADD3
        701 |CFPP$ NODEPCHK
        702 |CMIC$ DO ALL SHARED(JADD, IJ, LEN31, UP, W) PRIVATE(K, IQ2)
        703 |      DO 245 K = 1, 14
        704 |         DO 77021 IQ2 = 1, LEN31
        705 |            UP(JADD+IJ*K+IQ2-1) = UP(JADD+IJ*K+IQ2-1) + W(JADD+IJ*K+IQ2-
        706 |     1         1) + W(JADD+IJ*(K+1)+IQ2-1)
        707 |77021    CONTINUE
        708 |  245 CONTINUE
        709 |C
        710 |C---VER V----- THE VERTICAL ADVECTION OF V NEEDS BACKWARD
        711 |C              I-AVERAGE OF ZDIS.
        712 |      JADD  = LADD2
        713 |CFPP$ NODEPCHK
        714 |CMIC$ DO ALL SHARED(JADD, IJ, LEN21, VP, ZDIS, W) PRIVATE(K, IQ2)
        715 |      DO 250 K = 1, 15
        716 |         DO 77022 IQ2 = 1, LEN21
        717 |            W(JADD+IJ*K+IQ2-1) = (VP(JADD+IJ*K+IQ2-1)-VP(JADD+IJ*(K-1)+
        718 |     1         IQ2-1))*(0.5*(ZDIS(JADD+IJ*K-2+IQ2)+ZDIS(JADD+IJ*K+IQ2-1)
        719 |     2         ))
        720 |77022    CONTINUE
        721 |  250 CONTINUE
        722 |      JADD = 15*IJ + JADD
        723 |CMIC$ PARALLEL SHARED(LEN21, JADD, VP, W, LADD2, IJ) PRIVATE(IQ2)
        724 |CMIC$ DO PARALLEL VECTOR
        725 |      DO 90060 IQ2=1,LEN21
        726 |         VP(LADD2+IQ2-1)=VP(LADD2+IQ2-1)+W(LADD2+IJ+IQ2-1)
        727 |90060 CONTINUE
        728 |CMIC$ DO PARALLEL VECTOR
        729 |      DO 90061 IQ2=1,LEN21
        730 |         VP(JADD+IQ2-1)=VP(JADD+IQ2-1)+W(JADD+IQ2-1)
        731 |90061 CONTINUE
        732 |CMIC$ END PARALLEL
        733 |C
        734 |      JADD  = LADD2
        735 |CFPP$ NODEPCHK
        736 |CMIC$ DO ALL SHARED(JADD, IJ, LEN21, VP, W) PRIVATE(K, IQ2)
        737 |      DO 255 K = 1, 14
        738 |         DO 77023 IQ2 = 1, LEN21
        739 |            VP(JADD+IJ*K+IQ2-1) = VP(JADD+IJ*K+IQ2-1) + W(JADD+IJ*K+IQ2-
        740 |     1         1) + W(JADD+IJ*(K+1)+IQ2-1)
        741 |77023    CONTINUE
        742 |  255 CONTINUE
        743 |C
        744 |C=====================HORIZONTAL ADVECTION===========================
        745 |C
        746 |      JADD  = LADD2 - IJ
        747 |CFPP$ NODEPCHK
        748 |CMIC$ DO ALL SHARED(JADD, IJ, LEN21, LADD2, LEN20, EMU, U, XDIS, EMV, V
        749 |CMIC$1   , YDIS) PRIVATE(K, IQ2)
        750 |      DO 260 K = 1, 16
        751 |         DO 77024 IQ2 = 1, LEN21
        752 |            XDIS(JADD+IJ*K+IQ2-1) = -EMU(LADD2+IQ2-1)*U(JADD+IJ*K+IQ2-1)
        753 |77024    CONTINUE
        754 |         DO 77025 IQ2 = 1, LEN20
        755 |            YDIS(JADD+IJ*K+IQ2-1) = -EMV(LADD2+IQ2-1)*V(JADD+IJ*K+IQ2-1)
        756 |77025    CONTINUE
        757 |  260 CONTINUE
        758 |C
        759 |C---ADV TH AND Q
        760 |C
        761 |      JADD  = LADD2 - IJ
        762 |CFPP$ NODEPCHK
        763 |CMIC$ DO ALL SHARED(JADD, IJ, LEN21, IM, LADD2, LEN20, DTOV2DX, LADD3,
        764 |CMIC$1   TH, Q, XDIS, YDIS, QP, THP) PRIVATE(S6, S9, S7, S8, K, IQ2, S5S
        765 |CMIC$2   , S6S) SAVELAST
        766 |      DO 270 K = 1, 16
        767 |C
        768 |         DO 77026 IQ2 = 1, LEN21
        769 |            S6(LADD2+IQ2-1) = 0.5*(TH(JADD+IJ*K+IM+IQ2-1)+TH(JADD+IJ*K+
        770 |     1         IQ2-1))
        771 |            S9(LADD2+IQ2-1) = 0.5*(Q(JADD+IJ*K+IM+IQ2-1)+Q(JADD+IJ*K+IQ2
        772 |     1         -1))
        773 |77026    CONTINUE
        774 |         DO 77027 IQ2 = 1, LEN20
        775 |            S7(LADD2+IQ2-1)=0.5*(TH(JADD+IJ*K+IQ2)+TH(JADD+IJ*K+IQ2-1))
        776 |            S8(LADD2+IQ2-1) = 0.5*(Q(JADD+IJ*K+IQ2)+Q(JADD+IJ*K+IQ2-1))
        777 |77027    CONTINUE
        778 |C
        779 |         DO 77028 IQ2 = 1, LEN21
        780 |            S5S = 0.5*(XDIS(JADD+IJ*K+IQ2-1)+XDIS(JADD+IJ*K+IQ2))
        781 |            S6S = 0.5*(YDIS(JADD+IJ*K+IQ2-1)+YDIS(JADD+IJ*K+IM+IQ2-1))
        782 |C
        783 |            QP(JADD+IJ*K+IQ2-1) = QP(JADD+IJ*K+IQ2-1) + DTOV2DX*((S9(
        784 |     1         LADD2+IQ2)-S9(LADD2+IQ2-1))*S5S+S6S*(S8(LADD3+IQ2-1)-S8(
        785 |     2         LADD2+IQ2-1)))
        786 |C
        787 |            THP(JADD+IJ*K+IQ2-1) = THP(JADD+IJ*K+IQ2-1) + DTOV2DX*((S6(
        788 |     1         LADD2+IQ2)-S6(LADD2+IQ2-1))*S5S+S6S*(S7(LADD3+IQ2-1)-S7(
        789 |     2         LADD2+IQ2-1)))
        790 |77028    CONTINUE
        791 |  270 CONTINUE
        792 |C
        793 |C
        794 |C---ADV U
        795 |C
        796 |      JADD  = LADD3 - IJ
        797 |CFPP$ NODEPCHK
        798 |CMIC$ DO ALL SHARED(JADD, IJ, LEN21, IM, LADD2, LEN31, LADD3, DTOV2DX, U
        799 |CMIC$1   , XDIS, UP, YDIS) PRIVATE(S8, S6, S7, K, IQ2) SAVELAST
        800 |      DO 280 K = 1, 16
        801 |C
        802 |         DO 77029 IQ2 = 1, LEN21
        803 |            S8(LADD2+IQ2-1) = 0.5*(U(JADD+IJ*K-IM+IQ2-1)+U(JADD+IJ*K-IM+
        804 |     1         IQ2))
        805 |77029    CONTINUE
        806 |         DO 77030 IQ2 = 1, LEN31
        807 |            S6(LADD3+IQ2-1) = 0.5*(XDIS(JADD+IJ*K+IQ2-1)+XDIS(JADD+IJ*K-
        808 |     1         IM+IQ2-1))
        809 |            S7(LADD3+IQ2-1) = 0.5*(U(JADD+IJ*K+IQ2-1)+U(JADD+IJ*K-IM+IQ2
        810 |     1         -1))
        811 |77030    CONTINUE
        812 |C
        813 |         DO 77031 IQ2 = 1, LEN31
        814 |C              ADD XDIS TIMES I DIFFERENCE OF U TERM TO UP.
        815 |C              ADD YDIS TIMES J DIFFERENCE OF U TERM TO UP.
        816 |            UP(JADD+IJ*K+IQ2-1) = UP(JADD+IJ*K+IQ2-1) + DTOV2DX*((S7(
        817 |     1         LADD3+IQ2)-S7(LADD3+IQ2-1))*0.5*(S6(LADD3+IQ2)+S6(LADD3+
        818 |     2         IQ2-1))+YDIS(JADD+IJ*K+IQ2-1)*(S8(LADD3+IQ2-1)-S8(LADD2+
        819 |     3         IQ2-1)))
        820 |77031    CONTINUE
        821 |  280 CONTINUE
        822 |C
        823 |C
        824 |C---ADV V
        825 |C
        826 |      JADD  = LADD2 - IJ
        827 |C
        828 |CFPP$ NODEPCHK
        829 |CMIC$ DO ALL SHARED(JADD, IJ, LEN20, LADD2, LEN21, IM, DTOV2DX, LADD3,
        830 |CMIC$1   YDIS, V, VP, XDIS) PRIVATE(S8, S7, S6, K, IQ2) SAVELAST
        831 |      DO 290 K = 1, 16
        832 |         DO 77032 IQ2 = 1, LEN20
        833 |            S8(LADD2+IQ2-1) = 0.5*(YDIS(JADD+IJ*K+IQ2-1)+YDIS(JADD+IJ*K-
        834 |     1         2+IQ2))
        835 |            S7(LADD2+IQ2-1)=0.5*(V(JADD+IJ*K+IQ2-1)+V(JADD+IJ*K-2+IQ2))
        836 |77032    CONTINUE
        837 |C
        838 |         DO 77033 IQ2 = 1, LEN21
        839 |            S6(LADD2+IQ2-1) = 0.5*(V(JADD+IJ*K+IQ2-1)+V(JADD+IJ*K+IM+IQ2
        840 |     1         -1))
        841 |77033    CONTINUE
        842 |C
        843 |         DO 77034 IQ2 = 1, LEN21
        844 |            VP(JADD+IJ*K+IQ2-1) = VP(JADD+IJ*K+IQ2-1) + DTOV2DX*((S6(
        845 |     1         LADD2+IQ2-1)-S6(LADD2-2+IQ2))*XDIS(JADD+IJ*K+IQ2-1)+(S7(
        846 |     2         LADD3+IQ2-1)-S7(LADD2+IQ2-1))*0.5*(S8(LADD3+IQ2-1)+S8(
        847 |     3         LADD2+IQ2-1)))
        848 |77034    CONTINUE
        849 |  290 CONTINUE
        850 |C
        851 |C
        852 |C
        853 |C================= HYDROSTATIC FOR HALF TIME STEP ==============
        854 |C
        855 |C------------- DO THE HYDROSTATIC COMPUTATIONS,J = J2,JM0.
        856 |C              MAKE VIRTUAL TEMPERATURE CORRECTION TO THETA.
        857 |      JADD  = LADD2 - IJ
        858 |CFPP$ NODEPCHK
        859 |CMIC$ DO ALL SHARED(JADD, IJ, LEN20, Q, TH, VT) PRIVATE(K, IQ2)
        860 |      DO 300 K = 1, 16
        861 |         DO 77035 IQ2 = 1, LEN20
        862 |            VT(JADD+IJ*K+IQ2-1) = (1.0+0.609*Q(JADD+IJ*K+IQ2-1))*TH(JADD
        863 |     1         +IJ*K+IQ2-1)
        864 |77035    CONTINUE
        865 |  300 CONTINUE
        866 |C
        867 |C-------------- GET SFC PRESS(H) **KAPPA INTO S9(J2,JM0)
        868 |      LADKAPPA = MADDH - 1 + LADD2
        869 |C
        870 |C*****  Code Expanded From Routine:  P2KAP
        871 |CMIC$ DO ALL VECTOR SHARED(LEN20, LADKAPPA, LADD2, VBL, S9) PRIVATE(I1X)
        872 |      DO 77001 I1X = 1, LEN20
        873 |         S9(I1X+LADD2-1) = (0.34757549+VBL(I1X+LADKAPPA-1)*(4.36732956+
        874 |     1      VBL(I1X+LADKAPPA-1)*3.91557032))/(1.+VBL(I1X+LADKAPPA-1)*(
        875 |     2      5.44053037+VBL(I1X+LADKAPPA-1)*(2.27693825+VBL(I1X+LADKAPPA-
        876 |     3      1)*(-0.0869930591))))
        877 |77001 CONTINUE
        878 |C*****  End of Code Expanded From Routine:  P2KAP
        879 |C
        880 |C------------- COMPUTE PSI FOR K = 1,(J2,JM0).
        881 |      C1 = ONE - TWO * PR(1)
        882 |      JADD = IADDRG(10,NG) - 1 + LADD2
        883 |CMIC$ DO ALL VECTOR SHARED(LEN20, C1, LADD2, JADD, VT, S9, VBL, PSI)
        884 |CMIC$1    PRIVATE(IQ2)
        885 |      DO 90960 IQ2=1,LEN20
        886 |         PSI(LADD2+IQ2-1)= C1 * VT(LADD2+IQ2-1) *
        887 |     *             S9(LADD2+IQ2-1) + VBL(JADD+IQ2-1)
        888 |90960 CONTINUE
        889 |C
        890 |C              NOW EXTEND TO PSI AT K = 2,KM
        891 |      JADD  = LADD2
        892 |      DO 320 K = 2,IKM
        893 |         JADD  = JADD + IJ
        894 |         C1 = PR(K - 1) - PR(K)
        895 |CMIC$ DO ALL VECTOR IF (ABS(IJ).GE.LEN20 .OR. IJ.EQ.0) SHARED(LEN20,
        896 |CMIC$1   JADD, IJ, C1, LADD2, PSI, VT, S9) PRIVATE(IQ2)
        897 |      DO 90990 IQ2=1,LEN20
        898 |         PSI(JADD+IQ2-1)=PSI(JADD-IJ+IQ2-1)+
        899 |     *         C1*(VT(JADD+IQ2-1)+VT(JADD-IJ+IQ2-1))*
        900 |     *         S9(LADD2+IQ2-1)
        901 |90990 CONTINUE
        902 |  320 CONTINUE
        903 |C
        904 |C========================PRESSURE GRADIENT FORCE======================
        905 |C
        906 |C---PGF U----- COMPUTE U ACCEL. TERM AND ADD TO UP(J3,JM1).
        907 |C
        908 |      C2 = CSUBP * DTOV2DX
        909 |C
        910 |CMIC$ DO ALL VECTOR SHARED(LEN31, LADD3, S9, S6) PRIVATE(IQ2)
        911 |      DO 91020 IQ2=1,LEN31
        912 |         S6(LADD3+IQ2-1)=S9(LADD3+IQ2)-S9(LADD3+IQ2-1)
        913 |91020 CONTINUE
        914 |C
        915 |      JADD  = LADD3 - IJ
        916 |CFPP$ NODEPCHK
        917 |CMIC$ PARALLEL SHARED(LEN21, LADD3, LADD2, S9, S6, JADD, IJ, LEN31, C2,
        918 |CMIC$1   PR, UP, VT, PSI, EMV) PRIVATE(IQ2, K, C1)
        919 |CFPP$ NODEPCHK
        920 |CMIC$ DO PARALLEL
        921 |      DO 330 K = 1, 16
        922 |         C1 = PR(K)*2.E0
        923 |         DO 77036 IQ2 = 1, LEN31
        924 |            UP(JADD+IJ*K+IQ2-1) = UP(JADD+IJ*K+IQ2-1) - C2*(C1*0.5*(VT(
        925 |     1         JADD+IJ*K+IQ2-1)+VT(JADD+IJ*K+IQ2))*S6(LADD3+IQ2-1)+PSI(
        926 |     2         JADD+IJ*K+IQ2)-PSI(JADD+IJ*K+IQ2-1))*EMV(LADD3+IQ2-1)
        927 |77036    CONTINUE
        928 |  330 CONTINUE
        929 |C
        930 |C---PGF V------COMPUTE V ACCELERATION TERM AND ADD TO VP(J2,JM1).
        931 |CMIC$ DO PARALLEL VECTOR
        932 |      DO 91150 IQ2=1,LEN21
        933 |         S6(LADD2+IQ2-1)=S9(LADD3+IQ2-1)-S9(LADD2+IQ2-1)
        934 |91150 CONTINUE
        935 |CMIC$ END PARALLEL
        936 |C
        937 |      JADD  = LADD2 - IJ
        938 |CFPP$ NODEPCHK
        939 |CMIC$ PARALLEL SHARED(LEN20, LADD2, MADDH, SINLAT, VBL, S5, JADD, IJ,
        940 |CMIC$1   LEN21, C2, IM, PR, VP, VT, S6, PSI, EMU) PRIVATE(IQ2, K, C1)
        941 |CFPP$ NODEPCHK
        942 |CMIC$ DO PARALLEL
        943 |      DO 340 K = 1, 16
        944 |         C1 = PR(K)*2.0
        945 |         DO 77037 IQ2 = 1, LEN21
        946 |            VP(JADD+IJ*K+IQ2-1) = VP(JADD+IJ*K+IQ2-1) - C2*(C1*0.5*(VT(
        947 |     1         JADD+IJ*K+IQ2-1)+VT(JADD+IJ*K+IM+IQ2-1))*S6(LADD2+IQ2-1)+
        948 |     2         PSI(JADD+IJ*K+IM+IQ2-1)-PSI(JADD+IJ*K+IQ2-1))*EMU(LADD2+
        949 |     3         IQ2-1)
        950 |77037    CONTINUE
        951 |  340 CONTINUE
        952 |C
        953 |C ======================= CORIOLIS FORCE ===========================
        954 |C
        955 |C              FIRST PUT SINE LATITUDE TIMES H INTO S5(J2,JM0).
        956 |CMIC$ DO PARALLEL VECTOR
        957 |      DO 91280 IQ2=1,LEN20
        958 |         S5(LADD2+IQ2-1)=SINLAT(LADD2+IQ2-1)*VBL(MADDH+LADD2+IQ2-2)
        959 |91280 CONTINUE
        960 |CMIC$ END PARALLEL
        961 |         C1 = TWO * ANGVEL * DTIME
        962 |C
        963 |C
        964 |         JADD  = LADD2 - IJ
        965 |         JADDU = MADDU - 1 + JADD
        966 |         JADDV = MADDV - 1 + JADD
        967 |C
        968 |CFPP$ NODEPCHK
        969 |CMIC$ DO ALL SHARED(JADD, IJ, JADDU, JADDV, LEN20, IM, LADD2, C1, LEN21
        970 |CMIC$1   , LADD3, VBL, DJCOR, DICOR, S5, CORU, CORV) PRIVATE(S6, S7, K,
        971 |CMIC$2   IQ2) SAVELAST
        972 |      DO 400 K = 1, 16
        973 |C
        974 |         DO 77038 IQ2 = 1, LEN20
        975 |            S6(LADD2+IQ2-1) = 0.5*(VBL(JADDU+IJ*K+IQ2-1)+VBL(JADDU+IJ*K-
        976 |     1         IM+IQ2-1))
        977 |            S7(LADD2+IQ2-1) = 0.5*(VBL(JADDV+IJ*K-2+IQ2)+VBL(JADDV+IJ*K+
        978 |     1         IQ2-1))
        979 |            S6(LADD2+IQ2-1) = DJCOR(LADD2+IQ2-1)*S6(LADD2+IQ2-1) - DICOR
        980 |     1         (LADD2+IQ2-1)*S7(LADD2+IQ2-1) + C1*S5(LADD2+IQ2-1)
        981 |77038    CONTINUE
        982 |C
        983 |C              S6 NOW CONTAINS THE QUANTITY (H * DTIME) TIMES
        984 |C              ( 2 * ANGVEL * SIN(LATITUDE)
        985 |C              + (U * Y - V * X) / (2 * RADIUS * RADIUS)),
        986 |C              AT H POINTS.
        987 |C              AVERAGE S6 IN J TO GET CORU(J2,JM1).
        988 |         DO 77039 IQ2 = 1, LEN21
        989 |            CORU(JADD+IJ*K+IQ2-1)=0.5*(S6(LADD2+IQ2-1)+S6(LADD3+IQ2-1))
        990 |            CORV(JADD+IJ*K+IQ2-1) = 0.5*(S6(LADD2+IQ2-1)+S6(LADD2+IQ2))
        991 |77039    CONTINUE
        992 |  400 CONTINUE
        993 |C
        994 |C
        995 |C--------------DO THE CORIOLIS ACCELERATIONS FOR THE HALF TIME STEP.
        996 |C              FIRST FOR UP(J3,JM1).
        997 |C              S2 STILL HAS 1/H AT V POINTS.
        998 |      JADD  = LADD3 - IJ
        999 |CFPP$ NODEPCHK
       1000 |CMIC$ DO ALL SHARED(JADD,IJ,LEN31,LADD3,UP,CORV,V,S2)PRIVATE(K,IQ2)
       1001 |      DO 410 K = 1, 16
       1002 |         DO 77040 IQ2 = 1, LEN31
       1003 |            UP(JADD+IJ*K+IQ2-1) = UP(JADD+IJ*K+IQ2-1) + 0.5*CORV(JADD+IJ
       1004 |     1         *K+IQ2-1)*V(JADD+IJ*K+IQ2-1)*S2(LADD3+IQ2-1)
       1005 |77040    CONTINUE
       1006 |  410 CONTINUE
       1007 |C
       1008 |C--------------DO CORIOLIS ACCEL. FOR THE HALF TIME STEP VP(J2,JM1).
       1009 |C              S4 STILL HAS 1/H AT U POINTS.
       1010 |      JADD = LADD2 - IJ
       1011 |CFPP$ NODEPCHK
       1012 |CMIC$ DO ALL SHARED(JADD,IJ,LEN21,LADD2,VP,CORU,U,S4)PRIVATE(K,IQ2)
       1013 |      DO 420 K = 1, 16
       1014 |         DO 77041 IQ2 = 1, LEN21
       1015 |            VP(JADD+IJ*K+IQ2-1) = VP(JADD+IJ*K+IQ2-1) - 0.5*CORU(JADD+IJ
       1016 |     1         *K+IQ2-1)*U(JADD+IJ*K+IQ2-1)*S4(LADD2+IQ2-1)
       1017 |77041    CONTINUE
       1018 |  420 CONTINUE
       1019 |C
       1020 |C************* START THE FULL TIME STEP **************************
       1021 |C
       1022 |C--------------PRESET ADDRESSES FOR (VP,THP,QP) AND FOR UP.
       1023 |C              (J3,JM1) FOR UP, (J=J2,JM1) FOR VP, THP, AND QP.
       1024 |C
       1025 |C------------- FIX THE PRESET ADDRESSES IN VBL FOR ENTRY INTO K-LOOPS
       1026 |C              THAT STORE MODIFIED VARIABLE BACK INTO ARRAY VBL.
       1027 |         III = LADD3 - 1 - IJ
       1028 |         NADDU = MADDU + III
       1029 |         NADDV = MADDV + III
       1030 |         NADDT = MADDT + III
       1031 |         NADDQ = MADDQ + III
       1032 |C
       1033 |C------------- DO CORIOLIS FORCE FOR HU(J3,JM2) AND HV(J3,JM1),
       1034 |      JADDU = NADDU
       1035 |      JADDV = NADDV
       1036 |      JADD  = LADD3 - IJ
       1037 |CFPP$ NODEPCHK
       1038 |CMIC$ PARALLEL SHARED(LEN31, LADD2, LADD3, HP, S2, EMVIN, S5, LEN21, S1
       1039 |CMIC$1   , EMUIN, S6, JADDU, IJ, JADDV, JADD, LEN32, NPAIR, NG, VBL,
       1040 |CMIC$2   ABITGRDU, CORU, VP, ABITGRDV, CORV, UP) PRIVATE(IQ2, K)
       1041 |CFPP$ NODEPCHK
       1042 |CMIC$ DO PARALLEL
       1043 |      DO 600 K = 1, 16
       1044 |         DO 77042 IQ2 = 1, LEN32
       1045 |            VBL(JADDU+IJ*K+IQ2-1) = VBL(JADDU+IJ*K+IQ2-1) + ABITGRDU(
       1046 |     1         LADD3+IQ2-1,NPAIR,NG)*CORU(JADD+IJ*K+IQ2-1)*VP(JADD+IJ*K+
       1047 |     2         IQ2-1)
       1048 |77042    CONTINUE
       1049 |         DO 77043 IQ2 = 1, LEN31
       1050 |            VBL(JADDV+IJ*K+IQ2-1) = VBL(JADDV+IJ*K+IQ2-1) - ABITGRDV(
       1051 |     1         LADD3+IQ2-1,NPAIR,NG)*CORV(JADD+IJ*K+IQ2-1)*UP(JADD+IJ*K+
       1052 |     2         IQ2-1)
       1053 |77043    CONTINUE
       1054 |  600 CONTINUE
       1055 |C
       1056 |C              PLACE HP AT HU PTS INTO S1(J2,JM1).
       1057 |C              HP / EM AT VP PTS(J2,JM1) INTO S6.
       1058 |CMIC$ DO PARALLEL VECTOR
       1059 |      DO 91550 IQ2=1,LEN21
       1060 |         S1(LADD2+IQ2-1)=P5*(HP(LADD2+IQ2-2)+HP(LADD2+IQ2-1))
       1061 |         S6(LADD2+IQ2-1)=S1(LADD2+IQ2-1)*EMUIN(LADD2+IQ2-1)
       1062 |91550 CONTINUE
       1063 |C
       1064 |C              PLACE HP AT HV PTS(J3,JM1) INTO S2.
       1065 |C              HP / EM AT UP PTS(J3,JM1) INTO S5.
       1066 |CMIC$ DO PARALLEL VECTOR
       1067 |      DO 91560 IQ2=1,LEN31
       1068 |         S2(LADD3+IQ2-1)=P5*(HP(LADD2+IQ2-1)+HP(LADD3+IQ2-1))
       1069 |         S5(LADD3+IQ2-1)=S2(LADD3+IQ2-1)*EMVIN(LADD3+IQ2-1)
       1070 |91560 CONTINUE
       1071 |CMIC$ END PARALLEL
       1072 |C
       1073 |         JADD2 = LADD2 - IJ
       1074 |         JADD3 = LADD3 - IJ
       1075 |C
       1076 |CFPP$ NODEPCHK
       1077 |CMIC$ DO ALL SHARED(JADD2, IJ, JADD3, LEN31, LADD3, LEN21, LADD2, S5, UP
       1078 |CMIC$1   , FLX, S6, VP, FLY) PRIVATE(K, IQ2)
       1079 |      DO 630 K = 1, 16
       1080 |         DO 77044 IQ2 = 1, LEN31
       1081 |            FLX(JADD3+IJ*K+IQ2-1) = S5(LADD3+IQ2-1)*UP(JADD3+IJ*K+IQ2-1)
       1082 |77044    CONTINUE
       1083 |         DO 77045 IQ2 = 1, LEN21
       1084 |            FLY(JADD2+IJ*K+IQ2-1) = S6(LADD2+IQ2-1)*VP(JADD2+IJ*K+IQ2-1)
       1085 |77045    CONTINUE
       1086 |  630 CONTINUE
       1087 |C
       1088 |C============= CONVERGENCE FOR FULL TIME STEP ======================
       1089 |C
       1090 |C------------- EVALUATE CONV AT H POINTS(J3,JM1), PUT IN ARRAY CONV.
       1091 |      JADD = LADD3 - IJ
       1092 |CFPP$ NODEPCHK
       1093 |CMIC$ DO ALL SHARED(JADD, IJ, LEN31, LADD3, IM, DELSIG, EM2H, FLX, FLY,
       1094 |CMIC$1   CONV) PRIVATE(K, C1, IQ2)
       1095 |      DO 640 K = 1, 16
       1096 |C               DELX HUP/M
       1097 |         C1 = -DELSIG(K)
       1098 |         DO 77046 IQ2 = 1, LEN31
       1099 |            CONV(JADD+IJ*K+IQ2-1) = EM2H(LADD3+IQ2-1)*C1*(FLX(JADD+IJ*K+
       1100 |     1         IQ2-1)-FLX(JADD+IJ*K-2+IQ2)+FLY(JADD+IJ*K+IQ2-1)-FLY(JADD
       1101 |     2         +IJ*K-IM+IQ2-1))
       1102 |77046    CONTINUE
       1103 |  640 CONTINUE
       1104 |C
       1105 |C------------- ACCUMULATE VERT. SUM OF CONV IN S7,(J3,JM1)
       1106 |      JADD = LADD3 + IJ
       1107 |CMIC$ DO ALL VECTOR SHARED(LEN31, LADD3, JADD, CONV, S7) PRIVATE(IQ2)
       1108 |      DO 91730 IQ2=1,LEN31
       1109 |         S7(LADD3+IQ2-1)=CONV(LADD3+IQ2-1)+CONV(JADD+IQ2-1)
       1110 |91730 CONTINUE
       1111 |C
       1112 |      DO 650 K = 3,IKM
       1113 |         JADD = JADD + IJ
       1114 |CMIC$ DO ALL VECTOR SHARED(LEN31, LADD3, JADD, S7, CONV) PRIVATE(IQ2)
       1115 |      DO 91740 IQ2=1,LEN31
       1116 |         S7(LADD3+IQ2-1)=S7(LADD3+IQ2-1)+CONV(JADD+IQ2-1)
       1117 |91740 CONTINUE
       1118 |  650 CONTINUE
       1119 |C
       1120 |C              ARRAY S7 NOW HAS DELX * (THE LOCAL TIME DERIVATIVE
       1121 |C              OF SURFACE PRESSURE, WHICH EQUALS THE VERTICAL
       1122 |C              SUM OF CONV) AT H POINTS.
       1123 |C
       1124 |C------------- COMPUTE NEW VALUE OF H IN ARRAY VBL(J3,JM1)
       1125 |C
       1126 |      JADD = MADDH - 1 + LADD3
       1127 |CMIC$ DO ALL VECTOR SHARED(LEN31, JADD, LADD3, NPAIR, NG, DTOVDX, VBL,
       1128 |CMIC$1   ABITGRDH, S7) PRIVATE(IQ2)
       1129 |      DO 91750 IQ2=1,LEN31
       1130 |         VBL(JADD+IQ2-1)=VBL(JADD+IQ2-1)+
       1131 |     *               ABITGRDH(LADD3+IQ2-1, NPAIR, NG) *
       1132 |     *               DTOVDX*S7(LADD3+IQ2-1)
       1133 |91750 CONTINUE
       1134 |C
       1135 |C--------------COMPUTE INTERFACE W(K) AT H PTS(J3,JM1),(K = 2,KM).
       1136 |      C1 =  - DELSIG(1)
       1137 |CMIC$ DO ALL VECTOR SHARED(LEN31,LADD3,C1,IJ,CONV,S7,W)PRIVATE(IQ2)
       1138 |      DO 91760 IQ2=1,LEN31
       1139 |         W(LADD3+IJ+IQ2-1)=CONV(LADD3+IQ2-1)+C1*S7(LADD3+IQ2-1)
       1140 |91760 CONTINUE
       1141 |      JADD = LADD3
       1142 |C
       1143 |      DO 660 K = 3,IKM
       1144 |         JADD = JADD + IJ
       1145 |         C1   =  - DELSIG(K - 1)
       1146 |CMIC$ DO ALL VECTOR IF (ABS(IJ).GE.LEN31 .OR. IJ.EQ.0) SHARED(LEN31,
       1147 |CMIC$1   JADD, C1, LADD3, IJ, W, CONV, S7) PRIVATE(IQ2)
       1148 |      DO 91770 IQ2=1,LEN31
       1149 |         W(JADD+IJ+IQ2-1)=W(JADD+IQ2-1)+
       1150 |     *           CONV(JADD+IQ2-1)+C1*S7(LADD3+IQ2-1)
       1151 |91770 CONTINUE
       1152 |  660 CONTINUE
       1153 |C
       1154 |C============== FULL ADVECTION FOR FULL TIME STEP =====================
       1155 |C
       1156 |C--ADV HU----- DO FULL TIME STEP ADVECTION OF HU.
       1157 |C              PUT UP AT H PTS INTO SBAR(J3,JM1)
       1158 |      JADD = LADD3 - IJ
       1159 |CFPP$ NODEPCHK
       1160 |CMIC$ DO ALL SHARED(JADD, IJ, LEN31, UP, SBAR) PRIVATE(K, IQ2)
       1161 |      DO 700 K = 1, 16
       1162 |         DO 77047 IQ2 = 1, LEN31
       1163 |            SBAR(JADD+IJ*K+IQ2-1) = 0.5*(UP(JADD+IJ*K-2+IQ2)+UP(JADD+IJ*
       1164 |     1         K+IQ2-1))
       1165 |77047    CONTINUE
       1166 |  700 CONTINUE
       1167 |C
       1168 |C              PUT W * UP AT H INTERFACE PTS INTO
       1169 |C              FKS(K = 2,KM),J = J3,JM1.
       1170 |      JADD = LADD3
       1171 |C
       1172 |CFPP$ NODEPCHK
       1173 |CMIC$ DO ALL SHARED(JADD, IJ, LEN31, W, SBAR, FKS) PRIVATE(K, IQ2)
       1174 |      DO 710 K = 1, 15
       1175 |C
       1176 |C              UP(H PTS) AT INTERFACE K INTO S8.
       1177 |         DO 77048 IQ2 = 1, LEN31
       1178 |            FKS(JADD+IJ*K+IQ2-1) = W(JADD+IJ*K+IQ2-1)*0.5*(SBAR(JADD+IJ*
       1179 |     1         K+IQ2-1)+SBAR(JADD+IJ*(K-1)+IQ2-1))
       1180 |77048    CONTINUE
       1181 |  710 CONTINUE
       1182 |C
       1183 |C              AVERAGE THIS TO HU POINTS(J3,JM2) TO GET FKU.
       1184 |      JADD = LADD3
       1185 |      DO 720 K = 2,IKM
       1186 |         JADD = JADD + IJ
       1187 |CMIC$ PARALLEL SHARED(LEN32, JADD, S23, FKS, IM) PRIVATE(IQ2)
       1188 |CMIC$ DO PARALLEL VECTOR
       1189 |      DO 91870 IQ2=1,LEN32
       1190 |         S23(IQ2)=P5*(FKS(JADD+IQ2-1)+FKS(JADD+IM+IQ2-1))
       1191 |91870 CONTINUE
       1192 |CMIC$ DO PARALLEL VECTOR
       1193 |      DO 91871 IQ2 = 1, LEN32
       1194 |         FKS(JADD+IQ2-1) = S23(IQ2)
       1195 |91871 CONTINUE
       1196 |CMIC$ END PARALLEL
       1197 |  720 CONTINUE
       1198 |C
       1199 |C              TAKE THE VERTICAL DIFFS AND ACCUMULATE D(HU) IN FKS
       1200 |C              FOR J = J3,JM2.
       1201 |         C1  =  - DTOVDX / DELSIG(1)
       1202 |CMIC$ DO ALL VECTOR IF (ABS(IJ).GE.LEN32 .OR. IJ.EQ.0) SHARED(LEN32, C1
       1203 |CMIC$1   , LADD3, IJ, FKS) PRIVATE(IQ2)
       1204 |      DO 91890 IQ2=1,LEN32
       1205 |         FKS(LADD3+IQ2-1)=C1*FKS(LADD3+IJ+IQ2-1)
       1206 |91890 CONTINUE
       1207 |         JADD = LADD3
       1208 |      DO 730 K = 2,IKMM1
       1209 |         JADD = JADD + IJ
       1210 |         C1   =  - DTOVDX / DELSIG(K)
       1211 |CMIC$ DO ALL VECTOR IF (ABS(IJ).GE.LEN32 .OR. IJ.EQ.0) SHARED(LEN32, C1
       1212 |CMIC$1   , JADD, IJ, FKS) PRIVATE(IQ2)
       1213 |      DO 91900 IQ2=1,LEN32
       1214 |         FKS(JADD+IQ2-1)=C1*(FKS(JADD+IJ+IQ2-1)-FKS(JADD+IQ2-1))
       1215 |91900 CONTINUE
       1216 |  730 CONTINUE
       1217 |C
       1218 |         C1 = DTOVDX / DELSIG(KM)
       1219 |         JADD = JADD + IJ
       1220 |CMIC$ DO ALL VECTOR SHARED(LEN32, C1, JADD, FKS) PRIVATE(IQ2)
       1221 |      DO 91910 IQ2=1,LEN32
       1222 |         FKS(JADD+IQ2-1)=C1*FKS(JADD+IQ2-1)
       1223 |91910 CONTINUE
       1224 |C
       1225 |C  --HOR ADV-- GET CONTRIBUTIONS FROM HORIZONTAL FLUXES(J3,JM2)
       1226 |      JADD = LADD3 - IJ
       1227 |      JADDU = NADDU
       1228 |C
       1229 |CFPP$ NODEPCHK
       1230 |CMIC$ DO ALL SHARED(JADD, IJ, JADDU, LEN31, LADD3, LEN32, IM, NPAIR, NG
       1231 |CMIC$1   , DTOVDX, FLX, UP, FLY, SBAR, VBL, ABITGRDU, FKS, EM2U)
       1232 |CMIC$2    PRIVATE(S23, S22, S21, K, IQ2) SAVELAST
       1233 |      DO 740 K = 1, 16
       1234 |C
       1235 |         DO 77049 IQ2 = 1, LEN31
       1236 |            S23(LADD3+IQ2-1) = FLX(JADD+IJ*K+IQ2-1)*UP(JADD+IJ*K+IQ2-1)
       1237 |77049    CONTINUE
       1238 |C
       1239 |C     (HP)     FIU AT CENTER OF GRID SQUARE.
       1240 |         DO 77050 IQ2 = 1, LEN32
       1241 |            S22(LADD3+IQ2-1)=0.5*(S23(LADD3+IQ2-1)+S23(LADD3+IM+IQ2-1))
       1242 |77050    CONTINUE
       1243 |C
       1244 |C        FJU AT H PTS(J3,JM1)
       1245 |         DO 77051 IQ2 = 1, LEN31
       1246 |            S21(LADD3+IQ2-1) = 0.5*(FLY(JADD+IJ*K+IQ2-1)+FLY(JADD+IJ*K-
       1247 |     1         IM+IQ2-1))*SBAR(JADD+IJ*K+IQ2-1)
       1248 |77051    CONTINUE
       1249 |C
       1250 |C         X-DIFF. CENTERED AT HU PTS(J3,JM2)
       1251 |C         Y-DIFF. CENTERED AT HU PTS(J3,JM2)
       1252 |         DO 77052 IQ2 = 1, LEN32
       1253 |C              COMBINE WITH X-DIFF, MULT BY EM**2,
       1254 |C              AND ADD TO VERTICAL TERM ALREADY IN FKS.
       1255 |C              STORE NEW HU VALUE IN VBL WHERE ALLOWED.
       1256 |            VBL(JADDU+IJ*K+IQ2-1) = VBL(JADDU+IJ*K+IQ2-1) + ABITGRDU(
       1257 |     1         LADD3+IQ2-1,NPAIR,NG)*(FKS(JADD+IJ*K+IQ2-1)-DTOVDX*(S21(
       1258 |     2         LADD3+IM+IQ2-1)-S21(LADD3+IQ2-1)+S22(LADD3+IQ2-1)-S22(
       1259 |     3         LADD3-2+IQ2))*EM2U(LADD3+IQ2-1))
       1260 |77052    CONTINUE
       1261 |  740 CONTINUE
       1262 |C
       1263 |C
       1264 |C---ADV HV-----DO FULL TIME STEP ADVECTION OF HV.
       1265 |C              PUT VP AT H PTS INTO SBAR(J3,JM1)
       1266 |      JADD = LADD3 - IJ
       1267 |C
       1268 |CFPP$ NODEPCHK
       1269 |CMIC$ DO ALL SHARED(JADD, IJ, LEN31, IM, VP, SBAR) PRIVATE(K, IQ2)
       1270 |      DO 750 K = 1, 16
       1271 |         DO 77053 IQ2 = 1, LEN31
       1272 |            SBAR(JADD+IJ*K+IQ2-1) = 0.5*(VP(JADD+IJ*K+IQ2-1)+VP(JADD+IJ*
       1273 |     1         K-IM+IQ2-1))
       1274 |77053    CONTINUE
       1275 |  750 CONTINUE
       1276 |C
       1277 |C              PUT W * VP (H, INTERFACE PTS) INTO
       1278 |C              FKS(K = 2,KM AND J = J3,JM1).
       1279 |      JADD = LADD3
       1280 |CFPP$ NODEPCHK
       1281 |CMIC$ DO ALL SHARED(JADD, IJ, LEN31, W, SBAR, FKS) PRIVATE(K, IQ2)
       1282 |      DO 760 K = 1, 15
       1283 |         DO 77054 IQ2 = 1, LEN31
       1284 |            FKS(JADD+IJ*K+IQ2-1) = W(JADD+IJ*K+IQ2-1)*0.5*(SBAR(JADD+IJ*
       1285 |     1         K+IQ2-1)+SBAR(JADD+IJ*(K-1)+IQ2-1))
       1286 |77054    CONTINUE
       1287 |  760 CONTINUE
       1288 |C
       1289 |C              AVERAGE THIS TO HV POINTS (J = J3,JM1).
       1290 |      JADD = LADD3
       1291 |      DO 770 K = 2,IKM
       1292 |         JADD = JADD + IJ
       1293 |CMIC$ PARALLEL SHARED(LEN31, JADD, S23, FKS) PRIVATE(IQ2)
       1294 |CMIC$ DO PARALLEL VECTOR
       1295 |      DO 92160 IQ2=1,LEN31
       1296 |         S23(IQ2)=P5*(FKS(JADD+IQ2-1)+FKS(JADD+IQ2))
       1297 |92160 CONTINUE
       1298 |CMIC$ DO PARALLEL VECTOR
       1299 |      DO 92161 IQ2 = 1, LEN31
       1300 |         FKS(JADD+IQ2-1) = S23(IQ2)
       1301 |92161 CONTINUE
       1302 |CMIC$ END PARALLEL
       1303 |  770 CONTINUE
       1304 |C
       1305 |C              TAKE VERTICAL DIFFS AND ACCUMULATE D(HV) IN FKS(J3,JM1).
       1306 |         C1   =  - DTOVDX / DELSIG(1)
       1307 |CMIC$ DO ALL VECTOR IF (ABS(IJ).GE.LEN31 .OR. IJ.EQ.0) SHARED(LEN31, C1
       1308 |CMIC$1   , LADD3, IJ, FKS) PRIVATE(IQ2)
       1309 |      DO 92180 IQ2=1,LEN31
       1310 |         FKS(LADD3+IQ2-1)=C1*FKS(LADD3+IJ+IQ2-1)
       1311 |92180 CONTINUE
       1312 |      JADD = LADD3
       1313 |      DO 780 K = 2,IKMM1
       1314 |         JADD = JADD + IJ
       1315 |         C1   =  - DTOVDX / DELSIG(K)
       1316 |CMIC$ DO ALL VECTOR IF (ABS(IJ).GE.LEN31 .OR. IJ.EQ.0) SHARED(LEN31, C1
       1317 |CMIC$1   , JADD, IJ, FKS) PRIVATE(IQ2)
       1318 |      DO 92190 IQ2=1,LEN31
       1319 |         FKS(JADD+IQ2-1)=C1*(FKS(JADD+IJ+IQ2-1)-FKS(JADD+IQ2-1))
       1320 |92190 CONTINUE
       1321 |  780 CONTINUE
       1322 |C
       1323 |         C1   = DTOVDX / DELSIG(KM)
       1324 |         JADD = JADD + IJ
       1325 |CMIC$ DO ALL VECTOR SHARED(LEN31, C1, JADD, FKS) PRIVATE(IQ2)
       1326 |      DO 92200 IQ2=1,LEN31
       1327 |         FKS(JADD+IQ2-1)=C1*FKS(JADD+IQ2-1)
       1328 |92200 CONTINUE
       1329 |C
       1330 |C --HOR ADV--  GET CONTRIBUTIONS FROM HORIZONTAL FLUXES AND COMBINE
       1331 |C              WITH THE VERTICAL TERM.(J3,JM1).
       1332 |      JADD  = LADD3 - IJ
       1333 |      JADDV = NADDV
       1334 |CFPP$ NODEPCHK
       1335 |CMIC$ DO ALL SHARED(JADD, IJ, JADDV, LEN31, LADD3, LEN21, IM, LADD2,
       1336 |CMIC$1   NPAIR, NG, DTOVDX, FLX, SBAR, FLY, VP, VBL, ABITGRDV, FKS, EM2V
       1337 |CMIC$2   ) PRIVATE(S23, S21, S22, K, IQ2) SAVELAST
       1338 |      DO 790 K = 1, 16
       1339 |C              FIV AT H PTS (J = J3,JM1).
       1340 |         DO 77055 IQ2 = 1, LEN31
       1341 |            S23(LADD3+IQ2-1) = 0.5*(FLX(JADD+IJ*K-2+IQ2)+FLX(JADD+IJ*K+
       1342 |     1         IQ2-1))*SBAR(JADD+IJ*K+IQ2-1)
       1343 |77055    CONTINUE
       1344 |C
       1345 |C      (HP)    FJV IN CENTER OF GRID SQUARE(J2,JM1)
       1346 |         DO 77056 IQ2 = 1, LEN21
       1347 |            S21(LADD2+IQ2-1) = FLY(JADD+IJ*K-IM+IQ2-1)*VP(JADD+IJ*K-IM+
       1348 |     1         IQ2-1)
       1349 |77056    CONTINUE
       1350 |         DO 77057 IQ2 = 1, LEN21
       1351 |            S22(LADD2+IQ2-1) = 0.5*(S21(LADD2+IQ2-1)+S21(LADD2+IQ2))
       1352 |77057    CONTINUE
       1353 |C
       1354 |C      (HV)    X-DIFF. CENTERED AT HV PTS(J3,JM1)
       1355 |C      (HV)    Y-DIFF. CENTERED AT HV PTS(J3,JM1)
       1356 |         DO 77058 IQ2 = 1, LEN31
       1357 |C              COMBINE WITH X-DIFF,MULT. BY M**2 AND ADD TO VERT.TERM.
       1358 |C              STORE NEW HV VALUE IN VBL WHERE ALLOWED.
       1359 |            VBL(JADDV+IJ*K+IQ2-1) = VBL(JADDV+IJ*K+IQ2-1) + ABITGRDV(
       1360 |     1         LADD3+IQ2-1,NPAIR,NG)*(FKS(JADD+IJ*K+IQ2-1)-DTOVDX*(S23(
       1361 |     2         LADD3+IQ2)-S23(LADD3+IQ2-1)+S22(LADD3+IQ2-1)-S22(LADD2+
       1362 |     3         IQ2-1))*EM2V(LADD3+IQ2-1))
       1363 |77058    CONTINUE
       1364 |  790 CONTINUE
       1365 |C
       1366 |C              ARRAYS UP AND VP ARE NO LONGER NEEDED.
       1367 |C              THEREFORE, THE SPACE USED BY VP IS AVAILABLE FOR USE
       1368 |C              AS THPATH AND THEN QPATH.
       1369 |C
       1370 |C---ADV HTH---- DO FULL TIME STEP ADVECTION OF HTH (H * THETA).
       1371 |C              CALCULATE THP AT HV PTS AND PUT IT INTO SBAR(J=J3,JM1).
       1372 |C              CALCULATE THP AT H PTS AND PUT IT INTO THPATH (J3,JM1).
       1373 |      JADD  = LADD3 - IJ
       1374 |CFPP$ NODEPCHK
       1375 |CMIC$ DO ALL SHARED(JADD,IJ,LEN31,IM,THP,SBAR,THPATH)PRIVATE(K,IQ2)
       1376 |      DO 800 K = 1, 16
       1377 |         DO 77059 IQ2 = 1, LEN31
       1378 |            SBAR(JADD+IJ*K+IQ2-1) = 0.5*(THP(JADD+IJ*K+IQ2-1)+THP(JADD+
       1379 |     1         IJ*K-IM+IQ2-1))
       1380 |77059    CONTINUE
       1381 |         DO 77060 IQ2 = 1, LEN31
       1382 |            THPATH(JADD+IJ*K+IQ2-1) = 0.5*(SBAR(JADD+IJ*K-2+IQ2)+SBAR(
       1383 |     1         JADD+IJ*K+IQ2-1))
       1384 |77060    CONTINUE
       1385 |  800 CONTINUE
       1386 |C
       1387 |C              PUT W * THP(H INTERFACE PTS) INTO
       1388 |C              FKS(K = 2,KM AND J = J3,JM1).
       1389 |      JADD  = LADD3
       1390 |C
       1391 |CFPP$ NODEPCHK
       1392 |CMIC$ DO ALL SHARED(JADD, IJ, LEN31, W, THPATH, FKS) PRIVATE(K, IQ2)
       1393 |      DO 810 K = 1, 15
       1394 |         DO 77061 IQ2 = 1, LEN31
       1395 |            FKS(JADD+IJ*K+IQ2-1) = W(JADD+IJ*K+IQ2-1)*0.5*(THPATH(JADD+
       1396 |     1         IJ*K+IQ2-1)+THPATH(JADD+IJ*(K-1)+IQ2-1))
       1397 |77061    CONTINUE
       1398 |  810 CONTINUE
       1399 |C
       1400 |C              TAKE VERTICAL DIFFS AND ACCUMULATE D(HT) IN FKS.
       1401 |         C1   = - DTOVDX / DELSIG(1)
       1402 |CMIC$ DO ALL VECTOR IF (ABS(IJ).GE.LEN31 .OR. IJ.EQ.0) SHARED(LEN31, C1
       1403 |CMIC$1   , LADD3, IJ, FKS) PRIVATE(IQ2)
       1404 |      DO 92470 IQ2=1,LEN31
       1405 |         FKS(LADD3+IQ2-1)=C1*FKS(LADD3+IJ+IQ2-1)
       1406 |92470 CONTINUE
       1407 |         JADD = LADD3
       1408 |C
       1409 |      DO 820 K = 2,IKMM1
       1410 |         JADD = JADD + IJ
       1411 |         C1   =  - DTOVDX / DELSIG(K)
       1412 |CMIC$ DO ALL VECTOR IF (ABS(IJ).GE.LEN31 .OR. IJ.EQ.0) SHARED(LEN31, C1
       1413 |CMIC$1   , JADD, IJ, FKS) PRIVATE(IQ2)
       1414 |      DO 92480 IQ2=1,LEN31
       1415 |         FKS(JADD+IQ2-1)=C1*(FKS(JADD+IJ+IQ2-1)-FKS(JADD+IQ2-1))
       1416 |92480 CONTINUE
       1417 |  820 CONTINUE
       1418 |C
       1419 |         C1   = DTOVDX / DELSIG(KM)
       1420 |         JADD = JADD + IJ
       1421 |CMIC$ DO ALL VECTOR SHARED(LEN31, C1, JADD, FKS) PRIVATE(IQ2)
       1422 |      DO 92490 IQ2=1,LEN31
       1423 |         FKS(JADD+IQ2-1)=C1*FKS(JADD+IQ2-1)
       1424 |92490 CONTINUE
       1425 |C
       1426 |C              GET CONTRIBUTIONS FROM X AND Y FLUX.
       1427 |         JADD  = LADD3 - IJ
       1428 |         JADDT = NADDT
       1429 |C
       1430 |CFPP$ NODEPCHK
       1431 |CMIC$ DO ALL SHARED(JADD, IJ, JADDT, LEN31, LADD3, LEN21, IM, LADD2,
       1432 |CMIC$1   NPAIR, NG, DTOVDX, FLX, SBAR, THP, FLY, VBL, ABITGRDH, FKS,
       1433 |CMIC$2   EM2H) PRIVATE(S22, S23, K, IQ2) SAVELAST
       1434 |      DO 830 K = 1, 16
       1435 |C
       1436 |C              FIT AT HV PTS(J3,JM1)
       1437 |         DO 77062 IQ2 = 1, LEN31
       1438 |            S22(LADD3+IQ2-1)=FLX(JADD+IJ*K+IQ2-1)*SBAR(JADD+IJ*K+IQ2-1)
       1439 |77062    CONTINUE
       1440 |C
       1441 |C        FJT AT HU PTS(J2,JM1)
       1442 |         DO 77063 IQ2 = 1, LEN21
       1443 |            S23(LADD2+IQ2-1) = 0.5*(THP(JADD+IJ*K-IM-2+IQ2)+THP(JADD+IJ*
       1444 |     1         K-IM+IQ2-1))*FLY(JADD+IJ*K-IM+IQ2-1)
       1445 |77063    CONTINUE
       1446 |C
       1447 |C          X-DIFF CENTERED AT H PTS(J3,JM1)
       1448 |C          Y-DIFF CENTERED AT H PTS(J3,JM1)
       1449 |         DO 77064 IQ2 = 1, LEN31
       1450 |C              COMBINE WITH X-DIFF, MULT BY M**2 AND ADD TO VERT. TERM.
       1451 |C              PUT NEW H * THETA IN VBL WHERE ALLOWED TO.
       1452 |            VBL(JADDT+IJ*K+IQ2-1) = VBL(JADDT+IJ*K+IQ2-1) + ABITGRDH(
       1453 |     1         LADD3+IQ2-1,NPAIR,NG)*(FKS(JADD+IJ*K+IQ2-1)-DTOVDX*(S22(
       1454 |     2         LADD3+IQ2-1)-S22(LADD3-2+IQ2)+S23(LADD3+IQ2-1)-S23(LADD2+
       1455 |     3         IQ2-1))*EM2H(LADD3+IQ2-1))
       1456 |77064    CONTINUE
       1457 |  830 CONTINUE
       1458 |C
       1459 |C---ADV HQ---- DO FULL TIME STEP ADVECTION OF HQ
       1460 |C              PUT QP AT HV PTS INTO SBAR(J3,JM1)
       1461 |C              AND QP AT H PTS INTO QPATH (J = J3, JM1).
       1462 |         JADD = LADD3 - IJ
       1463 |C
       1464 |CFPP$ NODEPCHK
       1465 |CMIC$ DO ALL SHARED(JADD,IJ,LEN31,IM,QP,SBAR,QPATH)PRIVATE(K,IQ2)
       1466 |      DO 850 K = 1, 16
       1467 |         DO 77065 IQ2 = 1, LEN31
       1468 |            SBAR(JADD+IJ*K+IQ2-1) = 0.5*(QP(JADD+IJ*K+IQ2-1)+QP(JADD+IJ*
       1469 |     1         K-IM+IQ2-1))
