          1 |       SUBROUTINE INTERCON(IK,RK,BITK,XPT,NPT,X,NX,NKX,EXTRAP)
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .                                       .
          4 |C SUBPROGRAM:    INTERCON    GET INTERPOLATION CONSTANTS
          5 |C   PRGMMR: PARRISH          ORG: W/NMC22    DATE: 88-08-08
          6 |C
          7 |C ABSTRACT: DO ONE ITERATION OF INITIALIZATION.
          8 |C ABSTRACT: OBTAIN INTERPOLATION CONSTANTS FOR POINTS XPT ON
          9 |C   INTERVAL OF NX POINTS WHICH ARE MONOTONIC.
         10 |C
         11 |C PROGRAM HISTORY LOG:
         12 |C   88-08-08  PARRISH
         13 |C
         14 |C USAGE:    CALL INTERCON(IK,RK,BITK,XPT,NPT,X,NX,NKX,EXTRAP)
         15 |C   INPUT ARGUMENT LIST:
         16 |C     BITK     - =1--INTERPOLATEE.
         17 |C              - =0--EXTRAPOLATEE.
         18 |C     XPT      - COORDINATES OF INTERPOLATEE POINTS.
         19 |C     NPT      - NUMBER OF INTERPOLATEE POINTS.
         20 |C     X        - COORDINATES OF INTERPOLATOR POINTS (MONOTONIC)
         21 |C     NX       - NUMBER OF INTERPOLATOR POINTS.
         22 |C     NKX      - ORDER OF INTERPOLATION
         23 |C              -    =1--ASSIGN VALUE OF NEAREST POINT
         24 |C              -    =2--2-POINT (LINEAR)
         25 |C              -    =3--3-POINT (QUADRATIC)
         26 |C              -    =4--4-POINT (CUBIC)
         27 |C     EXTRAP   - FRACTION OF FIRST AND LAST INTERVAL WE ARE ALLOWED TO
         28 |C              - EXTRAPOLATE TO.
         29 |C
         30 |C   OUTPUT ARGUMENT LIST:      (INCLUDING WORK ARRAYS)
         31 |C     IK       - GATHER INDEX FOR INTERPOLATORS
         32 |C     RK       - INTERPOLATOR WEIGHTS
         33 |C     BITK     - =1--INTERPOLATEE
         34 |C              - =0--EXTRAPOLATEE
         35 |C
         36 |C ATTRIBUTES:
         37 |C   LANGUAGE: FORTRAN200
         38 |C   MACHINE:  CYBER
         39 |C
         40 |C$$$
         41 |C--------
         42 |         include "myparam"
  1       1 |        INCLUDE 'parmodel'
  2       1 |C     PARAMETER FILE
  2       2 |C
  2       3 |C     SETUP FOR IBM SINGLE PRECISION REPRESENTATION
  2       4 |C     ON OUTPUT
  2       5 |C                      THIS VERSION 21 February 1991 (DiMego)
  2       6 |C
  2       7 |      PARAMETER (
  2       8 |     *      IBPW      =      4,
  2       9 |     *      IBPOR     = 120000,
  2      10 |     *      INUMREC   = IBPOR / IBPW,
  2      11 |     *      INSPECS   =    700,
  2      12 |     *      ILATG2    =    120,
  2      13 |     *      ILEVH     =     12,
  2      14 |     *      ILEVS     =     16,
  2      15 |     *      ILONF     =    360,
  2      16 |     *      INLATSH   =      5,
  2      17 |     *      IIJMAX    =  23668,
  2      18 |     *      IIJMAXB   =  25536,
  2      19 |     *      IIJRAD    =    892,
  2      20 |     *      IJCAP     =     80,
  2      21 |     *      IWPCPL    =     80,
  2      22 |     *      IKM       =     16 )
  2      23 |      PARAMETER (
  2      24 |     *      ILBCH     =   7*(147+111-7),
  2      25 |     *      ILBHI     =    (147+111-26),
  2      26 |     *      ILBUI     =    (2*147+111-39),
  2      27 |     *      ILBUO     =   (8*147+7*111-56),
  2      28 |     *      ILBVI     =    (147+2*111-39),
  2      29 |     *      ILBVO     =   (7*147+8*111-56),
  2      30 |     *      ILMOUT    =     20 )
  2      31 |      PARAMETER (
  2      32 |     *      IMAXOUTF  =     17,
  2      33 |     *      IMAXOUTT  =      3,
  2      34 |     *      IMAXOUTV  =    100,
  2      35 |     *      IMAXPROF  =      6,
  2      36 |     *      INFILLER  =  10000,
  2      37 |     *      INH       =     65,
  2      38 |     *      INGRDUSE  =      2,
  2      39 |     *      INGRDUS1  =      1,
  2      40 |     *      INIADDRS  =     44,
  2      41 |     *      INSCR     =     23,
  2      42 |     *      INSCRGEO  =     12,
  2      43 |     *      INSCR3    =     10,
  2      44 |     *      INSOIL    =      2,
  2      45 |     *      INSUMG    =  42164,
  2      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  2      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  2      48 |     *      INSHEM    = 13 * INH + 23,
  2      49 |     *      IIJKMAX   = IKM * IIJMAX )
  2      50 |      PARAMETER (
  2      51 |     *  IIWORDSB  = IIJMAXB / 64,
  2      52 |     *  IKMP1     = IKM + 1,
  2      53 |     *  ILVLTOT   = 7 * IKM + 37,
  2      54 |     *  ILONFP1   = ILONF + 1,
  2      55 |     *  INVBL     = ILVLTOT * INSUMG,
  2      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  2      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  2      58 |     *  IBLK1D    =  2 * IKM + 14,
  2      59 |     *  IBLK2D    = 16 * IKM + 36,
  2      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  2      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  2      62 |C
  1       2 |C-------
  1       3 |C------- FOLLOWING IS EXTRA STUFF FOR INITIALIZATION
  1       4 |C-------
  1       5 |         PARAMETER (INLATH=101,INUMCOEF=3*INLATH+35,
  1       6 |     *        INUMGRID=4,IJCAPDP=80,INLON=300,INKLON=2,INKLAT=2,
  1       7 |     *        IIORBPL=3,IIORDPL=3,IIORBLP=3,IIORDLP=3,IMCUBIC=1,
  1       8 |     *        INUMVERT=2,INSIG=IKM,INMODES=18,INGMAX=3000,
  1       9 |     *        INKSIG=2,INSIGP1=INSIG+1,INLONEX=INLON+INKLON,
  1      10 |     *        INLATDP=2*INLATH,INLOLA=INLATDP*INLONEX,
  1      11 |     *        IJCAP1DP=IJCAPDP+1,IJCAP12=2*IJCAP1DP,
  1      12 |     *        INCOEFS=IJCAP1DP*(IJCAPDP+2),
  1      13 |     *     IDPFILLR=(4*IKM+1)*INSUMG+       3*(IIJKMAX*4+IIJMAX) )
  1      14 |C    *     IDPFILLR=(4*IKM+1)*INSUMG+INGRDUSE*(IIJKMAX*4+IIJMAX) )
         43 |         REAL RK(NPT,NKX),XPT(NPT),X(NX)
         44 |         INTEGER IK(NPT)
         45 |         LOGICAL BITK(NPT)
         46 |C--------
         47 |         IF(NX.EQ.1) THEN
         48 |           DO 10002 I=1,NPT
         49 |             IK(I)=0
         50 |             RK(I,1)=1.
         51 |10002      CONTINUE
         52 |           RETURN
         53 |         END IF
         54 |         MKX=MIN(NX,NKX,4)
         55 |C--------
         56 |C-------- DETERMINE ORDERING OF INTERPOLATOR POINTS (THEY MUST BE
         57 |C-------- EITHER MONOTONIC INCREASING OR DECREASING)
         58 |C--------
         59 |         DIRX=X(2)-X(1)
         60 |C        IF(DIRX.EQ.0.) STOP 'DIRX=0. IN INTERCON'
         61 |         IF(DIRX.EQ.0.) WRITE(6,99471)
         62 |99471    FORMAT(' DIRX = 0 IN INTERCON')
         63 |         DELX1=EXTRAP*ABS(DIRX)
         64 |         DELXN=EXTRAP*ABS(X(NX)-X(NX-1))
         65 |C        XPTMAX=Q8SMAX(XPT(1;NPT))
         66 |C        XPTMIN=Q8SMIN(XPT(1;NPT))
         67 |C        XMAX=Q8SMAX(X(1;NX))
         68 |C        XMIN=Q8SMIN(X(1;NX))
         69 |C        WRITE(6,21)XPTMAX,XPTMIN,XMAX,XMIN,DELX1,DELXN,EXTRAP,DIRX
         70 |C21      FORMAT(' XPX,XPMN,XX,XMN,DLX1,DLXN,EXTP,DIRX=',8E11.3)
         71 |C--------
         72 |C-------- FLAG EXTRAPOLATEES
         73 |C--------
         74 |         IF(DIRX.GT.0.) THEN
         75 |           DO 10004 I=1,NPT
         76 |             BITK(I)=BITK(I).AND.
         77 |     *                       (XPT(I).GT.X(1)-DELX1).AND.
         78 |     *                       (XPT(I).LT.X(NX)+DELXN)
         79 |10004      CONTINUE
         80 |         END IF
         81 |         IF(DIRX.LT.0.) THEN
         82 |           DO 10006 I=1,NPT
         83 |             BITK(I)=BITK(I).AND.
         84 |     *                       (XPT(I).LT.X(1)+DELX1).AND.
         85 |     *                       (XPT(I).GT.X(NX)-DELXN)
         86 |10006      CONTINUE
         87 |         END IF
         88 |C--------
         89 |C-------- DETERMINE GATHER INDEX.
         90 |C--------
         91 |C        IF(DIRX.GT.0.) IK(1;NPT)=Q8VLTI(XPT(1;NPT),X(1;NX);IK(1;NPT))
         92 |         DO 10008 I=1,NPT
         93 |           IK(I)=0
         94 |10008    CONTINUE
         95 |         IF(DIRX.GT.0.) THEN
         96 |c          DO 10012 K=1,NX-1
         97 |c            DO 10010 I=1,NPT
         98 |c              IF(X(K).LE.XPT(I).AND.XPT(I).LT.X(K+1)) IK(I)=K
         99 |c              if(xpt(i).le.x(1)) ik(i)=1
        100 |c              if(xpt(i).ge.x(nx)) ik(i)=nx
        101 |c0010        CONTINUE
        102 |c0012      CONTINUE
        103 |           do 10012 i=1,npt
        104 |             if(xpt(i).le.x(1)) then
        105 |               ik(i)=1
        106 |             else
        107 |               ik(i)=isrchfge(nx-1,x,1,xpt(i))-1
        108 |             end if
        109 |10012      continue
        110 |         END IF
        111 |C        IF(DIRX.LT.0.) IK(1;NPT)=Q8VGEI(XPT(1;NPT),X(1;NX);IK(1;NPT))
        112 |         IF(DIRX.LT.0.) THEN
        113 |c          DO 19012 K=1,NX-1
        114 |c            DO 19010 I=1,NPT
        115 |c              IF(X(K).GE.XPT(I).AND.XPT(I).GT.X(K+1)) IK(I)=K
        116 |c              if(xpt(i).ge.x(1)) ik(i)=1
        117 |c              if(xpt(i).le.x(nx)) ik(i)=nx
        118 |c9010        CONTINUE
        119 |c9012      CONTINUE
        120 |           do 19012 i=1,npt
        121 |             if(xpt(i).ge.x(1)) then
        122 |               ik(i)=1
        123 |             else
        124 |               ik(i)=isrchfle(nx-1,x,1,xpt(i))-1
        125 |             end if
        126 |19012      continue
        127 |         END IF
        128 |         DO 10014 I=1,NPT
        129 |           IK(I)=MAX(1,IK(I))
        130 |10014    CONTINUE
        131 |C--------
        132 |C-------- NOW DETERMINE INTERPOLATION WEIGHTS
        133 |C--------
        134 |         GO TO (100,200,300,400),MKX
        135 |100      CONTINUE
        136 |C----------
        137 |C---------- 0 ORDER INTERPOLATION
        138 |C----------
        139 |C---------- FIND NEAREST POINT, AND ADJUST IK ACCORDINGLY
        140 |C----------
        141 |           DO 198 I=1,NPT
        142 |             IK(I)=MIN(IK(I),NX-1)
        143 |             W1D=X(IK(I))
        144 |             W2D=X(IK(I)+1)
        145 |             W1D=ABS(W1D-XPT(I))
        146 |             W2D=ABS(W2D-XPT(I))
        147 |             IF(W2D.LT.W1D) IK(I)=IK(I)+1
        148 |             RK(I,1)=1.
        149 |198        CONTINUE
        150 |           GO TO 500
        151 |200      CONTINUE
        152 |C----------
        153 |C---------- 1ST ORDER (LINEAR) INTERPOLATION
        154 |C----------
        155 |           DO 298 I=1,NPT
        156 |             IK(I)=MIN(IK(I),NX-1)
        157 |             W1D=X(IK(I))
        158 |             W2D=X(IK(I)+1)
        159 |             W12ID=1./(W1D-W2D)
        160 |             RK(I,1)=(XPT(I)-W2D)*W12ID
        161 |             RK(I,2)=(W1D-XPT(I))*W12ID
        162 |298        CONTINUE
        163 |           GO TO 500
        164 |300      CONTINUE
        165 |C----------
        166 |C---------- 2ND ORDER (QUADRATIC) INTERPOLATION
        167 |C----------
        168 |C---------- COMPUTE WEIGHTS RK, AND ADJUST IK ACCORDINGLY
        169 |C----------
        170 |           DO 398 I=1,NPT
        171 |             IK(I)=MIN(IK(I),NX-1)
        172 |             W1D=X(IK(I))
        173 |             W2D=X(IK(I)+1)
        174 |             W1D=ABS(W1D-XPT(I))
        175 |             W2D=ABS(W2D-XPT(I))
        176 |             IF(W2D.LT.W1D) IK(I)=IK(I)+1
        177 |             IK(I)=IK(I)-1
        178 |             IK(I)=MAX(1,MIN(IK(I),NX-2))
        179 |             W1D=X(IK(I))
        180 |             W2D=X(IK(I)+1)
        181 |             W3D=X(IK(I)+2)
        182 |             W12ID=1./(W1D-W2D)
        183 |             W13ID=1./(W1D-W3D)
        184 |             W23ID=1./(W2D-W3D)
        185 |             RK(I,1)=(XPT(I)-W2D)*(XPT(I)-W3D)*W12ID*W13ID
        186 |             RK(I,2)=(W1D-XPT(I))*(XPT(I)-W3D)*W12ID*W23ID
        187 |             RK(I,3)=(W1D-XPT(I))*(W2D-XPT(I))*W13ID*W23ID
        188 |398        CONTINUE
        189 |           GO TO 500
        190 |400      CONTINUE
        191 |C----------
        192 |C---------- 3RD ORDER (CUBIC) INTERPOLATION
        193 |C----------
        194 |C---------- IF MCUBIC = 1, THEN
        195 |C---------- USE CUBIC FITTED TO NEAREST 4 POINTS
        196 |C----------
        197 |C---------- IF MCUBIC = 2, THEN
        198 |C---------- USE QUASI-HERMITE CUBIC SPLINE IN INTERIOR
        199 |C---------- USE QUADRATIC IN END INTERVALS
        200 |C----------
        201 |C          IK(1;NPT)=IK(1;NPT)-2
        202 |           MCUBIC = IMCUBIC
        203 |           DO 498 I=1,NPT
        204 |             IK(I)=IK(I)-1
        205 |             IF (MCUBIC.EQ.2) THEN
        206 |               IEND=0
        207 |               IF(IK(I).LT.1) IEND=-1
        208 |               IF(IK(I).GT.NX-3) IEND=1
        209 |             END IF
        210 |             IK(I)=MAX(1,MIN(IK(I),NX-3))
        211 |             W1D=X(IK(I))
        212 |             W2D=X(IK(I)+1)
        213 |             W3D=X(IK(I)+2)
        214 |             W4D=X(IK(I)+3)
        215 |             W12ID=1./(W1D-W2D)
        216 |             W13ID=1./(W1D-W3D)
        217 |             W14ID=1./(W1D-W4D)
        218 |             W23ID=1./(W2D-W3D)
        219 |             W24ID=1./(W2D-W4D)
        220 |             W34ID=1./(W3D-W4D)
        221 |             IF (MCUBIC.EQ.2) THEN
        222 |               WM21=(W2D-W3D)*W12ID*W13ID
        223 |               WM22=(W1D+W3D-2.*W2D)*W12ID*W23ID
        224 |               WM23=(W2D-W1D)*W13ID*W23ID
        225 |               WM24=0.
        226 |               WM31=0.
        227 |               WM32=(W3D-W4D)*W23ID*W24ID
        228 |               WM33=(W2D+W4D-2.*W3D)*W23ID*W34ID
        229 |               WM34=(W3D-W2D)*W24ID*W34ID
        230 |               IF(IEND.EQ.-1) THEN
        231 |                 WM31=(W3D-W2D)*W12ID*W13ID
        232 |                 WM32=(W1D-W3D)*W12ID*W23ID
        233 |                 WM33=(2.*W3D-W1D-W2D)*W13ID*W23ID
        234 |                 WM34=0.
        235 |               END IF
        236 |               IF(IEND.EQ.1) THEN
        237 |                 WM21=0.
        238 |                 WM22=(2.*W2D-W3D-W4D)*W23ID*W24ID
        239 |                 WM23=(W4D-W2D)*W23ID*W34ID
        240 |                 WM24=(W2D-W3D)*W24ID*W34ID
        241 |               END IF
        242 |               WM2=(XPT(I)-W2D)*(((XPT(I)-W3D)*W23ID)**2)
        243 |               WM3=(XPT(I)-W3D)*(((XPT(I)-W2D)*W23ID)**2)
        244 |               RK(I,1)=WM2*WM21+WM3*WM31
        245 |               RK(I,2)=WM2*WM22+WM3*WM32-
        246 |     *               ((XPT(I)-W3D)**2)*
        247 |     *               (2.*XPT(I)-3.*W2D+W3D)*W23ID**3
        248 |               RK(I,3)=WM2*WM23+WM3*WM33+
        249 |     *               ((XPT(I)-W2D)**2)*
        250 |     *               (2.*XPT(I)-3.*W3D+W2D)*W23ID**3
        251 |               RK(I,4)=WM2*WM24+WM3*WM34
        252 |             ELSE
        253 |               RK(I,1)=(XPT(I)-W2D)*(XPT(I)-W3D)
        254 |     *               *(XPT(I)-W4D)*W12ID*W13ID*W14ID
        255 |               RK(I,2)=(W1D-XPT(I))*(XPT(I)-W3D)
        256 |     *               *(XPT(I)-W4D)*W12ID*W23ID*W24ID
        257 |               RK(I,3)=(W1D-XPT(I))*(W2D-XPT(I))
        258 |     *               *(XPT(I)-W4D)*W13ID*W23ID*W34ID
        259 |               RK(I,4)=(W1D-XPT(I))*(W2D-XPT(I))
        260 |     *               *(W3D-XPT(I))*W14ID*W24ID*W34ID
        261 |             END IF
        262 |498        CONTINUE
        263 |500      CONTINUE
        264 |C--------
        265 |C-------- FINALLY SUBTRACT ONE FROM IK
        266 |C--------
        267 |         DO 60002 I=1,NPT
        268 |           IK(I)=IK(I)-1
        269 |60002    CONTINUE
        270 |       RETURN
        271 |       END
 
