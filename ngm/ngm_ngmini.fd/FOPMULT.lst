         54 |         ii2=0
         55 |         ii3=0
         56 |         do 10008 i=1,ncoefs
         57 |           if(bones(i).and..not.bfop(i)) then
         58 |             ii3=ii3+1
         59 |             ii1=ii1+1
         60 |             fz(ii3)=fz(ii1)
         61 |           end if
         62 |           if(.not.bones(i).and.bfop(i)) then
         63 |             ii3=ii3+1
         64 |             ii2=ii2+1
         65 |             fz(ii3)=work(ifop+ii2-1)
         66 |           end if
         67 |           if(bones(i).and.bfop(i)) then
         68 |             ii3=ii3+1
         69 |             ii1=ii1+1
         70 |             ii2=ii2+1
         71 |             fz(ii3)=fz(ii1)+work(ifop+ii2-1)
         72 |           end if
         73 |10008    continue
         74 |       RETURN
         75 |       END
 
