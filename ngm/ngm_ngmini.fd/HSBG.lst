          1 |      SUBROUTINE HSBG(N,A,IA)
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .                                       .
          4 |C SUBPROGRAM:    HSBG        CONVERT MATRIX TO UPPER HESSENBERG FORM
          5 |C   PRGMMR: PARRISH          ORG: W/NMC22    DATE: 88-09-08
          6 |C
          7 |C ABSTRACT: CONVERT MATRIX TO UPPER HESSENBERG FORM.
          8 |C
          9 |C PROGRAM HISTORY LOG:
         10 |C   88-09-08  PARRISH
         11 |C
         12 |C USAGE:    CALL HSBG(N,A,IA)
         13 |C   INPUT ARGUMENT LIST:
         14 |C     N        - ORDER OF MATRIX A
         15 |C     A        - INPUT MATRIX
         16 |C     IA       - FIRST DIMENSION OF A IN CALLING PROGRAM
         17 |C
         18 |C   OUTPUT ARGUMENT LIST:
         19 |C     A        - OUTPUT UPPER HESSENBERG FORM OF MATRIX
         20 |C
         21 |C ATTRIBUTES:
         22 |C   LANGUAGE: FORTRAN200
         23 |C   MACHINE:  CYBER
         24 |C
         25 |C$$$
         26 |      DIMENSION A(1)
         27 |      DOUBLE PRECISION S
         28 |      L=N
         29 |      NIA=L*IA
         30 |      LIA=NIA-IA
         31 |   20 IF(L-3) 360,40,40
         32 |   40 LIA=LIA-IA
         33 |      L1=L-1
         34 |      L2=L1-1
         35 |      ISUB=LIA+L
         36 |      IPIV=ISUB-IA
         37 |      PIV=ABS(A(IPIV))
         38 |      IF(L-3) 90,90,50
         39 |   50 M=IPIV-IA
         40 |      DO 80 I=L,M,IA
         41 |      T=ABS(A(I))
         42 |      IF(T-PIV) 80,80,60
         43 |   60 IPIV=I
         44 |      PIV=T
         45 |   80 CONTINUE
         46 |   90 IF(PIV) 100,320,100
         47 |  100 IF(PIV-ABS(A(ISUB))) 180,180,120
         48 |  120 M=IPIV-L
         49 |      DO 140 I=1,L
         50 |      J=M+I
         51 |      T=A(J)
         52 |      K=LIA+I
         53 |      A(J)=A(K)
         54 |  140 A(K)=T
         55 |      M=L2-M/IA
         56 |      DO 160 I=L1,NIA,IA
         57 |      T=A(I)
         58 |      J=I-M
         59 |      A(I)=A(J)
         60 |  160 A(J)=T
         61 |  180 DO 200 I=L,LIA,IA
         62 |  200 A(I)=A(I)/A(ISUB)
         63 |      J=-IA
         64 |      DO 240 I=1,L2
         65 |      J=J+IA
         66 |      LJ=L+J
         67 |      DO 220 K=1,L1
         68 |      KJ=K+J
         69 |      KL=K+LIA
         70 |  220 A(KJ)=A(KJ)-A(LJ)*A(KL)
         71 |  240 CONTINUE
         72 |      K=-IA
         73 |      DO 300 I=1,N
         74 |      K=K+IA
         75 |      LK=K+L1
         76 |      S=A(LK)
         77 |      LJ=L-IA
         78 |      DO 280 J=1,L2
         79 |      JK=K+J
         80 |      LJ=LJ+IA
         81 |  280 S=S+A(LJ)*A(JK)*1.0D0
         82 |  300 A(LK)=S
         83 |      DO 310 I=L,LIA,IA
         84 |  310 A(I)=0.0
         85 |  320 L=L1
         86 |      GO TO 20
         87 |  360 RETURN
         88 |      END
 
