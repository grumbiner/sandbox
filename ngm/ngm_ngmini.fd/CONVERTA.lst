          1 |       SUBROUTINE CONVERTA(U,V,TH,H,IJ,IMG,JMG,PR,GASCON,TBAR,BITH,
          2 |     *    WORKS,PIS,PSINVS,HU,HV)
          3 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          4 |C                .      .    .                                       .
          5 |C SUBPROGRAM:    CONVERTA    CONVERT VARIABLES TO HOSKINS-SIMMONS FORM
          6 |C   PRGMMR: PARRISH          ORG: W/NMC22    DATE: 88-08-08
          7 |C
          8 |C ABSTRACT: CONVERT VARIABLES FROM PHILLIPS FORM TO HOSKINS-SIMMONS
          9 |C   FORM.  (INPUT SFCP IS IN BARS).  CONVERT POTTEMP USING ARAKAWA SIG.
         10 |C   (PR = .5*SIG**KAPPA).
         11 |C
         12 |C PROGRAM HISTORY LOG:
         13 |C   88-08-08  PARRISH
         14 |C
         15 |C USAGE:    CALL CONVERTA(U,V,TH,H,IJ,IMG,JMG,PR,GASCON,TBAR,BITH,
         16 |C                   WORKS,PIS,PSINVS,HU,HV)
         17 |C   INPUT ARGUMENT LIST:
         18 |C     U        - H*U, PHILLIPS FORM OF U-WIND
         19 |C     V        - H*V, PHILLIPS FORM OF V-WIND
         20 |C     TH       - H*THETA, PHILLIPS FORM OF TEMP
         21 |C     H        - PHILLIPS SURFACE PRESSURE (IN BARS)
         22 |C     IJ       - NUMBER OF GRID POINTS ON GRID (IMG*JMG)
         23 |C     IMG      - X-DIMENSION OF GRID
         24 |C     JMG      - Y-DIMENSION OF GRID
         25 |C     PR       - RELATED TO EXNER FUNCTION (PR=.5*SIG**KAPPA)
         26 |C     GASCON   - GAS CONSTANT
         27 |C     BITH     - BIT ARRAY TELLING WHICH GRID POINTS OF H ARE FORECAST
         28 |C     NONLOOPB - TOTAL NUMBER OF ITERATIONS
         29 |C
         30 |C   OUTPUT ARGUMENT LIST:      (INCLUDING WORK ARRAYS)
         31 |C     U        - U, HOSKINS-SIMMONS FORM
         32 |C     V        - V, HOSKINS-SIMMONS FORM
         33 |C     TH       - T, HOSKINS-SIMMONS FORM
         34 |C     H        - HOSKINS-SIMMONS FORM
         35 |C     WORKS    - WORK ARRAY
         36 |C     PIS      - WORK ARRAY
         37 |C     PSINVS   - WORK ARRAY
         38 |C     HU       - WORK ARRAY
         39 |C     HV       - WORK ARRAY
         40 |C
         41 |C   SUBPROGRAMS CALLED:  P2KAP
         42 |C
         43 |C ATTRIBUTES:
         44 |C   LANGUAGE: FORTRAN200
         45 |C   MACHINE:  CYBER
         46 |C
         47 |C$$$
         48 |         include "myparam"
  1       1 |        INCLUDE 'parmodel'
  2       1 |C     PARAMETER FILE
  2       2 |C
  2       3 |C     SETUP FOR IBM SINGLE PRECISION REPRESENTATION
  2       4 |C     ON OUTPUT
  2       5 |C                      THIS VERSION 21 February 1991 (DiMego)
  2       6 |C
  2       7 |      PARAMETER (
  2       8 |     *      IBPW      =      4,
  2       9 |     *      IBPOR     = 120000,
  2      10 |     *      INUMREC   = IBPOR / IBPW,
  2      11 |     *      INSPECS   =    700,
  2      12 |     *      ILATG2    =    120,
  2      13 |     *      ILEVH     =     12,
  2      14 |     *      ILEVS     =     16,
  2      15 |     *      ILONF     =    360,
  2      16 |     *      INLATSH   =      5,
  2      17 |     *      IIJMAX    =  23668,
  2      18 |     *      IIJMAXB   =  25536,
  2      19 |     *      IIJRAD    =    892,
  2      20 |     *      IJCAP     =     80,
  2      21 |     *      IWPCPL    =     80,
  2      22 |     *      IKM       =     16 )
  2      23 |      PARAMETER (
  2      24 |     *      ILBCH     =   7*(147+111-7),
  2      25 |     *      ILBHI     =    (147+111-26),
  2      26 |     *      ILBUI     =    (2*147+111-39),
  2      27 |     *      ILBUO     =   (8*147+7*111-56),
  2      28 |     *      ILBVI     =    (147+2*111-39),
  2      29 |     *      ILBVO     =   (7*147+8*111-56),
  2      30 |     *      ILMOUT    =     20 )
  2      31 |      PARAMETER (
  2      32 |     *      IMAXOUTF  =     17,
  2      33 |     *      IMAXOUTT  =      3,
  2      34 |     *      IMAXOUTV  =    100,
  2      35 |     *      IMAXPROF  =      6,
  2      36 |     *      INFILLER  =  10000,
  2      37 |     *      INH       =     65,
  2      38 |     *      INGRDUSE  =      2,
  2      39 |     *      INGRDUS1  =      1,
  2      40 |     *      INIADDRS  =     44,
  2      41 |     *      INSCR     =     23,
  2      42 |     *      INSCRGEO  =     12,
  2      43 |     *      INSCR3    =     10,
  2      44 |     *      INSOIL    =      2,
  2      45 |     *      INSUMG    =  42164,
  2      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  2      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  2      48 |     *      INSHEM    = 13 * INH + 23,
  2      49 |     *      IIJKMAX   = IKM * IIJMAX )
  2      50 |      PARAMETER (
  2      51 |     *  IIWORDSB  = IIJMAXB / 64,
  2      52 |     *  IKMP1     = IKM + 1,
  2      53 |     *  ILVLTOT   = 7 * IKM + 37,
  2      54 |     *  ILONFP1   = ILONF + 1,
  2      55 |     *  INVBL     = ILVLTOT * INSUMG,
  2      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  2      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  2      58 |     *  IBLK1D    =  2 * IKM + 14,
  2      59 |     *  IBLK2D    = 16 * IKM + 36,
  2      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  2      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  2      62 |C
  1       2 |C-------
  1       3 |C------- FOLLOWING IS EXTRA STUFF FOR INITIALIZATION
  1       4 |C-------
  1       5 |         PARAMETER (INLATH=101,INUMCOEF=3*INLATH+35,
  1       6 |     *        INUMGRID=4,IJCAPDP=80,INLON=300,INKLON=2,INKLAT=2,
  1       7 |     *        IIORBPL=3,IIORDPL=3,IIORBLP=3,IIORDLP=3,IMCUBIC=1,
  1       8 |     *        INUMVERT=2,INSIG=IKM,INMODES=18,INGMAX=3000,
  1       9 |     *        INKSIG=2,INSIGP1=INSIG+1,INLONEX=INLON+INKLON,
  1      10 |     *        INLATDP=2*INLATH,INLOLA=INLATDP*INLONEX,
  1      11 |     *        IJCAP1DP=IJCAPDP+1,IJCAP12=2*IJCAP1DP,
  1      12 |     *        INCOEFS=IJCAP1DP*(IJCAPDP+2),
  1      13 |     *     IDPFILLR=(4*IKM+1)*INSUMG+       3*(IIJKMAX*4+IIJMAX) )
  1      14 |C    *     IDPFILLR=(4*IKM+1)*INSUMG+INGRDUSE*(IIJKMAX*4+IIJMAX) )
         49 |C--------
         50 |         REAL TBAR(1)
         51 |         REAL PR(1)
         52 |         REAL WORKS(IIJMAX),PIS(IIJMAX),PSINVS(IIJMAX)
         53 |         REAL U(IJ,1),V(IJ,1),TH(IJ,1),H(1)
         54 |         REAL HU(IIJMAX),HV(IIJMAX)
         55 |         LOGICAL BITH(1)
         56 |C--------
         57 |c        WRITE(6,110)
         58 |c110     FORMAT(' ENTERING SUBROUTINE CONVERTA')
         59 |C--------
         60 |C-------- OBTAIN HU AND HV (SFCP INTERPOLATED TO U AND V GRIDS)
         61 |C--------
         62 |         NVECT=IMG*JMG
         63 |         DO 11402 I=1,NVECT
         64 |           HU(I)=H(I)
         65 |11402    CONTINUE
         66 |         DO 114 J=1,JMG-1
         67 |           DO 112 I=1,IMG
         68 |             HU(I+(J-1)*IMG)=.5*(H(I+(J-1)*IMG)+H(I+J*IMG))
         69 |112        CONTINUE
         70 |114      CONTINUE
         71 |         DO 11404 I=1,NVECT
         72 |           HV(I)=H(I)
         73 |11404    CONTINUE
         74 |         DO 118 J=1,JMG
         75 |           DO 116 I=1,IMG-1
         76 |             HV(I+(J-1)*IMG)=.5*(H(I+(J-1)*IMG)+H(I+1+(J-1)*IMG))
         77 |116        CONTINUE
         78 |118      CONTINUE
         79 |         DO 1182 I=1,NVECT
         80 |           HU(I)=1./HU(I)
         81 |           HV(I)=1./HV(I)
         82 |           PSINVS(I)=1./H(I)
         83 |1182     CONTINUE
         84 |         CALL P2KAP(H,PIS,NVECT)
         85 |         TNORM=0.
         86 |         DO 1184 I=1,NVECT
         87 |           IF(BITH(I)) TNORM=TNORM+1.
         88 |           H(I)=LOG(H(I))
         89 |1184     CONTINUE
         90 |         TNORM=1./TNORM
         91 |         DO 230 KR=1,IKM
         92 |           K=IKM+1-KR
         93 |           CONST=PR(KR)*GASCON*2.
         94 |           TBAR(K)=0.
         95 |           DO 2301 I=1,NVECT
         96 |             TH(I,KR)=CONST*TH(I,KR)*PIS(I)*PSINVS(I)
         97 |             IF(BITH(I)) TBAR(K)=TBAR(K)+TNORM*TH(I,KR)
         98 |             U(I,KR)=U(I,KR)*HU(I)
         99 |             V(I,KR)=V(I,KR)*HV(I)
        100 |2301       CONTINUE
        101 |230      CONTINUE
        102 |       RETURN
        103 |       END
 
