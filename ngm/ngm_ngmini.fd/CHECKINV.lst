          1 |       SUBROUTINE CHECKINV(E,EINV,N)
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .                                       .
          4 |C SUBPROGRAM:    CHECKINV    CHECK INVERSE OF A MATRIX
          5 |C   PRGMMR: PARRISH          ORG: W/NMC22    DATE: 88-09-08
          6 |C
          7 |C ABSTRACT: CHECK ACCURACY OF MATRIX INVERSE.
          8 |C
          9 |C PROGRAM HISTORY LOG:
         10 |C   88-09-08  PARRISH
         11 |C
         12 |C USAGE:    CALL CHECKINV(E,EINV,N)
         13 |C   INPUT ARGUMENT LIST:
         14 |C     E        - MATRIX
         15 |C     EINV     - INVERSE OF MATRIX TO BE TESTED
         16 |C     N        - DIMENSION OF MATRICES
         17 |C
         18 |C ATTRIBUTES:
         19 |C   LANGUAGE: FORTRAN200
         20 |C   MACHINE:  CYBER
         21 |C
         22 |C$$$
         23 |         REAL E(N,N),EINV(N,N)
         24 |         ERMAX=0.
         25 |         DO 300 J=1,N
         26 |           DO 200 I=1,N
         27 |             PROD=0.
         28 |             IF(I.EQ.J) PROD=-1.
         29 |             DO 100 K=1,N
         30 |               PROD=PROD+E(I,K)*EINV(K,J)
         31 |100          CONTINUE
         32 |             ERMAX=AMAX1(ERMAX,ABS(PROD))
         33 |200        CONTINUE
         34 |300      CONTINUE
         35 |c        WRITE(6,400)N,ERMAX
         36 | 400     FORMAT(' CHECK MATRIX INVERSE, N,ERMAX=',I5,G12.4)
         37 |       RETURN
         38 |       END
 
