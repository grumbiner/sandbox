          1 |       SUBROUTINE AOPMULT(Z,D,H,AZ,AD,AH,JCAPIN,DEPTH)
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .                                       .
          4 |C SUBPROGRAM:    AOPMULT     APPLY "A" OPERATOR
          5 |C   PRGMMR: PARRISH          ORG: W/NMC22    DATE: 88-06-13
          6 |C
          7 |C ABSTRACT: APPLY "A" OPERATOR, CORRESPONDING TO TEMPERTON*S "B"
          8 |C   LINEARIZATION.
          9 |C
         10 |C PROGRAM HISTORY LOG:
         11 |C   88-06-13  PARRISH
         12 |C
         13 |C USAGE:    CALL AOPMULT(Z,D,H,AZ,AD,AH,JCAPIN,DEPTH)
         14 |C   INPUT ARGUMENT LIST:
         15 |C     Z        - VORTICITY COEFFICIENTS.
         16 |C     D        - DIVERGENCE COEFFICIENTS.
         17 |C     H        - HEIGHT COEFFICIENTS.
         18 |C     JCAPIN   - TRIANGULAR TRUNCATION
         19 |C     DEPTH    - SCALE HEIGHT.
         20 |C
         21 |C   OUTPUT ARGUMENT LIST:      (INCLUDING WORK ARRAYS)
         22 |C     AZ       - VORTICITY COEFFICIENTS.
         23 |C     AD       - DIVERGENCE COEFFICIENTS.
         24 |C     AH       - HEIGHT COEFFICIENTS.
         25 |C
         26 |C REMARKS: MUST INCLUDE BLOCK DATA BLOCKSP02 AND DEFINITION OF
         27 |C           PL1 VARIABLES BEFORE COMPILING.
         28 |C
         29 |C ATTRIBUTES:
         30 |C   LANGUAGE: FORTRAN200
         31 |C   MACHINE:  CYBER
         32 |C
         33 |C$$$
         34 |         include "myparam"
  1       1 |        INCLUDE 'parmodel'
  2       1 |C     PARAMETER FILE
  2       2 |C
  2       3 |C     SETUP FOR IBM SINGLE PRECISION REPRESENTATION
  2       4 |C     ON OUTPUT
  2       5 |C                      THIS VERSION 21 February 1991 (DiMego)
  2       6 |C
  2       7 |      PARAMETER (
  2       8 |     *      IBPW      =      4,
  2       9 |     *      IBPOR     = 120000,
  2      10 |     *      INUMREC   = IBPOR / IBPW,
  2      11 |     *      INSPECS   =    700,
  2      12 |     *      ILATG2    =    120,
  2      13 |     *      ILEVH     =     12,
  2      14 |     *      ILEVS     =     16,
  2      15 |     *      ILONF     =    360,
  2      16 |     *      INLATSH   =      5,
  2      17 |     *      IIJMAX    =  23668,
  2      18 |     *      IIJMAXB   =  25536,
  2      19 |     *      IIJRAD    =    892,
  2      20 |     *      IJCAP     =     80,
  2      21 |     *      IWPCPL    =     80,
  2      22 |     *      IKM       =     16 )
  2      23 |      PARAMETER (
  2      24 |     *      ILBCH     =   7*(147+111-7),
  2      25 |     *      ILBHI     =    (147+111-26),
  2      26 |     *      ILBUI     =    (2*147+111-39),
  2      27 |     *      ILBUO     =   (8*147+7*111-56),
  2      28 |     *      ILBVI     =    (147+2*111-39),
  2      29 |     *      ILBVO     =   (7*147+8*111-56),
  2      30 |     *      ILMOUT    =     20 )
  2      31 |      PARAMETER (
  2      32 |     *      IMAXOUTF  =     17,
  2      33 |     *      IMAXOUTT  =      3,
  2      34 |     *      IMAXOUTV  =    100,
  2      35 |     *      IMAXPROF  =      6,
  2      36 |     *      INFILLER  =  10000,
  2      37 |     *      INH       =     65,
  2      38 |     *      INGRDUSE  =      2,
  2      39 |     *      INGRDUS1  =      1,
  2      40 |     *      INIADDRS  =     44,
  2      41 |     *      INSCR     =     23,
  2      42 |     *      INSCRGEO  =     12,
  2      43 |     *      INSCR3    =     10,
  2      44 |     *      INSOIL    =      2,
  2      45 |     *      INSUMG    =  42164,
  2      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  2      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  2      48 |     *      INSHEM    = 13 * INH + 23,
  2      49 |     *      IIJKMAX   = IKM * IIJMAX )
  2      50 |      PARAMETER (
  2      51 |     *  IIWORDSB  = IIJMAXB / 64,
  2      52 |     *  IKMP1     = IKM + 1,
  2      53 |     *  ILVLTOT   = 7 * IKM + 37,
  2      54 |     *  ILONFP1   = ILONF + 1,
  2      55 |     *  INVBL     = ILVLTOT * INSUMG,
  2      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  2      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  2      58 |     *  IBLK1D    =  2 * IKM + 14,
  2      59 |     *  IBLK2D    = 16 * IKM + 36,
  2      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  2      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  2      62 |C
  1       2 |C-------
  1       3 |C------- FOLLOWING IS EXTRA STUFF FOR INITIALIZATION
  1       4 |C-------
  1       5 |         PARAMETER (INLATH=101,INUMCOEF=3*INLATH+35,
  1       6 |     *        INUMGRID=4,IJCAPDP=80,INLON=300,INKLON=2,INKLAT=2,
  1       7 |     *        IIORBPL=3,IIORDPL=3,IIORBLP=3,IIORDLP=3,IMCUBIC=1,
  1       8 |     *        INUMVERT=2,INSIG=IKM,INMODES=18,INGMAX=3000,
  1       9 |     *        INKSIG=2,INSIGP1=INSIG+1,INLONEX=INLON+INKLON,
  1      10 |     *        INLATDP=2*INLATH,INLOLA=INLATDP*INLONEX,
  1      11 |     *        IJCAP1DP=IJCAPDP+1,IJCAP12=2*IJCAP1DP,
  1      12 |     *        INCOEFS=IJCAP1DP*(IJCAPDP+2),
  1      13 |     *     IDPFILLR=(4*IKM+1)*INSUMG+       3*(IIJKMAX*4+IIJMAX) )
  1      14 |C    *     IDPFILLR=(4*IKM+1)*INSUMG+INGRDUSE*(IIJKMAX*4+IIJMAX) )
         35 |C--------
         36 |         real Z(1),D(1),H(1),AZ(1),AD(1),AH(1)
         37 |         INTEGER WORK
         38 |C--------
         39 |         COMMON/DAVEBUFF/ COF(INCOEFS,INUMCOEF),GRD(INLOLA,INUMGRID)
         40 |         real COF,GRD
         41 |C--------
         42 |         COMMON/FIXSP02/JCAP,NCOEFS,NFOP,JFOP,
         43 |     *      ISCALZD,IUNSCLZD,
         44 |     *      IBSHAT,IBVHAT,IBVIHAT,
         45 |     *      IBOP,ICOP,IFOP,ICINVOP,
         46 |     *      IRFOP,IRCINVOP,
         47 |     *      BFOP(INCOEFS),BONES(INCOEFS)
         48 |         LOGICAL BFOP,BONES
         49 |         LOGICAL BWORK(INCOEFS)
         50 |C--------
         51 |C-------- CHECK TO SEE IF INITIALIZATION OF INTERNAL VARIABLES NEEDED
         52 |         IF(JCAP.NE.JCAPIN) CALL SETSP02(JCAPIN)
         53 |C--------
         54 |C-------- VORTICITY FIRST
         55 |C--------
         56 |         CALL GETCOEF(WORK,1)
         57 |         CALL FOPMULT(D,AZ,COF(1,IFOP),NCOEFS,NFOP,JFOP,COF(1,WORK),
         58 |     *           BFOP,BONES,BWORK)
         59 |C--------
         60 |C-------- DIVERGENCE NEXT
         61 |C--------
         62 |         CALL FOPMULT(Z,AD,COF(1,IFOP),NCOEFS,NFOP,JFOP,COF(1,WORK),
         63 |     *           BFOP,BONES,BWORK)
         64 |         C0=SQRT(9.8062*DEPTH)/(6.3712E6*7.2921E-5)
         65 |         do 10002 i=1,ncoefs
         66 |         COF(i,WORK)=COF(i,IBOP)*D(i)
         67 |         AD(i)=AD(i)+COF(i,WORK)
         68 |         COF(i,WORK)=COF(i,ICOP)*H(i)
         69 |         AD(i)=AD(i)+C0*COF(i,WORK)
         70 |10002    continue
         71 |C--------
         72 |C-------- FINALLY HEIGHT
         73 |C--------
         74 |         do 10004 i=1,ncoefs
         75 |         COF(i,WORK)=COF(i,ICOP)*D(i)
         76 |         AH(i)=C0*COF(i,WORK)
         77 |10004    continue
         78 |         CALL FREECOEF(WORK,1)
         79 |       RETURN
         80 |       END
 
