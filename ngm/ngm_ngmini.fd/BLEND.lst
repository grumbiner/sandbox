          1 |      SUBROUTINE BLEND(A,IM,JM,LTBN)
          2 |C...TRANSLATED BY FPP 3.00Z36 11/09/90  14:45:34
          3 |C...SWITCHES: OPTON=I47,OPTOFF=VAE0
          4 |      DIMENSION A(1)
          5 |C
          6 |      DATA FI/0.5E0/,FO/0.5E0/,NGO/1/,NGI/1/
          7 |C
          8 |      LTBNP1 = LTBN + 1
          9 |      LTBNP2 = LTBN + 2
         10 |C
         11 |C          DEFINE FACTOR
         12 |C SOUTH ROW
         13 |      LENX= IM - 2 * LTBNP1
         14 |      N = LTBN * IM + LTBNP2
         15 |      NN = N + IM
         16 |      NS = N - IM
         17 |CMIC$ DO ALL VECTOR IF ((ABS(NN-N).GE.LENX .OR. NN-N.EQ.0) .AND. (ABS(NS
         18 |CMIC$1   -N).GE.LENX .OR. NS-N.EQ.0)) SHARED(LENX, NN, FI, NS, FO, N, A)
         19 |CMIC$2    PRIVATE(IQ2W6E)
         20 |      DO 88890 IQ2W6E=1,LENX
         21 |         A(N+IQ2W6E-1)=A(NN+IQ2W6E-1)*FI+A(NS+IQ2W6E-1)*FO
         22 |88890 CONTINUE
         23 |C NORTH ROW (LAST ROW IS USED, BLEND NEXT TO LAST ROW)
         24 |      N = (JM-LTBNP1) * IM + LTBNP2
         25 |      NN = N + IM
         26 |      NS = N - IM
         27 |CMIC$ DO ALL VECTOR IF ((ABS(NN-N).GE.LENX .OR. NN-N.EQ.0) .AND. (ABS(NS
         28 |CMIC$1   -N).GE.LENX .OR. NS-N.EQ.0)) SHARED(LENX, NN, FO, NS, FI, N, A)
         29 |CMIC$2    PRIVATE(IQ2W6E)
         30 |      DO 88900 IQ2W6E=1,LENX
         31 |         A(N+IQ2W6E-1)=A(NN+IQ2W6E-1)*FO+A(NS+IQ2W6E-1)*FI
         32 |88900 CONTINUE
         33 |C
         34 |      IWEST = LTBNP1
         35 |      IEAST = IM - LTBN
         36 |C
         37 |      DO 100 J = LTBNP2,JM-LTBNP1
         38 |      M = (J-1) * IM
         39 |C
         40 |C WEST COLUMN (1ST COLUMN IS UN-USED, KEEP 2ND COLUMN, BLEND 3RD COLUMN)
         41 |      N = M + IWEST
         42 |      NE = N + NGI
         43 |      NW = N - NGO
         44 |      A(N) = A(NE)*FI + A(NW)*FO
         45 |C
         46 |C EAST COLUMN
         47 |      N = M + IEAST
         48 |      NE = N + NGO
         49 |      NW = N - NGI
         50 |      A(N) = A(NE)*FO + A(NW)*FI
         51 |C
         52 | 100  CONTINUE
         53 |C
         54 |C FOUR CORNERS
         55 |C (SOUTH-WEST)
         56 |      N = LTBN*IM + LTBNP1
         57 |      A(N) = 0.25E0 * (A(N+1) + A(N-1) + A(N+IM) + A(N-IM))
         58 |C (SOUTH-EAST)
         59 |      N = LTBNP1*IM - LTBN
         60 |      A(N) = 0.25E0 * (A(N+1) + A(N-1) + A(N+IM) + A(N-IM))
         61 |C (NORTH-WEST)
         62 |      N = (JM-LTBNP1)*IM + LTBNP1
         63 |      A(N) = 0.25E0 * (A(N+1) + A(N-1) + A(N+IM) + A(N-IM))
         64 |C (NORTH-EAST)
         65 |      N = (JM-LTBN)*IM - LTBN
         66 |      A(N) = 0.25E0 * (A(N+1) + A(N-1) + A(N+IM) + A(N-IM))
         67 |C
         68 |      RETURN
         69 |      END
 
