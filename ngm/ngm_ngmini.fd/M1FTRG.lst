          1 |      SUBROUTINE M1FTRG(TRIGS,N,MODE)
          2 |      implicit double precision (a-h,o-z)
          3 |      DIMENSION TRIGS(1)
          4 |      PI=2.0d0*ASIN(1.0d0)
          5 |      IMODE=IABS(MODE)
          6 |      NN=N
          7 |      IF (IMODE.GT.1.AND.IMODE.LT.6) NN=N/2
          8 |      DEL=(PI+PI)/FLOAT(NN)
          9 |      L=NN+NN
         10 |      DO 10 I=1,L,2
         11 |      ANGLE=0.5 d0*FLOAT(I-1)*DEL
         12 |      TRIGS(I)=COS(ANGLE)
         13 |      TRIGS(I+1)=SIN(ANGLE)
         14 |   10 CONTINUE
         15 |      IF (IMODE.EQ.1) RETURN
         16 |      IF (IMODE.EQ.8) RETURN
         17 |      DEL=0.5 d0*DEL
         18 |      NH=(NN+1)/2
         19 |      L=NH+NH
         20 |      LA=NN+NN
         21 |      DO 20 I=1,L,2
         22 |      ANGLE=0.5 d0*FLOAT(I-1)*DEL
         23 |      TRIGS(LA+I)=COS(ANGLE)
         24 |      TRIGS(LA+I+1)=SIN(ANGLE)
         25 |   20 CONTINUE
         26 |      IF (IMODE.LE.3) RETURN
         27 |      DEL=0.5 d0*DEL
         28 |      LA=LA+NN
         29 |      IF (MODE.EQ.5) GO TO 40
         30 |      DO 30 I=2,NN
         31 |      ANGLE=FLOAT(I-1)*DEL
         32 |      TRIGS(LA+I)=2.0 d0*SIN(ANGLE)
         33 |   30 CONTINUE
         34 |      RETURN
         35 |   40 CONTINUE
         36 |      DEL=0.5 d0*DEL
         37 |      DO 50 I=2,N
         38 |      ANGLE=FLOAT(I-1)*DEL
         39 |      TRIGS(LA+I)=SIN(ANGLE)
         40 |   50 CONTINUE
         41 |      RETURN
         42 |      END
 
