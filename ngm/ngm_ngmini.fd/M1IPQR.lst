          1 |       SUBROUTINE M1IPQR(PE,QE,RO,SLAT,CLAT,NLATH,JCAP)
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .                                       .
          4 |C SUBPROGRAM:    M1IPQR     INITIALIZE LEGENDRE RECURSIONS
          5 |C   PRGMMR: PARRISH          ORG: W/NMC22    DATE: 90-09-21
          6 |C
          7 |C ABSTRACT: INITIALIZE LEGENDRE RECURSIONS.
          8 |C
          9 |C PROGRAM HISTORY LOG:
         10 |C   90-09-21  PARRISH
         11 |C
         12 |C USAGE:    CALL M1IPQR(PE,QE,RO,SLAT,CLAT,NLATH,JCAP)
         13 |C   INPUT ARGUMENT LIST:
         14 |C     SLAT     - SIN OF LATITUDES WHERE FUNCTIONS ARE EVALUATED
         15 |C     CLAT     - COS OF LATITUDES WHERE FUNCTIONS ARE EVALUATED
         16 |C     NLATH    - NUMBER OF LATITUDES WHERE FUNCTIONS ARE EVALUATED
         17 |C     JCAP     - TRIANGULAR TRUNCATION
         18 |C
         19 |C   OUTPUT ARGUMENT LIST:
         20 |C     PE       - P(L,L) FOR 0 LE L LE JCAP
         21 |C     QE       - Q(L,L)
         22 |C     RO       - R(L,L)
         23 |C
         24 |C ATTRIBUTES:
         25 |C   LANGUAGE: CFT77
         26 |C   MACHINE:  CRAY YMP
         27 |C
         28 |C$$$
         29 |         implicit double precision (a-h,o-z)
         30 |         DIMENSION PE(NLATH,0:JCAP),QE(NLATH,0:JCAP),RO(NLATH,0:JCAP)
         31 |         DIMENSION SLAT(NLATH),CLAT(NLATH)
         32 |         real*4 conmc
         33 |C--------
         34 |         RERTH=CONMC('RERTH$')
         35 |         DO 60 J=1,NLATH
         36 |           PE(J,0)=1.d0/SQRT(2.d0)
         37 |           QE(J,0)=0.d0
         38 |           QE(J,1)=RERTH*SQRT(3.d0/16.d0)
         39 |           DO 30 L=1,JCAP
         40 |             PE(J,L)=SQRT((2.d0*L+1.d0)/(2.d0*L))*CLAT(J)*PE(J,L-1)
         41 |30         CONTINUE
         42 |           DO 40 L=2,JCAP
         43 |             QE(J,L)=SQRT((2.d0*L+1.d0)/(2.d0*L))*(L/(L+1.d0))*CLAT(J)
         44 |     *                        *QE(J,L-1)
         45 | 40        CONTINUE
         46 |           DO 50 L=0,JCAP
         47 |             RO(J,L)=-QE(J,L)*SLAT(J)
         48 | 50        CONTINUE
         49 |60       CONTINUE
         50 |       RETURN
         51 |       END
 
