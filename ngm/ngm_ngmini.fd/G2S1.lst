          1 |       SUBROUTINE G2S1(ZS,DS,TS,PS,RS,QS,U,V,T,P,R,Q,
          2 |     *             NSIG,JCAP,NLON,NLATH,WGTS,
          3 |     *       IRS,IIS,AP,BP,AQR,BQR,GR,SLAT,CLAT,PE0,QE0,RO0,TRIGS,IFAX)
          4 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          5 |C                .      .    .                                       .
          6 |C SUBPROGRAM:    G2S1       GRID TO SPECTRAL TRANSFORM
          7 |C   PRGMMR: PARRISH          ORG: W/NMC22    DATE: 90-09-21
          8 |C
          9 |C ABSTRACT: CONVERT ALL SPECTRAL MODEL VARIABLES FROM GRID
         10 |C   TO COEFFICIENT FORM.
         11 |C
         12 |C PROGRAM HISTORY LOG:
         13 |C   90-09-21  PARRISH
         14 |C
         15 |C USAGE:    CALL G2S1(ZS,DS,TS,PS,RS,QS,U,V,T,P,R,Q,
         16 |C    *             NSIG,JCAP,NLON,NLATH,WGTS,
         17 |C    *       IRS,IIS,AP,BP,AQR,BQR,GR,SLAT,CLAT,PE0,QE0,RO0,TRIGS,IFAX)
         18 |C   INPUT ARGUMENT LIST:
         19 |C     U        - LONGITUDE COMPONENT OF WINDS
         20 |C     V        - LATITUDE COMPONENT OF WINDS
         21 |C     T        - TEMPS
         22 |C     P        - LOG(PSFC)
         23 |C     R        - TERRAIN
         24 |C     Q        - MOISTURE VARIABLES
         25 |C     NSIG     - NUMBER OF SIGMA LEVELS
         26 |C     JCAP     - TRIANGULAR TRUNCATION
         27 |C     NLON     - NUMBER OF LONGITUDES
         28 |C     NLATH    - NUMBER OF GAUSSIAN LATS IN ONE HEMISPHERE
         29 |C     WGTS     - WGTS(NLATH): INTEGRATION WEIGHTS
         30 |C     IRS      - IRS(0:JCAP,0:JCAP): ADDR OF RE(ZS(N,L)), ETC.
         31 |C     IIS      - IIS(0:JCAP,0:JCAP): SAME FOR IM PART
         32 |C     AP       - AP(0:JCAP,0:JCAP):  RECURSION
         33 |C     BP       - BP(0:JCAP,0:JCAP):    CONSTANTS
         34 |C     AQR      - AQR(0:JCAP,0:JCAP):     FOR GENERATING
         35 |C     BQR      - BQR(0:JCAP,0:JCAP):       VECTOR SPHERICAL
         36 |C     GR       - GR(0:JCAP,0:JCAP):          HARMONICS
         37 |C     SLAT     - SLAT(NLATH):  SIN(GAUSSIAN LATITUDES)
         38 |C     CLAT     - CLAT(NLATH):  COS(GAUSSIAN LATS)
         39 |C     PE0      - PE0(NLATH,0:JCAP): P(L,L)
         40 |C     QE0      - QE0(NLATH,0:JCAP): Q(L,L)
         41 |C     RO0      - RO0(NLATH,0:JCAP): R(L,L)
         42 |C     TRIGS    - TRIGS(NLON*2)
         43 |C     IFAX     - IFAX(10)
         44 |C
         45 |C   OUTPUT ARGUMENT LIST:
         46 |C     ZS       - ZS((JCAP+1)*(JCAP+2)+1,NSIG): VORTICITY COEFFICIENTS
         47 |C     DS       - DS((JCAP+1)*(JCAP+2)+1,NSIG): DIVERGENCE COEFFICIENTS
         48 |C     TS       - TS((JCAP+1)*(JCAP+2)+1,NSIG): VIRTUAL TEMP COEFS (K)
         49 |C     PS       - PS((JCAP+1)*(JCAP+2)+1): LOG(PSF) COEFS  (P IN CB)
         50 |C     RS       - RS((JCAP+1)*(JCAP+2)+1): TERRAIN COEFS (M)
         51 |C     QS       - QS((JCAP+1)*(JCAP+2)+1,NSIG): SPECIFIC HUMITITY COEFS
         52 |C
         53 |C ATTRIBUTES:
         54 |C   LANGUAGE: CFT77
         55 |C   MACHINE:  CRAY YMP
         56 |C
         57 |C$$$
         58 |         DIMENSION ZS((JCAP+1)*(JCAP+2)+1,NSIG)
         59 |         DIMENSION DS((JCAP+1)*(JCAP+2)+1,NSIG)
         60 |         DIMENSION TS((JCAP+1)*(JCAP+2)+1,NSIG)
         61 |         DIMENSION PS((JCAP+1)*(JCAP+2)+1)
         62 |         DIMENSION RS((JCAP+1)*(JCAP+2)+1)
         63 |         DIMENSION QS((JCAP+1)*(JCAP+2)+1,NSIG)
         64 |         DIMENSION U(2*NLATH,NLON+1,NSIG),V(2*NLATH,NLON+1,NSIG)
         65 |         DIMENSION T(2*NLATH,NLON+1,NSIG),Q(2*NLATH,NLON+1,NSIG)
         66 |         DIMENSION P(2*NLATH,NLON+1),R(2*NLATH,NLON+1)
         67 |         double precision WGTS(NLATH)
         68 |         DIMENSION IRS(0:JCAP,0:JCAP),IIS(0:JCAP,0:JCAP)
         69 |         double precision AP(0:JCAP,0:JCAP),BP(0:JCAP,0:JCAP)
         70 |         double precision AQR(0:JCAP,0:JCAP),BQR(0:JCAP,0:JCAP)
         71 |         double precision GR(0:JCAP,0:JCAP)
         72 |         double precision  SLAT(NLATH),CLAT(NLATH)
         73 |         double precision PE0(NLATH,0:JCAP),QE0(NLATH,0:JCAP)
         74 |         double precision RO0(NLATH,0:JCAP)
         75 |         DIMENSION TRIGS(NLON*2),IFAX(10)
         76 |C--------
         77 |C-------- INTERNAL SCRATCH DYNAMIC SPACE FOLLOWS:
         78 |C--------
         79 |         double precision PE(NLATH,0:JCAP),QE(NLATH,0:JCAP)
         80 |         double precision RO(NLATH,0:JCAP)
         81 |         double precision PO(NLATH,0:JCAP),QO(NLATH,0:JCAP)
         82 |         double precision RE(NLATH,0:JCAP)
         83 |         DIMENSION WORK(2*NLATH*NLON)
         84 |C--------
         85 |         RERTH=CONMC('RERTH$')
         86 |C--------
         87 |C-------- NEXT DO FOURIER ANALYSIS IN LONGITUDE
         88 |C--------
         89 |         LOT=NLATH*2
         90 |         DO 1400 K=1,NSIG
         91 |           CALL M1FFTM(U(1,1,K),WORK,TRIGS,IFAX,LOT,1,NLON,LOT,-1)
         92 |           CALL M1FFTM(V(1,1,K),WORK,TRIGS,IFAX,LOT,1,NLON,LOT,-1)
         93 |           CALL M1FFTM(T(1,1,K),WORK,TRIGS,IFAX,LOT,1,NLON,LOT,-1)
         94 |           CALL M1FFTM(Q(1,1,K),WORK,TRIGS,IFAX,LOT,1,NLON,LOT,-1)
         95 | 1400    CONTINUE
         96 |         CALL M1FFTM(P,WORK,TRIGS,IFAX,LOT,1,NLON,LOT,-1)
         97 |         CALL M1FFTM(R,WORK,TRIGS,IFAX,LOT,1,NLON,LOT,-1)
         98 |         JCAP2=JCAP+2
         99 |         PO=0.d0
        100 |         QO=0.d0
        101 |         RE=0.d0
        102 |         PE=PE0
        103 |         QE=QE0
        104 |         RO=RO0
        105 |C--------
        106 |C-------- NOW SUM IN LATITUDE (AFTER ZEROING OUTPUT ARRAYS)
        107 |C--------
        108 |         ZS=0.
        109 |         DS=0.
        110 |         TS=0.
        111 |         PS=0.
        112 |         RS=0.
        113 |         QS=0.
        114 |C--------
        115 |         DO 1300 J=2,NLATH
        116 |           JR=2*NLATH+1-J
        117 |C----------
        118 |C---------- FIRST SEPERATE INTO EVEN AND ODD PARTS
        119 |C----------
        120 |CDIR$ IVDEP
        121 |             DO 1000 LL=1,NSIG*(NLON+1)
        122 |               TRE=T(J,LL,1)
        123 |               TRO=T(JR,LL,1)
        124 |               T(J,LL,1)=(TRE+TRO)*WGTS(J)
        125 |               T(JR,LL,1)=(TRE-TRO)*WGTS(J)
        126 |               QRE=Q(J,LL,1)
        127 |               QRO=Q(JR,LL,1)
        128 |               Q(J,LL,1)=(QRE+QRO)*WGTS(J)
        129 |               Q(JR,LL,1)=(QRE-QRO)*WGTS(J)
        130 |               URE=U(J,LL,1)
        131 |               URO=U(JR,LL,1)
        132 |               U(J,LL,1)=(URE+URO)*WGTS(J)
        133 |               U(JR,LL,1)=(URE-URO)*WGTS(J)
        134 |               VRE=V(J,LL,1)
        135 |               VRO=V(JR,LL,1)
        136 |               V(J,LL,1)=(VRE+VRO)*WGTS(J)
        137 |               V(JR,LL,1)=(VRE-VRO)*WGTS(J)
        138 | 1000        CONTINUE
        139 |CDIR$ IVDEP
        140 |           DO 1200 LL=1,JCAP*2+2
        141 |             PRE=P(J,LL)
        142 |             PRO=P(JR,LL)
        143 |             P(J,LL)=(PRE+PRO)*WGTS(J)
        144 |             P(JR,LL)=(PRE-PRO)*WGTS(J)
        145 |             RRE=R(J,LL)
        146 |             RRO=R(JR,LL)
        147 |             R(J,LL)=(RRE+RRO)*WGTS(J)
        148 |             R(JR,LL)=(RRE-RRO)*WGTS(J)
        149 |1200       CONTINUE
        150 |           DO 900 M=0,JCAP,2
        151 |C------------
        152 |C------------ FIRST SUM EVEN/ODD TERMS (N=L,L+2,...)
        153 |C------------
        154 |             DO 200 K=1,NSIG
        155 |               IR=IRS(M,0)-2
        156 |               LR=-1
        157 |CDIR$ IVDEP
        158 |               DO 100 L=0,JCAP-M
        159 |                 LR=LR+2
        160 |                 LI=LR+1
        161 |                 IR=IR+2
        162 |                 II=IR+1
        163 |                 TS(IR,K)=TS(IR,K)+PE(J,L)*T(J,LR,K)
        164 |                 TS(II,K)=TS(II,K)+PE(J,L)*T(J,LI,K)
        165 |                 QS(IR,K)=QS(IR,K)+PE(J,L)*Q(J,LR,K)
        166 |                 QS(II,K)=QS(II,K)+PE(J,L)*Q(J,LI,K)
        167 |                 ZS(IR,K)=ZS(IR,K)-V(J,LI,K)*QE(J,L)+U(JR,LR,K)*RO(J,L)
        168 |                 ZS(II,K)=ZS(II,K)+V(J,LR,K)*QE(J,L)+U(JR,LI,K)*RO(J,L)
        169 |                 DS(IR,K)=DS(IR,K)-U(J,LI,K)*QE(J,L)-V(JR,LR,K)*RO(J,L)
        170 |                 DS(II,K)=DS(II,K)+U(J,LR,K)*QE(J,L)-V(JR,LI,K)*RO(J,L)
        171 | 100           CONTINUE
        172 | 200         CONTINUE
        173 |             IR=IRS(M,0)-2
        174 |             LR=-1
        175 |CDIR$ IVDEP
        176 |             DO 300 L=0,JCAP-M
        177 |               LR=LR+2
        178 |               LI=LR+1
        179 |               IR=IR+2
        180 |               II=IR+1
        181 |               PS(IR)=PS(IR)+PE(J,L)*P(J,LR)
        182 |               PS(II)=PS(II)+PE(J,L)*P(J,LI)
        183 |               RS(IR)=RS(IR)+PE(J,L)*R(J,LR)
        184 |               RS(II)=RS(II)+PE(J,L)*R(J,LI)
        185 |300          CONTINUE
        186 |C------------
        187 |C------------ NOW DO ODD/EVEN  (N=L+1,L+3,...)
        188 |C------------
        189 |             IF(M+1.LE.JCAP) THEN
        190 |               I=M-JCAP2
        191 |CDIR$ IVDEP
        192 |               DO 400 L=0,JCAP-M-1
        193 |                 I=I+JCAP2
        194 |                 PO(J,L)=AP(I,0)*SLAT(J)*PE(J,L)+BP(I,0)*PO(J,L)
        195 |                 QO(J,L)=AQR(I,0)*SLAT(J)*QE(J,L)
        196 |     *                     +BQR(I,0)*QO(J,L)
        197 |                 RE(J,L)=AQR(I,0)*SLAT(J)*RO(J,L)
        198 |     *                 +BQR(I,0)*RE(J,L)+GR(I,0)*PE(J,L)*CLAT(J)
        199 |400            CONTINUE
        200 |               DO 600 K=1,NSIG
        201 |                 IR=IRS(M+1,0)-2
        202 |                 LR=-1
        203 |CDIR$ IVDEP
        204 |                 DO 500 L=0,JCAP-M-1
        205 |                   IR=IR+2
        206 |                   II=IR+1
        207 |                   LR=LR+2
        208 |                   LI=LR+1
        209 |                   TS(IR,K)=TS(IR,K)+PO(J,L)*T(JR,LR,K)
        210 |                   TS(II,K)=TS(II,K)+PO(J,L)*T(JR,LI,K)
        211 |                   QS(IR,K)=QS(IR,K)+PO(J,L)*Q(JR,LR,K)
        212 |                   QS(II,K)=QS(II,K)+PO(J,L)*Q(JR,LI,K)
        213 |                  ZS(IR,K)=ZS(IR,K)+U(J,LR,K)*RE(J,L)-V(JR,LI,K)*QO(J,L)
        214 |                  ZS(II,K)=ZS(II,K)+U(J,LI,K)*RE(J,L)+V(JR,LR,K)*QO(J,L)
        215 |                  DS(IR,K)=DS(IR,K)-V(J,LR,K)*RE(J,L)-U(JR,LI,K)*QO(J,L)
        216 |                  DS(II,K)=DS(II,K)-V(J,LI,K)*RE(J,L)+U(JR,LR,K)*QO(J,L)
        217 | 500             CONTINUE
        218 | 600           CONTINUE
        219 |               IR=IRS(M+1,0)-2
        220 |               LR=-1
        221 |CDIR$ IVDEP
        222 |               DO 700 L=0,JCAP-M-1
        223 |                 IR=IR+2
        224 |                 II=IR+1
        225 |                 LR=LR+2
        226 |                 LI=LR+1
        227 |                 PS(IR)=PS(IR)+PO(J,L)*P(JR,LR)
        228 |                 PS(II)=PS(II)+PO(J,L)*P(JR,LI)
        229 |                 RS(IR)=RS(IR)+PO(J,L)*R(JR,LR)
        230 |                 RS(II)=RS(II)+PO(J,L)*R(JR,LI)
        231 |700            CONTINUE
        232 |C--------------
        233 |C-------------- GET NEXT PE,QE,RO
        234 |C--------------
        235 |               I=M-JCAP2+1
        236 |CDIR$ IVDEP
        237 |               DO 800 L=0,MAX(0,JCAP-M-2)
        238 |                 I=I+JCAP2
        239 |                 PE(J,L)=AP(I,0)*SLAT(J)*PO(J,L)+BP(I,0)*PE(J,L)
        240 |                 QE(J,L)=AQR(I,0)*SLAT(J)*QO(J,L)
        241 |     *                     +BQR(I,0)*QE(J,L)
        242 |                 RO(J,L)=AQR(I,0)*SLAT(J)*RE(J,L)
        243 |     *                 +BQR(I,0)*RO(J,L)+GR(I,0)*PO(J,L)*CLAT(J)
        244 |800            CONTINUE
        245 |             END IF
        246 |900        CONTINUE
        247 |1300     CONTINUE
        248 |C--------
        249 |C------- MULTIPLY DS,ZS BY DIMENSIONAL LAPLACIAN
        250 |C--------
        251 |         DO 1292 L=0,JCAP
        252 |           DO 1290 N=L,JCAP
        253 |             DEL2=N*(N+1.)/(RERTH*RERTH)
        254 |CDIR$ IVDEP
        255 |             DO 1280 K=1,NSIG
        256 |               ZS(IRS(N,L),K)=DEL2*ZS(IRS(N,L),K)
        257 |               ZS(IIS(N,L),K)=DEL2*ZS(IIS(N,L),K)
        258 |               DS(IRS(N,L),K)=DEL2*DS(IRS(N,L),K)
        259 |               DS(IIS(N,L),K)=DEL2*DS(IIS(N,L),K)
        260 | 1280        CONTINUE
        261 | 1290      CONTINUE
        262 | 1292    CONTINUE
        263 |C--------
        264 |C-------- MAKE SURE THAT ZONAL IMAGINARY COEFS ARE ZERO, AND
        265 |C-------- (0,0) COMPONENT OF Z,D ALSO
        266 |C--------
        267 |         DO 20 K=1,NSIG
        268 |           DO 10 N=0,JCAP
        269 |             ZS(IIS(N,0),K)=0.
        270 |             DS(IIS(N,0),K)=0.
        271 |             TS(IIS(N,0),K)=0.
        272 |             QS(IIS(N,0),K)=0.
        273 | 10        CONTINUE
        274 |           ZS(IRS(0,0),K)=0.
        275 |           DS(IRS(0,0),K)=0.
        276 | 20      CONTINUE
        277 |         DO 30 N=0,JCAP
        278 |           PS(IIS(N,0))=0.
        279 |           RS(IIS(N,0))=0.
        280 |30       CONTINUE
        281 |       RETURN
        282 |       END
 
