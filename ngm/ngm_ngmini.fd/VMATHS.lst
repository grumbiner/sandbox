          1 |       SUBROUTINE VMATHS(HTBAR,HDSIG,HPR,HCP,HRKAPPA,HGASCON,
          2 |     *                   HSIGHAT,HDEPTHS,HEINVT,HEINV,HEINVG,
          3 |     *                   HPIBINVE,HTBINVE,HE)
          4 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          5 |C                .      .    .                                       .
          6 |C SUBPROGRAM:    VMATHS      COMPUTE VERT MODES AND DEPTHS
          7 |C   PRGMMR: PARRISH          ORG: W/NMC22    DATE: 88-09-08
          8 |C
          9 |C ABSTRACT: COMPUTE VERTICAL MODES AND SCALE DEPTHS.
         10 |C
         11 |C PROGRAM HISTORY LOG:
         12 |C   88-09-08  PARRISH
         13 |C
         14 |C USAGE:    CALL VMATHS(HTBAR,HDSIG,HPR,HCP,HRKAPPA,HGASCON,
         15 |C    *                   HSIGHAT,HDEPTHS,HEINVT,HEINV,HEINVG,
         16 |C    *                   HPIBINVE,HTBINVE,HE)
         17 |C   INPUT ARGUMENT LIST:
         18 |C     HTBAR    - REF TEMP PROFILE
         19 |C     HDSIG    - SIGMA INCREMENTS
         20 |C     HPR      - RELATED TO EXNER FUNCTION
         21 |C     HSIGHAT  - SIGMA VALUES AT CENTERS OF SIGMA LAYERS.
         22 |C     HCP      - SPEC HEAT AT CONST PRESSURE
         23 |C     HRKAPPA  - RATIO GAS CONST TO SPEC HEAT
         24 |C     HGASCON  - GAS CONST
         25 |C
         26 |C   OUTPUT ARGUMENT LIST:
         27 |C     HDEPTHS  - SCALE DEPTHS
         28 |C     HEINVT   - MATRIX OPERATOR
         29 |C     HEINV    - INVERSE OF MATRIX OF EIGENVALUES
         30 |C     HEINVG   - MATRIX OPERATOR
         31 |C     HPIBINVE - MATRIX OPERATOR
         32 |C     HTBINVE  - MATRIX OPERATOR
         33 |C     HE       - MATRIX OF EIGENVECTORS OF B.
         34 |C
         35 |C ATTRIBUTES:
         36 |C   LANGUAGE: FORTRAN200
         37 |C   MACHINE:  CYBER
         38 |C
         39 |C$$$
         40 |         include "myparam"
  1       1 |        INCLUDE 'parmodel'
  2       1 |C     PARAMETER FILE
  2       2 |C
  2       3 |C     SETUP FOR IBM SINGLE PRECISION REPRESENTATION
  2       4 |C     ON OUTPUT
  2       5 |C                      THIS VERSION 21 February 1991 (DiMego)
  2       6 |C
  2       7 |      PARAMETER (
  2       8 |     *      IBPW      =      4,
  2       9 |     *      IBPOR     = 120000,
  2      10 |     *      INUMREC   = IBPOR / IBPW,
  2      11 |     *      INSPECS   =    700,
  2      12 |     *      ILATG2    =    120,
  2      13 |     *      ILEVH     =     12,
  2      14 |     *      ILEVS     =     16,
  2      15 |     *      ILONF     =    360,
  2      16 |     *      INLATSH   =      5,
  2      17 |     *      IIJMAX    =  23668,
  2      18 |     *      IIJMAXB   =  25536,
  2      19 |     *      IIJRAD    =    892,
  2      20 |     *      IJCAP     =     80,
  2      21 |     *      IWPCPL    =     80,
  2      22 |     *      IKM       =     16 )
  2      23 |      PARAMETER (
  2      24 |     *      ILBCH     =   7*(147+111-7),
  2      25 |     *      ILBHI     =    (147+111-26),
  2      26 |     *      ILBUI     =    (2*147+111-39),
  2      27 |     *      ILBUO     =   (8*147+7*111-56),
  2      28 |     *      ILBVI     =    (147+2*111-39),
  2      29 |     *      ILBVO     =   (7*147+8*111-56),
  2      30 |     *      ILMOUT    =     20 )
  2      31 |      PARAMETER (
  2      32 |     *      IMAXOUTF  =     17,
  2      33 |     *      IMAXOUTT  =      3,
  2      34 |     *      IMAXOUTV  =    100,
  2      35 |     *      IMAXPROF  =      6,
  2      36 |     *      INFILLER  =  10000,
  2      37 |     *      INH       =     65,
  2      38 |     *      INGRDUSE  =      2,
  2      39 |     *      INGRDUS1  =      1,
  2      40 |     *      INIADDRS  =     44,
  2      41 |     *      INSCR     =     23,
  2      42 |     *      INSCRGEO  =     12,
  2      43 |     *      INSCR3    =     10,
  2      44 |     *      INSOIL    =      2,
  2      45 |     *      INSUMG    =  42164,
  2      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  2      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  2      48 |     *      INSHEM    = 13 * INH + 23,
  2      49 |     *      IIJKMAX   = IKM * IIJMAX )
  2      50 |      PARAMETER (
  2      51 |     *  IIWORDSB  = IIJMAXB / 64,
  2      52 |     *  IKMP1     = IKM + 1,
  2      53 |     *  ILVLTOT   = 7 * IKM + 37,
  2      54 |     *  ILONFP1   = ILONF + 1,
  2      55 |     *  INVBL     = ILVLTOT * INSUMG,
  2      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  2      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  2      58 |     *  IBLK1D    =  2 * IKM + 14,
  2      59 |     *  IBLK2D    = 16 * IKM + 36,
  2      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  2      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  2      62 |C
  1       2 |C-------
  1       3 |C------- FOLLOWING IS EXTRA STUFF FOR INITIALIZATION
  1       4 |C-------
  1       5 |         PARAMETER (INLATH=101,INUMCOEF=3*INLATH+35,
  1       6 |     *        INUMGRID=4,IJCAPDP=80,INLON=300,INKLON=2,INKLAT=2,
  1       7 |     *        IIORBPL=3,IIORDPL=3,IIORBLP=3,IIORDLP=3,IMCUBIC=1,
  1       8 |     *        INUMVERT=2,INSIG=IKM,INMODES=18,INGMAX=3000,
  1       9 |     *        INKSIG=2,INSIGP1=INSIG+1,INLONEX=INLON+INKLON,
  1      10 |     *        INLATDP=2*INLATH,INLOLA=INLATDP*INLONEX,
  1      11 |     *        IJCAP1DP=IJCAPDP+1,IJCAP12=2*IJCAP1DP,
  1      12 |     *        INCOEFS=IJCAP1DP*(IJCAPDP+2),
  1      13 |     *     IDPFILLR=(4*IKM+1)*INSUMG+       3*(IIJKMAX*4+IIJMAX) )
  1      14 |C    *     IDPFILLR=(4*IKM+1)*INSUMG+INGRDUSE*(IIJKMAX*4+IIJMAX) )
         41 |         COMPLEX DEPTHC(INSIG),EC(INSIG,INSIG),CTEMP
         42 |         REAL DEPTHS(INSIG)
         43 |         REAL HCP,HRKAPPA,HGASCON
         44 |         REAL HDEPTHS(INSIG)
         45 |         REAL DEPTHCR(INSIG),DEPTHCI(INSIG)
         46 |         REAL ECR(INSIG),ECI(INSIG)
         47 |         REAL G(INSIG,INSIG),TAU(INSIG,INSIG),PIVERT(INSIG)
         48 |         REAL B(INSIG,INSIG),E(INSIG,INSIG)
         49 |         REAL HE(INSIG,INSIG)
         50 |         REAL SIGHAT(INSIG)
         51 |         REAL HSIGHAT(INSIG)
         52 |         REAL DSIG(INSIG),TBAR(INSIG)
         53 |         REAL HDSIG(INSIG),HTBAR(INSIG)
         54 |         REAL HPR(INSIG)
         55 |         REAL WORK(INSIG,INSIG),WORK2(INSIG,INSIG)
         56 |         REAL EINVG(INSIG,INSIG)
         57 |         REAL HEINVG(INSIG,INSIG)
         58 |         REAL EINV(INSIG,INSIG),EINVT(INSIG)
         59 |         REAL HEINV(INSIG,INSIG),HEINVT(INSIG)
         60 |         REAL TBINVE(INSIG,INSIG),PIBINVE(INSIG)
         61 |         REAL HTBINVE(INSIG,INSIG),HPIBINVE(INSIG)
         62 |         REAL BINV(INSIG,INSIG)
         63 |         INTEGER LWORK(INSIG,2)
         64 |         REAL ALPHAR(INSIG)
         65 |         REAL ALPHAP(INSIG),BETAP(INSIG)
         66 |C--------
         67 |         N=INSIG
         68 |         NHAT=N+1
         69 |C--------
         70 |C--------START BY COMPUTING PIVERT
         71 |C--------
         72 |         DO 100 K=1,N
         73 |           PIVERT(K)=HDSIG(K)
         74 |           DSIG(K)=HDSIG(K)
         75 |           SIGHAT(K)=HSIGHAT(K)
         76 |           TBAR(K)=HTBAR(K)*HGASCON
         77 |100      CONTINUE
         78 |         RKAPPA=HRKAPPA
         79 |         GASCON=HGASCON
         80 |C--------
         81 |C--------NOW GET ALPHAR
         82 |C--------
         83 |         NM1=N-1
         84 |         DO 200 K=1,NM1
         85 |           ALPHAR(K)=.5*LOG(SIGHAT(K+1)/SIGHAT(K))
         86 |200      CONTINUE
         87 |         ALPHAR(N)=LOG(1./SIGHAT(N))
         88 |C--------
         89 |C-------- FOR PSEUDO-ARAKAWA HYDROSTATIC MATRIX NEED ALPHAP, BETAP.
         90 |C--------
         91 |         DO 210 K=1,NM1
         92 |           KR=NHAT-K
         93 |           KRP1=KR-1
         94 |           ALPHAP(K)=.5*(HPR(KRP1)/HPR(KR)-1.)/RKAPPA
         95 |           BETAP(K+1)=.5*(1.-HPR(KR)/HPR(KRP1))/RKAPPA
         96 |210      CONTINUE
         97 |         BETAP(1)=0.
         98 |         ALPHAP(N)=.5*(1./HPR(1)-2.)/RKAPPA
         99 |C--------
        100 |C--------SET UP HYDROSTATIC MATRIX G
        101 |C--------
        102 |C        DO 400 J=1,N
        103 |C          DO 300 I=1,N
        104 |C            IF(I.GT.J) G(I,J)=0.
        105 |C            IF(I.EQ.J) G(I,J)=ALPHAR(J)
        106 |C            IF(I.LT.J) G(I,J)=ALPHAR(J-1)+ALPHAR(J)
        107 |C300       CONTINUE
        108 |C400     CONTINUE
        109 |C--------
        110 |C--------SET UP HYDROSTATIC MATRIX G (PSEUDO-ARAKAWA)
        111 |C--------
        112 |         DO 400 J=1,N
        113 |           DO 300 I=1,N
        114 |             IF(I.GT.J) G(I,J)=0.
        115 |             IF(I.EQ.J) G(I,J)=ALPHAP(J)
        116 |             IF(I.LT.J) G(I,J)=ALPHAP(J-1)+BETAP(J)
        117 |300        CONTINUE
        118 |400      CONTINUE
        119 |C--------
        120 |C--------NOW OBTAIN TAU.
        121 |C--------
        122 |         DO 700 IR=1,N
        123 |           SUMR=0.
        124 |           DO 500 I=1,IR
        125 |             SUMR=SUMR+DSIG(I)
        126 |500        CONTINUE
        127 |           SUMRM=SUMR-DSIG(IR)
        128 |           IRM=IR-1
        129 |           IF(IR.EQ.1) IRM=1
        130 |           IRP=IR+1
        131 |           IF(IR.EQ.N) IRP=N
        132 |           DO 600 IS=1,N
        133 |             TERM1=DSIG(IS)*SUMR
        134 |             IF(IR.GE.IS) TERM1=TERM1-DSIG(IS)
        135 |             TERM2=DSIG(IS)*SUMRM
        136 |             IF(IR-1.GE.IS) TERM2=TERM2-DSIG(IS)
        137 |             TERM3=ALPHAR(IR)
        138 |             IF(IS.GT.IR) TERM3=0.
        139 |             IF(IS.LE.IR-1) TERM3=TERM3+ALPHAR(IR-1)
        140 |             TAU(IR,IS)=((TBAR(IRP)-TBAR(IR))*TERM1
        141 |     *                  +(TBAR(IR)-TBAR(IRM))*TERM2
        142 |     *        +2.*RKAPPA*TBAR(IR)*DSIG(IS)*TERM3)/(2.*DSIG(IR))
        143 |600        CONTINUE
        144 |700      CONTINUE
        145 |C--------
        146 |C--------GENERATE MATRIX B
        147 |C--------
        148 |         CALL MMULT(G,N,N,N,TAU,N,N,N,B,N,N,N)
        149 |         DO 1900 J=1,N
        150 |           DO 1800 K=1,N
        151 |             B(J,K)=B(J,K)+TBAR(J)*PIVERT(K)
        152 |1800       CONTINUE
        153 |1900     CONTINUE
        154 |C--------
        155 |C--------LAST STEP NOW IS TO COMPUTE EIGENVALUES, EIGENVECTORS
        156 |C--------
        157 |         IVECT=0
        158 |         CALL EGVLVE(B,DEPTHCR,DEPTHCI,ECR,ECI,N,IVECT,LWORK,
        159 |     *                 WORK,WORK2,IERROR)
        160 |         DO 1920 IVECT=1,N
        161 |           DEPTHC(IVECT)=CMPLX(DEPTHCR(IVECT),DEPTHCI(IVECT))
        162 |1920     CONTINUE
        163 |C--------REORDER EIGENVALUES FROM LARGEST TO SMALLEST
        164 |C--------
        165 |         DO 1980 I=1,N
        166 |           DO 1960 J=I,N
        167 |             IF  (CABS(DEPTHC(J)).LE.CABS(DEPTHC(I))) GO TO 1960
        168 |               CTEMP=DEPTHC(I)
        169 |               DEPTHC(I)=DEPTHC(J)
        170 |               DEPTHC(J)=CTEMP
        171 |1960       CONTINUE
        172 |1980     CONTINUE
        173 |         DO 1981 I=1,N
        174 |           DEPTHCR(I)=REAL(DEPTHC(I))
        175 |           DEPTHCI(I)=AIMAG(DEPTHC(I))
        176 |1981     CONTINUE
        177 |C--------
        178 |C--------COMPUTE ALL EIGENVECTORS, EVEN THOUGH ONLY NVERT ARE USED.
        179 |C--------
        180 |         DO 1983 IVECT=1,N
        181 |           CALL EGVLVE(B,DEPTHCR,DEPTHCI,ECR,ECI,N,IVECT,LWORK,
        182 |     *                   WORK,WORK2,IERROR)
        183 |           DO 1982 I=1,N
        184 |             EC(I,IVECT)=CMPLX(ECR(I),ECI(I))
        185 |1982       CONTINUE
        186 |1983     CONTINUE
        187 |         DO 2200 I=1,N
        188 |c        WRITE(6,2000)I, DEPTHC(I)
        189 |2000     FORMAT('  FOR MODE  ',I3,', REAL AND IMAGINARY PARTS OF ',
        190 |     *             'EIGENVALUE = ',2E12.4)
        191 |           DEPTHS(I)=DEPTHC(I)
        192 |           evecmax=0.
        193 |           ievecmax=1
        194 |           do 21002 k=1,n
        195 |             if(evecmax.lt.abs(ec(k,i))) then
        196 |               ievecmax=k
        197 |               evecmax=abs(ec(k,i))
        198 |             end if
        199 |21002      continue
        200 |           DO 2100 K=1,N
        201 |             E(K,I)=EC(K,I)/ec(ievecmax,i)
        202 |2100       CONTINUE
        203 |c          WRITE(6,2150)(E(K,I),K=1,N)
        204 |2150       FORMAT(1H ,10E13.3)
        205 |2200     CONTINUE
        206 |C--------
        207 |C--------NOW OBTAIN EINV, EINVG, EINVT, TBINVE, PIBINVE.
        208 |C--------
        209 |         DO 2400 J=1,N
        210 |           DO 2300 I=1,N
        211 |             EINV(I,J)=E(I,J)
        212 |2300       CONTINUE
        213 |2400     CONTINUE
        214 |         CALL MINV(EINV,N,DET,LWORK,LWORK(1,2))
        215 |c        CALL CHECKINV(E,EINV,N)
        216 |         DO 2500 J=1,N
        217 |           EINVT(J)=0.
        218 |2500     CONTINUE
        219 |         DO 2700 I=1,N
        220 |           DO 2600 J=1,N
        221 |             EINVT(J)=EINV(J,I)*TBAR(I)+EINVT(J)
        222 |             BINV(J,I)=B(J,I)
        223 |2600       CONTINUE
        224 |2700     CONTINUE
        225 |         CALL MINV(BINV,N,DET,LWORK,LWORK(1,2))
        226 |         CALL CHECKINV(B,BINV,N)
        227 |         CALL MMULT(BINV,N,N,N,E,N,N,N,WORK,N,N,N)
        228 |         CALL MMULT(TAU,N,N,N,WORK,N,N,N,TBINVE,N,N,N)
        229 |         DO 2900 J=1,N
        230 |           PIBINVE(J)=0.
        231 |           DO 2800 I=1,N
        232 |             PIBINVE(J)=PIVERT(I)*WORK(I,J)+PIBINVE(J)
        233 |2800       CONTINUE
        234 |2900     CONTINUE
        235 |         CALL MMULT(EINV,N,N,N,G,N,N,N,EINVG,N,N,N)
        236 |C-------- CONVERT TO UNITS OF METERS
        237 |         DO 1996 I=1,N
        238 |           HDEPTHS(I)=DEPTHS(I)/9.8
        239 |           HEINVT(I)=EINVT(I)
        240 |           HPIBINVE(I)=PIBINVE(I)
        241 |1996     CONTINUE
        242 |         DO 1998 I=1,N*N
        243 |           HEINV(I,1)=EINV(I,1)
        244 |           HEINVG(I,1)=EINVG(I,1)
        245 |           HTBINVE(I,1)=TBINVE(I,1)
        246 |           HE(I,1)=E(I,1)
        247 |1998     CONTINUE
        248 |       RETURN
        249 |       END
 
