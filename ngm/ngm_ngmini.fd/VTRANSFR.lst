         35 |     *       RTBINVE(IKM,IKM),RE(IKM,IKM)
         36 |         COMMON/FIXSP03/NSIG,NMODES,DELSIG(IKM),HEINVT(IKM),
         37 |     *       HEINV(IKM,IKM),HEINVG(IKM,IKM),HPIBINVE(IKM),
         38 |     *       HTBINVE(IKM,IKM),HE(IKM,IKM),HDEPTHS(INMODES)
         39 |C--------
         40 |         IF(NSIG.NE.INSIG) CALL SETSP03
         41 |         KM=NSIG
         42 |         KM2=KM*KM
         43 |         DO 100 I=1,KM
         44 |           RDEPTHS(I)=HDEPTHS(I)
         45 |           REINVT(I)=HEINVT(I)
         46 |           RPIBINVE(I)=HPIBINVE(I)
         47 |100      CONTINUE
         48 |         DO 200 I=1,KM2
         49 |           REINV(I,1)=HEINV(I,1)
         50 |           REINVG(I,1)=HEINVG(I,1)
         51 |           RTBINVE(I,1)=HTBINVE(I,1)
         52 |           RE(I,1)=HE(I,1)
         53 |200      CONTINUE
         54 |       RETURN
         55 |       END
 
