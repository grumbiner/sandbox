         31 |C--------
         32 |         REAL XOBS(1),YOBS(1)
         33 |C--------
         34 |             MNC=MC*NC
         35 |             DO 100 I=1,MC
         36 |               XOBS(I)=I
         37 |               YOBS(I)=1.
         38 |100          CONTINUE
         39 |             DO 210 J=2,NC
         40 |               J0=(J-1)*MC
         41 |               DO 207 I=1,MC
         42 |                 XOBS(J0+I)=XOBS(I)
         43 |                 YOBS(J0+I)=YOBS(J0-MC+I)+1.
         44 |207            CONTINUE
         45 |210          CONTINUE
         46 |             DO 220 I=1,MNC
         47 |               XOBS(I)=DX*(XOBS(I)-XPOLET)
         48 |               YOBS(I)=DY*(YOBS(I)-YPOLET)
         49 |220          CONTINUE
         50 |       RETURN
         51 |       END
 
