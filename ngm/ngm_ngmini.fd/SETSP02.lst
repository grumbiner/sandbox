         58 |         CALL getcoef(IRCINVOP,1)
         59 |         JCAP=MIN(IJCAPDP,JCAPIN)
         60 |         NCOEFS=(JCAP+1)*(JCAP+2)
         61 |         do 10002 i=1,ncoefs
         62 |         BONES(i)=.true.
         63 |10002    continue
         64 |         IB=1
         65 |         DO 100 M=0,JCAP
         66 |           LEN1=2*JCAP+2-2*M
         67 |           LEN2=LEN1-2
