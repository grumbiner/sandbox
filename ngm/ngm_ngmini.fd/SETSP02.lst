         69 |             bfop(ib+i-1)=.true.
         70 |10004      continue
         71 |           do 10006 i=len2+1,len1
         72 |             bfop(ib+i-1)=.false.
         73 |10006      continue
         74 |           IB=IB+LEN1
         75 |100      CONTINUE
         76 |         JFOP=2*JCAP+3
         77 |         NFOP=NCOEFS-JFOP+1
