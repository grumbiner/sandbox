          1 |       SUBROUTINE SCALCOFS(ZIN,DIN,HIN,ZOUT,DOUT,HOUT,DEPTH,JCAPIN)
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .                                       .
          4 |C SUBPROGRAM:    SCALCOFS    SCALE VORT,DIV,HEIGHT FOR SLOW-FAST OPS.
          5 |C   PRGMMR: PARRISH          ORG: W/NMC22    DATE: 88-06-13
          6 |C
          7 |C ABSTRACT: SCALE VORTICITY, DIVERGENCE, AND HEIGHT COEFFICIENTS
          8 |C   SO THAT SLOW-FAST TRANSFORMS CAN BE APPLIED.
          9 |C
         10 |C PROGRAM HISTORY LOG:
         11 |C   88-06-13  PARRISH
         12 |C
         13 |C USAGE:    CALL SCALCOFS(ZIN,DIN,HIN,ZOUT,DOUT,HOUT,DEPTH,JCAPIN)
         14 |C   INPUT ARGUMENT LIST:
         15 |C     ZIN      - UNSCALED VORTICITY COEFFICIENTS.
         16 |C     DIN      - UNSCALED DIVERGENCE COEFFICIENTS.
         17 |C     HIN      - UNSCALED HEIGHT COEFFICIENTS.
         18 |C     DEPTH    - SCALE HEIGHT.
         19 |C
         20 |C   OUTPUT ARGUMENT LIST:      (INCLUDING WORK ARRAYS)
         21 |C     ZOUT     - SCALED VORTICITY COEFFICIENTS.
         22 |C     DOUT     - SCALED DIVERGENCE COEFFICIENTS.
         23 |C     HOUT     - SCALED HEIGHT COEFFICIENTS.
         24 |C
         25 |C REMARKS: MUST INCLUDE BLOCK DATA BLOCKSP02 AND DEFINITION OF
         26 |C           PL1 VARIABLES BEFORE COMPILING.
         27 |C
         28 |C ATTRIBUTES:
         29 |C   LANGUAGE: FORTRAN200
         30 |C   MACHINE:  CYBER
         31 |C
         32 |C$$$
         33 |         include "myparam"
  1       1 |        INCLUDE 'parmodel'
  2       1 |C     PARAMETER FILE
  2       2 |C
  2       3 |C     SETUP FOR IBM SINGLE PRECISION REPRESENTATION
  2       4 |C     ON OUTPUT
  2       5 |C                      THIS VERSION 21 February 1991 (DiMego)
  2       6 |C
  2       7 |      PARAMETER (
  2       8 |     *      IBPW      =      4,
  2       9 |     *      IBPOR     = 120000,
  2      10 |     *      INUMREC   = IBPOR / IBPW,
  2      11 |     *      INSPECS   =    700,
  2      12 |     *      ILATG2    =    120,
  2      13 |     *      ILEVH     =     12,
  2      14 |     *      ILEVS     =     16,
  2      15 |     *      ILONF     =    360,
  2      16 |     *      INLATSH   =      5,
  2      17 |     *      IIJMAX    =  23668,
  2      18 |     *      IIJMAXB   =  25536,
  2      19 |     *      IIJRAD    =    892,
  2      20 |     *      IJCAP     =     80,
  2      21 |     *      IWPCPL    =     80,
  2      22 |     *      IKM       =     16 )
  2      23 |      PARAMETER (
  2      24 |     *      ILBCH     =   7*(147+111-7),
  2      25 |     *      ILBHI     =    (147+111-26),
  2      26 |     *      ILBUI     =    (2*147+111-39),
  2      27 |     *      ILBUO     =   (8*147+7*111-56),
  2      28 |     *      ILBVI     =    (147+2*111-39),
  2      29 |     *      ILBVO     =   (7*147+8*111-56),
  2      30 |     *      ILMOUT    =     20 )
  2      31 |      PARAMETER (
  2      32 |     *      IMAXOUTF  =     17,
  2      33 |     *      IMAXOUTT  =      3,
  2      34 |     *      IMAXOUTV  =    100,
  2      35 |     *      IMAXPROF  =      6,
  2      36 |     *      INFILLER  =  10000,
  2      37 |     *      INH       =     65,
  2      38 |     *      INGRDUSE  =      2,
  2      39 |     *      INGRDUS1  =      1,
  2      40 |     *      INIADDRS  =     44,
  2      41 |     *      INSCR     =     23,
  2      42 |     *      INSCRGEO  =     12,
  2      43 |     *      INSCR3    =     10,
  2      44 |     *      INSOIL    =      2,
  2      45 |     *      INSUMG    =  42164,
  2      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  2      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  2      48 |     *      INSHEM    = 13 * INH + 23,
  2      49 |     *      IIJKMAX   = IKM * IIJMAX )
  2      50 |      PARAMETER (
  2      51 |     *  IIWORDSB  = IIJMAXB / 64,
  2      52 |     *  IKMP1     = IKM + 1,
  2      53 |     *  ILVLTOT   = 7 * IKM + 37,
  2      54 |     *  ILONFP1   = ILONF + 1,
  2      55 |     *  INVBL     = ILVLTOT * INSUMG,
  2      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  2      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  2      58 |     *  IBLK1D    =  2 * IKM + 14,
  2      59 |     *  IBLK2D    = 16 * IKM + 36,
  2      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  2      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  2      62 |C
  1       2 |C-------
  1       3 |C------- FOLLOWING IS EXTRA STUFF FOR INITIALIZATION
  1       4 |C-------
  1       5 |         PARAMETER (INLATH=101,INUMCOEF=3*INLATH+35,
  1       6 |     *        INUMGRID=4,IJCAPDP=80,INLON=300,INKLON=2,INKLAT=2,
  1       7 |     *        IIORBPL=3,IIORDPL=3,IIORBLP=3,IIORDLP=3,IMCUBIC=1,
  1       8 |     *        INUMVERT=2,INSIG=IKM,INMODES=18,INGMAX=3000,
  1       9 |     *        INKSIG=2,INSIGP1=INSIG+1,INLONEX=INLON+INKLON,
  1      10 |     *        INLATDP=2*INLATH,INLOLA=INLATDP*INLONEX,
  1      11 |     *        IJCAP1DP=IJCAPDP+1,IJCAP12=2*IJCAP1DP,
  1      12 |     *        INCOEFS=IJCAP1DP*(IJCAPDP+2),
  1      13 |     *     IDPFILLR=(4*IKM+1)*INSUMG+       3*(IIJKMAX*4+IIJMAX) )
  1      14 |C    *     IDPFILLR=(4*IKM+1)*INSUMG+INGRDUSE*(IIJKMAX*4+IIJMAX) )
         34 |C--------
         35 |         real ZIN(1),DIN(1),HIN(1),ZOUT(1),DOUT(1),HOUT(1)
         36 |C--------
         37 |         COMMON/DAVEBUFF/ COF(INCOEFS,INUMCOEF),GRD(INLOLA,INUMGRID)
         38 |         real COF,GRD
         39 |C--------
         40 |         COMMON/FIXSP02/JCAP,NCOEFS,NFOP,JFOP,
         41 |     *      ISCALZD,IUNSCLZD,
         42 |     *      IBSHAT,IBVHAT,IBVIHAT,
         43 |     *      IBOP,ICOP,IFOP,ICINVOP,
         44 |     *      IRFOP,IRCINVOP,
         45 |     *      BFOP(INCOEFS),BONES(INCOEFS)
         46 |         LOGICAL BFOP,BONES
         47 |C--------
         48 |C-------- CHECK TO SEE IF INITIALIZATION OF INTERNAL VARIABLES NEEDED
         49 |         IF(JCAP.NE.JCAPIN) CALL SETSP02(JCAPIN)
         50 |C--------
         51 |C-------- MAKE SURE ZONAL PART OF COEFS HAS PROPER FORM
         52 |C--------
         53 |         do 10002 i=1,ncoefs
         54 |         ZIN(i)=COF(i,IBVHAT)*ZIN(i)
         55 |         DIN(i)=COF(i,IBVHAT)*DIN(i)
         56 |         HIN(i)=COF(i,IBSHAT)*HIN(i)
         57 |10002    continue
         58 |         SCALH=SQRT(9.8062/DEPTH)
         59 |         do 10004 i=1,ncoefs
         60 |         ZOUT(i)=COF(i,ISCALZD)*ZIN(i)
         61 |         HOUT(i)=SCALH*HIN(i)
         62 |10004    continue
         63 |C-------- MULTIPLY DIVERGENCE BY SQRT(-1)
         64 |         DO 100 I=1,NCOEFS,2
         65 |           DOUT(I)=-DIN(I+1)
         66 |           DOUT(I+1)=DIN(I)
         67 |100      CONTINUE
         68 |         do 10006 i=1,ncoefs
         69 |         DOUT(i)=COF(i,ISCALZD)*DOUT(i)
         70 |10006    continue
         71 |       RETURN
         72 |       END
 
