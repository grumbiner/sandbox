          1 |       SUBROUTINE UNAVERTA(UE,VE,THE,HE,QE,IJ,IMG,JMG,PR,GASCON,
          2 |     *       U,V,TH,H,HU0,HV0,HTH0,H0,Q0,H1ONU,H1ONV,DHONU,DHONV,
          3 |     *       H11MK,DH1MK,DH,WORK)
          4 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          5 |C                .      .    .                                       .
          6 |C SUBPROGRAM:    UNAVERTA    ADD INCREMENTS TO FULL FIELDS
          7 |C   PRGMMR: PARRISH          ORG: W/NMC22    DATE: 88-08-08
          8 |C
          9 |C ABSTRACT: ADD HOSKINS-SIMMONS FORM INCREMENTS ON TO PHILLIPS
         10 |C   FORM FULL FIELDS.
         11 |C
         12 |C PROGRAM HISTORY LOG:
         13 |C   88-08-08  PARRISH
         14 |C
         15 |C USAGE:    CALL UNAVERTA(UE,VE,THE,HE,QE,IJ,IMG,JMG,PR,GASCON,
         16 |C    *       U,V,TH,H,HU0,HV0,HTH0,H0,Q0,H1ONU,H1ONV,DHONU,DHONV,
         17 |C    *       H11MK,DH1MK,DH,WORK)
         18 |C   INPUT ARGUMENT LIST:
         19 |C     IJ       - IMG*JMG, NUMBER OF GRID-POINTS.
         20 |C     IMG      - X-COORDINATE DIMENSION
         21 |C     JMG      - Y-COORDINATE DIMENSION
         22 |C     PR       - RELATED TO EXNER FUNCTION (PR=.5*SIG**KAPPA)
         23 |C     GASCON   - GAS CONSTANT
         24 |C     U        - U-INCREMENT IN HOSKINS-SIMMONS FORM
         25 |C     V        - V-INCREMENT IN HOSKINS-SIMMONS FORM
         26 |C     TH       - T-INCREMENT IN HOSKINS-SIMMONS FORM
         27 |C     H        - LN(PSFC)-INCREMENT IN HOSKINS-SIMMONS FORM
         28 |C     HU0      - ORIGINAL PHILLIPS U-WIND
         29 |C     HV0      - ORIGINAL PHILLIPS V-WIND
         30 |C     HTH0     - ORIGINAL PHILLIPS POT-TEMP
         31 |C     H0       - ORIGINAL PHILLIPS SFCP
         32 |C     Q0       - ORIGINAL PHILLIPS MOISTURE
         33 |C
         34 |C   OUTPUT ARGUMENT LIST:      (INCLUDING WORK ARRAYS)
         35 |C     UE       - NEW PHILLIPS U-WIND
         36 |C     VE       - NEW PHILLIPS V-WIND
         37 |C     THE      - NEW PHILLIPS POT-TEMP
         38 |C     HE       - NEW PHILLIPS SFCP
         39 |C     QE       - NEW PHILLIPS MOISTURE
         40 |C     H1ONU    - WORK ARRAY
         41 |C     H1ONV    - WORK ARRAY
         42 |C     DHONU    - WORK ARRAY
         43 |C     DHONV    - WORK ARRAY
         44 |C     H11MK    - WORK ARRAY
         45 |C     DH1MK    - WORK ARRAY
         46 |C     DH       - WORK ARRAY
         47 |C     WORK     - WORK ARRAY
         48 |C
         49 |C   SUBPROGRAMS CALLED: P2KAP,SETSHIFT
         50 |C
         51 |C ATTRIBUTES:
         52 |C   LANGUAGE: FORTRAN200
         53 |C   MACHINE:  CYBER
         54 |C
         55 |C$$$
         56 |         include "myparam"
  1       1 |        INCLUDE 'parmodel'
  2       1 |C     PARAMETER FILE
  2       2 |C
  2       3 |C     SETUP FOR IBM SINGLE PRECISION REPRESENTATION
  2       4 |C     ON OUTPUT
  2       5 |C                      THIS VERSION 21 February 1991 (DiMego)
  2       6 |C
  2       7 |      PARAMETER (
  2       8 |     *      IBPW      =      4,
  2       9 |     *      IBPOR     = 120000,
  2      10 |     *      INUMREC   = IBPOR / IBPW,
  2      11 |     *      INSPECS   =    700,
  2      12 |     *      ILATG2    =    120,
  2      13 |     *      ILEVH     =     12,
  2      14 |     *      ILEVS     =     16,
  2      15 |     *      ILONF     =    360,
  2      16 |     *      INLATSH   =      5,
  2      17 |     *      IIJMAX    =  23668,
  2      18 |     *      IIJMAXB   =  25536,
  2      19 |     *      IIJRAD    =    892,
  2      20 |     *      IJCAP     =     80,
  2      21 |     *      IWPCPL    =     80,
  2      22 |     *      IKM       =     16 )
  2      23 |      PARAMETER (
  2      24 |     *      ILBCH     =   7*(147+111-7),
  2      25 |     *      ILBHI     =    (147+111-26),
  2      26 |     *      ILBUI     =    (2*147+111-39),
  2      27 |     *      ILBUO     =   (8*147+7*111-56),
  2      28 |     *      ILBVI     =    (147+2*111-39),
  2      29 |     *      ILBVO     =   (7*147+8*111-56),
  2      30 |     *      ILMOUT    =     20 )
  2      31 |      PARAMETER (
  2      32 |     *      IMAXOUTF  =     17,
  2      33 |     *      IMAXOUTT  =      3,
  2      34 |     *      IMAXOUTV  =    100,
  2      35 |     *      IMAXPROF  =      6,
  2      36 |     *      INFILLER  =  10000,
  2      37 |     *      INH       =     65,
  2      38 |     *      INGRDUSE  =      2,
  2      39 |     *      INGRDUS1  =      1,
  2      40 |     *      INIADDRS  =     44,
  2      41 |     *      INSCR     =     23,
  2      42 |     *      INSCRGEO  =     12,
  2      43 |     *      INSCR3    =     10,
  2      44 |     *      INSOIL    =      2,
  2      45 |     *      INSUMG    =  42164,
  2      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  2      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  2      48 |     *      INSHEM    = 13 * INH + 23,
  2      49 |     *      IIJKMAX   = IKM * IIJMAX )
  2      50 |      PARAMETER (
  2      51 |     *  IIWORDSB  = IIJMAXB / 64,
  2      52 |     *  IKMP1     = IKM + 1,
  2      53 |     *  ILVLTOT   = 7 * IKM + 37,
  2      54 |     *  ILONFP1   = ILONF + 1,
  2      55 |     *  INVBL     = ILVLTOT * INSUMG,
  2      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  2      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  2      58 |     *  IBLK1D    =  2 * IKM + 14,
  2      59 |     *  IBLK2D    = 16 * IKM + 36,
  2      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  2      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  2      62 |C
  1       2 |C-------
  1       3 |C------- FOLLOWING IS EXTRA STUFF FOR INITIALIZATION
  1       4 |C-------
  1       5 |         PARAMETER (INLATH=101,INUMCOEF=3*INLATH+35,
  1       6 |     *        INUMGRID=4,IJCAPDP=80,INLON=300,INKLON=2,INKLAT=2,
  1       7 |     *        IIORBPL=3,IIORDPL=3,IIORBLP=3,IIORDLP=3,IMCUBIC=1,
  1       8 |     *        INUMVERT=2,INSIG=IKM,INMODES=18,INGMAX=3000,
  1       9 |     *        INKSIG=2,INSIGP1=INSIG+1,INLONEX=INLON+INKLON,
  1      10 |     *        INLATDP=2*INLATH,INLOLA=INLATDP*INLONEX,
  1      11 |     *        IJCAP1DP=IJCAPDP+1,IJCAP12=2*IJCAP1DP,
  1      12 |     *        INCOEFS=IJCAP1DP*(IJCAPDP+2),
  1      13 |     *     IDPFILLR=(4*IKM+1)*INSUMG+       3*(IIJKMAX*4+IIJMAX) )
  1      14 |C    *     IDPFILLR=(4*IKM+1)*INSUMG+INGRDUSE*(IIJKMAX*4+IIJMAX) )
         57 |C--------
         58 |         REAL PR(1)
         59 |         REAL UE(IJ,1),VE(IJ,1),THE(IJ,1),HE(1),QE(IJ,1)
         60 |         REAL U(IIJMAX,1)
         61 |         REAL V(IIJMAX,1)
         62 |         REAL TH(IIJMAX,1),H(1)
         63 |         REAL HU0(IIJMAX,1),HV0(IIJMAX,1)
         64 |         REAL HTH0(IIJMAX,1),H0(1)
         65 |         REAL Q0(IIJMAX,1)
         66 |         REAL H1ONU(IIJMAX),H1ONV(IIJMAX)
         67 |         REAL DHONU(IIJMAX),DHONV(IIJMAX)
         68 |         REAL H11MK(IIJMAX),DH1MK(IIJMAX),DH(IIJMAX)
         69 |         REAL WORK(IIJMAX)
         70 |         LOGICAL BITA(IIJMAX)
         71 |C--------
         72 |C        WRITE(6,110)
         73 |C110     FORMAT(' ENTERING SUBROUTINE UNAVERTA')
         74 |         NVECT=IMG*JMG
         75 |         DO 1101 I=1,NVECT
         76 |           DH(I)=EXP(H(I))
         77 |           HE(I)=DH(I)*H0(I)
         78 |1101     CONTINUE
         79 |         DO 112 K=1,IKM
         80 |           DO 1121 I=1,NVECT
         81 |             QE(I,K)=DH(I)*Q0(I,K)
         82 |1121       CONTINUE
         83 |112      CONTINUE
         84 |C--------
         85 |C-------- OBTAIN H1ONU,H1ONV,DHONU,DHONV
         86 |C--------
         87 |         DO 1123 I=1,NVECT
         88 |           H1ONU(I)=HE(I)
         89 |           DHONU(I)=DH(I)
         90 |1123     CONTINUE
         91 |         CALL SETSHIFT(IMG,JMG,  0,  0,  0,  0,  0,
         92 |     *                           0,  1,  0,  0,  0,
         93 |     *       IS1,IS2,IS3,IS4,IS5,
         94 |     *       LEN,BITA)
         95 |         DO 1125 I=0,LEN-1
         96 |           IF(BITA(I+1)) THEN
         97 |             H1ONU(IS1+I)=.5*(HE(IS1+I)+HE(IS2+I))
         98 |             DHONU(IS1+I)=.5*(DH(IS1+I)+DH(IS2+I))
         99 |           END IF
        100 |1125     CONTINUE
        101 |         DO 1127 I=1,NVECT
        102 |           H1ONV(I)=HE(I)
        103 |           DHONV(I)=DH(I)
        104 |1127     CONTINUE
        105 |         CALL SETSHIFT(IMG,JMG,  0,  1,  0,  0,  0,
        106 |     *                           0,  0,  0,  0,  0,
        107 |     *       IS1,IS2,IS3,IS4,IS5,
        108 |     *       LEN,BITA)
        109 |         DO 1129 I=0,LEN-1
        110 |           IF(BITA(I+1)) THEN
        111 |             H1ONV(IS1+I)=.5*(HE(IS1+I)+HE(IS2+I))
        112 |             DHONV(IS1+I)=.5*(DH(IS1+I)+DH(IS2+I))
        113 |           END IF
        114 |1129     CONTINUE
        115 |C--------
        116 |C-------- COMPUTE H11MK, DH1MK
        117 |C--------
        118 |         CALL P2KAP(HE,H11MK,NVECT)
        119 |         DO 1131 I=1,NVECT
        120 |           H11MK(I)=HE(I)/H11MK(I)
        121 |1131     CONTINUE
        122 |         CALL P2KAP(DH,DH1MK,NVECT)
        123 |         DO 1133 I=1,NVECT
        124 |           DH1MK(I)=DH(I)/DH1MK(I)
        125 |1133     CONTINUE
        126 |         DO 230 KR=1,IKM
        127 |           CONST=.5/(PR(KR)*GASCON)
        128 |           DO 2301 I=1,NVECT
        129 |             THE(I,KR)=DH1MK(I)*HTH0(I,KR)+CONST*
        130 |     *                    H11MK(I)*TH(I,KR)
        131 |             UE(I,KR)=DHONU(I)*HU0(I,KR)
        132 |     *                   +H1ONU(I)*U(I,KR)
        133 |             VE(I,KR)=DHONV(I)*HV0(I,KR)
        134 |     *                   +H1ONV(I)*V(I,KR)
        135 |2301       CONTINUE
        136 |230      CONTINUE
        137 |       RETURN
        138 |       END
 
