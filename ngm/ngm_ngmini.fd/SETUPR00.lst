        256 |               IADDU=MADDU+(K-1)*IJ
        257 |               IADDV=MADDV+(K-1)*IJ
        258 |               IADDT=MADDT+(K-1)*IJ
        259 |               DO 21002 I=1,IJ
        260 |                 R0U(I,K)=(VBL(IADDU+I-1)-W0U(I,K))*DELTATAI
        261 |                 R0V(I,K)=(VBL(IADDV+I-1)-W0V(I,K))*DELTATAI
        262 |                 R0TH(I,K)=(VBL(IADDT+I-1)-W0TH(I,K))*DELTATAI
        263 |21002          CONTINUE
        264 |210          CONTINUE
        265 |             DO 21004 I=1,IJ
        266 |               R0H(I)=(VBL(MADDH+I-1)-W0H(I))*DELTATAI
        267 |21004        CONTINUE
        268 |C------------
        269 |C------------ ANALYSIS WILL ALWAYS BE FULL FIELDS, SO JUST COPY
        270 |C------------ VBLCOPY TO VBL.
        271 |C------------
        272 |             CALL CTOVBL(VBLCOPY,NG,IADDRGDP)
        273 |C------------
        274 |C------------ PUT TENDENCIES OF FIRST GUESS INTO VBLCOPY NOW
