        278 |               IADDVC=MADDVC+(K-1)*IJ
        279 |               IADDTC=MADDTC+(K-1)*IJ
        280 |               IADDQC=MADDQC+(K-1)*IJ
        281 |               DO 23002 I=1,IJ
        282 |                 VBLCOPY(IADDUC+I-1)=R0U(I,K)
        283 |                 VBLCOPY(IADDVC+I-1)=R0V(I,K)
        284 |                 VBLCOPY(IADDTC+I-1)=R0TH(I,K)
        285 |                 VBLCOPY(IADDQC+I-1)=R0Q(I,K)
        286 |23002          CONTINUE
        287 |230          CONTINUE
        288 |             DO 23004 I=1,IJ
        289 |               VBLCOPY(MADDHC+I-1)=R0H(I)
        290 |23004        CONTINUE
