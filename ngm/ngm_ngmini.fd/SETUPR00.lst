        308 |C--------
        309 |C-------- FIX UP TENDENCIES IN GRID OVERLAP AREAS.
        310 |C--------
        311 |         CALL SWAPVBL(VBLCOPY,IADDRGDP)
        312 |         DO 1100 NG=1,INGRDUSE
        313 |           IF(NG.GT.1) THEN
        314 |             NGM=NG-1
        315 |             CALL LATBND(NGM,KHINTERP)
        316 |           ELSE
        317 |             CALL SHEM(KHINTERP)
        318 |           END IF
        319 |1100     CONTINUE
        320 |         CALL SWAPVBL(VBLCOPY,IADDRGDP)
        321 |       RETURN
        322 |       END
 
