        179 |         LOGICAL LFRSTGES
        180 |C--------
        181 |C--------
        182 |         KHINTERP=2*IKM+1
        183 |         DO 1000 NG=1,NGRDUSE
        184 |           IJ=IMG(NG)*JMG(NG)
        185 |           MADDU=IADDRG(1,NG)
        186 |           MADDV=IADDRG(2,NG)
        187 |           MADDT=IADDRG(3,NG)
        188 |           MADDQ=IADDRG(4,NG)
        189 |           MADDH=IADDRG(5,NG)
        190 |           MADDUC=IADDRGDP(1,NG)
        191 |           MADDVC=IADDRGDP(2,NG)
        192 |           MADDTC=IADDRGDP(3,NG)
        193 |           MADDQC=IADDRGDP(4,NG)
        194 |           MADDHC=IADDRGDP(5,NG)
        195 |C----------
        196 |           IF(LFRSTGES) THEN
        197 |C------------
        198 |C------------WE HAVE FIRST GUESS FIELD.  COMPUTE ITS TENDENCY AND SAVE.
        199 |C------------ CALL ONEGRID TO DO ONE MODEL TIME-STEP.
        200 |C------------
        201 |             CALL FROMVBL(W0U,W0V,W0TH,W0Q,W0H,NG)
        202 |             NPAIR=1
        203 |             LASTSTEP=0
        204 |C------------ MAKE SURE PHYSICS IS TURNED OFF (SET IPHYSPL=1)
        205 |             IPHYSPL=1
        206 |C???????????????????????????????????????????????????????????????????????
        207 |C???????THIS CALL TO ONEGRID SHOULD USE THE SAME TIME-STEP ON ALL GRIDS?
        208 |C???????????????????????????????????????????????????????????????????????
        209 |             CALL ONEGRID(NG,NPAIR,LASTSTEP)
        210 |             CALL SHEM(KHINTERP)
        211 |C------------
        212 |C------------ NOW CONVERT VARIABLES.
        213 |C------------
        214 |             IBITNG2=MIN(NG,2)
        215 |             DO 100 K=1,KM
        216 |               KR=KM+1-K
        217 |               DSIG(K)=DELSIG(KR)
        218 |100          CONTINUE
        219 |             CPDP=1005.
        220 |             GASCON=287.05
        221 |             RKAPPA=GASCON/CPDP
        222 |             SIGOLD=0.
        223 |             DO 112 I=1,KM
        224 |               SIGNEW=SIGOLD+DSIG(I)
        225 |               SIGHAT(I)=.5*(SIGNEW+SIGOLD)
        226 |               SIGKAP(I)=EXP(RKAPPA*LOG(SIGHAT(I)))
        227 |               SIGOLD=SIGNEW
        228 |C              WRITE(6,111)DSIG(I),SIGHAT(I),SIGKAP(I)
        229 |C11            FORMAT(' DSIG,SIGHAT,SIGKAP=',3E12.4)
        230 |112          CONTINUE
        231 |           CALL CONVERTA(VBL(MADDU),VBL(MADDV),VBL(MADDT),VBL(MADDH),IJ,
        232 |     *      IMG(NG),JMG(NG),PR,GASCON,TBAR,BITGRDH(1,IBITNG2,NG),
        233 |     *       R0U(1,1),R0V(1,1),R0U(1,2),R0V(1,2),R0U(1,3))
        234 |           CALL CONVERTA(W0U,W0V,W0TH,W0H,IJMAX,IMG(NG),JMG(NG),
        235 |     *            PR,GASCON,TBAR,BITGRDH(1,IBITNG2,NG),
        236 |     *       R0U(1,1),R0V(1,1),R0U(1,2),R0V(1,2),R0U(1,3))
        237 |C------------
        238 |C------------ NEXT OBTAIN TENDENCIES, STORING IN R0.
        239 |C------------
        240 |             DELTAXA=(2.*RADIUS)*2./(REAL(NH)+.5)
        241 |             DO 204 I=1,NG
        242 |               DELTAXA=.5*DELTAXA
        243 |204          CONTINUE
        244 |             DELTATA=DTOVDX*DELTAXA
        245 |             DELTATAI=1./DELTATA
        246 |C???????????????????????????????????????????????????????????????
        247 |C???????AFTER CHANGING TIME-STEP IN CALL TO ONEGRID ABOVE,??????
        248 |C??????? MAKE CORRESPONDING CHANGE HERE.????????????????????????
        249 |C???????????????????????????????????????????????????????????????
        250 |C            WRITE(6,205)NG,DELTAXA,DELTATA,RADIUS,NH,DTOVDX,
        251 |C    *                DELTATAI
        252 |C05      FORMAT(' BEFORE OBTAINING FULL MODEL TENDENCIES ON GRID',I4,/,
        253 |C    *        ' DELTAXA,DELTATA=',2E12.4,/,
        254 |C    *        ' RADIUS,NH=',E12.4,I6,/,' DTOVDX,DELTATAI=',2E12.4)
