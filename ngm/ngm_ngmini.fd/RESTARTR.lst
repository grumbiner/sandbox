          1 |      SUBROUTINE RESTARTR(IOUTURST, IOUTUPRT, SPECS, VBL,
          2 |     1                    BITSEA, BITSNO, BITWVL)
          3 |C
          4 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          5 |C
          6 |C SUBPROGRAM:    RESTARTR    READS RESTART FILE
          7 |C   PRGMMR: J TUCCILLO       ORG: W/NMC4     DATE: 90-06-19
          8 |C
          9 |C ABSTRACT:    READ RESTART FILE WITH A MAXIMUM RECORD LENGTH OF
         10 |C              INUMREC ELEMENTS
         11 |C
         12 |C PROGRAM HISTORY LOG:
         13 |C   90-06-19  J TUCCILLO
         14 |C   98-11-19  E ROGERS MODIFIED CODE TO READ NCEP DATE WORD INSTEAD
         15 |C             OF ON85 DATE LABEL
         16 |C
         17 |C USAGE:  CALL RESTARTR(IOUTURST, IOUTUPRT, SPECS, VBL,
         18 |C                         BITSEA, BITSNO, BITWVL)
         19 |C   INPUT ARGUMENTS:
         20 |C     IOUTURST  -  UNIT NUMBER OF RESTART FILE
         21 |C     IOUTUPRT  -  UNIT NUMBER OF OUTPUT FILE
         22 |C     SPECS     -  SPECIFICATION FILE
         23 |C     VBL       -  CONTAINS NGM MODEL FIELDS
         24 |C     BITSEA    -  CONTAINS UNFROZEN WATER LOGICAL MAP
         25 |C     BITSNO    -  CONTAINS SNOW COVER LOGICAL MAP
         26 |C     BITWVL    -  CONTAINS WATER VS. LAND LOGICAL MAP
         27 |C
         28 |C ATTRIBUTES:
         29 |C   LANGUAGE: FORTRAN
         30 |C   MACHINE:  CRAY Y-MP
         31 |C
         32 |C$$$
         33 |C
         34 |      INCLUDE 'parmodel'
  1       1 |C     PARAMETER FILE
  1       2 |C
  1       3 |C     SETUP FOR IBM SINGLE PRECISION REPRESENTATION
  1       4 |C     ON OUTPUT
  1       5 |C                      THIS VERSION 21 February 1991 (DiMego)
  1       6 |C
  1       7 |      PARAMETER (
  1       8 |     *      IBPW      =      4,
  1       9 |     *      IBPOR     = 120000,
  1      10 |     *      INUMREC   = IBPOR / IBPW,
  1      11 |     *      INSPECS   =    700,
  1      12 |     *      ILATG2    =    120,
  1      13 |     *      ILEVH     =     12,
  1      14 |     *      ILEVS     =     16,
  1      15 |     *      ILONF     =    360,
  1      16 |     *      INLATSH   =      5,
  1      17 |     *      IIJMAX    =  23668,
  1      18 |     *      IIJMAXB   =  25536,
  1      19 |     *      IIJRAD    =    892,
  1      20 |     *      IJCAP     =     80,
  1      21 |     *      IWPCPL    =     80,
  1      22 |     *      IKM       =     16 )
  1      23 |      PARAMETER (
  1      24 |     *      ILBCH     =   7*(147+111-7),
  1      25 |     *      ILBHI     =    (147+111-26),
  1      26 |     *      ILBUI     =    (2*147+111-39),
  1      27 |     *      ILBUO     =   (8*147+7*111-56),
  1      28 |     *      ILBVI     =    (147+2*111-39),
  1      29 |     *      ILBVO     =   (7*147+8*111-56),
  1      30 |     *      ILMOUT    =     20 )
  1      31 |      PARAMETER (
  1      32 |     *      IMAXOUTF  =     17,
  1      33 |     *      IMAXOUTT  =      3,
  1      34 |     *      IMAXOUTV  =    100,
  1      35 |     *      IMAXPROF  =      6,
  1      36 |     *      INFILLER  =  10000,
  1      37 |     *      INH       =     65,
  1      38 |     *      INGRDUSE  =      2,
  1      39 |     *      INGRDUS1  =      1,
  1      40 |     *      INIADDRS  =     44,
  1      41 |     *      INSCR     =     23,
  1      42 |     *      INSCRGEO  =     12,
  1      43 |     *      INSCR3    =     10,
  1      44 |     *      INSOIL    =      2,
  1      45 |     *      INSUMG    =  42164,
  1      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  1      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  1      48 |     *      INSHEM    = 13 * INH + 23,
  1      49 |     *      IIJKMAX   = IKM * IIJMAX )
  1      50 |      PARAMETER (
  1      51 |     *  IIWORDSB  = IIJMAXB / 64,
  1      52 |     *  IKMP1     = IKM + 1,
  1      53 |     *  ILVLTOT   = 7 * IKM + 37,
  1      54 |     *  ILONFP1   = ILONF + 1,
  1      55 |     *  INVBL     = ILVLTOT * INSUMG,
  1      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  1      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  1      58 |     *  IBLK1D    =  2 * IKM + 14,
  1      59 |     *  IBLK2D    = 16 * IKM + 36,
  1      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  1      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  1      62 |C
         35 |C
         36 |      DIMENSION VBL    ( INVBL ),
         37 |     *          SPECS  ( INSPECS ),
         38 |     *          BITSEA ( IIJMAX * INGRDUSE ),
         39 |     *          BITSNO ( IIJMAX * INGRDUSE ),
         40 |     *          BITWVL ( IIJMAX * INGRDUSE )
         41 |C
         42 |      LOGICAL BITSEA, BITSNO, BITWVL
         43 |C
         44 |      DIMENSION INPDAT(8)
         45 |C
         46 |C=======================================================================
         47 |C
         48 |      READ (IOUTURST, ERR=600, END=700) INPDAT
         49 |C
         50 |      ILEFT = MOD ( INSPECS, INUMREC )
         51 |      ILOOP = INSPECS / INUMREC
         52 |      IST   = 1
         53 |      ISP   = INUMREC
         54 |      DO 1 II = 1, ILOOP
         55 |         READ ( IOUTURST, ERR=600, END=700 ) (SPECS(I),I=IST,ISP)
         56 |         IST = ISP + 1
         57 |         ISP = ISP + INUMREC
         58 |1     CONTINUE
         59 |      IF ( ILEFT .GT. 0 ) THEN
         60 |         ISP = INSPECS
         61 |         READ ( IOUTURST, ERR=600, END=700 ) (SPECS(I),I=IST,ISP)
         62 |      END IF
         63 |C
         64 |      ILEFT = MOD ( INVBL, INUMREC )
         65 |      ILOOP = INVBL / INUMREC
         66 |      IST   =     1
         67 |      ISP   = INUMREC
         68 |      DO 2 II = 1, ILOOP
         69 |        READ (IOUTURST, ERR=600, END=700) (VBL(I), I=IST, ISP)
         70 |        IST = ISP + 1
         71 |        ISP = ISP + INUMREC
         72 |2     CONTINUE
         73 |      IF( ILEFT .NE. 0) THEN
         74 |        ISP = INVBL
         75 |        READ (IOUTURST, ERR=600, END=700) (VBL(I), I=IST, ISP)
         76 |      END IF
         77 |C
         78 |C     SAME VALUES FOR ALL LOGICAL MASKS
         79 |C
         80 |      ILEN = IIJMAX * INGRDUSE
         81 |      ILEFT = MOD ( ILEN, INUMREC )
         82 |      ILOOP = ILEN / INUMREC
         83 |C
         84 |C
         85 |      IST   =     1
         86 |      ISP   = INUMREC
         87 |      DO 3 II = 1, ILOOP
         88 |        READ (IOUTURST, ERR=600, END=700) (BITSEA(I), I=IST, ISP)
         89 |        IST = ISP + 1
         90 |        ISP = ISP + INUMREC
         91 |3     CONTINUE
         92 |      IF( ILEFT .NE. 0) THEN
         93 |        ISP = ILEN
         94 |        READ (IOUTURST, ERR=600, END=700) (BITSEA(I), I=IST, ISP)
         95 |      END IF
         96 |C
         97 |      IST   =     1
         98 |      ISP   = INUMREC
         99 |      DO 4 II = 1, ILOOP
        100 |        READ (IOUTURST, ERR=600, END=700) (BITSNO(I), I=IST, ISP)
        101 |        IST = ISP + 1
        102 |        ISP = ISP + INUMREC
        103 |4     CONTINUE
        104 |      IF( ILEFT .NE. 0) THEN
        105 |        ISP = ILEN
        106 |        READ (IOUTURST, ERR=600, END=700) (BITSNO(I), I=IST, ISP)
        107 |      END IF
        108 |C
        109 |      IST   =     1
        110 |      ISP   = INUMREC
        111 |      DO 5 II = 1, ILOOP
        112 |        READ (IOUTURST, ERR=600, END=700) (BITWVL(I), I=IST, ISP)
        113 |        IST = ISP + 1
        114 |        ISP = ISP + INUMREC
        115 |5     CONTINUE
        116 |      IF( ILEFT .NE. 0) THEN
        117 |        ISP = ILEN
        118 |        READ (IOUTURST, ERR=600, END=700) (BITWVL(I), I=IST, ISP)
        119 |      END IF
        120 |C
        121 |      RETURN
        122 |C
        123 |C*****READ ERROR
        124 |C
        125 |  600     WRITE (IOUTUPRT, 650)
        126 |  650     FORMAT ('0', 'ERROR WHILE READING RESTART FILE '/
        127 |     1            '0', 11X, 'EXECUTION TERMINATING.' /////)
        128 |C
        129 |      STOP
        130 |C
        131 |C*****END OF FILE
        132 |C
        133 |  700     WRITE (IOUTUPRT, 800)
        134 |  800     FORMAT ('0', 'INPUT END-OF-FILE ENCOUNTERED PREMATURELY ',
        135 |     1                 'WHEN READING RESTART FILE '/
        136 |     2            '0', 11X, 'EXECUTION TERMINATING.' /////)
        137 |C
        138 |      STOP
        139 |      END
 
