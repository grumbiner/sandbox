          1 |       SUBROUTINE GETLALO(RLATS,RLONS,WGTS,
          2 |     *                    JCAP,NLATH,NLON,NLONEX,
          3 |     *       IRS,IIS,AP,BP,AQR,BQR,GR,SLAT,CLAT,PE0,QE0,RO0,
          4 |     *       TRIGSout,IFAX)
          5 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          6 |C                .      .    .                                       .
          7 |C SUBPROGRAM:    GETLALO    RETURN GAUSSIAN LATS AND LONS AND TRANCONS
          8 |C   PRGMMR: PARRISH          ORG: W/NMC22    DATE: 90-09-21
          9 |C
         10 |C ABSTRACT: RETURN GAUSSIAN LATS, LONS, AND TRANSFORM STUFF.
         11 |C
         12 |C PROGRAM HISTORY LOG:
         13 |C   90-09-21  PARRISH
         14 |C
         15 |C USAGE:  CALL GETLALO(RLATS,RLONS,WGTS,
         16 |C    *                    JCAP,NLATH,NLON,NLONEX,
         17 |C    *       IRS,IIS,AP,BP,AQR,BQR,GR,SLAT,CLAT,PE0,QE0,RO0,TRIGS,IFAX)
         18 |C   INPUT ARGUMENT LIST:
         19 |C     JCAP     - TRIANGULAR TRUNCATION
         20 |C     NLATH    - NUMBER OF GAUSSIAN LATS IN ONE HEMISPHERE
         21 |C     NLON     - NUMBER OF LONGITUDES
         22 |C     NLONEX   - GE NLON, TO ALLOW FOR SOME REPEAT LONGITUDES
         23 |C
         24 |C   OUTPUT ARGUMENT LIST:
         25 |C     RLATS    - RLATS(NLATH*2): GRID LATITUDES (RADIANS)
         26 |C     RLONS    - RLONS(NLONEX): GRID LONGITUDES (RADIANS)
         27 |C     WGTS     - WGTS(NLATH*2): GAUSSIAN INTEGRATION WEIGHTS
         28 |C     IRS      - IRS(0:JCAP,0:JCAP): SPECTRAL COEF
         29 |C     IIS      - IIS(0:JCAP,0:JCAP):    GATHER ARRAYS
         30 |C     AP       - AP(0:JCAP,0:JCAP):  RECURSION
         31 |C     BP       - BP(0:JCAP,0:JCAP):    CONSTANTS
         32 |C     AQR      - AQR(0:JCAP,0:JCAP):     FOR GENERATING
         33 |C     BQR      - BQR(0:JCAP,0:JCAP):       VECTOR SPHERICAL
         34 |C     GR       - GR(0:JCAP,0:JCAP):          HARMONICS
         35 |C     SLAT     - SLAT(NLATH):  SIN(GAUSSIAN LATITUDES)
         36 |C     CLAT     - CLAT(NLATH):  COS(GAUSSIAN LATS)
         37 |C     PE0      - PE0(NLATH,0:JCAP): P(L,L)
         38 |C     QE0      - QE0(NLATH,0:JCAP): Q(L,L)
         39 |C     RO0      - RO0(NLATH,0:JCAP): R(L,L)
         40 |C     TRIGS    - TRIGS(NLON*2)
         41 |C     IFAX     - IFAX(10)
         42 |C
         43 |C ATTRIBUTES:
         44 |C   LANGUAGE: CFT77
         45 |C   MACHINE:  CRAY YMP
         46 |C
         47 |C$$$
         48 |         implicit double precision (a-h,o-z)
         49 |         DIMENSION RLATS(NLATH*2),WGTS(NLATH*2),RLONS(NLONEX)
         50 |         DIMENSION W1(NLATH-1),W2(NLATH-1),W3(NLATH-1),W4(NLATH-1)
         51 |         DIMENSION IRS(0:JCAP,0:JCAP),IIS(0:JCAP,0:JCAP)
         52 |         DIMENSION AP(0:JCAP,0:JCAP),BP(0:JCAP,0:JCAP)
         53 |         DIMENSION AQR(0:JCAP,0:JCAP),BQR(0:JCAP,0:JCAP)
         54 |         DIMENSION GR(0:JCAP,0:JCAP)
         55 |         DIMENSION SLAT(NLATH),CLAT(NLATH)
         56 |         real*4 TRIGSout(NLON*2)
         57 |         integer IFAX(10)
         58 |         DIMENSION PE0(NLATH,0:JCAP),QE0(NLATH,0:JCAP),RO0(NLATH,0:JCAP)
         59 |
         60 |         dimension trigs(nlon*2)
         61 |C--------
         62 |C-------- GET STUFF FOR FFT*S
         63 |C--------
         64 |         CALL M1FAX(IFAX,NLON,3)
         65 |         CALL M1FTRG(TRIGS,NLON,3)
         66 |         trigsout=trigs
         67 |C--------
         68 |C-------- COMPUTE RECURSION CONSTANTS AND INITIALIZE LEGENDRE FUNCTIONS
         69 |C--------
         70 |         CALL M1RCONS(AP,BP,AQR,BQR,GR,JCAP)
         71 |C--------
         72 |C-------- GET LATITUDES, LONGITUDES, AND INTEGRATION WEIGHTS.
         73 |C--------
         74 |         PIH=2.d0*ATAN(1.d0)
         75 |         CALL M1GLAT(NLATH-1,W1,W2,W3,W4)
         76 |         RLATS(1)=-PIH
         77 |         WGTS(1)=0.d0
         78 |CDIR$ IVDEP
         79 |         DO 100 I=1,NLATH-1
         80 |           RLATS(I+1)=W1(I)-PIH
         81 |           WGTS(I+1)=W2(I)
         82 |100      CONTINUE
         83 |         DO 200 I=1,NLATH
         84 |           RLATS(NLATH*2+1-I)=-RLATS(I)
         85 |           WGTS(NLATH*2+1-I)=WGTS(I)
         86 |           SLAT(I)=SIN(RLATS(I))
         87 |           CLAT(I)=COS(RLATS(I))
         88 |200      CONTINUE
         89 |         DLON=8.d0*ATAN(1.d0)/NLON
         90 |         DO 300 I=1,NLONEX
         91 |           RLONS(I)=(I-1.d0)*DLON
         92 |300      CONTINUE
         93 |         II=0
         94 |         IRS=0
         95 |         IIS=0
         96 |         DO 500 M=0,JCAP
         97 |           DO 400 L=0,JCAP-M
         98 |             II=II+1
         99 |             IRS(M+L,L)=II
        100 |             II=II+1
        101 |             IIS(M+L,L)=II
        102 |400        CONTINUE
        103 |500      CONTINUE
        104 |C--------
        105 |C-------- FINALLY GET INITIAL P,Q,R.
        106 |C--------
        107 |         CALL M1IPQR(PE0,QE0,RO0,SLAT,CLAT,NLATH,JCAP)
        108 |       RETURN
        109 |       END
 
