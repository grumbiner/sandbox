          1 |      SUBROUTINE SHLLET (IDATE, SI, LATG2, LONF, LEVS, LEVH,
          2 |     1                   ITUNIT, XLAT)
          3 |C$$$  SUBPROGRAM DOCUMENTATION  BLOCK
          4 |C                .      .    .                                       .
          5 |C SUBPROGRAM:    SHLLET      ID-RECORD FOR LOLA SIGMA FILE
          6 |C   PRGMMR: HOKE             ORG: W/NMC22    DATE: 88-03-31
          7 |C
          8 |C ABSTRACT: CREATES THE 700 WORD SPECIFICATION
          9 |C   FILE USED FOR CONTROL AND HISTORICAL
         10 |C   PURPOSES BY THE NGM INPUT CODE.
         11 |C
         12 |C PROGRAM HISTORY LOG:
         13 |C   84-06-07  JIM HOKE
         14 |C   88-03-31  DIMEGO      HALF PRECISION FOR ALL BUT
         15 |C   90-05-21  DIMEGO      CRAY CONVERSION
         16 |C
         17 |C USAGE:    CALL SHLLET(IDATE,SI,LATG2,LONF,LEVS,LEVH,
         18 |C                       ITUNIT,XLAT)
         19 |C
         20 |C   INPUT ARGUMENT LIST:
         21 |C     IDATE    - THE DATE/TIME GROUP.  (SELA CONVENTION.)
         22 |C              -  1)  HOUR OF THE DAY. (1200 GMT IS 12.)
         23 |C              -  2)  MONTH OF THE YEAR.
         24 |C              -  3)  DAY OF THE MONTH.
         25 |C              -  4)  YEAR OF THE CENTURY.
         26 |C     SI       -  THE SIGMA VALUES FOR THE INTERFACES STARTING
         27 |C              -  FROM THE GROUND, WITH SIGMA DECREASING UPWARD.
         28 |C              -  SI IS     F U L L   P R E C I S I O N  * * * *
         29 |C     LATG2    -  THE NUMBER OF NORTHERN HEMISPHERIC LATITUDINAL
         30 |C              -  STRIPS FROM THE TROPICS TOWARD THE NORTH POLE.
         31 |C     LONF     -  THE NUMBER OF NONDUPLICATE LONGITUDES.
         32 |C     LEVS     -  THE NUMBER OF LEVELS FOR U, V, AND TV.
         33 |C     LEVH     -  THE NUMBER OF LEVELS FOR Q.
         34 |C     ITUNIT   -  THE OUTPUT UNIT NUMBER FOR THE IDENTIFICATION
         35 |C              -  (SPECIFICATIONS) RECORD.
         36 |C     XLAT     -  THE LATITUDES IN DEGREES OF THE LATITUDINAL
         37 |C              -  STRIPS, STARTING NEAR THE EQUATOR.
         38 |C
         39 |C   OUTPUT FILES:
         40 |C     ITUNIT   - THE SPECIFICATIONS FILE AS IS READ BY
         41 |C              - THE NGM INPUT CODE
         42 |C     UNIT6    - LISTING OF THE SPECIFICATIONS FILE
         43 |C
         44 |C ATTRIBUTES:
         45 |C   LANGUAGE: STANDARD FORTRAN
         46 |C   MACHINE:
         47 |C
         48 |C$$$
         49 |C
         50 |      IMPLICIT REAL (A-H,O-Z)
         51 |      REAL SI
         52 |C
         53 |C     THE ACTUAL DIMENSIONS ARE XLAT(LATG2) AND SI(LEVS+1).
         54 |      DIMENSION XLAT(*), SI(*)
         55 |      DIMENSION SPECS(700)
         56 |      DIMENSION IDATE(4)
         57 |C
         58 |C     FIRST THE RECORD IS INITIALIZED TO ZERO.
         59 |      DO 88890 IQ2W6E=1,700
         60 |         SPECS(IQ2W6E)=0.0 E+0
         61 |88890 CONTINUE
         62 |C
         63 |C     HOUR OF THE DAY, IN GMT.
         64 |      SPECS(2) = (IDATE(1))
         65 |C
         66 |C     DAY OF THE MONTH.
         67 |      SPECS(3) = (IDATE(3))
         68 |C
         69 |C     MONTH OF THE YEAR.
         70 |      SPECS(4) = (IDATE(2))
         71 |C
         72 |C     YEAR OF THE CENTURY.
         73 |      SPECS(5) = (IDATE(4))
         74 |C
         75 |C     RESET FORECAST HOUR.
         76 |      SPECS(233) = 0. E+0
         77 |C
         78 |C     NUMBER OF NORTHERN HEMISPHERIC LATITUDINAL STRIPS.
         79 |      SPECS(10) = (LATG2)
         80 |C
         81 |C     TOTAL NUMBER OF LATITUDINAL STRIPS.
         82 |      SPECS(11) = (LATG2)
         83 |C
         84 |C     NUMBER OF NONDUPLICATE GRID POINTS ALONG A LATITUDINAL STRIP.
         85 |      SPECS(12) = (LONF)
         86 |C
         87 |C     NUMBER OF VERTICAL LEVELS FOR WIND AND VIRTUAL TEMPERATURE.
         88 |      SPECS(13) = (LEVS)
         89 |C
         90 |C     NUMBER OF VERTICAL LEVELS FOR THE HUMIDITY FIELD.
         91 |      SPECS(14) = (LEVH)
         92 |C
         93 |C
         94 |C     THE SIGMA VALUES FOR THE LAYERS ARE INSERTED.
         95 |      DO 275 L=1,LEVS
         96 |         SPECS(21 + L) = (SI(L) - SI(L+1))
         97 |  275 CONTINUE
         98 |C
         99 |C     LATITUDES OF THE LATITUDINAL STRIPS.
        100 |C     ORDER IS FROM LOW LATITUDE POLEWARD.
        101 |C     THE LATITUDES ARE IN UNITS OF DEGREES IN ARRAY SPECS.
        102 |      DO 400 J=1, LATG2
        103 |         SPECS(500 + J) = XLAT(J)
        104 |  400 CONTINUE
        105 |C
        106 |C     WRITE THE SPECS ARRAY TO DISK.
        107 |      WRITE (ITUNIT) SPECS
        108 |      ENDFILE ITUNIT
        109 |C
        110 |C     PRINT THE CONTENTS OF THE SPECS ARRAY.
        111 |      WRITE (6, 1200)
        112 | 1200 FORMAT ('0', 'THE VALUES IN THE SPECS ARRAY ARE:' /)
        113 |C
        114 |      I1 = -6
        115 |      I2 =  0
        116 |      DO 1400 LINE=1,100
        117 |         I1 = I1 + 7
        118 |         I2 = I2 + 7
        119 |         WRITE (6, 1300) I1, (SPECS(I), I=I1, I2), I2
        120 | 1300    FORMAT (' ', 'I=', I3, ',',E15.6, 6(E16.6),
        121 |     1                ', I=', I3)
        122 | 1400 CONTINUE
        123 |C
        124 |C     TASK IS DONE.  RETURN TO CALLING PROGRAM.
        125 |C
        126 |      RETURN
        127 |C
        128 |      END
 
