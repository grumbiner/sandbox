          3 |      SUBROUTINE TASKB (INDEXCD, INUDRAG)
          4 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          5 |C                .      .    .                                       .
          6 |C SUBPROGRAM:    TASKB       INTERPOLATED DRAG COEFFICIENT TO NGM GRID
          7 |C   PRGMMR: JIM HOKE         ORG: W/NMC22    DATE: 85-03-27
          8 |C
          9 |C ABSTRACT: INTERPOLATES HORIZONTALLY THE DRAG COEFFICIENT TO
         10 |C   THE NGM FORECAST GRID POINTS FROM THE LATITUDE-LONGITUDE GRIDS
         11 |C
         12 |C PROGRAM HISTORY LOG:
         13 |C   85-03-27  JIM HOKE
         14 |C   88-08-03  BRIAN SCHMIDT ADDED THE DOCBLOCK
         15 |C
         16 |C USAGE:    CALL TASKB (INDEXCD, INUDRAG)
         17 |C   INPUT ARGUMENT LIST:
         18 |C     INDEXCD  - ADDRESS REFERENCE FOR THE POSITION WHERE THE
         19 |C                DRAG COEFFICIENTS ARE TO BE LOCATED IN ARRAY VBL
         20 |C     INUDRAG  - FILE NUMBER OF THE DRAG COEFFICIENTS
         21 |C
         22 |C   INPUT FILES:
         23 |C     INUDRAG  - DRAG COEFFICIENT FILE
         24 |C
         25 |C   SUBPROGRAMS CALLED:
         26 |C     LIBRARY:
         27 |C       COMMON   - DUMMY2
         28 |C
         29 |C REMARKS: THE FIELD IS ASSUMED TO BE AN EVEN FUNCTION OF
         30 |C   LATITUDE (WITH BLENDING OF NORTHERN AND SOUTHERN HEMISPHERIC
         31 |C   VALUES ALREADY COMPLETED), SO THAT SOUTHERN HEMISPHERIC VALUES
         32 |C   ARE SET EQUAL TO THEIR NORTHERN HEMISPHERIC COUNTERPARTS.
         33 |C
         34 |C ATTRIBUTES:
         35 |C   LANGUAGE: CYBER FORTRAN 200
         36 |C   MACHINE:  CYBER 205
         37 |C
         38 |C$$$
         39 |C
         40 |      IMPLICIT REAL (A-H, O-Z)
         41 |C
         42 |C
         43 |      INCLUDE 'parmodel'
  1       1 |C     PARAMETER FILE
  1       2 |C
  1       3 |C     SETUP FOR IBM SINGLE PRECISION REPRESENTATION
  1       4 |C     ON OUTPUT
  1       5 |C                      THIS VERSION 1 May 1991 (DiMego)
  1       6 |C
  1       7 |      PARAMETER (
  1       8 |     *      IBPW      =      4,
  1       9 |     *      IBPOR     = 120000,
  1      10 |     *      INUMREC   = IBPOR / IBPW,
  1      11 |     *      INSPECS   =    700,
  1      12 |     *      ILATG2    =    120,
  1      13 |     *      ILEVH     =     12,
  1      14 |     *      ILEVS     =     16,
  1      15 |     *      ILONF     =    360,
  1      16 |     *      INLATSH   =      5,
  1      17 |     *      IIJMAX    =  23668,
  1      18 |     *      IIJMAXB   =  25536,
  1      19 |     *      IIJRAD    =    892,
  1      20 |     *      IJCAP     =     80,
  1      21 |     *      IWPCPL    =     80,
  1      22 |     *      IKM       =     16 )
  1      23 |      PARAMETER (
  1      24 |     *      ILBCH     =   7*(147+111-7),
  1      25 |     *      ILBHI     =    (147+111-26),
  1      26 |     *      ILBUI     =    (2*147+111-39),
  1      27 |     *      ILBUO     =   (8*147+7*111-56),
  1      28 |     *      ILBVI     =    (147+2*111-39),
  1      29 |     *      ILBVO     =   (7*147+8*111-56),
  1      30 |     *      ILMOUT    =     20 )
  1      31 |      PARAMETER (
  1      32 |     *      IMAXOUTF  =     17,
  1      33 |     *      IMAXOUTT  =      5,
  1      34 |     *      IMAXOUTV  =    100,
  1      35 |     *      IMAXPROF  =      6,
  1      36 |     *      INFILLER  =2500000,
  1      37 |     *      INH       =     65,
  1      38 |     *      INGRDUSE  =      2,
  1      39 |     *      INGRDUS1  =      1,
  1      40 |     *      INIADDRS  =     44,
  1      41 |     *      INSCR     =     23,
  1      42 |     *      INSCRGEO  =     12,
  1      43 |     *      INSCR3    =     10,
  1      44 |     *      INSOIL    =      2,
  1      45 |     *      INSUMG    =  42164,
  1      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  1      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  1      48 |     *      INSHEM    = 13 * INH + 23,
  1      49 |     *      IIJKMAX   = IKM * IIJMAX )
  1      50 |      PARAMETER (
  1      51 |     *  IIWORDSB  = IIJMAXB / 64,
  1      52 |     *  IKMP1     = IKM + 1,
  1      53 |     *  ILVLTOT   = 7 * IKM + 37,
  1      54 |     *  ILONFP1   = ILONF + 1,
  1      55 |     *  INVBL     = ILVLTOT * INSUMG,
  1      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  1      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  1      58 |     *  IBLK1D    =  2 * IKM + 14,
  1      59 |     *  IBLK2D    = 16 * IKM + 36,
  1      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  1      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  1      62 |C
         44 |C
         45 |C
         46 |      COMMON /DUMMY2/ ANAL( ILONFP1 ,  INLAT ,  ILEVS )
         47 |C
         48 |      DIMENSION CD(361, 91)
         49 |      DIMENSION ALONG(361)
         50 |      DIMENSION ALAT(91)
         51 |C
         52 |      EQUIVALENCE (ANAL(1,1,1), CD(1,1))
         53 |C
         54 |C
         55 |C     INTERPOLATE THE DRAG COEFFICIENT ON A 1-DEGREE-BY-1-DEGREE
         56 |C     GRID TO THE NGM GRIDS.
         57 |C     I = 1 CORRESPONDS TO THE GREENWICH MERIDIAN.
         58 |      IMAX = 361
         59 |      JMAX = 91
         60 |      KMAX = 1
         61 |      ISYM = 2
         62 |      FACTOR = 1.0E0
         63 |C
         64 |C     FIRST, SPECIFY THE LOCATIONS OF THE GRID IN LATITUDE AND
         65 |C     LONGITUDE.
         66 |      DO 1100 I=1,IMAX
         67 |         ALONG(I) = REAL(I-1)
         68 | 1100 CONTINUE
         69 |C
         70 |      DO 1200 J=1,JMAX
         71 |         ALAT(J) = REAL(J-1)
         72 | 1200 CONTINUE
         73 |C
         74 |C     INPUT THE DRAG COEFFICIENTS.
         75 |      READ (INUDRAG) CD
         76 |C
         77 |      CALL STATS (CD, 1,IMAX,IMAX, 1,JMAX,JMAX, 1,KMAX,KMAX,
         78 |     1            'INPUT DRAG COEF ')
         79 |C
         80 |      CALL INTERP(CD, IMAX, JMAX, KMAX, ALONG, ALAT,
         81 |     1            ISYM, INDEXCD, FACTOR)
         82 |C
         83 |C
         84 |      RETURN
         85 |C
         86 |      END
 
