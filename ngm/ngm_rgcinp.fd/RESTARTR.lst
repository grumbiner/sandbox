          1 |      SUBROUTINE RESTARTR(IOUTURST, IOUTUPRT, SPECS, VBL,
          2 |     1                    BITSEA, BITSNO, BITWVL)
          3 |C
          4 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          5 |C
          6 |C SUBPROGRAM:    RESTARTR    READS RESTART FILE
          7 |C   PRGMMR: J TUCCILLO       ORG: W/NMC4     DATE: 90-06-19
          8 |C
          9 |C ABSTRACT:    READ RESTART FILE WITH A MAXIMUM RECORD LENGTH OF
         10 |C              INUMREC ELEMENTS
         11 |C
         12 |C PROGRAM HISTORY LOG:
         13 |C   90-06-19  J TUCCILLO
         14 |C   98-11-19  E ROGERS REPLACED READ OF ON85 LABEL IN FIRST RECORD
         15 |C             WITH READ ON AN BINARY NCEP DATE WORD
         16 |C
         17 |C USAGE:  CALL RESTARTR(IOUTURST, IOUTUPRT, SPECS, VBL,
         18 |C                         BITSEA, BITSNO, BITWVL)
         19 |C   INPUT ARGUMENTS:
         20 |C     IOUTURST  -  UNIT NUMBER OF RESTART FILE
         21 |C     IOUTUPRT  -  UNIT NUMBER OF OUTPUT FILE
         22 |C     SPECS     -  SPECIFICATION FILE
         23 |C     VBL       -  CONTAINS NGM MODEL FIELDS
         24 |C     BITSEA    -  CONTAINS UNFROZEN WATER LOGICAL MAP
         25 |C     BITSNO    -  CONTAINS SNOW COVER LOGICAL MAP
         26 |C     BITWVL    -  CONTAINS WATER VS. LAND LOGICAL MAP
         27 |C
         28 |C ATTRIBUTES:
         29 |C   LANGUAGE: FORTRAN
         30 |C   MACHINE:  CRAY Y-MP
         31 |C
         32 |C$$$
         33 |C
         34 |C
         35 |      INCLUDE 'parmodel'
  1       1 |C     PARAMETER FILE
  1       2 |C
  1       3 |C     SETUP FOR IBM SINGLE PRECISION REPRESENTATION
  1       4 |C     ON OUTPUT
  1       5 |C                      THIS VERSION 1 May 1991 (DiMego)
  1       6 |C
  1       7 |      PARAMETER (
  1       8 |     *      IBPW      =      4,
  1       9 |     *      IBPOR     = 120000,
  1      10 |     *      INUMREC   = IBPOR / IBPW,
  1      11 |     *      INSPECS   =    700,
  1      12 |     *      ILATG2    =    120,
  1      13 |     *      ILEVH     =     12,
  1      14 |     *      ILEVS     =     16,
  1      15 |     *      ILONF     =    360,
  1      16 |     *      INLATSH   =      5,
  1      17 |     *      IIJMAX    =  23668,
  1      18 |     *      IIJMAXB   =  25536,
  1      19 |     *      IIJRAD    =    892,
  1      20 |     *      IJCAP     =     80,
  1      21 |     *      IWPCPL    =     80,
  1      22 |     *      IKM       =     16 )
  1      23 |      PARAMETER (
  1      24 |     *      ILBCH     =   7*(147+111-7),
  1      25 |     *      ILBHI     =    (147+111-26),
  1      26 |     *      ILBUI     =    (2*147+111-39),
  1      27 |     *      ILBUO     =   (8*147+7*111-56),
  1      28 |     *      ILBVI     =    (147+2*111-39),
  1      29 |     *      ILBVO     =   (7*147+8*111-56),
  1      30 |     *      ILMOUT    =     20 )
  1      31 |      PARAMETER (
  1      32 |     *      IMAXOUTF  =     17,
  1      33 |     *      IMAXOUTT  =      5,
  1      34 |     *      IMAXOUTV  =    100,
  1      35 |     *      IMAXPROF  =      6,
  1      36 |     *      INFILLER  =2500000,
  1      37 |     *      INH       =     65,
  1      38 |     *      INGRDUSE  =      2,
  1      39 |     *      INGRDUS1  =      1,
  1      40 |     *      INIADDRS  =     44,
  1      41 |     *      INSCR     =     23,
  1      42 |     *      INSCRGEO  =     12,
  1      43 |     *      INSCR3    =     10,
  1      44 |     *      INSOIL    =      2,
  1      45 |     *      INSUMG    =  42164,
  1      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  1      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  1      48 |     *      INSHEM    = 13 * INH + 23,
  1      49 |     *      IIJKMAX   = IKM * IIJMAX )
  1      50 |      PARAMETER (
  1      51 |     *  IIWORDSB  = IIJMAXB / 64,
  1      52 |     *  IKMP1     = IKM + 1,
  1      53 |     *  ILVLTOT   = 7 * IKM + 37,
  1      54 |     *  ILONFP1   = ILONF + 1,
  1      55 |     *  INVBL     = ILVLTOT * INSUMG,
  1      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  1      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  1      58 |     *  IBLK1D    =  2 * IKM + 14,
  1      59 |     *  IBLK2D    = 16 * IKM + 36,
  1      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  1      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  1      62 |C
         36 |C
         37 |C
         38 |      DIMENSION VBL    ( INVBL ),
         39 |     *          SPECS  ( INSPECS ),
         40 |     *          BITSEA ( IIJMAX * INGRDUSE ),
         41 |     *          BITSNO ( IIJMAX * INGRDUSE ),
         42 |     *          BITWVL ( IIJMAX * INGRDUSE )
         43 |C
         44 |      LOGICAL BITSEA, BITSNO, BITWVL
         45 |C
         46 |      DIMENSION INPDAT(8)
         47 |C
         48 |C=======================================================================
         49 |C
         50 |      READ (IOUTURST, ERR=600, END=700) INPDAT
         51 |C
         52 |      ILEFT = MOD ( INSPECS, INUMREC )
         53 |      ILOOP = INSPECS / INUMREC
         54 |      IST   = 1
         55 |      ISP   = INUMREC
         56 |      DO 1 II = 1, ILOOP
         57 |         READ ( IOUTURST, ERR=600, END=700 ) (SPECS(I),I=IST,ISP)
         58 |         IST = ISP + 1
         59 |         ISP = ISP + INUMREC
         60 |1     CONTINUE
         61 |      IF ( ILEFT .GT. 0 ) THEN
         62 |         ISP = INSPECS
         63 |         READ ( IOUTURST, ERR=600, END=700 ) (SPECS(I),I=IST,ISP)
         64 |      END IF
         65 |C
         66 |      ILEFT = MOD ( INVBL, INUMREC )
         67 |      ILOOP = INVBL / INUMREC
         68 |      IST   =     1
         69 |      ISP   = INUMREC
         70 |      DO 2 II = 1, ILOOP
         71 |        READ (IOUTURST, ERR=600, END=700) (VBL(I), I=IST, ISP)
         72 |        IST = ISP + 1
         73 |        ISP = ISP + INUMREC
         74 |2     CONTINUE
         75 |      IF( ILEFT .NE. 0) THEN
         76 |        ISP = INVBL
         77 |        READ (IOUTURST, ERR=600, END=700) (VBL(I), I=IST, ISP)
         78 |      END IF
         79 |C
         80 |C     SAME VALUES FOR ALL LOGICAL MASKS
         81 |C
         82 |      ILEN = IIJMAX * INGRDUSE
         83 |      ILEFT = MOD ( ILEN, INUMREC )
         84 |      ILOOP = ILEN / INUMREC
         85 |C
         86 |C
         87 |      IST   =     1
         88 |      ISP   = INUMREC
         89 |      DO 3 II = 1, ILOOP
         90 |        READ (IOUTURST, ERR=600, END=700) (BITSEA(I), I=IST, ISP)
         91 |        IST = ISP + 1
         92 |        ISP = ISP + INUMREC
         93 |3     CONTINUE
         94 |      IF( ILEFT .NE. 0) THEN
         95 |        ISP = ILEN
         96 |        READ (IOUTURST, ERR=600, END=700) (BITSEA(I), I=IST, ISP)
         97 |      END IF
         98 |C
         99 |      IST   =     1
        100 |      ISP   = INUMREC
        101 |      DO 4 II = 1, ILOOP
        102 |        READ (IOUTURST, ERR=600, END=700) (BITSNO(I), I=IST, ISP)
        103 |        IST = ISP + 1
        104 |        ISP = ISP + INUMREC
        105 |4     CONTINUE
        106 |      IF( ILEFT .NE. 0) THEN
        107 |        ISP = ILEN
        108 |        READ (IOUTURST, ERR=600, END=700) (BITSNO(I), I=IST, ISP)
        109 |      END IF
        110 |C
        111 |      IST   =     1
        112 |      ISP   = INUMREC
        113 |      DO 5 II = 1, ILOOP
        114 |        READ (IOUTURST, ERR=600, END=700) (BITWVL(I), I=IST, ISP)
        115 |        IST = ISP + 1
        116 |        ISP = ISP + INUMREC
        117 |5     CONTINUE
        118 |      IF( ILEFT .NE. 0) THEN
        119 |        ISP = ILEN
        120 |        READ (IOUTURST, ERR=600, END=700) (BITWVL(I), I=IST, ISP)
        121 |      END IF
        122 |C
        123 |      RETURN
        124 |C
        125 |C*****READ ERROR
        126 |C
        127 |  600     WRITE (IOUTUPRT, 650)
        128 |  650     FORMAT ('0', 'ERROR WHILE READING RESTART FILE '/
        129 |     1            '0', 11X, 'EXECUTION TERMINATING.' /////)
        130 |C
        131 |      STOP
        132 |C
        133 |C*****END OF FILE
        134 |C
        135 |  700     WRITE (IOUTUPRT, 800)
        136 |  800     FORMAT ('0', 'INPUT END-OF-FILE ENCOUNTERED PREMATURELY ',
        137 |     1                 'WHEN READING RESTART FILE '/
        138 |     2            '0', 11X, 'EXECUTION TERMINATING.' /////)
        139 |C
        140 |      STOP
        141 |      END
 
