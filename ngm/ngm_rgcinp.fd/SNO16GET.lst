          1 |      SUBROUTINE SNO16GET(SCVH,IYR,IMN,IDY,   INSNOIMS, LIST)
          2 |C
          3 |C ABSTRACT: READS THE NESDIS IMS DAILY SNOW/ICE ANALYSIS ON THE
          4 |C           1/16TH MESH.
          5 |C           SCVH = 0 --- NO SNOW OR ICE
          6 |C                  1 --- WITH SNOW/ICE
          7 |C
          8 |C  INPUT ARGUMENTS:
          9 |C     INSNOSAB - UNIT NO. FOR NESDIS WEEKLY SNOW/ICE ANAL
         10 |C     LIST     - UNIT NO. FOR PRINT
         11 |C  OUTPUT ARGUMENTS:
         12 |C     SCVH - REAL SNOW/ICE ANAL FLAGS OF ZEROE'S AND ONE'S
         13 |C     IYR  - YEAR  OF ANAL
         14 |C     IMN  - MONTH OF ANAL
         15 |C     IDY  - DAY   OF ANAL
         16 |C
         17 |      REAL SCVH(1024,1024), ICE(1024,1024)
         18 |      LOGICAL BITMAP(1024,1024)
         19 |      INTEGER KPDS(25), KGDS(22), JPDS(25), JGDS(22)
         20 |C
         21 |      CALL BAOPEN(INSNOIMS,'fort.22',IRETO)
         22 |      JPDS = -1
         23 |      CALL GETGB(INSNOIMS,0,1024*1024,0,JPDS,JGDS,KF,KNUM,KPDS,KGDS,
         24 |     &     BITMAP,ICE,IRET1)
         25 |      WRITE(6,*) 'AFTER GETGB FOR IMS ICE, IRET1=', IRET1
         26 |C
         27 |      JPDS = -1
         28 |      CALL GETGB(INSNOIMS,0,1024*1024,1,JPDS,JGDS,KF,KNUM,KPDS,KGDS,
         29 |     &     BITMAP,SCVH,IRET2)
         30 |      WRITE(6,*) 'AFTER GETGB FOR IMS SNOW, IRET2=', IRET2
         31 |C
         32 |      IF (IRET1.NE.0 .OR. IRET2.NE.0) THEN
         33 |         WRITE(LIST,*) 'READ ERROR FOR IMS SNOW/ICE, IRET1=',IRET1,
         34 |     &      ' IRET2=', IRET2
         35 |         CALL EXIT(16)
         36 |      ENDIF
         37 |C
         38 |      IYR2D = KPDS(8)
         39 |      IMN = KPDS(9)
         40 |      IDY = KPDS(10)
         41 |      ICENT = KPDS(21)
         42 |      IF(IYR2D.LT.100) THEN
         43 |       IYR = (ICENT - 1) * 100 + IYR2D
         44 |      ELSE
         45 |       IYR = ICENT * 100
         46 |      ENDIF
         47 |C
         48 |      DO 20 I = 1, 1024
         49 |        DO 10 J = 1, 1024
         50 |           IF (ICE(I,J).GT.0. .OR. SCVH(I,J) .GT.0.) THEN
         51 |              SCVH(I,J) = 1.
         52 |           ELSE
         53 |              SCVH(I,J) = 0.
         54 |           ENDIF
         55 | 10     CONTINUE
         56 | 20   CONTINUE
         57 |      RETURN
         58 |      END
 
