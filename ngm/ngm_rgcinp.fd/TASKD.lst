          1 |C
          2 |C
          3 |C
          4 |      SUBROUTINE TASKD
          5 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          6 |C                .      .    .                                       .
          7 |C SUBPROGRAM:    TASKD       INTERPOLATES QUANTITIES TO NGM GRID PTS
          8 |C   PRGMMR: JIM HOKE         ORG: W/NMC/22    DATE: 85-03-27
          9 |C
         10 |C ABSTRACT: INTERPOLATES HORIZONTALLY THE SURFACE PRESSURE,
         11 |C   U AND V WIND COMPONENTS, VIRTUAL TEMPERATURE, SPECIFIC
         12 |C   HUMIDITY, AND TERRAIN HEIGHT TO THE NGM GRID POINTS.
         13 |C   THE INTERPOLATION IS DONE ON SIGMA SURFACES.
         14 |C   THE INPUT FIELDS ARE IN ARRAY STRIPARA, WHICH CONTAINS
         15 |C   LATITUDE-LONGITUDE GRIDDED FIELDS ON LATITUDINAL STRIPS ON
         16 |C   SIGMA SURFACES.
         17 |C
         18 |C PROGRAM HISTORY LOG:
         19 |C   85-03-27  JIM HOKE
         20 |C   88-08-03  BRIAN SCHMIDT ADDED DOCBLOCK
         21 |C   90-07-01  KEN MITCHELL CONVERTED TO CRAY Y-MP
         22 |C
         23 |C USAGE:    CALL TASKD
         24 |C
         25 |C   SUBPROGRAMS CALLED:
         26 |C     UNIQUE:    - CULL
         27 |C                  GRDSET
         28 |C                  INTERP
         29 |C                  GRDSET
         30 |C     LIBRARY:
         31 |C       COMMON   - CONSTS
         32 |C                  CONSTN
         33 |C                  DUMMY2
         34 |C                  BLANK (CONTAINS ARRAY VBL)
         35 |C
         36 |C REMARKS: THE FIELDS ARE ASSUMED TO HAVE HAD THE EQUATORIAL
         37 |C   SYMMETRY CONDITION ALREADY ALLIED TO THEM, SO THAT
         38 |C   NO SYMMETRY CONDITION IS NEEDED IN THIS SUBROUTINE FOR
         39 |C   THE HORIZONTAL INTERPOLATION.
         40 |C
         41 |C ATTRIBUTES:
         42 |C   LANGUAGE: CYBER FORTRAN 200
         43 |C   MACHINE:  CYBER 205
         44 |C
         45 |C$$$
         46 |C
         47 |C
         48 |      IMPLICIT REAL (A-H, O-Z)
         49 |C
         50 |C
         51 |      INCLUDE 'parmodel'
  1       1 |C     PARAMETER FILE
  1       2 |C
  1       3 |C     SETUP FOR IBM SINGLE PRECISION REPRESENTATION
  1       4 |C     ON OUTPUT
  1       5 |C                      THIS VERSION 1 May 1991 (DiMego)
  1       6 |C
  1       7 |      PARAMETER (
  1       8 |     *      IBPW      =      4,
  1       9 |     *      IBPOR     = 120000,
  1      10 |     *      INUMREC   = IBPOR / IBPW,
  1      11 |     *      INSPECS   =    700,
  1      12 |     *      ILATG2    =    120,
  1      13 |     *      ILEVH     =     12,
  1      14 |     *      ILEVS     =     16,
  1      15 |     *      ILONF     =    360,
  1      16 |     *      INLATSH   =      5,
  1      17 |     *      IIJMAX    =  23668,
  1      18 |     *      IIJMAXB   =  25536,
  1      19 |     *      IIJRAD    =    892,
  1      20 |     *      IJCAP     =     80,
  1      21 |     *      IWPCPL    =     80,
  1      22 |     *      IKM       =     16 )
  1      23 |      PARAMETER (
  1      24 |     *      ILBCH     =   7*(147+111-7),
  1      25 |     *      ILBHI     =    (147+111-26),
  1      26 |     *      ILBUI     =    (2*147+111-39),
  1      27 |     *      ILBUO     =   (8*147+7*111-56),
  1      28 |     *      ILBVI     =    (147+2*111-39),
  1      29 |     *      ILBVO     =   (7*147+8*111-56),
  1      30 |     *      ILMOUT    =     20 )
  1      31 |      PARAMETER (
  1      32 |     *      IMAXOUTF  =     17,
  1      33 |     *      IMAXOUTT  =      5,
  1      34 |     *      IMAXOUTV  =    100,
  1      35 |     *      IMAXPROF  =      6,
  1      36 |     *      INFILLER  =2500000,
  1      37 |     *      INH       =     65,
  1      38 |     *      INGRDUSE  =      2,
  1      39 |     *      INGRDUS1  =      1,
  1      40 |     *      INIADDRS  =     44,
  1      41 |     *      INSCR     =     23,
  1      42 |     *      INSCRGEO  =     12,
  1      43 |     *      INSCR3    =     10,
  1      44 |     *      INSOIL    =      2,
  1      45 |     *      INSUMG    =  42164,
  1      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  1      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  1      48 |     *      INSHEM    = 13 * INH + 23,
  1      49 |     *      IIJKMAX   = IKM * IIJMAX )
  1      50 |      PARAMETER (
  1      51 |     *  IIWORDSB  = IIJMAXB / 64,
  1      52 |     *  IKMP1     = IKM + 1,
  1      53 |     *  ILVLTOT   = 7 * IKM + 37,
  1      54 |     *  ILONFP1   = ILONF + 1,
  1      55 |     *  INVBL     = ILVLTOT * INSUMG,
  1      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  1      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  1      58 |     *  IBLK1D    =  2 * IKM + 14,
  1      59 |     *  IBLK2D    = 16 * IKM + 36,
  1      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  1      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  1      62 |C
         52 |C
         53 |C
         54 |      COMMON /CONSTS/ IH, IQ, IT, IU, IV, IZ,
         55 |     1                NLAT, LEVH, LEVS, LONF, NSIGSTP,
         56 |     2                SLAT( INLAT ), STRIPARA( INSIGSTP ,  INLAT )
         57 |C
         58 |C       *****   END OF INCLUDED COMMON BLK CONSTS   *****
         59 |C
         60 |C
         61 |      LOGICAL  BITSEA, BITSNO, BITWVL
         62 |C
         63 |      COMMON /CONSTN/ IMG( INGRDUSE ), JMG( INGRDUSE ),
         64 |     1                IAG( INGRDUS1 ), JAG( INGRDUS1 ),
         65 |     2                IBG( INGRDUS1 ), JBG( INGRDUS1 ),
         66 |     3                IADDRG( INIADDRS ,  INGRDUSE ),
         67 |     4                INDEXHU,  INDEXHV,  INDEXHTH, INDEXHQ,
         68 |     5                INDEXH,   INDEXST,  INDEXSQ,  INDEXALB,
         69 |     6                INDEXSLP, INDEXPSI, INDEXCD,  INDEXCC,  INDEXGSP,
         70 |     7                INDEXRAD, INDEXDLW, INDEXDSW, INDEXCPR, INDEXMA,
         71 |     8                INDEXSEC, INDEXZ0, INDEXTSS,  INDEXCG,  INDEXDHQ,
         72 |     9                INDEXTSD, KM, NH, NGRDUSE,
         73 |     1                SPECS( INSPECS ), DELSIG( IKM ),
         74 |     2                POVH( IKM ), PIOVHK( IKM ),
         75 |     3                CMUU  ( INGRDUSE ),
         76 |     4                XPOLEH( INGRDUSE ), YPOLEH( INGRDUSE ),
         77 |     5                XPOLEU( INGRDUSE ), YPOLEU( INGRDUSE ),
         78 |     6                XPOLEV( INGRDUSE ), YPOLEV( INGRDUSE ),
         79 |     7                DLAMNGM,
         80 |     8                BITSEA( IIJMAX ,  INGRDUSE ),
         81 |     9                BITSNO( IIJMAX ,  INGRDUSE ),
         82 |     1                BITWVL( IIJMAX ,  INGRDUSE )
         83 |C
         84 |C       ****   END OF INCLUDED COMMON BLOCK CONSTN   ***
         85 |C
         86 |C
         87 |      COMMON          VBL( INVBL )
         88 |      COMMON /DUMMY2/ ANAL( ILONFP1 ,  INLAT ,  ILEVS )
         89 |C
         90 |      DIMENSION  ALONG( ILONFP1 )
         91 |C
         92 |C
         93 |      IMAX   = LONF + 1
         94 |      JMAX   = NLAT
         95 |      ISYM   = 0
         96 |      DLONG  = 360.E0 / REAL(LONF)
         97 |C
         98 |C     FIRST, SPECIFY THE LOCATIONS OF THE INPUT GRID LONGITUDE.  THE
         99 |C     GRID LATITUDES ARE ALREADY AVAILABLE IN SLAT.
        100 |      DO 100 I=1,IMAX
        101 |         ALONG(I) = REAL(I-1) * DLONG
        102 |  100 CONTINUE
        103 |C
        104 |C     THE SURFACE PRESSURE IS INTERPOLATED FIRST.
        105 |C     THE UNITS ARE CONVERTED FROM CENTIBARS TO BARS.
        106 |      KMAX    = 1
        107 |      FACTOR  = 0.01E0
        108 |C
        109 |      CALL CULL (IH, ANAL, IMAX, JMAX, KMAX)
        110 |C
        111 |      CALL STATS (ANAL, 1,IMAX,IMAX, 1,JMAX,JMAX, 1,KMAX,KMAX,
        112 |     1            'INPUT SFC PRESS ')
        113 |C
        114 |      CALL INTERP (ANAL, IMAX, JMAX, KMAX, ALONG, SLAT, ISYM,
        115 |     1             INDEXH, FACTOR)
        116 |C
        117 |C
        118 |C     THE U WIND COMPONENT IS INTERPOLATED NEXT.
        119 |      KMAX = LEVS
        120 |      FACTOR = 1.0E0
        121 |C
        122 |      CALL CULL (IU, ANAL, IMAX, JMAX, KMAX)
        123 |C
        124 |      CALL STATS (ANAL, 1,IMAX,IMAX, 1,JMAX,JMAX, 1,KMAX,KMAX,
        125 |     1            'INPUT U FIELD   ')
        126 |C
        127 |      CALL INTERP (ANAL, IMAX, JMAX, KMAX, ALONG, SLAT, ISYM,
        128 |     1             INDEXHU, FACTOR)
        129 |C
        130 |C
        131 |C     THE V COMPONENT IS NOW INTERPOLATED.
        132 |      KMAX = LEVS
        133 |      FACTOR = 1.0E0
        134 |C
        135 |      CALL CULL (IV, ANAL, IMAX, JMAX, KMAX)
        136 |      CALL STATS (ANAL, 1,IMAX,IMAX, 1,JMAX,JMAX, 1,KMAX,KMAX,
        137 |     1            'INPUT V FIELD   ')
        138 |C
        139 |      CALL INTERP (ANAL, IMAX, JMAX, KMAX, ALONG, SLAT, ISYM,
        140 |     1             INDEXHV, FACTOR)
        141 |C
        142 |C
        143 |C     NOW THE VIRTUAL TEMPERATURE.
        144 |      KMAX = LEVS
        145 |      FACTOR = 1.0E0
        146 |C
        147 |      CALL CULL (IT, ANAL, IMAX, JMAX, KMAX)
        148 |C
        149 |      CALL STATS (ANAL, 1,IMAX,IMAX, 1,JMAX,JMAX, 1,KMAX,KMAX,
        150 |     1            'INPUT VIRT TEMP ')
        151 |C
        152 |      CALL INTERP (ANAL, IMAX, JMAX, KMAX, ALONG, SLAT, ISYM,
        153 |     1             INDEXHTH, FACTOR)
        154 |C
        155 |C
        156 |C     NOW THE SPECIFIC HUMIDITY.
        157 |C     NOTE THAT THE SPECIFIC HUMIDITY FOR LAYERS LEVH+1 THROUGH LEVS
        158 |C     IS SET TO ZERO. LATER IN SUBROUTINE TASKE, SPECIFIC HUMIDITY
        159 |C     IS EXTRAPOLATED UPWARD THRU THESE LAYERS VIA CALL EXMOIS.
        160 |C
        161 |      KMAX = LEVH
        162 |      FACTOR = 1.0E0
        163 |C
        164 |      CALL CULL (IQ, ANAL, IMAX, JMAX, KMAX)
        165 |C
        166 |      CALL STATS (ANAL, 1,IMAX,IMAX, 1,JMAX,JMAX, 1,KMAX,KMAX,
        167 |     1            'INPUT SPEC HUMID')
        168 |C
        169 |      CALL INTERP (ANAL, IMAX, JMAX, KMAX, ALONG, SLAT, ISYM,
        170 |     1             INDEXHQ, FACTOR)
        171 |C
        172 |      IF (LEVH .LT. LEVS) THEN
        173 |C
        174 |         DO 300 NG=1, NGRDUSE
        175 |            CALL GRDSET (NG, INDEXHQ,
        176 |     1                   MADDRS, IM, JM, DUMMY, DUMMY, DUMMY, DUMMY)
        177 |            IJ = IM * JM
        178 |C
        179 |            JADDRS = MADDRS + (LEVH - 1) * IJ
        180 |            DO 200 K=LEVH+1, LEVS
        181 |               JADDRS = JADDRS + IJ
        182 |                 DO 150  L=1,IJ
        183 |                   VBL(JADDRS+L-1) = 0.0E0
        184 |  150            CONTINUE
        185 |  200       CONTINUE
        186 |C
        187 |  300    CONTINUE
        188 |C
        189 |      END IF
        190 |C
        191 |C
        192 |C     FINALLY THE TERRAIN HEIGHT.
        193 |C     THE TERRAIN HEIGHTS ARE MULTIPLIED BY G/CP BEFORE
        194 |C     PLACEMENT INTO ARRAY VBL.
        195 |      KMAX = 1
        196 |      FACTOR = 9.8E0 / 1005.E0
        197 |C
        198 |      CALL CULL (IZ, ANAL, IMAX, JMAX, KMAX)
        199 |C
        200 |      CALL STATS (ANAL, 1,IMAX,IMAX, 1,JMAX,JMAX, 1,1,KMAX,
        201 |     1            'INPUT GROUND HT ')
        202 |C
        203 |      CALL INTERP (ANAL, IMAX, JMAX, KMAX, ALONG, SLAT,
        204 |     1            ISYM, INDEXPSI, FACTOR)
        205 |C
        206 |      RETURN
        207 |C
        208 |      END
 
