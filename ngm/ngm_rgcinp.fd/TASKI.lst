         73 |     4                INDEXHU,  INDEXHV,  INDEXHTH, INDEXHQ,
         74 |     5                INDEXH,   INDEXST,  INDEXSQ,  INDEXALB,
         75 |     6                INDEXSLP, INDEXPSI, INDEXCD,  INDEXCC,  INDEXGSP,
         76 |     7                INDEXRAD, INDEXDLW, INDEXDSW, INDEXCPR, INDEXMA,
         77 |     8                INDEXSEC, INDEXZ0, INDEXTSS,  INDEXCG,  INDEXDHQ,
         78 |     9                INDEXTSD, KM, NH, NGRDUSE,
         79 |     1                SPECS( INSPECS ), DELSIG( IKM ),
