         83 |     5                XPOLEU( INGRDUSE ), YPOLEU( INGRDUSE ),
         84 |     6                XPOLEV( INGRDUSE ), YPOLEV( INGRDUSE ),
         85 |     7                DLAMNGM,
         86 |     8                BITSEA( IIJMAX ,  INGRDUSE ),
         87 |     9                BITSNO( IIJMAX ,  INGRDUSE ),
         88 |     1                BITWVL( IIJMAX ,  INGRDUSE )
         89 |C
         90 |C       ****   END OF INCLUDED COMMON BLOCK CONSTN   ***
         91 |C
         92 |C
         93 |C     SPECIFY THE EXPECTED SIZE OF THE INPUT SFC AIR TEMPERATURE
         94 |C     CLIMATOLOGY (5 DEG LONG FROM 0 TO 360 WITH WRAP AROUND AND
         95 |C     4 DEG LAT FROM -90 TO 90) AND THE ASSUMED SOIL TEMPERATURE
         96 |C     TIME CONSTANT (LAG) IN DAYS FOR THE NOMINAL 10 CM DEPTH OF
         97 |C     THE NGM SOIL SLAB.
         98 |C
         99 |      PARAMETER (IMAX = 73,
        100 |     *           JMAX = 46,
        101 |     *            LAG = 10)
        102 |C
        103 |C
        104 |      DIMENSION  TSS(IMAX,JMAX),TSS1(IMAX,JMAX)
        105 |     *                         ,TSS2(IMAX,JMAX)
        106 |      DIMENSION  ALONGTSS(IMAX)
        107 |      DIMENSION  ALATTSS (JMAX)
        108 |C
        109 |      DIMENSION  DAYHF(13),RMDAY(12)
        110 |C
        111 |      EQUIVALENCE  (ANAL(1,1,1),  TSS(1,1))
        112 |      EQUIVALENCE  (ANAL(1,1,2), TSS1(1,1))
        113 |      EQUIVALENCE  (ANAL(1,1,3), TSS2(1,1))
        114 |C
        115 |      DATA  LTAB/'TSS1','TSS2'/
        116 |C
        117 |      DATA LMN/'SOIL TEMP MNTH01','SOIL TEMP MNTH02','SOIL TEMP MNTH03',
        118 |     *         'SOIL TEMP MNTH04','SOIL TEMP MNTH05','SOIL TEMP MNTH06',
        119 |     *         'SOIL TEMP MNTH07','SOIL TEMP MNTH08','SOIL TEMP MNTH09',
        120 |     *         'SOIL TEMP MNTH10','SOIL TEMP MNTH11','SOIL TEMP MNTH12'/
        121 |C
        122 |C  SPECIFY JULIAN DAY OF THE MIDDLE OF EACH MONTH.  REPEAT JANUARY
        123 |C  AS A 13TH MONTH TO SIMPLIFY INTERPOLATION IN TIME.  ALSO SPECIFY
        124 |C  ACCUMULATED JULIAN DAYS FOR THE START OF EACH MONTH.
        125 |C
        126 |      DATA  DAYHF/ 15.5,  45.0,  74.5, 105.0, 135.5, 166.0,
        127 |     *            196.5, 227.5, 258.0, 288.5, 319.0, 349.5, 380.5/
        128 |C
        129 |      DATA  RMDAY/  0.0,  31.0,  59.0,  90.0, 120.0, 151.0,
        130 |     *            181.0, 212.0, 243.0, 273.0, 304.0, 334.0/
        131 |C
        132 |C
        133 |C     SPECIFY THE UNIT NUMBER OF THE PRINTER.
        134 |      IOPRT = 6
        135 |C
        136 |C  CONSTRAIN LAG TO WITHIN 0 TO 60 DAYS.  ALGORITHM WILL CORRECTLY
        137 |C  HANDLE LAG UP TO 349 DAYS, BUT WE CHOOSE TO FURTHER RESTRICT LAG
        138 |C  HERE FROM PHYSICAL CONSIDERATIONS.
        139 |C
        140 |      IF ((LAG.LT.0).OR.(LAG.GT.60)) THEN
        141 |C
        142 |         WRITE(IOPRT,40) LAG
        143 |         ERRMSG(1) = 'IN SUBROUTINE TASKI    NEAR STATEME'
        144 |         ERRMSG(2) = 'NT NUMBER 25:                      '
        145 |         ERRMSG(3) = 'THE SPECIFIED TIME LAG IN DAYS FOR '
        146 |         ERRMSG(4) = 'SOIL TEMPERATURE IS OUT OF RANGE.  '
        147 |         CALL ERRSTOP (IOPRT,ERRMSG)
        148 |      END IF
        149 |C
        150 |C     ENSURE THAT DIMENSIONS OF ARRAY ANAL ARE APPROPRIATE FOR
        151 |C     EQUIVALENCE TO ARRAYS TSS, TSS1, TSS2
        152 |C
        153 |      LONFP10 =  ILONFP1
        154 |      NLAT0   =  INLAT
        155 |      LEVS0   =  ILEVS
        156 |      LANLSZ  = LONFP10*NLAT0
        157 |      IJSZTSS = IMAX*JMAX
        158 |C
        159 |      IF ((LEVS0.LT.3).OR.(LANLSZ.LT.IJSZTSS)) THEN
        160 |         WRITE (IOPRT, 50)
        161 |         ERRMSG(1) = 'IN SUBROUTINE TASKI    NEAR STATEME'
        162 |         ERRMSG(2) = 'NT NUMBER 50:                      '
        163 |         ERRMSG(3) = 'THE SIZE OF ARRAY ANAL IS SMALLER T'
        164 |         ERRMSG(4) = 'HAN NECESSARY FOR EQUIVALENCE USED '
        165 |         CALL ERRSTOP (IOPRT,ERRMSG)
        166 |      END IF
        167 |C
        168 |C  SPECIFY THE BASETIME OF INPUT FOR THIS FORECAST.  THEN
        169 |C  COMPUTE RJDAY: THE NO. OF DAYS (FRACTIONAL) TRANSPIRED SINCE
        170 |C  00Z JAN 1ST OF CURRENT YEAR.  (NOTE THIS "JULIAN DAY" DEFINITION
        171 |C  IS PURPOSELY ONE DAY LESS THAN USUAL DEFINITION.)  LASTLY APPLY
        172 |C  SPECIFIED TIME LAG IN DAYS AND COMPUTE JULIAN DAY (RJDLAG) OF THE
        173 |C  TIME "LAG DAYS" EARLIER THAN CURRENT ANALYSIS TIME.
        174 |C
        175 |      IH = NINT(SPECS(2))
        176 |      ID = NINT(SPECS(3))
        177 |      IM = NINT(SPECS(4))
        178 |      IY = NINT(SPECS(5))
        179 |C
        180 |      RJDAY = RMDAY(IM) + REAL(ID-1) + REAL(IH)/24.0E0
        181 |C
        182 |      RJDLAG = RJDAY - REAL(LAG)
        183 |      IF (RJDLAG.LT.DAYHF(1))     RJDLAG = RJDLAG + 365.E0
        184 |C
        185 |C  NOTE FROM ABOVE THAT RJDAY RANGES FROM 0. TO 364.5.  WHILE
        186 |C  RJDLAG RANGES FROM 15.5 TO 380.5.  THESE LATTER RJDLAG
        187 |C  LOWER AND UPPER LIMITS REFLECT THE FACT THAT JANUARY IS
        188 |C  TREATED BOTH AS 1ST AND "13TH" MONTH FOR EASIER INTERPOLATION.
        189 |C
        190 |C  NOW PRINT KEY PARAMETERS AT THIS POINT
        191 |C
        192 |      WRITE(IOPRT,60)     IY,IM,ID,IH,RJDAY,LAG,RJDLAG
        193 |C
        194 |C  NEXT FIND THE TWO CONSECUTIVE MONTHS WHOSE MID-MONTH
        195 |C  JULIAN DAYS BRACKET THE VALUE OF RJDLAG.  IF BRACKETING
        196 |C  MONTHS NOT FOUND, PRINT ERROR AND STOP.
        197 |C
        198 |           MON1=0
        199 |      DO 200  MON=1,12
        200 |        IF ((RJDLAG.GE.DAYHF(MON)).AND.(RJDLAG.LT.DAYHF(MON+1))) THEN
        201 |           MON1 = MON
        202 |           MON2 = MON+1
        203 |        END IF
        204 |  200 CONTINUE
        205 |C
        206 |      IF (MON1.EQ.0) THEN
        207 |         WRITE(IOPRT,65)
        208 |         ERRMSG(1) = ' ERROR IN SUBROUTINE TASKI:        '
        209 |         ERRMSG(2) = ' THE VALUEOF LAGGED JULDAY        '
        210 |         ERRMSG(3) = ' IS OUT OF RANGE                   '
        211 |         ERRMSG(4) = ' EXECUTION TERMINATING ABNORMALLY  '
        212 |         CALL ERRSTOP (IOPRT,ERRMSG)
        213 |      END IF
        214 |C
        215 |C  COMPUTE I/O RECORDS OF BRACKETING MONTHS IN CLIMATOLOGY FILE.
        216 |C  RECORD MREC1 WILL DENOTE EARLIER MONTH TO BE READ INTO TSS1.
        217 |C  RECORD MREC2 WILL DENOTE LATER MONTH TO BE READ INTO TSS2.
        218 |C
        219 |      IF(MON2.LE.12) THEN
        220 |         MREC1 = MON1
        221 |         MREC2 = MON2
        222 |         WRITE(IOPRT,70)     MON1,MON2,MREC1,MREC2,INSSOILT
        223 |         DO 300  KREC=1,MREC1
        224 |         READ(INSSOILT,ERR=2010,END=2050) TSS1
        225 |         CALL STATS(TSS1,1,IMAX-1,IMAX,1,JMAX,JMAX,1,1,1,LMN(KREC))
        226 |  300    CONTINUE
        227 |         READ(INSSOILT,ERR=2010,END=2050) TSS2
        228 |         CALL STATS(TSS2,1,IMAX-1,IMAX,1,JMAX,JMAX,1,1,1,LMN(MREC2))
        229 |C
        230 |      ELSE
        231 |C
        232 |         MREC1 = 12
        233 |         MREC2 = 1
        234 |         WRITE(IOPRT,70)     MON1,MON2,MREC1,MREC2,INSSOILT
        235 |         READ(INSSOILT,ERR=2010,END=2050) TSS2
        236 |         CALL STATS(TSS2,1,IMAX-1,IMAX,1,JMAX,JMAX,1,1,1,LMN(MREC2))
        237 |         DO 400  KREC=2,MREC1
        238 |         READ(INSSOILT,ERR=2010,END=2050) TSS1
        239 |         CALL STATS(TSS1,1,IMAX-1,IMAX,1,JMAX,JMAX,1,1,1,LMN(KREC))
        240 |  400    CONTINUE
        241 |C
        242 |      END IF
        243 |C
        244 |C  PROCEED WITH LINEAR INTERPOLATION IN TIME
        245 |C
        246 |      WGHT1 = (DAYHF(MON2)-RJDLAG)/(DAYHF(MON2)-DAYHF(MON1))
        247 |      WGHT2 = (RJDLAG-DAYHF(MON1))/(DAYHF(MON2)-DAYHF(MON1))
        248 |C
        249 |      DO 425  J=1,JMAX
        250 |      DO 425  I=1,IMAX
        251 |      TSS(I,J) = WGHT1*TSS1(I,J) + WGHT2*TSS2(I,J)
        252 |  425 CONTINUE
        253 |C
        254 |C        COMPUTE/PRINT SUMMARY STATS OF SOIL TEMPERATURES AFTER
        255 |C        TEMPORAL INTERPOLATION BUT STILL ON ORIGINAL INPUT GRID
        256 |C
        257 |      CALL STATS (TSS,1,IMAX-1,IMAX,1,JMAX,JMAX,1,1,1,
        258 |     *            'INPUT SOIL T ANAL ')
        259 |C
        260 |C     PREPARE PARAMETERS TO INTERPOLATE TO THE NGM GRIDS
        261 |C
        262 |      KMAX=1
        263 |      ISYM = 0
        264 |      FACTOR = 1.0E0
        265 |C
        266 |C     SPECIFY LATITUDES AND LONGITUDES OF INPUT GRID
        267 |C
        268 |      DO 450 I=1,IMAX
        269 |         ALONGTSS(I)=0.0E0 +  REAL(I-1)*5.0E0
        270 |  450 CONTINUE
        271 |      ALONGTSS(IMAX)=ALONGTSS(1)
        272 |C
        273 |      DO 475 J=1,JMAX
        274 |         ALATTSS(J)=-90.0E0 +  REAL(J-1)*4.0E0
        275 |  475 CONTINUE
        276 |C
        277 |C        INTERPOLATE SOIL TEMPERATURES HORIZONTALLY IN SPACE TO
        278 |C        ALL NGM GRIDS AND STORE IN VBL ARRAY LOCATIONS FOR SOIL TEMP
        279 |C
        280 |      CALL INTERP(TSS, IMAX, JMAX, KMAX, ALONGTSS, ALATTSS,
        281 |     *            ISYM, INDEXTSS, FACTOR)
        282 |C
        283 |C..................  OLD NAS APPROACH (PART A) .....................
        284 |C
        285 |C  NOW ON THE DOMAIN OF THE 36-DAY ARCHIVE C-GRID, READ-IN
        286 |C  THE TIME-AVERAGED SIGMA-ONE ANAL OF TEMP AND OVERLAY CLIMO
        287 |C
        288 |C  REPLACE INSSOILT UNIT NO. WITH INSSOILC IN ORDER TO
        289 |C  UTILIZE THE SAME I/O ERROR MESSAGES
        290 |C
        291 |C....INSSOILT=INSSOILC
        292 |C
        293 |C  SET DIMENSIONS OF C-GRID AS RETAINED IN 36-DAY ARCHIVE
        294 |C  (STILL NEED IN POST-NAS ERA FOR LATER DIAGNOSTIC PRINT)
        295 |C
        296 |      IDIM=83
        297 |      JDIM=83
        298 |C
        299 |C  SPECIFY NO. OF FORMATTED VALUES IN EACH RECORD OF
        300 |C  FORMATTED INPUT FILE INSSOILC
        301 |C
        302 |      NFV =18
        303 |C
        304 |C  DETERMINE NO. OF FORMATTED INPUT RECORDS PER ROW OF
        305 |C   83x83 C-GRID IN THE ARCHIVE
        306 |C
        307 |      LZF = IDIM/NFV
        308 |      IF((IDIM-LZF*NFV).NE.0)  LZF=LZF+1
        309 |C
        310 |C  SET I,J OFFSETS BETWEEN ARCHIVE C-GRID AND RAFS OPS C-GRID
        311 |C  for the old 113 x 91 (PRIOR TO AUG 91 JIF):
        312 |C...... JOFF=4
        313 |C...... IOFF=18
        314 |C  for the new B-grid (POST AUG 91, BUT BEFORE NAS RETIREMENT)
        315 |C...... JOFF=4 + 14
        316 |C...... IOFF=18 + 16
        317 |C
        318 |C................ END OLD NAS APPROACH (PART A) ................
        319 |C
        320 |C  RETRIEVE VBL ADDRESS/DIMENSIONS OF OPS C-GRID SUBSOIL-T
        321 |C
        322 |C...old B-grid...CALL GRDSET(3,INDEXTSS,MADDRS,IM,JM,DUM,DUM,DUM,DUM)
        323 |C
        324 |C  for the new B-grid
        325 |C
        326 |      CALL GRDSET(2,INDEXTSS,MADDRS,IM,JM,DUM,DUM,DUM,DUM)
        327 |      MADDTSS = MADDRS - 1 - IM
        328 |C
        329 |C  PRINT-OUT climatological SUBSOIL-T IN VBL before read
        330 |C
        331 |      kLZF = IM/NFV
        332 |      IF((IM-kLZF*NFV).NE.0)  kLZF=kLZF+1
        333 |      DO 98630  L=1,kLZF
        334 |      IS = (L-1)*NFV + 1
        335 |      IE = L*NFV
        336 |      IF(IE.GT.IM)  IE=IM
        337 |      WRITE(6,79)  (I, I=IE,IS)
        338 |  79  FORMAT(9X,19I6//)
        339 |      DO 98625  J=JM,1,-1
        340 |      KB = MADDTSS + IM*J + IS
        341 |      KE = MADDTSS + IM*J + IE
        342 |      WRITE(6,80)  J, ((VBL(K)-273.2E0), K=KB,KE)
        343 |  80  FORMAT(I7,3X,18F6.1)
        344 |98625 CONTINUE
        345 |98630 CONTINUE
        346 |C
        347 |C................ OLD NAS APPROACH (PART B) .....................
        348 |C
        349 |C.......    READ ARCHIVE C-GRID SUBSOIL-T INTO PROPER VBL LOCATIONS
        350 |C
        351 |C.... JB =    1 + JOFF
        352 |C.... JE = JDIM + JOFF
        353 |C.... WRITE(6,75) MADDRS,MADDTSS,IM,JM,IOFF,JOFF,IDIM,JDIM,
        354 |C....*            JB,JE,NFV,LZF
        355 |C.75  FORMAT('0',3X,2I10,10I5/)
        356 |C
        357 |C FIRST READ ID RECORD OF INPUT C-GRID SOIL-T FILE
        358 |C
        359 |C.... READ(INSSOILC,76,ERR=2010,END=2050) IYRE,IMNE,IDYE,IHRE,NDAYK
        360 |C.76  FORMAT(8X,5I2)
        361 |C.... WRITE(6,78) IYRE,IMNE,IDYE,IHRE,NDAYK
        362 |C.78  FORMAT('0',3X,5I5/)
        363 |C
        364 |C NEXT READ DATA FROM INPUT C-GRID SOIL-T FILE
        365 |C
        366 |C.... DO 620  J=JB,JE
        367 |C.... DO 610  L=1,LZF
        368 |C.... IS = ((L-1)*NFV+1) + IOFF
        369 |C.... IE = L*NFV
        370 |C.... IF(IE.GT.IDIM)  IE=IDIM
        371 |C.... IE = IE + IOFF
        372 |C.... KB = MADDTSS + IM*J + IS
        373 |C.... KE = MADDTSS + IM*J + IE
        374 |C.... READ(INSSOILC,77,ERR=2010,END=2050) (VBL(K), K=KB,KE)
        375 |C..77 FORMAT(18F6.1)
        376 |C.610 CONTINUE
        377 |C.620 CONTINUE
        378 |C
        379 |C  PRINT-OUT OPS C-GRID SUBSOIL-T IN VBL
        380 |C
        381 |C.... LZF = IM/NFV
        382 |C.... IF((IM-LZF*NFV).NE.0)  LZF=LZF+1
        383 |C.... DO 630  L=1,LZF
        384 |C.... IS = (L-1)*NFV + 1
        385 |C.... IE = L*NFV
        386 |C.... IF(IE.GT.IM)  IE=IM
        387 |C.... WRITE(6,79)  (I, I=IE,IS)
        388 |C.... DO 625  J=JM,1,-1
        389 |C.... KB = MADDTSS + IM*J + IS
        390 |C.... KE = MADDTSS + IM*J + IE
        391 |C.... WRITE(6,80)  J, ((VBL(K)-273.2E0), K=KB,KE)
        392 |C.625 CONTINUE
        393 |C.630 CONTINUE
        394 |C
        395 |C ...............  END OLD NAS APPROACH (PART B) ................
        396 |C
        397 |C       FINISH OF NORMAL PROCESSING REACHED SO JUMP TO RETURN
        398 |C
        399 |      GO TO 666
        400 |C
        401 |C***********************************************************************
        402 |C*****************  HANDLE READ ERRORS BELOW  **************************
        403 |C
        404 |C
        405 | 2010 CONTINUE
        406 |C     ERROR OCCURRED WHEN INPUTING THE SURFACE TEMP
        407 |C     MONTHLY CLIMO.
        408 |      WRITE (IOPRT, 2020) INSSOILT
        409 | 2020 FORMAT ('1', 'ERROR OCCURRED WHEN READING IN THE SFC TMP',
        410 |     1             'CLIMATOLOGY ON UNIT',I3//)
        411 |C
        412 |      GO TO 2100
        413 |C
        414 | 2050 CONTINUE
        415 |C     UNEXPECTED END OF FILE HIT
        416 |C     WHEN INPUTING THE SFC TEMP MONTHLY CLIMO
        417 |      WRITE (IOPRT, 2060) INSSOILT
        418 | 2060 FORMAT ('1', 'UNEXPECTED END OF FILE HIT WHEN READING ',
        419 |     1             'IN THE SFC TEMP MONTHLY CLIMO',
        420 |     2             'ON UNIT', I3//)
        421 |C
        422 | 2100 CONTINUE
        423 |C
        424 |C***********************************************************
        425 |C***********   RETURN TO MAIN PROGRAM   ********************
        426 |C
        427 |  666 RETURN
