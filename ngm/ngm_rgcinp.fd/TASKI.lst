        435 |     1            ' IS TOO SMALL FOR APPLIED EQUIVALENCE'/
        436 |     2       ' ', ' EXECUTION TERMINATING.'//)
        437 |   60 FORMAT('0', ' TASKI PARAMETERS: BASETIME OF INPUT IS  YEAR = ',I4,
        438 |     1           '  MONTH = ',I3,'   DAY = ',I3,'  HOUR = ',I3/' ',
        439 |     2         19X,'BASETIME JULDAY = ',F7.2,'   LAG = ',I4,
        440 |     3             '  LAGGED JULDAY = ',F7.2)
        441 |   65 FORMAT('0','  THE VALUE OF LAGGED JULDAY IN SUBROUTINE TASKI',
        442 |     1            ' IS OUT OF RANGE.  EXECUTION IS TERMINATING')
        443 |   70 FORMAT('0', ' MON1 = ',I3,'   MON2 = ',I3,'   MREC1 = ',I3,
        444 |     1         '   MREC2 = ',I3,'   FROM I/O UNIT = ',I3)
