          1 |      SUBROUTINE INTERP (FIELD, IMAX, JMAX, KMAX, ALONG, ALAT,
          2 |     1                   ISYM, INDEX, FACTOR)
          3 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          4 |C                .      .    .                                       .
          5 |C SUBPROGRAM:    INTERP      INTERPOLATES TO ALL NGM GRIDS
          6 |C   PRGMMR: JIM HOKE         ORG: W/NMC22    DATE: 85-03-27
          7 |C
          8 |C ABSTRACT: INTERPOLATES A FIELD ON A LATITUDE-LONGITUDE GRID TO
          9 |C   ALL THE NGM GRIDS, AND THEN STORES THE RESULT INTO ARRAY VBL
         10 |C
         11 |C PROGRAM HISTORY LOG:
         12 |C   85-03-27  JIM HOKE
         13 |C   88-08-02  BRIAN SCHMIDT ADDED DOCBLOCKS
         14 |C   89-02-03  KEN MITCHELL ADDED SOIL TEMPERATURE INDEX CHECK
         15 |C
         16 |C USAGE:    CALL INTERP(FIELD, IMAX, JMAX, KMAX, ALONG, ALAT,
         17 |C                       ISYM, INDEX, FACTOR)
         18 |C   INPUT ARGUMENT LIST:
         19 |C     FIELD    - 3D ARRAY
         20 |C     IMAX     - FIRST DIMENSION OF ARRAY FIELD
         21 |C     JMAX     - SECOND DIMENSION OF ARRAY FIELD
         22 |C     KMAX     - THIRD DIMENSION OF ARRAY FIELD
         23 |C     ALONG    - ARRAY OF LONGITUDES DESCRIBING THE ARRAY FIELD
         24 |C     ALAT     - ARRAY OF LATITUDES DESCRIBING THE ARRAY FIELD
         25 |C     ISYM     - EQUATORIAL SYMMETRY VARIABLE
         26 |C     INDEX    - ADDRESS REFERENCE FOR STORING THE ARRAY FIELD
         27 |C                IN THE ARRAY VBL.
         28 |C     FACTOR   - SCALING PARAMETER
         29 |C
         30 |C   OUTPUT ARGUMENT LIST:
         31 |C     NONE
         32 |C
         33 |C   OUTPUT FILES:
         34 |C     FT06F001 - FOR PRINTOUT
         35 |C
         36 |C   SUBPROGRAMS CALLED:
         37 |C     UNIQUE:    - ERRSTOP
         38 |C                  GRDSET
         39 |C                  CORNER
         40 |C                  HORINT
         41 |C     LIBRARY:
         42 |C       COMMON   - CONSTN
         43 |C                  BLANK (CONTAINS ARRAY VBL)
         44 |C
         45 |C ATTRIBUTES:
         46 |C   LANGUAGE: CYBER FORTRAN 200
         47 |C   MACHINE:  CYBER 205
         48 |C
         49 |C$$$
         50 |C
         51 |      IMPLICIT REAL (A-H, O-Z)
         52 |C
         53 |C
         54 |      INCLUDE 'parmodel'
  1       1 |C     PARAMETER FILE
  1       2 |C
  1       3 |C     SETUP FOR IBM SINGLE PRECISION REPRESENTATION
  1       4 |C     ON OUTPUT
  1       5 |C                      THIS VERSION 1 May 1991 (DiMego)
  1       6 |C
  1       7 |      PARAMETER (
  1       8 |     *      IBPW      =      4,
  1       9 |     *      IBPOR     = 120000,
  1      10 |     *      INUMREC   = IBPOR / IBPW,
  1      11 |     *      INSPECS   =    700,
  1      12 |     *      ILATG2    =    120,
  1      13 |     *      ILEVH     =     12,
  1      14 |     *      ILEVS     =     16,
  1      15 |     *      ILONF     =    360,
  1      16 |     *      INLATSH   =      5,
  1      17 |     *      IIJMAX    =  23668,
  1      18 |     *      IIJMAXB   =  25536,
  1      19 |     *      IIJRAD    =    892,
  1      20 |     *      IJCAP     =     80,
  1      21 |     *      IWPCPL    =     80,
  1      22 |     *      IKM       =     16 )
  1      23 |      PARAMETER (
  1      24 |     *      ILBCH     =   7*(147+111-7),
  1      25 |     *      ILBHI     =    (147+111-26),
  1      26 |     *      ILBUI     =    (2*147+111-39),
  1      27 |     *      ILBUO     =   (8*147+7*111-56),
  1      28 |     *      ILBVI     =    (147+2*111-39),
  1      29 |     *      ILBVO     =   (7*147+8*111-56),
  1      30 |     *      ILMOUT    =     20 )
  1      31 |      PARAMETER (
  1      32 |     *      IMAXOUTF  =     17,
  1      33 |     *      IMAXOUTT  =      5,
  1      34 |     *      IMAXOUTV  =    100,
  1      35 |     *      IMAXPROF  =      6,
  1      36 |     *      INFILLER  =2500000,
  1      37 |     *      INH       =     65,
  1      38 |     *      INGRDUSE  =      2,
  1      39 |     *      INGRDUS1  =      1,
  1      40 |     *      INIADDRS  =     44,
  1      41 |     *      INSCR     =     23,
  1      42 |     *      INSCRGEO  =     12,
  1      43 |     *      INSCR3    =     10,
  1      44 |     *      INSOIL    =      2,
  1      45 |     *      INSUMG    =  42164,
  1      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  1      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  1      48 |     *      INSHEM    = 13 * INH + 23,
  1      49 |     *      IIJKMAX   = IKM * IIJMAX )
  1      50 |      PARAMETER (
  1      51 |     *  IIWORDSB  = IIJMAXB / 64,
  1      52 |     *  IKMP1     = IKM + 1,
  1      53 |     *  ILVLTOT   = 7 * IKM + 37,
  1      54 |     *  ILONFP1   = ILONF + 1,
  1      55 |     *  INVBL     = ILVLTOT * INSUMG,
  1      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  1      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  1      58 |     *  IBLK1D    =  2 * IKM + 14,
  1      59 |     *  IBLK2D    = 16 * IKM + 36,
  1      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  1      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  1      62 |C
         55 |C
         56 |C
         57 |      LOGICAL  BITSEA, BITSNO, BITWVL
         58 |C
         59 |      COMMON /CONSTN/ IMG( INGRDUSE ), JMG( INGRDUSE ),
         60 |     1                IAG( INGRDUS1 ), JAG( INGRDUS1 ),
         61 |     2                IBG( INGRDUS1 ), JBG( INGRDUS1 ),
         62 |     3                IADDRG( INIADDRS ,  INGRDUSE ),
         63 |     4                INDEXHU,  INDEXHV,  INDEXHTH, INDEXHQ,
         64 |     5                INDEXH,   INDEXST,  INDEXSQ,  INDEXALB,
         65 |     6                INDEXSLP, INDEXPSI, INDEXCD,  INDEXCC,  INDEXGSP,
         66 |     7                INDEXRAD, INDEXDLW, INDEXDSW, INDEXCPR, INDEXMA,
         67 |     8                INDEXSEC, INDEXZ0, INDEXTSS,  INDEXCG,  INDEXDHQ,
         68 |     9                INDEXTSD, KM, NH, NGRDUSE,
         69 |     1                SPECS( INSPECS ), DELSIG( IKM ),
         70 |     2                POVH( IKM ), PIOVHK( IKM ),
         71 |     3                CMUU  ( INGRDUSE ),
         72 |     4                XPOLEH( INGRDUSE ), YPOLEH( INGRDUSE ),
         73 |     5                XPOLEU( INGRDUSE ), YPOLEU( INGRDUSE ),
         74 |     6                XPOLEV( INGRDUSE ), YPOLEV( INGRDUSE ),
         75 |     7                DLAMNGM,
         76 |     8                BITSEA( IIJMAX ,  INGRDUSE ),
         77 |     9                BITSNO( IIJMAX ,  INGRDUSE ),
         78 |     1                BITWVL( IIJMAX ,  INGRDUSE )
         79 |C
         80 |C       ****   END OF INCLUDED COMMON BLOCK CONSTN   ***
         81 |C
         82 |C
         83 |      COMMON VBL( INVBL )
         84 |C
         85 |      CHARACTER*35 ERRMSG(4)
         86 |C
         87 |      DIMENSION COLUMN( ILEVS )
         88 |      DIMENSION FIELD(IMAX,JMAX,KMAX), ALONG(IMAX), ALAT(JMAX)
         89 |C
         90 |C
         91 |C
         92 |C              SPECIFY THE UNIT NUMBER OF THE PRINTER.
         93 |      IOUTUPRT = 6
         94 |C     WE SPECIFY THAT THE LONGITUDE RETURNED BY SUBROUTINE
         95 |C     CORNER IS TO RANGE FROM 0 TO 360 DEGREES AND THAT
         96 |C     THAT SUBROUTINE IS NOT TO PRINT OUT THE LATITUDES
         97 |C     AND LONGITUDES IT CALCULATES.
         98 |      IRANGE = 1
         99 |      IPRT = 0
        100 |C
        101 |C     THE DUMMY VALUES ARE SPECIFIED FOR USE AT POINTS FARTHER
        102 |C     SOUTH THAN THE NGM WILL EVER FORECAST.
        103 |C     THE DUMMY VALUES ARE THE VALUES BEFORE THE SCALING
        104 |C     PARAMETER (FACTOR) IS APPLIED.
        105 |      IF( (INDEX .NE. INDEXHU) .AND. (INDEX .NE. INDEXHV) .AND.
        106 |     1    (INDEX .NE. INDEXHQ) .AND. (INDEX. NE. INDEXPSI) .AND.
        107 |     2    (INDEX .NE. INDEXCD) ) GO TO 5
        108 |      DUMMY = 0.0E+0
        109 |      GO TO 30
        110 |C
        111 |    5 CONTINUE
        112 |      IF((INDEX .NE. INDEXHTH) .AND. (INDEX .NE. INDEXTSS)) GO TO 10
        113 |C     INDEXHTH REPRESENTS VIRTUAL TEMPERATURE AT THIS POINT.
        114 |C     INDEXTSS REPRESENTS SOIL TEMPERATURE
        115 |      DUMMY = 250.0E+0
        116 |      GO TO 30
        117 |C
        118 |   10 CONTINUE
        119 |      IF (INDEX .NE. INDEXH) GO TO 12
        120 |      DUMMY = 100.0E+0
        121 |      GO TO 30
        122 |C
        123 |   12 CONTINUE
        124 |      IF (INDEX .NE. INDEXSLP) GO TO 13
        125 |      DUMMY = 1000.0E+0
        126 |      GO TO 30
        127 |C
        128 |   13 CONTINUE
        129 |      IF (INDEX .NE. INDEXALB) GO TO 14
        130 |      DUMMY = 0.06E+0
        131 |      GO TO 30
        132 |C
        133 |   14 CONTINUE
        134 |      IF (INDEX .NE. INDEXZ0) GO TO 15
        135 |      DUMMY = 0.001E+0
        136 |      GO TO 30
        137 |C
        138 |   15 CONTINUE
        139 |      IF (INDEX .NE. INDEXMA) GO TO 16
        140 |      DUMMY = 0.10E+0
        141 |      GO TO 30
        142 |C
        143 |   16 CONTINUE
        144 |      IF (INDEX .NE. INDEXTSD) GO TO 17
        145 |      DUMMY = 0.E+0
        146 |      GO TO 30
        147 |C
        148 |   17 CONTINUE
        149 |C     THE SUBROUTINE IS BEING USED FOR INTERPOLATION OF A FIELD
        150 |C     FOR WHICH IT WAS NOT ORIGINALLY INTENDED.  A DUMMY VALUE
        151 |C     HAS NOT BEEN SPECIFIED IN THE CODE FOR THIS VARIABLE FOR
        152 |C     POINTS FARTHER SOUTH THAN THE NGM WILL EVER FORECAST.
        153 |C     SPECIFICATION OF DUMMY VALUES FOR SURFACE TEMPERATURE,
        154 |C     SURFACE MOISTURE, AND ALBEDO MAY CREATE SOME CONFUSION
        155 |C     WITH SPECIAL VALUES USED FOR THESE FIELDS TO INDICATE
        156 |C     SUCH THINGS AS LAND POINTS.
        157 |      WRITE (IOUTUPRT, 20) INDEX
        158 |   20 FORMAT ('0', '***** A VARIABLE TYPE WAS SPECIFIED FOR WHICH ',
        159 |     1             'SUBROUTINE INTERP WAS NOT PREPARED.'/
        160 |     2        ' ', 'INDEX=', I5, '  EXECUTION TERMINATED.')
        161 |C
        162 |C     TERMINATE THE CONTROLLEE WITH A RETURN CODE OF 8.
        163 |      ERRMSG(1) = 'IN SUBROUTINE INTERP   NEAR STATEME'
        164 |      ERRMSG(2) = 'NT NUMBER 20:                      '
        165 |      ERRMSG(3) = 'A VARIABLE TYPE WAS SPECIFIED FOR W'
        166 |      ERRMSG(4) = 'HICH SUB INTERP WAS NOT PREPARED.  '
        167 |C
        168 |      CALL ERRSTOP (IOUTUPRT, ERRMSG)
        169 |C
        170 |C
        171 |   30 CONTINUE
        172 |      DO 400 NG=1,NGRDUSE
        173 |      CALL GRDSET(NG, INDEX, MADDRS, IM, JM, XPOL, YPOL, CMU, DLAM)
        174 |C
        175 |      IJ     = IM * JM
        176 |      JADDRS = MADDRS - 1 - IM - IJ
        177 |C
        178 |      DO 300 J=1,JM
        179 |      Y = REAL(J) - YPOL
        180 |C
        181 |      DO 200 I=1,IM
        182 |      X = REAL(I) - XPOL
        183 |C
        184 |C     DETERMINE THE LATITUDE/LONGITUDE OF THE NGM GRID POINT.
        185 |      CALL CORNER(X, Y, CMU, DLAM, IRANGE, IPRT, XLONG, YLAT)
        186 |C
        187 |C     1) IF THE POINT IS FARTHER SOUTH THAN THE SOUTHERNMOST
        188 |C     LATITUDINAL STRIP, WHICH (FOR FIELDS ON THE LATITUDE-
        189 |C     LONGITUDE GRID IN LATITUDINAL STRIPS)
        190 |C     SHOULD HAVE BEEN CHOSEN (THROUGH
        191 |C     THE USE OF PARAMETER NLATSH) TO BE SOUTH OF THE
        192 |C     SOUTHERNMOST NGM FORECAST POINT FOR THE GIVEN NH AND
        193 |C     LATITUDINAL STRIPS (NOTE THAT THE DRAG COEFFICIENT
        194 |C     IS ON ANOTHER LOLA GRID THAT ONLY REACHS TO THE EQUATOR), AND
        195 |C     2) IF NORTHERN HEMISPHERIC VALUES ARE NOT TO BE USED TO
        196 |C     SPECIFY THE VALUE, THEN
        197 |C     A DUMMY VALUE IS ASSIGNED TO THE PARTICULAR VARIABLE.
        198 |      IF ((YLAT .GE. ALAT(1)) .OR. (ISYM .EQ. 1)
        199 |     1                        .OR. (ISYM .EQ. 2) ) GO TO 40
        200 |      DO 35 K=1,KMAX
        201 |      COLUMN(K) = DUMMY
        202 |   35 CONTINUE
        203 |      GO TO 45
        204 |C
        205 |   40 CONTINUE
        206 |C     HORIZONTALLY INTERPOLATE THE FIELD TO THE NGM GRID POINT.
        207 |      CALL HORINT(FIELD, IMAX, JMAX, KMAX, ALONG, ALAT, ISYM,
        208 |     1            XLONG, YLAT, COLUMN)
        209 |C
        210 |   45 CONTINUE
        211 |C     STORE THE INTERPOLATED VALUES IN ARRAY VBL.
        212 |      DO 100 K=1,KMAX
        213 |      VBL(JADDRS + I + IM*J + IJ*K ) = COLUMN(K) * FACTOR
        214 |  100 CONTINUE
        215 |C
        216 |  200 CONTINUE
        217 |C
        218 |  300 CONTINUE
        219 |C
        220 |  400 CONTINUE
        221 |C
        222 |      RETURN
        223 |C
        224 |      END
 
