         86 |      COMMON           VBL( INVBL )
         87 |C
         88 |      COMMON /COMMON1/ VIRTTEMP( IIJMAX ,  INGRDUSE )
         89 |C
         90 |      DIMENSION P( IKM ), PI( IKM ), THOLD( IKM ), THNEW( IKM ), RH( IKM
         91 |     1 )
         92 |C
         93 |      DIMENSION RHMAX ( IKM ,  INGRDUSE )
         94 |      DIMENSION IMAXRH( IKM ,  INGRDUSE )
         95 |      DIMENSION JMAXRH( IKM ,  INGRDUSE )
         96 |      DIMENSION NUMRH ( IKM ,  INGRDUSE )
         97 |C
         98 |      DATA KEX1 /0/
         99 |C
        100 |      RGASD  = 287.05E0
        101 |      RGASV  = 461.5E0
        102 |      CSUBP  = 1005.E0
        103 |      CKAPPA = RGASD  / CSUBP
        104 |      EPS    = RGASD /  RGASV
        105 |      FACTOR = (1.0E0 - EPS) / EPS
        106 |C
        107 |C     SPECIFY THE MINIMUM VALUE FOR SPECIFIC HUMIDITY.
        108 |      QMIN = 4.0E-6
        109 |C
        110 |CC....KB =  ILEVH  - 1
        111 |      KB =  ILEVH
        112 |C
        113 |C  SET COUNTERS AND CONSTANTS USED IN DIAGNOSTIC PRINT
        114 |C
        115 |      KX1 =0
        116 |      KX2 =0
        117 |      KX3 =0
        118 |      KX4 =0
        119 |      HMAX  = -1.E-8
        120 |      TMAX  = -1.E-8
        121 |      QSLOW =  1.E8
        122 |      QLOW  =  1.E8
        123 |C
        124 |C
        125 |C     IN THE CLAUSIUS-CLAPYRON EQUATION, THE CONSTANTS FOR LIQUID
        126 |C     WATER, AND NOT ICE, ARE ALWAYS USED.
        127 |      AL =  5.00650E0
        128 |      BL = 19.83923E0
        129 |      AI =  0.09859E0
        130 |      BI = 22.48869E0
        131 |C
        132 |      A = AL
        133 |      B = BL
        134 |      APB = A + B
        135 |C
        136 |C     INITIALIZE THE ARRAYS USED TO RECORD THE MAGNITUDE
        137 |C     OF THE LARGEST RELATIVE HUMIDITIES AND THE NUMBER OF POINTS
        138 |C     WITH RELATIVE HUMIDITIES GREATER THAN 100 PERCENT.
        139 |      DO 25 NG = 1, NGRDUSE
        140 |      DO 25 K  = 1, KM
        141 |         RHMAX(K, NG) = -1.0E+10
        142 |         NUMRH(K, NG) =  0
        143 |   25 CONTINUE
        144 |C
        145 |C     PROCESS EACH GRID POINT OF EACH NGM GRID.
        146 |      DO 1100 NG=1,NGRDUSE
        147 |C
        148 |C     ZERO THE COUNTERS OF THE NUMBER OF OCCURRENCES OF
        149 |C     NEGATIVE SPECIFIC HUMIDITY.
        150 |      KOUNT1 = 0
        151 |      KOUNT2 = 0
        152 |C
        153 |C     NUMPRT IS A COUNTER FOR THE DRY CONVECTIVE ADJUSTMENT.
        154 |      NUMPRT = 0
        155 |C
        156 |      CALL GRDSET (NG, INDEXHTH, DUMMY, IM, JM, XPOL, YPOL, CMU, DLAM)
        157 |C
        158 |C     SPECIFY POINTERS FOR VARIOUS VARIABLES IN ARRAY VBL.
        159 |      IJ = IM * JM
        160 |      MADDHU  = IADDRG(INDEXHU , NG) -1 -IM -IJ
        161 |      MADDHV  = IADDRG(INDEXHV , NG) -1 -IM -IJ
        162 |      MADDHTH = IADDRG(INDEXHTH, NG) -1 -IM -IJ
        163 |      MADDHQ  = IADDRG(INDEXHQ,  NG) -1 -IM -IJ
        164 |      MADDH   = IADDRG(INDEXH,   NG) -1 -IM
        165 |      MADDST  = IADDRG(INDEXST,  NG) -1 -IM
        166 |      MADDSQ  = IADDRG(INDEXSQ,  NG) -1 -IM
        167 |C
        168 |      KPRINT=0
        169 |C
        170 |      DO 1000 J=1,JM
        171 |C
        172 |      DO 900 I=1,IM
        173 |C
        174 |C=======================================================
        175 |C
        176 |C  CHK WHETHER TO TURN ON THE DIAGNOSTIC PRINT OF UPPER LVL
        177 |C  MOISTURE EXTRAPOLATION
        178 |C
        179 |c     IF((MOD(I,45).EQ.0).AND.(MOD(J,45).EQ.0)) KPRINT=1
        180 |C     IF((NG.EQ.1).AND.(I.EQ.54).AND.(J.EQ.4))  KPRINT=1
        181 |c     IF((NG.EQ.1).AND.(I.EQ.70).AND.(J.EQ.22))  KPRINT=1
        182 |C     IF((NG.EQ.1).AND.(I.EQ.58).AND.(J.EQ. 3))  KPRINT=1
        183 |C     IF((NG.EQ.1).AND.(I.EQ.60).AND.(J.EQ.14))  KPRINT=1
        184 |C     IF((NG.EQ.1).AND.(I.EQ.54).AND.(J.EQ. 9))  KPRINT=1
        185 |C     IF((NG.EQ.2).AND.(I.EQ.37).AND.(J.EQ.38))  KPRINT=1
        186 |c     IF((NG.EQ.2).AND.(I.EQ.27).AND.(J.EQ.45))  KPRINT=1
        187 |C     IF((NG.EQ.2).AND.(I.EQ.28).AND.(J.EQ.44))  KPRINT=1
        188 |C     IF((NG.EQ.2).AND.(I.EQ.72).AND.(J.EQ. 1))  KPRINT=1
        189 |C     IF((NG.EQ.3).AND.(I.EQ.57).AND.(J.EQ.60))  KPRINT=1
        190 |c     IF((NG.EQ.3).AND.(I.EQ.37).AND.(J.EQ.76))  KPRINT=1
        191 |C     IF((NG.EQ.3).AND.(I.EQ.56).AND.(J.EQ. 5))  KPRINT=1
        192 |C     IF((NG.EQ.1).AND.(I.EQ.43).AND.(J.EQ.43))  KPRINT=1
        193 |C     IF((NG.EQ.1).AND.(I.EQ. 5).AND.(J.EQ.40))  KPRINT=1
        194 |C     IF((NG.EQ.1).AND.(I.EQ.33).AND.(J.EQ.29))  KPRINT=1
        195 |C     IF((NG.EQ.2).AND.(I.EQ.52).AND.(J.EQ.67))  KPRINT=1
        196 |C     IF((NG.EQ.2).AND.(I.EQ.50).AND.(J.EQ.21))  KPRINT=1
        197 |C     IF((NG.EQ.2).AND.(I.EQ.32).AND.(J.EQ.40))  KPRINT=1
        198 |c     IF((NG.EQ.3).AND.(I.EQ.69).AND.(J.EQ.88))  KPRINT=1
        199 |C     IF((NG.EQ.3).AND.(I.EQ.67).AND.(J.EQ.21))  KPRINT=1
        200 |C     IF((NG.EQ.3).AND.(I.EQ.46).AND.(J.EQ.64))  KPRINT=1
        201 |C     IF((NG.EQ.3).AND.(I.EQ.15).AND.(J.EQ.38))  KPRINT=1
        202 |C     IF((NG.EQ.3).AND.(I.EQ. 8).AND.(J.EQ.26))  KPRINT=1
        203 |C     IF((NG.EQ.3).AND.(I.EQ.77).AND.(J.EQ.13))  KPRINT=1
        204 |C
        205 |C======================================================
        206 |C
        207 |C     SURFACE PRESSURE IS RETRIEVED AND EXPONENTIATED.
        208 |      H = VBL(MADDH + I + IM*J)
        209 |      HK = REAL(REAL(H) ** REAL(CKAPPA))
        210 |C
        211 |      IF ( H .GT. 1.1E0) KX3=KX3+1
        212 |C.....IF ( H .GT. 1.1E0) KPRINT=1
        213 |      IF ( H .GT. HMAX)  HMAX=H
        214 |C
        215 |      IF ( KPRINT .EQ. 1) THEN
        216 |      PRINT 14,  NG, KB, TMAX
        217 |   14 FORMAT('  '//'  ',4X,'NG = ',I1,3X,'KB = ',I2,
        218 |     * 3X,'TMAX = ',F6.1,'  I,J,K3-HLRG,K4-NEGQS,K1-QESS,K2-BQMIN,',
        219 |     * 'HMX,H,T,QSLO,QSFRS,QSAT,QLO,QFRS,Q,K ='/)
        220 |c    * 'HMX,H,HTH,T,QSLO,QSFRS,QSAT,QH,QLO,QFRS,Q,K ='/)
        221 |      ENDIF
        222 |C
        223 |C     THE SATURATION SPECIFIC HUMIDITY AT THE WATER SURFACE
        224 |C     IS CALCULATED FROM THE SST AND THE SURFACE PRESSURE.
        225 |C*****THE FOLLOWING STARRED LINES ALLOW THE USE OF SST VALUES
        226 |C*****OVER LAND OR ICE.
        227 |C*****VBL(MADDSQ + I + IM*J) = 0.0E0
        228 |      SST = VBL(MADDST + I +IM*J)
        229 |C*****IF (SST .LE. 272.E0) GO TO 100
        230 |      EWTEMP = 0.00611E0 * REAL( REAL(273.16E0/SST) ** REAL(A) )
        231 |     1     * EXP(APB * (1.0E0 - 273.16E0/SST))
        232 |      VBL(MADDSQ + I + IM*J) = 0.622E0 * EWTEMP
        233 |     1                          / (H - 0.378E0 * EWTEMP)
        234 |C*100 CONTINUE
        235 |C
        236 |C
        237 |C     THETA IS CALCULATED AND THE DRY ADIABATIC ADJUSTMENT PERFORMED.
        238 |C
        239 |      DO 200 K=1,KM
        240 |C
        241 |C     FIRST THE LAYER PRESSURES (IN BARS) AND LAYER EXNER FUNCTIONS
        242 |C     (PI) ARE CALCULATED.
        243 |      P(K) = POVH(K)   * H
        244 |      PI(K)= PIOVHK(K) * HK
        245 |C
        246 |C  TRACK OCCURRENCES OF LOW SPECIFIC HUMIDITY
        247 |C
        248 |      Q     = VBL(MADDHQ+I+IM*J+IJ*K)
        249 |      QFRST = Q
        250 |      IF ( Q .LT. QLOW) QLOW=Q
        251 |      IF ( Q .LT. QMIN) KX2=KX2+1
        252 |C
        253 |C     BOUND SPECIFIC HUMIDITY AWAY FROM ZERO.
        254 |      VBL(MADDHQ+I+IM*J+IJ*K) = MAX(QMIN, VBL(MADDHQ+I+IM*J+IJ*K))
        255 |C
        256 |C     THE TEMPERATURE IS COMPUTED FROM THE VIRTUAL
        257 |C     TEMPERATURE AND SPECIFIC HUMIDITY.
        258 |      T = VBL(MADDHTH+I+IM*J+IJ*K)
        259 |     1     / (1.0E0 + FACTOR * VBL(MADDHQ+I+IM*J+IJ*K))
        260 |C
        261 |C     THETA IS CALCULATED FROM TEMPERATURE AND THE EXNER FUNCTION.
        262 |      THOLD(K) = T / PI(K)
        263 |      THNEW(K) = THOLD(K)
        264 |C
        265 |C     CALCULATE SATURATION SPECIFIC HUMIDITY FROM
        266 |C     TEMPERATURE, AND PRESSURE (IN BARS).
        267 |      ESAT = 0.00611E0 * REAL ( REAL(273.16E0/T) ** REAL(A) )
        268 |     1       * EXP(APB * (1.0E0 - 273.16E0/T))
        269 |      QSAT = 0.622E0 * ESAT / (P(K) - 0.378E0 * ESAT)
        270 |C
        271 |      QSFRST = QSAT
        272 |      IF ( QSAT .LT. QSLOW) QSLOW=QSAT
        273 |      IF ( QSAT .LT. 0.E0 ) THEN
        274 |         QSAT = QMIN/0.70E0
        275 |         KX4 = KX4 + 1
        276 |      ENDIF
        277 |C
        278 |      Q = VBL(MADDHQ+I+IM*J+IJ*K)
        279 |C
        280 |C  EXTRAPOLATE MOISTURE (Q) UPWARD INTO THOSE LAYERS WITH NO
        281 |C  HUMIDITY ANALYSIS
        282 |C
        283 |      IF ( K .GT. KB )  THEN
        284 |C
        285 |         QB = VBL(MADDHQ+I+IM*J+IJ*KB)
        286 |C
        287 |         CALL EXMOIS( P(K) , Q , P(KB) , QB )
        288 |C
        289 |C  CONSTRAIN EXTRAPOLATED VALUES TO LESS THAN 70 PCT SATURATION
        290 |C
        291 |         QSAT7 = 0.70E0 * QSAT
        292 |         IF ( Q .GT. QSAT7)  THEN
        293 |            KEX1 = KEX1 + 1
        294 |            Q = MAX(QSAT7,QMIN)
        295 |         ENDIF
        296 |C
        297 |      ENDIF
        298 |C
        299 |C  CALCULATE RELATIVE HUMIDITY
        300 |C
        301 |      RH(K) = Q / QSAT
        302 |C
        303 |C  CHK WHETHER TO PRINT DIAGNOSTIC PROFILES
        304 |C
        305 |      IF ( KPRINT .EQ. 1)  THEN
        306 |c        PRINT 15, I,J,KX3,KX4,KX1,KX2,
        307 |c    *      HMAX,H,HTH,T,QSLOW,QSFRST,QSAT,QH,QLOW,QFRST,Q,K
        308 |c  15    FORMAT(1X,I3,1X,I2,4(1X,I5),2(1X,F5.3),
        309 |c    *   1X,F6.1,1X,F5.1,7(1X,E9.2),1X,I2)
        310 |         PRINT 15, I,J,KX3,KX4,KX1,KX2,
        311 |     *      HMAX,H,T,QSLOW,QSFRST,QSAT,QLOW,QFRST,Q,K
        312 |   15    FORMAT(1X,I3,1X,I2,4(1X,I5),2(1X,F5.3),
        313 |     *   1x,F5.1,6(1X,E9.2),1X,I2)
        314 |      ENDIF
        315 |C
        316 |  200 CONTINUE
        317 |C
        318 |C  RESET PRINT FLAG
        319 |C
        320 |      KPRINT=0
        321 |C
        322 |C     WE CAN NOW PERFORM THE DRY ADIABATIC ADJUSTMENT.
        323 |      LTOP = 0
        324 |C
        325 |  225 CONTINUE
        326 |C     RETURN POINT IF ANY ADJUSTMENT WAS NECESSARY.  THE ENTIRE
        327 |C     SOUNDING MUST BE CHECKED BECAUSE THE LAYER BELOW THE ADJUSTED
        328 |C     LAYERS MAY NOW BE UNSTABLE.
        329 |      DO 400 K=2,KM
        330 |      IF(THNEW(K) .GE. THNEW(K-1)) GO TO 400
        331 |C
        332 |C     ADJUSTMENT IS NECESSARY.  SET THE INITIAL LAYER LIMITS.
        333 |      LBOT = K - 1
        334 |      LTOP = K
        335 |C
        336 |C     RETURN POINT IF MORE THAN 2 ADJACENT LAYERS ARE SUPERADIABATIC.
        337 |  250 CONTINUE
        338 |C     COMPUTE THE ADJUSTED THETA FOR LAYERS LBOT TO LTOP.
        339 |      THNUM = 0.0E0
        340 |      THDEN = 0.0E0
        341 |      DO 300 KK=LBOT,LTOP
        342 |      THNUM = THNUM + DELSIG(KK) * PI(KK) * THNEW(KK)
        343 |      THDEN = THDEN + DELSIG(KK) * PI(KK)
        344 |  300 CONTINUE
        345 |C
        346 |      THADJ = THNUM / THDEN
        347 |C
        348 |      DO 350 KK=LBOT,LTOP
        349 |      THNEW(KK) = THADJ
        350 |  350 CONTINUE
        351 |C
        352 |C     IF THIS SUPERADIABATIC REGION HAS REACHED TO THE TOP OF THE
        353 |C     MODEL, CHECK THE ENTIRE SOUNDING AGAIN.
        354 |      IF (LTOP .GE. KM) GO TO 225
        355 |C
        356 |C     IF THE NEXT LAYER IS STABLE, CHECK THE ENTIRE SOUNDING AGAIN.
        357 |      IF (THNEW(LTOP+1) .GE. THNEW(LTOP) ) GO TO 225
        358 |C
        359 |C     NEXT LAYER IS UNSTABLE, WE MUST READJUST ALL ADJACENT UNSTABLE
        360 |C     LAYERS TOGETHER.
        361 |      LTOP = LTOP + 1
        362 |      GO TO 250
        363 |  400 CONTINUE
        364 |C
        365 |  500 CONTINUE
        366 |C     DRY CONVECTIIVE ADJUSTMENT IS COMPLETE FOR THIS POINT.
        367 |C     PRINT OUT INFORMATION CONCERNING IT, IF DESIRED.
        368 |      IF (LTOP .EQ. 0) GO TO 750
        369 |C     SOME DRY CONVECTIVE ADJUSTMENT HAD TO BE DONE FOR THIS POINT.
        370 |C     DETERMINE THE LATITUDE AND LONGITUDE OF THE POINT.
        371 |      IRANGE = 2
        372 |      IPRINT = 0
        373 |      CALL CORNER (REAL(I)-XPOL, REAL(J)-YPOL, CMU, DLAM, IRANGE,
        374 |     1             IPRINT, XLONG, YLAT)
        375 |C
        376 |C     ONLY THE FIRST 10 NORTHERN HEMISPHERIC POINTS
        377 |C     WITH LARGE DRY CONVECTIVE ADJUSTMENTS WILL BE PRINTED.
        378 |      IF (YLAT .LT. 0.0E0) GO TO 750
        379 |      NUMPRT = NUMPRT + 1
        380 |      IF (NUMPRT .GT. 10) GO TO 750
        381 |C
        382 |C     A LARGE CHANGE IN THETA IS PRINTED AFTER BEING STORED IN THOLD.
        383 |      DELTHMAX = -9999.E0
        384 |C
        385 |      DO 600 K=1,KM
        386 |         THOLD(K) = THNEW(K) - THOLD(K)
        387 |         DELTHMAX = MAX ( DELTHMAX, ABS(THOLD(K) ) )
        388 |  600 CONTINUE
        389 |C
        390 |      IF (DELTHMAX .GT. 5.0E0) THEN
        391 |         PRINT 700, I, J, XLONG, YLAT, (THOLD(K), K=1,KM)
        392 |  700    FORMAT('0', 'I, J, LONGITUDE, LATITUDE=', 2I4, 2F10.3/
        393 |     1          ' ', 16F8.3)
        394 |      END IF
        395 |C
        396 |  750 CONTINUE
        397 |C
        398 |      DO 800 K=1,KM
        399 |C
        400 |C     CALCULATE TEMPERATURE FROM THE ADJUSTED THETA
        401 |C     AND THE EXNER FUNCTION (PI).
        402 |      T = PI(K) * THNEW(K)
        403 |C
        404 |C     CALCULATE THE ADJUSTED SPECIFIC HUMIDITY FROM RELATIVE
        405 |C     HUMIDITY, ADJUSTED TEMPERATURE, AND PRESSURE.  SPECIFIC
        406 |C     HUMIDITY IS BOUND AWAY FROM ZERO AND THE PRESSURE-WEIGHTED
        407 |C     SPECIFIC HUMIDITY AND TEMPERATURE ARE CALCULATED.
        408 |C
        409 |      ESAT = 0.00611E0 * REAL( REAL(273.16E0/T) ** REAL(A) )
        410 |     1       * EXP(APB * (1.0E0 - 273.16E0/T))
        411 |      QSAT = 0.622E0 * ESAT / (P(K) - 0.378E0 * ESAT)
        412 |      Q = RH(K) * QSAT
        413 |C
        414 |C     NEGATIVE VALUES OF SPECIFIC HUMIDITY ARE PRINTED.
        415 |      IF (Q .GE. 0.0E0) GO TO 775
        416 |      KOUNT2 = KOUNT2 + 1
        417 |      IF (KOUNT2 .GE. 1) GO TO 775
        418 |      PRINT 126, KOUNT2, NG, I, J, K, VBL(MADDHU+I+IM*J+IJ*K),
        419 |     1     VBL(MADDHV+I+IM*J+IJ*K), VBL(MADDHTH+I+IM*J+IJ*K), Q
        420 |  126 FORMAT (' ', 'Q IS NEGATIVE.  KOUNT,NG,I,J,K,U,V,T,Q=',
        421 |     1             I6, 4I5, 4(1PE16.7))
        422 |C
        423 |  775 CONTINUE
        424 |C     BOUND SPECIFIC HUMIDITY AWAY FROM ZERO AND MULTIPLY BY
        425 |C     SURFACE PRESSURE.  THE BOUNDING MAY BE NECESSARY
        426 |C     IF SOME DRY CONVECTIVE ADJUSTMENT WAS DONE.
        427 |      Q = MAX (QMIN, Q)
        428 |      VBL(MADDHQ+I+IM*J+IJ*K) = H * Q
        429 |C
        430 |      VBL(MADDHTH+I+IM*J+IJ*K) = THNEW(K) * H
        431 |C
        432 |C     CALCULATE VIRTUAL TEMPERATURE FOR THE BOTTOM LAYER AND SAVE
        433 |C     IT FOR USE IN SUBROUTINE TASKG, WHICH SETS UP THE SURFACE
        434 |C     PRESSURE TENDENCY METHOD.
        435 |      IF (K .EQ. 1) VIRTTEMP(I+(J-1)*IM, NG) = T * (1.0E0 + FACTOR * Q)
        436 |C
        437 |C     DETERMINE THE MAGNITUDES AND LOCATIONS OF THE POINTS WITH
        438 |C     THE LARGEST RELATIVE HUMIDITIES, AND THE NUMBER OF POINTS WITH
        439 |C     RELATIVE HUMIDITIES GREATER THAN 100 PERCENT.
        440 |      RH(K) = Q / QSAT
        441 |C
        442 |      IF (RH(K) .GT. 1.0E0) THEN
        443 |         NUMRH(K, NG) = NUMRH(K, NG) + 1
        444 |      END IF
        445 |C
        446 |      IF (RH(K) .GT. RHMAX(K, NG)) THEN
        447 |         RHMAX (K, NG) = RH(K)
        448 |         IMAXRH(K, NG) = I
        449 |         JMAXRH(K, NG) = J
        450 |      END IF
        451 |C
        452 |  800 CONTINUE
        453 |C
        454 |C     FOOT OF THE I LOOP.
        455 |  900 CONTINUE
        456 |C
        457 |C     FOOT OF THE J LOOP.
        458 | 1000 CONTINUE
        459 |C
        460 |      PRINT 1050, NG, NUMPRT
        461 | 1050 FORMAT ('0', '***** THE NUMBER OF GRID POINTS WITH DRY ',
        462 |     1             'CONVECTIVE ADJUSTMENT IN THE NORTHERN HEMISPHERE ',
        463 |     2             'ON GRID', I2, ' IS', I7, '. *****')
        464 |C
        465 |C     SUMMARY OF NUMBER OF POINTS WITH NEGATIVE SPECIFIC HUMIDITY.
        466 |      PRINT 1060, NG, KOUNT1
        467 | 1060 FORMAT ('0', '***** THE NUMBER OF OCCURRENCES OF NEGATIVE ',
        468 |     1             'SPECIFIC HUMIDITIES ON GRID',I2,
        469 |     2             ' BEFORE DRY CONVECTIVE ',
        470 |     3             'ADJUSTMENT IS', I5, '.')
        471 |C
        472 |      PRINT 1070, NG, KOUNT2
        473 | 1070 FORMAT ('0', '***** THE NUMBER OF OCCURRENCES OF NEGATIVE ',
        474 |     1             'SPECIFIC HUMIDITIES ON GRID',I2,
        475 |     2             ' AFTER  DRY CONVECTIVE ',
        476 |     3             'ADJUSTMENT IS', I5, '.'/)
        477 |C
        478 |C     FOOT OF THE NGRDUSE LOOP.
        479 | 1100 CONTINUE
        480 |C
        481 |C
        482 |C     PRINT THE MAGNITUDES AND LOCATIONS OF THE LARGEST RELATIVE
        483 |C     HUMIDITIES, AND THE NUMBER OF POINTS WITH RELATIVE HUMIDITIES
        484 |C     GREATER THAN 100 PERCENT.
        485 |      WRITE (6, 1200)
        486 | 1200 FORMAT (/// '0', 'STATISTICS ON THE INITIAL RELATIVE ',
        487 |     1                 'HUMIDITIES:'/
        488 |     2            '0', 8X, 'LAYER', 7X, 'MAX RH (PERCENT)',
        489 |     3                 7X, 'I POINT OF MAX',
        490 |     4                 7X, 'J POINT OF MAX',
        491 |     5                 7X, 'NO. OF POINTS WITH RH GREATER THAN ',
        492 |     6                     '100 PERCENT')
        493 |C
        494 |      DO 1600 NG = 1, NGRDUSE
        495 |C
        496 |         WRITE (6, 1300) NG
        497 | 1300    FORMAT ('0', 'GRID', I2, ':')
        498 |C
        499 |         DO 1500 K = KM, 1, -1
        500 |C
        501 |            WRITE (6, 1400) K, RHMAX(K, NG), IMAXRH(K, NG),
        502 |     1                      JMAXRH(K, NG), NUMRH(K, NG)
        503 | 1400       FORMAT (' ', I11, 2PF20.2, I20, I21, I37)
        504 |C
        505 | 1500    CONTINUE
        506 |C
        507 | 1600 CONTINUE
        508 |C
        509 |C
        510 |      WRITE (6, 1700)
        511 | 1700 FORMAT ('0')
        512 |C
        513 |C
        514 |      RETURN
        515 |C
        516 |      END
 
