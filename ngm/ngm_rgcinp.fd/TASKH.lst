        415 |C     AVAILABILITY ARE LESS THAN 1.
        416 |      VBL(MADDMA) = 0.15E0
        417 |C
        418 |      WRITE (IOUTUPRT, 3450) LONG0, LAT0, AMA(LONG0,LAT0), II, JJ,
        419 |     1            AMA(II,JJ),     AMA(II,JJP1),
        420 |     2            AMA(IIP1,JJP1), AMA(IIP1,JJ),
        421 |     3            I, J, NG
        422 | 3450 FORMAT (' ', 'ISLAND?', 2X, 2I4, F5.1, 2I4, 4F7.1, 3I5)
        423 |      GO TO 4001
        424 |C
        425 | 3461 CONTINUE
        426 |C     IF 1, 2, OR 3 OF THE SURROUNDING POINTS HAVE MOISTURE AVAIL.
        427 |C     LESS THAN 1, THE VALUE FOR THE NEAREST
        428 |C     POINT OF THE MOISTURE AVAILABILITY FIELD FOUND
        429 |C     SATISFYING THIS CONSTRAINT IS USED.
        430 |C
        431 |      IF(KOUNT .EQ. 4) GO TO 3471
        432 |C
        433 |      VBL(MADDMA)  = AMA(IPOINT, JPOINT)
        434 |C
