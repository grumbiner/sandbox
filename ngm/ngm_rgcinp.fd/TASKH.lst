         92 |C
         93 |C
         94 |C     SPECIFY THE SIZES OF THE LAT/LON FIXED FIELDS FOR
         95 |C     SURFACE ROUGHNESS AND ALBEDO (ASSUME ONE-POINT WRAP
         96 |C        AROUND IN LONGITUDE).
         97 |C
         98 |      PARAMETER (IMAXZ0 = 129,
         99 |     *           JMAXZ0 = 102,
        100 |     *           IMAXALB = 361,
        101 |     *           JMAXALB = 180)
        102 |C
        103 |      REAL WGTCS(JMAXZ0/2),RCS2(JMAXZ0/2),COLRAD(JMAXZ0),
        104 |     *     WGT(JMAXZ0/2),SLAT(JMAXZ0),WLAT(JMAXZ0)
        105 |      DIMENSION BUFLAB85(8)
        106 |      DIMENSION SPECIF(10), ISPECIF(10)
        107 |      DIMENSION ALB(IMAXALB, JMAXALB),Z0(IMAXZ0, JMAXZ0),
        108 |     *          AMA(IMAXALB, JMAXALB),TSD(IMAXALB,JMAXALB)
        109 |      DIMENSION ALONGZ0(IMAXZ0)
        110 |      DIMENSION ALATZ0 (JMAXZ0)
        111 |      DIMENSION ALONGALB(IMAXALB)
        112 |      DIMENSION ALATALB (JMAXALB)
        113 |C
        114 |      EQUIVALENCE (BUFLAB85, LABEL85)
        115 |      EQUIVALENCE (ANAL(1,1,1), ALB(1,1))
        116 |      EQUIVALENCE (ITEMP, CHAR01(1))
        117 |C
        118 |C     SPECIFY THE UNIT NUMBER OF THE PRINTER.
        119 |      IOUTUPRT = 6
        120 |C
        121 |C
        122 |C     ENSURE THAT THE SIZE OF ARRAY ALB IS LESS THAN THE SIZE OF
        123 |C     ARRAY ANAL.
        124 |      LONFP10 =  ILONFP1
        125 |      NLAT0   =  INLAT
        126 |      LEVS0   =  ILEVS
        127 |      IF ((LONFP10 * NLAT0 * LEVS0) .LT. (IMAXALB * JMAXALB)) THEN
        128 |         WRITE (IOUTUPRT, 50)
        129 |   50    FORMAT ('0', 'THE SIZE OF ARRAY ANAL IN SUBROUTINE TASKH ',
        130 |     1                'IS SMALLER THAN THAT OF ARRAY ALB.'/
        131 |     2           ' ', 'EXECUTION TERMINATING.'//)
        132 |C
        133 |C     TERMINATE THE CONTROLLEE WITH A RETURN CODE OF 8.
        134 |         ERRMSG(1) = 'IN SUBROUTINE TASKH    NEAR STATEME'
        135 |         ERRMSG(2) = 'NT NUMBER 50:                      '
        136 |         ERRMSG(3) = 'THE SIZE OF ARRAY ANAL IS SMALLER T'
        137 |         ERRMSG(4) = 'HAN THAT OF ARRAY ALB.             '
        138 |C
        139 |         CALL ERRSTOP (IOUTUPRT, ERRMSG)
        140 |      END IF
        141 |C
        142 |C
        143 |      IHOUR0  = NINT(SPECS(2))
        144 |      IDAY0   = NINT(SPECS(3))
        145 |      IMONTH0 = NINT(SPECS(4))
        146 |      IYEAR0  = NINT(SPECS(5))
        147 |C
        148 |C     READ ALBEDO FIELD
        149 |C
        150 |      IMREAD = (IMONTH0-1)/3 + 1
        151 |      IRECORD = 0
        152 |      DO 666 KMONTH=1,IMREAD
        153 |            IRECORD = IRECORD + 1
        154 |            READ (INUALB, ERR=2010, END=2050) ALB
        155 |            PRINT 7739 , IRECORD
        156 |7739        FORMAT(' ***** JUST READ ALBEDO FOR SEASON ',I6)
        157 |666   CONTINUE
        158 |C
        159 |C     READ Z0 FIELD
        160 |C
        161 |      IRECORD = 1
        162 |      READ (INUZ0, ERR=3010, END=3050) Z0
        163 |      PRINT 5739
        164 |5739  FORMAT(' ***** JUST READ Z0 FIELD     ')
        165 |C
        166 |C     READ MOISTURE AVAIBALITY
        167 |C
        168 |      IRECORD = 1
        169 |      READ (INUMA, ERR=4010, END=4050) AMA
        170 |      PRINT 7785
        171 |7785  FORMAT(' ***** JUST READ MOIST AVAILABILITY FIELD ')
        172 |C
        173 |C     READ TERRAIN STANDARD DEVIATION
        174 |C
        175 |      IRECORD = 1
        176 |      READ (INUTSD, ERR=5010, END=5050) TSD
        177 |      PRINT 7786
        178 |7786  FORMAT(' ***** JUST READ TERRAIN STANDARD DEVIATION ')
        179 |C
        180 |C     ERROR CHECK ALBEDO
        181 |C     SCALE SO THAT IT RANGES FROM 0. - 1.
        182 |C
        183 |      DO 200  J=1,JMAXALB
        184 |         DO 200  I=1,IMAXALB
        185 |            ALB(I,J)=ALB(I,J)*0.01E0
        186 |            IF(ALB(I,J).GT.0.8E0) ALB(I,J)=0.8E0
        187 |            IF(ALB(I,J).LT.0.06E0) ALB(I,J)=0.06E0
        188 |  200 CONTINUE
        189 |      print *,' done with loop 200'
        190 |C
        191 |C     REVERSE ORDER OF Z0 FIELD
        192 |C     Z0 , FROM GLOBAL GROUP, STARTS AT NORTH POLE
        193 |C     JEH'S INTERPOLATION ASSUMES THE FIELD STARTS AT THE SOUTH POLE
        194 |C
        195 |      JLIM = JMAXZ0 / 2
        196 |      DO 6666 J=1,JLIM
        197 |         DO 6666 I=1,IMAXZ0
        198 |            ATEMP = Z0(I,J)
        199 |            Z0(I,J) = Z0(I , JMAXZ0 + 1 - J)
        200 |            Z0(I , JMAXZ0 + 1 - J ) = ATEMP
        201 |6666  CONTINUE
        202 |      print *,' done with loop 6666'
        203 |C
        204 |C     CONVERT Z0 TO METERS FROM CENTIMETERS
        205 |C
        206 |      DO 250  J = 1, JMAXZ0
        207 |      DO 250  I = 1, IMAXZ0
        208 |      Z0(I,J) = Z0(I,J) * 0.01E0
        209 |  250 CONTINUE
        210 |      print *,' done with loop 250'
        211 |C
        212 |C     INTERPOLATE THE Z0 ANALYSIS FROM THE R40 GAUSSIAN
        213 |C     GRID TO THE NGM POLAR STEREOGRAPHIC GRIDS
        214 |C
        215 |      KMAX = 1
        216 |      ISYM = 0
        217 |      FACTOR = 1.0E0
        218 |C
        219 |C     SPECIFY THE LOCATIONS OF THE GRID IN LATITUDE AND
        220 |C     LONGITUDE.
        221 |C
        222 |      DO 7753 I=1,IMAXALB
        223 |         ALONGALB(I)=0.5E0 +  REAL(I-1)
        224 |7753  CONTINUE
        225 |      ALONGALB(IMAXALB)=ALONGALB(1)
        226 |C
        227 |      DO 7754 J=1,JMAXALB
        228 |         ALATALB(J)=-89.5E0 +  REAL(J-1)
        229 |7754  CONTINUE
        230 |C
        231 |C
        232 |      DO 1100 I=1,IMAXZ0
        233 |         ALONGZ0(I)= 0.E0+( REAL(I-1) / REAL (IMAXZ0-1) ) * 360.E0
        234 | 1100 CONTINUE
        235 |      ALONGZ0(IMAXZ0) = 0.E0
        236 |C
        237 |      JLIM = JMAXZ0/2
        238 |      print *,' enter glats'
        239 |      CALL SPLAT(4,JMAXZ0,SLAT,WLAT)
        240 |c     CALL GLATS (JMAXZ0,COLRAD,WGT,WGTCS,RCS2)
        241 |      print *,' exit glats'
        242 |      JINC = 0
        243 |      DO 7777 J=JMAXZ0,1,-1
        244 |              JINC = JINC + 1
        245 |              COLRAD(J) = ASIN(SLAT(J))
        246 |              ALATZ0(JINC) = COLRAD(J)*180.E0/3.14159E0
        247 |              print *,j,jinc,colrad(j),alatz0(jinc)
        248 |7777  CONTINUE
        249 |C
        250 |      print *,'start interp '
        251 |      CALL STATS (ALB,1,IMAXALB,IMAXALB,1,JMAXALB,JMAXALB,1,KMAX,KMAX,
        252 |     1            'INPUT ALBEDO ANAL ')
        253 |C
        254 |      CALL INTERP(ALB, IMAXALB, JMAXALB, KMAX, ALONGALB, ALATALB,
        255 |     1            ISYM, INDEXALB, FACTOR)
        256 |C
        257 |      CALL STATS (AMA,1,IMAXALB,IMAXALB,1,JMAXALB,JMAXALB,1,KMAX,KMAX,
        258 |     1            'INPUT MOIST AVAIL ')
        259 |C
        260 |C     CALL INTERP(AMA, IMAXALB, JMAXALB, KMAX, ALONGALB, ALATALB,
        261 |C    1            ISYM, INDEXMA, FACTOR)
        262 |C
        263 |      CALL STATS (TSD,1,IMAXALB,IMAXALB,1,JMAXALB,JMAXALB,1,KMAX,KMAX,
        264 |     1            'INPUT TER STD DEV ')
        265 |C
        266 |      CALL INTERP(TSD, IMAXALB, JMAXALB, KMAX, ALONGALB, ALATALB,
        267 |     1            ISYM, INDEXTSD, FACTOR)
        268 |C
        269 |      CALL STATS (Z0, 1,IMAXZ0,IMAXZ0, 1,JMAXZ0,JMAXZ0, 1,KMAX,KMAX,
        270 |     1            'INPUT Z0   ANAL ')
        271 |C
        272 |      CALL INTERP(Z0, IMAXZ0, JMAXZ0, KMAX, ALONGZ0, ALATZ0,
        273 |     1            ISYM, INDEXZ0, FACTOR)
        274 |C
        275 |C     ASSIGN ALBEDO AND Z0 ANALYSIS CHARACTERISTICS TO SPECS ARRAY
        276 |C
        277 |      SPECS(301) = JMAXALB
        278 |      SPECS(302) = ALATALB(1)
        279 |      SPECS(303) = 1.E0
        280 |      SPECS(304) = ALATALB(JMAXALB)
        281 |      SPECS(305) = IMAXALB
        282 |      SPECS(306) = ALONGALB(1)
        283 |      SPECS(307) = 1.E0
        284 |      SPECS(308) = ALONGALB(IMAXALB)
        285 |C
        286 |C     RESOLVE WATER VS. LAND INCONSISTENCIES
        287 |C
        288 |      DO 4400 NG=1,NGRDUSE
        289 |         CALL GRDSET (NG, INDEXH,
        290 |     *                DUMMY, IM, JM, XPOL, YPOL, CMU, DLAM)
        291 |         IJ = IM * JM
        292 |         MADDALB = IADDRG(INDEXALB, NG)
        293 |         MADDZ0  = IADDRG(INDEXZ0,  NG)
        294 |         MADDMA  = IADDRG(INDEXMA,  NG)
        295 |         MADDCG  = IADDRG(INDEXCG,  NG)
        296 |       DO 300  L=1,IJ
        297 |         VBL(MADDCG+L-1) = 65000.E0
        298 |         IF(BITSEA(L,NG)) THEN
        299 |           VBL(MADDALB+L-1) = 0.06E0
        300 |         ELSE
        301 |           IF(VBL(MADDALB+L-1).LT.0.15E0) VBL(MADDALB+L-1)=0.15E0
        302 |           IF(VBL(MADDZ0 +L-1).LT.0.01E0) VBL(MADDZ0 +L-1)=0.01E0
        303 |         ENDIF
        304 |  300  CONTINUE
        305 |C
        306 |C   NOW DO THE MOISTURE AVAILABILITY INTERPOLATION
        307 |C
        308 |      DO 4300 J=1,JM
        309 |         YNGM = REAL(J) - YPOL
        310 |C
        311 |         DO 4299 I=1,IM
        312 |            XNGM = REAL(I) - XPOL
        313 |            MADDMA  = IADDRG(INDEXMA, NG)  -1 -IM +I +IM*J
        314 |C
        315 |      IJPLACE = (J-1)*IM + I
        316 |      IF (BITWVL(IJPLACE,NG)) THEN
        317 |         VBL(MADDMA) = 1.0E0
        318 |      ELSE
        319 |C
        320 |C     DETERMINE THE LATITUDE AND LONGITUDE OF THE NGM GRID POINT.
        321 |            IRANGE = 1
        322 |            IPRT = 0
        323 |C     THE LONGITUDE WILL RANGE FROM 0 AT THE GREENWICH MERIDIAN
        324 |C     EASTWARD TO 360 DEGREES FOLLOWING THE CALL TO SUBROUTINE
        325 |C     CORNER WITH IRANGE=1.
        326 |            CALL CORNER (XNGM, YNGM, CMU, DLAM, IRANGE, IPRT,
        327 |     1                   XLONG, YLAT)
        328 |C
        329 |C     ENSURE LATITUDE IS WITHIN RANGE.
        330 |            IF (ABS(YLAT) .LE. 90.E0) GO TO 3320
        331 |            WRITE (IOUTUPRT, 3310) YLAT
        332 | 3310       FORMAT ('0', 'LATITUDE OUT OF RANGE IN SUBROUTINE TASKH. ',
        333 |     1                   'YLAT=', F8.2, '.  JOB TERMINATED.')
        334 |C
        335 |C     TERMINATE THE CONTROLLEE WITH A RETURN CODE OF 8.
        336 |            ERRMSG(1) = 'IN SUBROUTINE TASKH    NEAR STATEME'
        337 |            ERRMSG(2) = 'NT NUMBER 3310:                    '
        338 |            ERRMSG(3) = 'THE LATITUDE IS OUT OF RANGE.      '
        339 |            ERRMSG(4) = '                                   '
        340 |C
        341 |            CALL ERRSTOP (IOUTUPRT, ERRMSG)
        342 |C
        343 | 3320       CONTINUE
        344 |C     SET UP EQUATORIAL BOUNDARY CONDITION.
        345 |C     ALL FIELDS IN THIS SUBROUTINE ARE ASSUMED TO BE EVEN
        346 |C     WITH RESPECT TO EQUATORIAL SYMMETRY.
        347 |      YYLAT = ABS(YLAT)
        348 |C
        349 |C    NOW DETERMINE THE LOCATION OF THE NGM POINT WITH RESPECT TO
        350 |C    THE VARIOUS DATA FIELDS
        351 |C
        352 |C     DETERMINE THE LOCATION OF THE NGM GRID POINT WITHIN THE
        353 |C     1-DEGREE-BY-1-DEGREE HORIZONTAL MOISTURE AVAILABILITY FIELD.
        354 |C
        355 |C
        356 |      CALL FIND (YYLAT,ALATALB,JMAXALB,LAT0)
        357 |      LONG0 = INT(XLONG - 0.5E0) + 1
        358 |C
        359 |C
        360 |      XRATIO = XLONG - ALONGALB(LONG0)
        361 |      YRATIO = YYLAT - ALATALB(LAT0)
        362 |C
        363 |      II = LONG0
        364 |      JJ = LAT0
        365 |      IIP1= II + 1
        366 |      JJP1= JJ + 1
        367 |C
        368 |      AREA11 = (1.0E0 - XRATIO) * (1.0E0 - YRATIO)
        369 |      AREA21 = XRATIO * (1.0E0 - YRATIO)
        370 |      AREA12 = (1.0E0 - XRATIO) * YRATIO
        371 |      AREA22 = XRATIO * YRATIO
        372 |C
        373 |      KOUNT = 0
        374 |C
        375 |      IF(AMA(II, JJ) .LT. 1.E0) THEN
        376 |         KOUNT = KOUNT + 1
        377 |         AREA = AREA11
        378 |         IPOINT = II
        379 |         JPOINT = JJ
        380 |      END IF
        381 |C
        382 |      IF(AMA(II, JJP1) .LT. 1.E0) THEN
        383 |         KOUNT = KOUNT + 1
        384 |         IF (AREA12 .GT. AREA) THEN
        385 |            AREA = AREA12
        386 |            IPOINT = II
        387 |            JPOINT = JJP1
