          1 |C
          2 |C
          3 |      SUBROUTINE PRINTBIT (BITS, IDIM, JDIM, NG, LABEL)
          4 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          5 |C                .      .    .                                       .
          6 |C SUBPROGRAM:    PRINTBIT    PRINTS THE LOGICAL ARRAY CALLED BITS
          7 |C   PRGMMR: JIM HOKE         ORG: W/NMC22    DATE: 85-03-27
          8 |C
          9 |C ABSTRACT: PRINTS THE LOGICAL ARRAY CALLED BITS
         10 |C
         11 |C PROGRAM HISTORY LOG:
         12 |C   85-03-27  JIM HOKE
         13 |C   88-08-03  BRIAN SCHMIDT ADDED DOCBLOCK
         14 |C   90-07-01  KEN MITCHELL CONVERTED TO CRAY Y-MP
         15 |C   92-07-14  KEN MITCHELL MODIFIED TO PRINT ENTIRE GRID
         16 |C
         17 |C USAGE:    CALL PRINTBIT (BITS, IDIM, JDIM, NG, LABEL)
         18 |C   INPUT ARGUMENT LIST:
         19 |C     BITS     - LOGICAL ARRAY TO BE PRINTED
         20 |C     IDIM     - FIRST DIMENSION OF ARRAY BITS
         21 |C     JDIM     - SECOND DIMENSION OF ARRAY BITS
         22 |C     NG       - GRID NUMBER FOR LOGICAL ARRAY TO BE PRINTED
         23 |C     LABEL    - LABEL DESCRIBING GRID NG
         24 |C
         25 |C   OUTPUT FILES:
         26 |C     FT06F001 - FOR PRINTOUT
         27 |C
         28 |C REMARKS: THE LABEL CAN HAVE UP TO 24 CHARACTERS
         29 |C
         30 |C ATTRIBUTES:
         31 |C   LANGUAGE: FORTRAN 77
         32 |C   MACHINE:  CRAY Y-MP
         33 |C
         34 |C$$$
         35 |C
         36 |      LOGICAL BITS
         37 |C
         38 |      DIMENSION BITS(IDIM, JDIM)
         39 |      DIMENSION LABEL(3)
         40 |      DIMENSION ILABEL1(IDIM),ILABEL2(IDIM),ILABEL3(IDIM)
         41 |C
         42 |C     ITOT IS NO. OF "PAGES" PRINTED PER FIELD
         43 |C      (MAX OF 74 COLUMNS OF VALUES PRINTED PER PAGE)
         44 |C
         45 |      ITOT = (IDIM - 1)/74 + 1
         46 |C
         47 |      DO 100 I=1,IDIM
         48 |      ILABEL1(I) = I / 100
         49 |      ILABEL2(I) = ( I - ILABEL1(I)*100 ) / 10
         50 |      ILABEL3(I) = MOD (I, 10)
         51 |  100 CONTINUE
         52 |C
         53 |      PRINT 200, NG, (LABEL(I), I=1,3)
         54 |  200 FORMAT (///,'LOGICAL MASK ARRAY FOR GRID',I2,' -- ',3A8)
         55 |C
         56 |C     PRINT THE NUMBER OF ELEMENTS AND THE NUMBER OF TRUE ELEMENTS
         57 |C
         58 |      IJ     = IDIM * JDIM
         59 |      NUMONES= 0
         60 |      DO 225  I=1,IDIM
         61 |      DO 225  J=1,JDIM
         62 |      IF(BITS(I,J)) NUMONES=NUMONES+1
         63 |  225 CONTINUE
         64 |      PRINT 250, IJ, NUMONES
         65 |  250 FORMAT (//,'THERE ARE', I6, ' ELEMENTS DISPLAYED, OF WHICH',
         66 |     1             I6, ' ARE TRUE.')
         67 |C
         68 |      DO 800  K=1,ITOT
         69 |C
         70 |      IB = 1 + (K-1)*74
         71 |      IE = IB + 74 - 1
         72 |      IF (IE .GT. IDIM) IE=IDIM
         73 |C
         74 |      PRINT 290, K,ITOT,IB,IE
         75 |  290 FORMAT(//,1X,'PAGE=',I2,' OF',I2,' (COLS ',I3,' TO ',I3,')')
         76 |C
         77 |      PRINT 300, (ILABEL1(I), I=IB,IE)
         78 |  300 FORMAT(/, 3X, 'I= ', 74I1)
         79 |C
         80 |      PRINT 400, (ILABEL2(I), I=IB,IE)
         81 |  400 FORMAT(  6X,  74I1)
         82 |C
         83 |      PRINT 500, (ILABEL3(I), I=IB,IE)
         84 |  500 FORMAT(   6X,  74I1)
         85 |C
         86 |      PRINT 550
         87 |  550 FORMAT (/)
         88 |C
         89 |      DO 700 J=1,JDIM
         90 |         JJ = JDIM + 1 - J
         91 |C
         92 |         PRINT 600, JJ, (BITS(I, JJ), I=IB,IE)
         93 |  600    FORMAT ( 'J=', I3, 1X, 74L1)
         94 |C
         95 |  700 CONTINUE
         96 |C
         97 |  800 CONTINUE
         98 |C
         99 |      PRINT 550
        100 |C
        101 |      RETURN
        102 |      END
 
