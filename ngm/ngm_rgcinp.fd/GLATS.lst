          1 |      SUBROUTINE GLATS(KHAF,COLRAD,WGT,WGTCS,RCS2)
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .                                       .
          4 |C SUBPROGRAM:    GLATS       COMPUTES COLAT OF R40 G.G. (FROM SELA)
          5 |C   PRGMMR: TUCCILLO         ORG: W/NMC22    DATE: 86-10-21
          6 |C
          7 |C ABSTRACT: COMPUTES COLATITUDE OF R40 G.G.
          8 |C
          9 |C PROGRAM HISTORY LOG:
         10 |C   86-10-21  JIM TUCCILLO
         11 |C   88-08-02  BRIAN SCHMIDT ADDED THE DOCBLOCK
         12 |C
         13 |C USAGE:    CALL GLATS ( KHAF, COLRAD, WGT, WGTCS, RCS2 )
         14 |C   INPUT ARGUMENT LIST:
         15 |C     KHAF     - NUMBER OF GAUSSIAN LATITUDES PER HEMISPHERE
         16 |C
         17 |C   OUTPUT ARGUMENT LIST:
         18 |C     COLRAD   - COLATITUDE IN RADIANS
         19 |C     WGT      - NOT USED
         20 |C     WGTCS    - NOT USED
         21 |C     RCS2     - NOT USED
         22 |C
         23 |C   OUTPUT FILES:
         24 |C     FT06F001 - FOR PRINTOUT
         25 |C
         26 |C   SUBPROGRAMS CALLED:
         27 |C     UNIQUE:    - POLY
         28 |C
         29 |C ATTRIBUTES:
         30 |C   LANGUAGE: CYBER FORTRAN 200
         31 |C   MACHINE:  CYBER 205
         32 |C
         33 |C$$$
         34 |C
         35 |      DIMENSION  COLRAD(KHAF) ,WGT(KHAF), WGTCS(KHAF), RCS2(KHAF)
         36 |C
         37 |      EPS=1.E-12
         38 |      SI=1.
         39 |      K2=2*KHAF
         40 |      RK2=K2
         41 |      SCALE=2.  /(RK2*RK2)
         42 |      K1=K2-1
         43 |      PI=ATAN(SI)*4.
         44 |      DRADZ=PI/360.
         45 |      RAD=0.
         46 |      DO 1000 K=1,KHAF
         47 |      ITER=0
         48 |      DRAD=DRADZ
         49 |1     CALL POLY(K2,RAD,P2)
         50 |2     P1 =P2
         51 |      ITER=ITER+1
         52 |      RAD=RAD+DRAD
         53 |      CALL POLY(K2,RAD,P2)
         54 |      IF ( SIGN(SI,P1) .EQ. SIGN(SI,P2) )  GO TO 2
         55 |      IF(DRAD.LT.EPS)GO TO 3
         56 |      RAD=RAD-DRAD
         57 |      DRAD=DRAD*0.25
         58 |      GO TO 1
         59 |3     CONTINUE
         60 |      COLRAD(K)=RAD
         61 |      PHI=RAD   *180.  /PI
         62 |      CALL POLY(K1,RAD,P1)
         63 |      X=  COS(RAD)
         64 |      W=SCALE*(1.  -X*X)/(P1*P1)
         65 |      WGT(K)=    W
         66 |      SN=  SIN(RAD)
         67 |      W=W/(SN*SN)
         68 |      WGTCS(K)=    W
         69 |      RC=1./(SN*SN)
         70 |      RCS2(K)=   RC
         71 |      CALL POLY(K2,RAD,P1)
         72 |      PRPHI =    PHI
         73 |      PRCOL =    COLRAD(K)
         74 |1000  CONTINUE
         75 |      PRINT 100,KHAF
         76 |100   FORMAT(1H ,'END GLATS FOR ',I3)
         77 |      RETURN
         78 |      END
 
