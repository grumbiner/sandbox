          1 |C
          2 |C
          3 |      SUBROUTINE TASKA
          4 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          5 |C                .      .    .                                       .
          6 |C SUBPROGRAM:    TASKA       ROTATE WINDS TO NGM COORDINATE SYSTEM
          7 |C   PRGMMR: JIM HOKE         ORG: W/NMC22    DATE: 85-03-27
          8 |C
          9 |C ABSTRACT: ROTATES THE WIND COMPONENTS IN A LATITUDE-LONGITUDE
         10 |C   COORDINATE SYSTEM TO THOSE IN THE NGM COORDINATE SYSTEM.  IT
         11 |C   IS ASSUMED THAT THE EQUATORIAL SYMMETRY CONDITION HAS ALREADY
         12 |C   BEEN APPLIED.
         13 |C
         14 |C PROGRAM HISTORY LOG:
         15 |C   85-03-27  JIM HOKE
         16 |C   88-08-03  BRIAN SCHMIDT ADDED THE DOCBLOCK
         17 |C
         18 |C USAGE:    CALL TASKA
         19 |C
         20 |C   SUBPROGRAMS CALLED:
         21 |C     LIBRARY:
         22 |C       COMMON   - CONSTS
         23 |C                  CONSTN
         24 |C
         25 |C REMARKS: NOTE THAT WIND COMPONENTS AT THE NORTH POLE ARE ROTATED,
         26 |C   EVEN THOUGH THEY HAVE YET TO BE CORRECTLY DETERMINED.
         27 |C   THE WIND COMPONENTS AT THE NORTH POLE STILL EQUAL THE
         28 |C   VALUES AT THE NORTHERNMOST GAUSSIAN LATITUDE.
         29 |C
         30 |C ATTRIBUTES:
         31 |C   LANGUAGE: CYBER FORTRAN 200
         32 |C   MACHINE:  CYBER 205
         33 |C
         34 |C$$$
         35 |C
         36 |C
         37 |      IMPLICIT REAL (A-H, O-Z)
         38 |C
         39 |C
         40 |      INCLUDE 'parmodel'
  1       1 |C     PARAMETER FILE
  1       2 |C
  1       3 |C     SETUP FOR IBM SINGLE PRECISION REPRESENTATION
  1       4 |C     ON OUTPUT
  1       5 |C                      THIS VERSION 1 May 1991 (DiMego)
  1       6 |C
  1       7 |      PARAMETER (
  1       8 |     *      IBPW      =      4,
  1       9 |     *      IBPOR     = 120000,
  1      10 |     *      INUMREC   = IBPOR / IBPW,
  1      11 |     *      INSPECS   =    700,
  1      12 |     *      ILATG2    =    120,
  1      13 |     *      ILEVH     =     12,
  1      14 |     *      ILEVS     =     16,
  1      15 |     *      ILONF     =    360,
  1      16 |     *      INLATSH   =      5,
  1      17 |     *      IIJMAX    =  23668,
  1      18 |     *      IIJMAXB   =  25536,
  1      19 |     *      IIJRAD    =    892,
  1      20 |     *      IJCAP     =     80,
  1      21 |     *      IWPCPL    =     80,
  1      22 |     *      IKM       =     16 )
  1      23 |      PARAMETER (
  1      24 |     *      ILBCH     =   7*(147+111-7),
  1      25 |     *      ILBHI     =    (147+111-26),
  1      26 |     *      ILBUI     =    (2*147+111-39),
  1      27 |     *      ILBUO     =   (8*147+7*111-56),
  1      28 |     *      ILBVI     =    (147+2*111-39),
  1      29 |     *      ILBVO     =   (7*147+8*111-56),
  1      30 |     *      ILMOUT    =     20 )
  1      31 |      PARAMETER (
  1      32 |     *      IMAXOUTF  =     17,
  1      33 |     *      IMAXOUTT  =      5,
  1      34 |     *      IMAXOUTV  =    100,
  1      35 |     *      IMAXPROF  =      6,
  1      36 |     *      INFILLER  =2500000,
  1      37 |     *      INH       =     65,
  1      38 |     *      INGRDUSE  =      2,
  1      39 |     *      INGRDUS1  =      1,
  1      40 |     *      INIADDRS  =     44,
  1      41 |     *      INSCR     =     23,
  1      42 |     *      INSCRGEO  =     12,
  1      43 |     *      INSCR3    =     10,
  1      44 |     *      INSOIL    =      2,
  1      45 |     *      INSUMG    =  42164,
  1      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  1      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  1      48 |     *      INSHEM    = 13 * INH + 23,
  1      49 |     *      IIJKMAX   = IKM * IIJMAX )
  1      50 |      PARAMETER (
  1      51 |     *  IIWORDSB  = IIJMAXB / 64,
  1      52 |     *  IKMP1     = IKM + 1,
  1      53 |     *  ILVLTOT   = 7 * IKM + 37,
  1      54 |     *  ILONFP1   = ILONF + 1,
  1      55 |     *  INVBL     = ILVLTOT * INSUMG,
  1      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  1      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  1      58 |     *  IBLK1D    =  2 * IKM + 14,
  1      59 |     *  IBLK2D    = 16 * IKM + 36,
  1      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  1      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  1      62 |C
         41 |C
         42 |C
         43 |      COMMON /CONSTS/ IH, IQ, IT, IU, IV, IZ,
         44 |     1                NLAT, LEVH, LEVS, LONF, NSIGSTP,
         45 |     2                SLAT( INLAT ), STRIPARA( INSIGSTP ,  INLAT )
         46 |C
         47 |C       *****   END OF INCLUDED COMMON BLK CONSTS   *****
         48 |C
         49 |C
         50 |      LOGICAL  BITSEA, BITSNO, BITWVL
         51 |C
         52 |      COMMON /CONSTN/ IMG( INGRDUSE ), JMG( INGRDUSE ),
         53 |     1                IAG( INGRDUS1 ), JAG( INGRDUS1 ),
         54 |     2                IBG( INGRDUS1 ), JBG( INGRDUS1 ),
         55 |     3                IADDRG( INIADDRS ,  INGRDUSE ),
         56 |     4                INDEXHU,  INDEXHV,  INDEXHTH, INDEXHQ,
         57 |     5                INDEXH,   INDEXST,  INDEXSQ,  INDEXALB,
         58 |     6                INDEXSLP, INDEXPSI, INDEXCD,  INDEXCC,  INDEXGSP,
         59 |     7                INDEXRAD, INDEXDLW, INDEXDSW, INDEXCPR, INDEXMA,
         60 |     8                INDEXSEC, INDEXZ0, INDEXTSS,  INDEXCG,  INDEXDHQ,
         61 |     9                INDEXTSD, KM, NH, NGRDUSE,
         62 |     1                SPECS( INSPECS ), DELSIG( IKM ),
         63 |     2                POVH( IKM ), PIOVHK( IKM ),
         64 |     3                CMUU  ( INGRDUSE ),
         65 |     4                XPOLEH( INGRDUSE ), YPOLEH( INGRDUSE ),
         66 |     5                XPOLEU( INGRDUSE ), YPOLEU( INGRDUSE ),
         67 |     6                XPOLEV( INGRDUSE ), YPOLEV( INGRDUSE ),
         68 |     7                DLAMNGM,
         69 |     8                BITSEA( IIJMAX ,  INGRDUSE ),
         70 |     9                BITSNO( IIJMAX ,  INGRDUSE ),
         71 |     1                BITWVL( IIJMAX ,  INGRDUSE )
         72 |C
         73 |C       ****   END OF INCLUDED COMMON BLOCK CONSTN   ***
         74 |C
         75 |C
         76 |      COMMON /DUMMY2/ ANAL( ILONFP1 ,  INLAT ,  ILEVS )
         77 |C
         78 |      DIMENSION SINE( ILONF ), COSINE( ILONF )
         79 |C
         80 |      EQUIVALENCE (ANAL(1,1,1),SINE(1)),(ANAL( ILONFP1 ,1,1),COSINE(1))
         81 |C
         82 |C
         83 |C     COMPUTE THE SINE AND COSINE OF THE ROTATION ANGLE FOR
         84 |C     EACH LONGITUDE.
         85 |      DEGRAD = 3.141592654E0 / 180.E0
         86 |      DLONG = 360.E0 / REAL(LONF)
         87 |C
         88 |      DO 100 I=1,LONF
         89 |         ROTANG = DEGRAD * (DLONG * REAL(I-1) - DLAMNGM)
         90 |         SINE(I) =   SIN(ROTANG)
         91 |         COSINE(I) = COS(ROTANG)
         92 |  100 CONTINUE
         93 |C
         94 |C     PROCESS EACH LATITUDINAL STRIP OF THE NORTHERN HEMISPHERE.
         95 |C     SOUTHERN HEMISPHERIC STRIPS ARE NOT NEEDED IN THIS
         96 |C     PROGRAM BECAUSE OF THE EQUATORIAL SYMMETRY CONDITION.
         97 |      LBLOCK = 0
         98 |C
         99 |      DO 300 LAT=1,NLAT
        100 |         LBLOCK = LBLOCK + 1
        101 |C
        102 |         LOCU = IU - 1
        103 |         LOCV = IV - 1
        104 |C
        105 |         DO 200 K=1,LEVS
        106 |         DO 200 I=1,LONF
        107 |            LOCU = LOCU + 1
        108 |            LOCV = LOCV + 1
        109 |            A =                      -STRIPARA(LOCV, LBLOCK) *COSINE(I)
        110 |     1                               -STRIPARA(LOCU, LBLOCK) *  SINE(I)
        111 |            STRIPARA(LOCV, LBLOCK) = -STRIPARA(LOCV, LBLOCK) * SINE(I)
        112 |     1                               +STRIPARA(LOCU, LBLOCK) *COSINE(I)
        113 |            STRIPARA(LOCU, LBLOCK) =  A
        114 |  200    CONTINUE
        115 |C
        116 |  300 CONTINUE
        117 |C
        118 |      RETURN
        119 |C
        120 |      END
 
