          1 |      SUBROUTINE FIND (X, A, IDIM, M0)
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .                                       .
          4 |C SUBPROGRAM:    FIND        LOCATES RELATIVE POSITION OF A VALUE
          5 |C   PRGMMR: JIM HOKE         ORG: W/NMC22    DATE: 85-03-27
          6 |C
          7 |C ABSTRACT: LOCATES THE RELATIVE POSITION OF THE VALUE
          8 |C   X WITH RESPECT TO THE ARRAY A OF DIMENSION IDIM.
          9 |C   THE QUANTITY M0 IS THE FIRST-GUESS LOCATION AND ULTIMATE
         10 |C   LOCATION.  THE ULTIMATE VALUE OF M0 IS SUCH THAT X IS
         11 |C   .GE. A(M0) AND X IS .LT. A(M0 + 1).
         12 |C   THE ARRAY A IS ASSUMED TO BE ORDERED BY INCREASING VALUE.
         13 |C
         14 |C PROGRAM HISTORY LOG:
         15 |C   85-03-27  JIM HOKE
         16 |C   88-08-02  BRIAN SCHMIDT ADDED DOCBLOCK
         17 |C
         18 |C USAGE:    CALL FIND (X, A, IDIM, M0)
         19 |C   INPUT ARGUMENT LIST:
         20 |C     X        - VALUE WHOSE LOCATION IN THE ARRAY IS TO BE FOUND
         21 |C     A        - ARRAY IN WHICH THE RELATIVE POSITION OF X IS TO
         22 |C                BE FOUND
         23 |C     IDIM     - FIRST DIMENSION OF THE ARRAY A
         24 |C
         25 |C   OUTPUT ARGUMENT LIST:      (INCLUDING WORK ARRAYS)
         26 |C     M0       - LOCATION OF THE VALUE X IN THE ARRAY A
         27 |C
         28 |C   OUTPUT FILES:
         29 |C     FT06F001 - FOR PRINTOUT
         30 |C
         31 |C   SUBPROGRAMS CALLED:
         32 |C     UNIQUE:    - ERRSTOP
         33 |C
         34 |C ATTRIBUTES:
         35 |C   LANGUAGE: CYBER FORTRAN 200
         36 |C   MACHINE:  CYBER 205
         37 |C
         38 |C$$$
         39 |C
         40 |C
         41 |      IMPLICIT REAL (A-H, O-Z)
         42 |C
         43 |      CHARACTER*35 ERRMSG(4)
         44 |C
         45 |      DIMENSION A(IDIM)
         46 |C
         47 |C     SPECIFY THE PRINTER UNIT NUMBER.
         48 |      IOUTUPRT = 6
         49 |C
         50 |C     SPECIFY COUNTERS FOR MAXIMUM AMOUNT OF PRINTING.
         51 |      DATA KOUNTMIN /0/
         52 |      DATA KOUNTMAX /0/
         53 |C
         54 |C     ENSURE THAT M0 IS A REASONABLE VALUE.
         55 |      M0 = MIN(M0, IDIM)
         56 |      M0 = MAX(M0, 1)
         57 |C
         58 |C     CHECK WHETHER X IS WITHIN RANGE.
         59 |      IF (X - A(IDIM)) 200, 50, 100
         60 |C
         61 |   50 M0 = IDIM - 1
         62 |      GO TO 500
         63 |C
         64 |  100 CONTINUE
         65 |C     X IS ABOVE RANGE OF A.  IT IS SET EQUAL TO A(IDIM).
         66 |      M0 = IDIM - 1
         67 |      X = A(IDIM)
         68 |      KOUNTMAX = KOUNTMAX + 1
         69 |C
         70 |      IF (KOUNTMAX .LE. 10) THEN
         71 |         WRITE (IOUTUPRT, 150) X, A(IDIM)
         72 |  150    FORMAT(' ', 'IN SUBROUTINE FIND, X=', 1PE13.5,
         73 |     1        ' IS LARGER THAN A(IDIM)=', 1PE13.5, '  .  IT IS SET ',
         74 |     2        'EQUAL TO A(IDIM).')
         75 |C
         76 |      ELSE IF (KOUNTMAX .EQ. 11) THEN
         77 |         WRITE (IOUTUPRT, 175)
         78 |  175    FORMAT (' ', 'IN SUBROUTINE FIND, THERE WERE MORE THAN ',
         79 |     1                '10 CASES WHERE X WAS LARGER THAN A(IDIM).',
         80 |     2                '     ********************'/)
         81 |      END IF
         82 |C
         83 |      GO TO 500
         84 |C
         85 |  200 IF (X - A(1)) 250, 225, 290
         86 |C
         87 |  225 M0 = 1
         88 |      GO TO 500
         89 |C
         90 |  250 CONTINUE
         91 |C     X IS LESS THAN THE RANGE OF A.  IT IS SET TO A(1).
         92 |      M0 = 1
         93 |      X = A(1)
         94 |      KOUNTMIN = KOUNTMIN + 1
         95 |      IF (KOUNTMIN .LE. 10) THEN
         96 |         WRITE (IOUTUPRT, 275) X, A(1)
         97 |  275    FORMAT(' ', 'IN SUBROUTINE FIND, X=', 1PE13.5,
         98 |     1        ' IS SMALLER THAN A(1)=', 1PE13.5, '  .  IT IS SET ',
         99 |     2        'EQUAL TO A(1).')
        100 |C
        101 |      ELSE IF (KOUNTMIN .EQ. 11) THEN
        102 |         WRITE (IOUTUPRT, 285)
        103 |  285    FORMAT (' ', 'IN SUBROUTINE FIND, THERE WERE MORE THAN ',
        104 |     1                '10 CASES WHERE X WAS SMALLER THAN A(1).',
        105 |     2                '     ********************')
        106 |      END IF
        107 |C
        108 |      GO TO 500
        109 |C
        110 |  290 CONTINUE
        111 |C
        112 |C     X IS WITHIN THE RANGE OF A.
        113 |      IF (X .LT. A(M0)) GO TO 350
        114 |C
        115 |C     SEARCH BY STEPPING UPWARD THROUGH A.
        116 |      MP1 = M0 + 1
        117 |      DO 300 M=MP1,IDIM
        118 |      IF (X .GE. A(M)) GO TO 300
        119 |      M0 = M - 1
        120 |      GO TO 500
        121 |  300 CONTINUE
        122 |      WRITE (IOUTUPRT, 325) X, M0
        123 |  325 FORMAT('0', 'ERROR IN ARRAY SEARCH IN SUBROUTINE FIND.  X, M0 =',
        124 |     1            1PE13.5, I5, ' .  JOB TERMINATED.')
        125 |C
        126 |C              TERMINATE THE CONTROLLEE WITH A RETURN CODE OF 8.
        127 |      ERRMSG(1) = 'IN SUBROUTINE FIND     NEAR STATEME'
        128 |      ERRMSG(2) = 'NT NUMBER 325:                     '
        129 |      ERRMSG(3) = 'THERE IS AN ERROR IN THE SEARCHING '
        130 |      ERRMSG(4) = 'PROCESS THROUGH THE ARRAY.         '
        131 |C
        132 |      CALL ERRSTOP (IOUTUPRT, ERRMSG)
        133 |C
        134 |C     SEARCH BY STEPPING DOWNWARD THROUGH A.
        135 |  350 CONTINUE
        136 |      MM1 = M0 - 1
        137 |      DO 400 M=1,MM1
        138 |      MM = MM1 - M + 1
        139 |      IF(X .LT. A(MM)) GO TO 400
        140 |      M0 = MM
        141 |      GO TO 500
        142 |  400 CONTINUE
        143 |C
        144 |      WRITE (IOUTUPRT, 325) X, M0
        145 |C
        146 |C              TERMINATE THE CONTROLLEE WITH A RETURN CODE OF 8.
        147 |      ERRMSG(1) = 'IN SUBROUTINE FIND     NEAR STATEME'
        148 |      ERRMSG(2) = 'NT NUMBER 400:                     '
        149 |      ERRMSG(3) = 'THERE IS AN ERROR IN THE SEARCHING '
        150 |      ERRMSG(4) = 'PROCESS THROUGH THE ARRAY.         '
        151 |C
        152 |      CALL ERRSTOP (IOUTUPRT, ERRMSG)
        153 |C
        154 |  500 CONTINUE
        155 |C
        156 |      RETURN
        157 |C
        158 |      END
 
