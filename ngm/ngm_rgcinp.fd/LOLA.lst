          1 |      SUBROUTINE LOLA (SPECS, LATG2, INULOLA)
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .                                       .
          4 |C SUBPROGRAM:    LOLA        PUTS VERT LAT STRIPS & CREATES LAT ARRAY
          5 |C   PRGMMR: JIM HOKE         ORG: W/NMC22    DATE: 85-03-27
          6 |C
          7 |C ABSTRACT: PERFORMS THE FOLLOWING PROCESSES
          8 |C   1)  INPUTS FIELDS IN VERTICAL LATITUDINAL STRIPS
          9 |C   .   ON A LATITUDE-LONGITUDE GRID FROM A
         10 |C   .   SEQUENTIAL FILE,
         11 |C   2)  PLACES THE STRIPS IN A LARGE ARRAY IN THE CENTRAL MEMORY,
         12 |C   3)  FILLS IN SYMMETRIC FIELDS SOUTH OF THE EQUATOR FOR AS
         13 |C   .   FAR SOUTH AS THE NGM WILL EVER REASONABLY REACH
         14 |C   .   (FOR NH = 10, THIS IS 9.94 S.),
         15 |C   4)  CREATES AN ARRAY GIVING THE LATITUDE OF EACH LATITUDINAL
         16 |C   .   STRIP.
         17 |C
         18 |C PROGRAM HISTORY LOG:
         19 |C   85-03-27  JIM HOKE
         20 |C   88-08-02  BRIAN SCHMIDT ADDED DOCBLOCKS
         21 |C   90-07-01  KEN MITCHELL CONVERTED TO CRAY Y-MP
         22 |C   98-11-25  ERIC ROGERS CHANGED READ OF ON85 LABEL AT BEGINNING
         23 |C             OF SELA RECORD WITH READ OF BINARY NCEP DATE RECORD
         24 |C
         25 |C USAGE:    CALL LOLA (SPECS, LATG2, INULOLA)
         26 |C   INPUT ARGUMENT LIST:
         27 |C     SPECS    - ARRAY CONTAINING LATITUDE INFORMATION
         28 |C     LATG2    - NORTHERNMOST LATITUDE OF INPUT FIELD
         29 |C     INULOLA  - UNIT NUMBER OF SEQUENTIAL DATA SET TO BE READ IN
         30 |C
         31 |C   OUTPUT ARGUMENT LIST:
         32 |C     NONE
         33 |C
         34 |C   INPUT FILES:
         35 |C     INULOLA  - FILE CONTAINING VERTICAL LATITUDINAL STRIPS
         36 |C
         37 |C   OUTPUT FILES:
         38 |C     FT06F001 - FOR PRINTOUT
         39 |C
         40 |C   SUBPROGRAMS CALLED:
         41 |C     LIBRARY:
         42 |C       COMMON   - CONSTS
         43 |C
         44 |C ATTRIBUTES:
         45 |C   LANGUAGE: CYBER FORTRAN 200
         46 |C   MACHINE:  CYBER 205
         47 |C
         48 |C$$$
         49 |C
         50 |C
         51 |      IMPLICIT REAL (A-H, O-Z)
         52 |C
         53 |C
         54 |      INCLUDE 'parmodel'
  1       1 |C     PARAMETER FILE
  1       2 |C
  1       3 |C     SETUP FOR IBM SINGLE PRECISION REPRESENTATION
  1       4 |C     ON OUTPUT
  1       5 |C                      THIS VERSION 1 May 1991 (DiMego)
  1       6 |C
  1       7 |      PARAMETER (
  1       8 |     *      IBPW      =      4,
  1       9 |     *      IBPOR     = 120000,
  1      10 |     *      INUMREC   = IBPOR / IBPW,
  1      11 |     *      INSPECS   =    700,
  1      12 |     *      ILATG2    =    120,
  1      13 |     *      ILEVH     =     12,
  1      14 |     *      ILEVS     =     16,
  1      15 |     *      ILONF     =    360,
  1      16 |     *      INLATSH   =      5,
  1      17 |     *      IIJMAX    =  23668,
  1      18 |     *      IIJMAXB   =  25536,
  1      19 |     *      IIJRAD    =    892,
  1      20 |     *      IJCAP     =     80,
  1      21 |     *      IWPCPL    =     80,
  1      22 |     *      IKM       =     16 )
  1      23 |      PARAMETER (
  1      24 |     *      ILBCH     =   7*(147+111-7),
  1      25 |     *      ILBHI     =    (147+111-26),
  1      26 |     *      ILBUI     =    (2*147+111-39),
  1      27 |     *      ILBUO     =   (8*147+7*111-56),
  1      28 |     *      ILBVI     =    (147+2*111-39),
  1      29 |     *      ILBVO     =   (7*147+8*111-56),
  1      30 |     *      ILMOUT    =     20 )
  1      31 |      PARAMETER (
  1      32 |     *      IMAXOUTF  =     17,
  1      33 |     *      IMAXOUTT  =      5,
  1      34 |     *      IMAXOUTV  =    100,
  1      35 |     *      IMAXPROF  =      6,
  1      36 |     *      INFILLER  =2500000,
  1      37 |     *      INH       =     65,
  1      38 |     *      INGRDUSE  =      2,
  1      39 |     *      INGRDUS1  =      1,
  1      40 |     *      INIADDRS  =     44,
  1      41 |     *      INSCR     =     23,
  1      42 |     *      INSCRGEO  =     12,
  1      43 |     *      INSCR3    =     10,
  1      44 |     *      INSOIL    =      2,
  1      45 |     *      INSUMG    =  42164,
  1      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  1      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  1      48 |     *      INSHEM    = 13 * INH + 23,
  1      49 |     *      IIJKMAX   = IKM * IIJMAX )
  1      50 |      PARAMETER (
  1      51 |     *  IIWORDSB  = IIJMAXB / 64,
  1      52 |     *  IKMP1     = IKM + 1,
  1      53 |     *  ILVLTOT   = 7 * IKM + 37,
  1      54 |     *  ILONFP1   = ILONF + 1,
  1      55 |     *  INVBL     = ILVLTOT * INSUMG,
  1      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  1      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  1      58 |     *  IBLK1D    =  2 * IKM + 14,
  1      59 |     *  IBLK2D    = 16 * IKM + 36,
  1      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  1      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  1      62 |C
         55 |C
         56 |C
         57 |      DIMENSION INPDAT(8)
         58 |      REAL         DUM1, DUM2( IKMP1 ), DUM3( IKM )
         59 |C
         60 |      COMMON /CONSTS/ IH, IQ, IT, IU, IV, IZ,
         61 |     1                NLAT, LEVH, LEVS, LONF, NSIGSTP,
         62 |     2                SLAT( INLAT ), STRIPARA( INSIGSTP ,  INLAT )
         63 |C
         64 |C       *****   END OF INCLUDED COMMON BLK CONSTS   *****
         65 |C
         66 |C
         67 |      DIMENSION SPECS( INSPECS )
         68 |      DIMENSION IDUM (4)
         69 |C
         70 |      NLATSH = NLAT - LATG2 - 1
         71 |C
         72 |C              FIRST, READ IN THE OFFICE NOTE 85 LABEL.
         73 |      READ (INULOLA) INPDAT
         74 |C
         75 |C     PRINT THE DATE WORD LABEL.
         76 |      IYEAR     = INPDAT(1)
         77 |C
         78 |      IMONTH    = INPDAT(2)
         79 |C
         80 |      IDAY      = INPDAT(3)
         81 |C
         82 |      IHOUR     = INPDAT(5)
         83 |C
         84 |      WRITE (6, 100) INULOLA,
         85 |     1               IYEAR, IMONTH, IDAY, IHOUR
         86 |C
         87 |  100 FORMAT ('0', 'THE NCEP DATE RECORD FOR THE EXTRA-FINE-',
         88 |     1             'RESOLUTION LOLA GRID WAS READ ',
         89 |     2             'FROM THE DISK WITH UNIT NUMBER ', I2, '.'/
         90 |     3             ' THE VALID DATE IS :' /
         91 |     5             'Y/M/D/H = ', I4, ' / ', I2, ' / ', I2,
         92 |     6             ' / ', I2)
         93 |C
         94 |C     NOW READ IN SELA'S FILE LABEL, BUT IGNORE IT.
         95 |      READ (INULOLA) DUM1, IDUM, DUM2, DUM3
         96 |C
         97 |C     THE LATITUDINAL STRIPS CAN NOW BE PROCESSED.
         98 |C
         99 |C     FIRST, THE N.H. LATITUDINAL STRIPS ARE READ IN AND PLACED
        100 |C     IN A LARGE ARRAY BY LATITUDINAL STRIP.
        101 |C
        102 |      LBLOCK = NLATSH
        103 |C
        104 |      DO 500 LAT=1, LATG2
        105 |         LBLOCK = LBLOCK + 1
        106 |         READ (INULOLA) (STRIPARA(I, LBLOCK), I=1, NSIGSTP)
        107 |         SLAT(LBLOCK) = SPECS(500 + LAT)
        108 |C
        109 |  500 CONTINUE
        110 |C
        111 |C
        112 |C     THE POLAR LATITUDE IS GIVEN THE SAME VALUES AS THE
        113 |C     NORTHERNMOST GAUSSIAN LATITUDE.  THE POLAR VALUES WILL BE
        114 |C     MORE CORRECTLY SPECIFIED LATER IN THE PROGRAM.
        115 |      SLAT(LBLOCK+1) = 90.
        116 |      DO 510  I=1,NSIGSTP
        117 |      STRIPARA(I,LBLOCK+1) = STRIPARA(I,LBLOCK)
        118 |  510 CONTINUE
        119 |C
        120 |C
        121 |C     FOR LATITUDES SOUTH OF THE EQUATOR,  ALL FIELDS DERIVED
        122 |C     FROM THE SPECTRAL COEFFICIENT FILE ARE SET EQUAL TO THEIR
        123 |C     NORTHERN HEMISPHERIC COUNTERPARTS (EVEN SYMMETRY), EXCEPT
        124 |C     FOR THE MERIDIONAL WIND FIELD (ODD SYMMETRY).
        125 |C
        126 |      IVMAX = IT - 1
        127 |C
        128 |      DO 600 LAT=1,NLATSH
        129 |C
        130 |         LBLKN = NLATSH + LAT
        131 |         LBLKS = NLATSH - LAT + 1
        132 |         SLAT(LBLKS) = -SLAT(LBLKN)
        133 |C
        134 |C     EVEN SYMMETRY IS IMPOSED ON ALL FIELDS (EXCEPT FOR
        135 |C     ODD SYMMETRY ON THE MERIDIONAL VELOCITY FIELD).
        136 |         DO 525 I=1, NSIGSTP
        137 |            STRIPARA(I, LBLKS) = STRIPARA(I, LBLKN)
        138 |  525    CONTINUE
        139 |C
        140 |         DO 550 I=IV,IVMAX
        141 |            STRIPARA(I, LBLKS) = - STRIPARA(I, LBLKN)
        142 |  550    CONTINUE
        143 |C
        144 |  600 CONTINUE
        145 |C
        146 |C
        147 |C     PRINT THE U AND V WIND COMPONENTS IN LAYERS 13 AND 14
        148 |C     OVER THE UNITED STATES.
        149 |C
        150 |      LAT1  = 20
        151 |      LAT2  = 60
        152 |      LONG1 = 205
        153 |      LONG2 = 264
        154 |      LEV1  = 12
        155 |      LEV2  = 14
        156 |C
        157 |c     DO 800 LEV = LEV1, LEV2
        158 |C
        159 |c     WRITE (6, 725) LEV
        160 |c 725 FORMAT ('0', 'U AT LEVEL', I3, ':'/)
        161 |C
        162 |c     DO 800 LAT = LAT2, LAT1, -1
        163 |C
        164 |c        WRITE (6, 730)  LAT
        165 |c 730    FORMAT (' ', 'LAT=', I3, ':')
        166 |C
        167 |c        WRITE (6, 750)
        168 |c    1               (STRIPARA(IU-1+ (LEV-1)*LONF + I, LAT+NLATSH),
        169 |c    2                I=LONG1,LONG2)
        170 |c 750    FORMAT (' ', 8X, 20F6.1)
        171 |C
        172 |C
        173 |C
        174 |c 800 CONTINUE
        175 |C
        176 |c     DO 1000 LEV = LEV1, LEV2
        177 |C
        178 |c     WRITE (6, 925) LEV
        179 |c 925 FORMAT ('0', 'V AT LEVEL', I3, ':'/)
        180 |C
        181 |c     DO 1000 LAT = LAT2, LAT1, -1
        182 |C
        183 |c        WRITE (6, 730)  LAT
        184 |C
        185 |c        WRITE (6, 950)
        186 |c    1               (STRIPARA(IV-1+ (LEV-1)*LONF + I, LAT+NLATSH),
        187 |c    2                I=LONG1,LONG2)
        188 |c 950    FORMAT (' ', 8X, 20F6.1)
        189 |C
        190 |c1000 CONTINUE
        191 |C
        192 |      RETURN
        193 |C
        194 |      END
 
