          1 |      SUBROUTINE PRINTIMS ( SCVH, MSKSCVH )
          2 |C
          3 |      REAL      SCVH    (1024,1024)
          4 |      INTEGER   MSKSCVH (1024,1024)
          5 |C
          6 |      DIMENSION ILABEL(64), ILABEL1(64)
          7 |C
          8 |      DO 10 I=1,64
          9 |        ILABEL(I) = I*16
         10 |   10 CONTINUE
         11 |C
         12 |      WRITE(6,20)
         13 |  20  FORMAT (/' LAND-SEA MASK FOR NESDIS-IMS SNOW-ICE ANAL'/
         14 |     &         ' ','   ( LAND=1, SEA/LAKE=0 )')
         15 |C
         16 |      WRITE(6,*)
         17 |C
         18 |C   NEXT 64 COLUMNS
         19 |      DO 30 J=1024,16,-16
         20 |        WRITE(6,40) J, (MSKSCVH(I,J), I=16,1024,16)
         21 |  30  CONTINUE
         22 |  40  FORMAT( ' ', ' J=', I4, 1X, 64I1)
         23 |C
         24 |      WRITE(6,50)
         25 |  50  FORMAT (//' NESDIS-IMS SNOW-ICE ANAL: '/
         26 |     &  ' ',' (ICE-FREE SEA=0, SNOW-FREE LAND=1, ICE=2, SNOW=3)'/)
         27 |C
         28 |C   NEXT 72 COLUMNS
         29 |      WRITE(6,*)
         30 |C
         31 |      DO 60 J=1024,16,-16
         32 |        WRITE(6,40) J, (MSKSCVH(I,J)+NINT(SCVH(I,J))*2, I=16,1024,16)
         33 |  60  CONTINUE
         34 |C
         35 |      RETURN
         36 |      END
 
