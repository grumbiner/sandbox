          1 |      SUBROUTINE TASKF
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .                                       .
          4 |C SUBPROGRAM:    TASKF       CALCULATES THE MASS-WEIGHTED U,V WINDS
          5 |C   PRGMMR: JIM HOKE         ORG: W/NMC22    DATE: 85-03-27
          6 |C
          7 |C ABSTRACT: CALCULATES THE MASS-WEIGHTED U AND V WIND COMPONENTS
          8 |C
          9 |C PROGRAM HISTORY LOG:
         10 |C   85-03-27  JIM HOKE
         11 |C   88-08-03  BRIAN SCHMIDT ADDED THE DOCBLOCK
         12 |C
         13 |C USAGE:    CALL TASKF
         14 |C   SUBPROGRAMS CALLED:
         15 |C     UNIQUE:    - GRDSET
         16 |C     LIBRARY:
         17 |C       COMMON   - CONSTN
         18 |C                  BLANK (CONTAINS THE VBL ARRAY)
         19 |C
         20 |C ATTRIBUTES:
         21 |C   LANGUAGE: CYBER FORTRAN 200
         22 |C   MACHINE:  CYBER 205
         23 |C
         24 |C$$$
         25 |C
         26 |      IMPLICIT REAL (A-H, O-Z)
         27 |C
         28 |C
         29 |      INCLUDE 'parmodel'
  1       1 |C     PARAMETER FILE
  1       2 |C
  1       3 |C     SETUP FOR IBM SINGLE PRECISION REPRESENTATION
  1       4 |C     ON OUTPUT
  1       5 |C                      THIS VERSION 1 May 1991 (DiMego)
  1       6 |C
  1       7 |      PARAMETER (
  1       8 |     *      IBPW      =      4,
  1       9 |     *      IBPOR     = 120000,
  1      10 |     *      INUMREC   = IBPOR / IBPW,
  1      11 |     *      INSPECS   =    700,
  1      12 |     *      ILATG2    =    120,
  1      13 |     *      ILEVH     =     12,
  1      14 |     *      ILEVS     =     16,
  1      15 |     *      ILONF     =    360,
  1      16 |     *      INLATSH   =      5,
  1      17 |     *      IIJMAX    =  23668,
  1      18 |     *      IIJMAXB   =  25536,
  1      19 |     *      IIJRAD    =    892,
  1      20 |     *      IJCAP     =     80,
  1      21 |     *      IWPCPL    =     80,
  1      22 |     *      IKM       =     16 )
  1      23 |      PARAMETER (
  1      24 |     *      ILBCH     =   7*(147+111-7),
  1      25 |     *      ILBHI     =    (147+111-26),
  1      26 |     *      ILBUI     =    (2*147+111-39),
  1      27 |     *      ILBUO     =   (8*147+7*111-56),
  1      28 |     *      ILBVI     =    (147+2*111-39),
  1      29 |     *      ILBVO     =   (7*147+8*111-56),
  1      30 |     *      ILMOUT    =     20 )
  1      31 |      PARAMETER (
  1      32 |     *      IMAXOUTF  =     17,
  1      33 |     *      IMAXOUTT  =      5,
  1      34 |     *      IMAXOUTV  =    100,
  1      35 |     *      IMAXPROF  =      6,
  1      36 |     *      INFILLER  =2500000,
  1      37 |     *      INH       =     65,
  1      38 |     *      INGRDUSE  =      2,
  1      39 |     *      INGRDUS1  =      1,
  1      40 |     *      INIADDRS  =     44,
  1      41 |     *      INSCR     =     23,
  1      42 |     *      INSCRGEO  =     12,
  1      43 |     *      INSCR3    =     10,
  1      44 |     *      INSOIL    =      2,
  1      45 |     *      INSUMG    =  42164,
  1      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  1      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  1      48 |     *      INSHEM    = 13 * INH + 23,
  1      49 |     *      IIJKMAX   = IKM * IIJMAX )
  1      50 |      PARAMETER (
  1      51 |     *  IIWORDSB  = IIJMAXB / 64,
  1      52 |     *  IKMP1     = IKM + 1,
  1      53 |     *  ILVLTOT   = 7 * IKM + 37,
  1      54 |     *  ILONFP1   = ILONF + 1,
  1      55 |     *  INVBL     = ILVLTOT * INSUMG,
  1      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  1      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  1      58 |     *  IBLK1D    =  2 * IKM + 14,
  1      59 |     *  IBLK2D    = 16 * IKM + 36,
  1      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  1      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  1      62 |C
         30 |C
         31 |C
         32 |      LOGICAL  BITSEA, BITSNO, BITWVL
         33 |C
         34 |      COMMON /CONSTN/ IMG( INGRDUSE ), JMG( INGRDUSE ),
         35 |     1                IAG( INGRDUS1 ), JAG( INGRDUS1 ),
         36 |     2                IBG( INGRDUS1 ), JBG( INGRDUS1 ),
         37 |     3                IADDRG( INIADDRS ,  INGRDUSE ),
         38 |     4                INDEXHU,  INDEXHV,  INDEXHTH, INDEXHQ,
         39 |     5                INDEXH,   INDEXST,  INDEXSQ,  INDEXALB,
         40 |     6                INDEXSLP, INDEXPSI, INDEXCD,  INDEXCC,  INDEXGSP,
         41 |     7                INDEXRAD, INDEXDLW, INDEXDSW, INDEXCPR, INDEXMA,
         42 |     8                INDEXSEC, INDEXZ0, INDEXTSS,  INDEXCG,  INDEXDHQ,
         43 |     9                INDEXTSD, KM, NH, NGRDUSE,
         44 |     1                SPECS( INSPECS ), DELSIG( IKM ),
         45 |     2                POVH( IKM ), PIOVHK( IKM ),
         46 |     3                CMUU  ( INGRDUSE ),
         47 |     4                XPOLEH( INGRDUSE ), YPOLEH( INGRDUSE ),
         48 |     5                XPOLEU( INGRDUSE ), YPOLEU( INGRDUSE ),
         49 |     6                XPOLEV( INGRDUSE ), YPOLEV( INGRDUSE ),
         50 |     7                DLAMNGM,
         51 |     8                BITSEA( IIJMAX ,  INGRDUSE ),
         52 |     9                BITSNO( IIJMAX ,  INGRDUSE ),
         53 |     1                BITWVL( IIJMAX ,  INGRDUSE )
         54 |C
         55 |C       ****   END OF INCLUDED COMMON BLOCK CONSTN   ***
         56 |C
         57 |C
         58 |      COMMON          VBL( INVBL )
         59 |C
         60 |C
         61 |C     FIRST THE V COMPONENT IS PROCESSED.
         62 |C
         63 |      DO 1100 NG=1,NGRDUSE
         64 |      CALL GRDSET (NG, INDEXHV, DUMMY, IM, JM, DUMMY, DUMMY,DUMMY,DUMMY)
         65 |      IJ     = IM * JM
         66 |C
         67 |C     SPECIFY THE POINTERS FOR VARIOUS VARIABLES IN ARRAY VBL.
         68 |      MADDHV = IADDRG(INDEXHV, NG) -1 -IM -IJ
         69 |      MADDH  = IADDRG(INDEXH , NG) -1 -IM
         70 |C
         71 |      DO 1000 J=1,JM
         72 |C
         73 |      DO 900 K=1,KM
         74 |C
         75 |      DO 800 I=1,IM-1
         76 |      VBL(MADDHV+I+IM*J+IJ*K)
         77 |     1     = 0.5E0 * (VBL(MADDH+I+IM*J) + VBL(MADDH+I+1+IM*J))
         78 |     2     * VBL(MADDHV+I+IM*J+IJ*K)
         79 |  800 CONTINUE
         80 |C
         81 |C     THE VALUE OF HV AT I=IM IS DEFINED HERE WITH A REASONABLE
         82 |C     VALUE EVEN THOUGH IT WILL BE OVERWRITTEN AT THE START
         83 |C     OF THE FORECAST.
         84 |      VBL(MADDHV+IM+IM*J+IJ*K)
         85 |     1     = VBL(MADDH+IM+IM*J) * VBL(MADDHV+IM+IM*J+IJ*K)
         86 |C
         87 |  900 CONTINUE
         88 |C
         89 | 1000 CONTINUE
         90 |C
         91 | 1100 CONTINUE
         92 |C
         93 |C
         94 |C     NOW THE U COMPONENT IS PROCESSED.
         95 |C
         96 |      DO 2100 NG=1,NGRDUSE
         97 |      CALL GRDSET (NG, INDEXHU, DUMMY, IM, JM, DUMMY, DUMMY,DUMMY,DUMMY)
         98 |C
         99 |C     SPECIFY THE POINTERS FOR VARIOUS VARIABLES IN ARRAY VBL.
        100 |      IJ     = IM * JM
        101 |      MADDHU = IADDRG(INDEXHU, NG) -1 -IM -IJ
        102 |      MADDH  = IADDRG(INDEXH , NG) -1 -IM
        103 |C
        104 |C
        105 |      DO 2000 J=1,JM
        106 |C
        107 |C     IF THIS IS THE LAST ROW WE ASSUME THAT
        108 |C     THE SURFACE PRESSURE OF THE NONEXISTENT NEXT ROW IS THE
        109 |C     SAME AS THE CURRENT ROW.
        110 |C     THE VALUE OF HU AT J=JM IS DEFINED ALTHOUGH IT WILL BE
        111 |C     OVERWRITTEN AT THE START OF THE FORECAST.
        112 |C
        113 |      DO 1900 K=1,KM
        114 |C
        115 |      DO 1800 I=1,IM
        116 |         IF (J .LT. JM) THEN
        117 |            VBL(MADDHU+I+IM*J+IJ*K)
        118 |     1          = 0.5E0 * (VBL(MADDH+I+IM*J) + VBL(MADDH+I+IM+IM*J))
        119 |     2          * VBL(MADDHU+I+IM*J+IJ*K)
        120 |         ELSE
        121 |            VBL(MADDHU+I+IM*J+IJ*K)
        122 |     1          = VBL(MADDH+I+IM*J)
        123 |     2          * VBL(MADDHU+I+IM*J+IJ*K)
        124 |         END IF
        125 | 1800 CONTINUE
        126 |C
        127 | 1900 CONTINUE
        128 |C
        129 | 2000 CONTINUE
        130 |C
        131 | 2100 CONTINUE
        132 |C
        133 |      RETURN
        134 |C
        135 |      END
 
