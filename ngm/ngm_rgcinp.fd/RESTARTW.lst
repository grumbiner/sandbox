          1 |      SUBROUTINE RESTARTW (IOUTURST, IOUTUPRT, DATANAME, SPECS, VBL,
          2 |     1                     BITSEA, BITSNO, BITWVL)
          3 |C
          4 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          5 |C
          6 |C SUBPROGRAM:    RESTARTW    WRITES RESTART FILE
          7 |C   PRGMMR: J TUCCILLO       ORG: W/NMC4     DATE: 90-06-19
          8 |C
          9 |C ABSTRACT:    WRITES RESTART FILE WITH A MAXIMUM RECORD LENGTH OF
         10 |C              INUMREC ELEMENTS
         11 |C
         12 |C PROGRAM HISTORY LOG:
         13 |C   90-06-19  J TUCCILLO
         14 |C   98-11-19  E ROGERS REPLACED CALL TO OUTOPN WITH ONE TO WRTDAT,
         15 |C             WHICH WILL WRITE A NCEP BINARY DATE WORD IN THE
         16 |C             FIRST RECORD OF THE RESTART FILE INSTEAD OF AN ON85
         17 |C             DATE LABEL
         18 |C
         19 |C USAGE: CALL RESTARTW (IOUTURST, IOUTUPRT, DATANAME, SPECS, VBL,
         20 |C                       BITSEA, BITSNO, BITWVL)
         21 |C   INPUT ARGUMENTS:
         22 |C     IOUTURST  -  UNIT NUMBER OF RESTART FILE
         23 |C     IOUTUPRT  -  UNIT NUMBER OF OUTPUT FILE
         24 |C     SPECS     -  SPECIFICATION FILE
         25 |C     VBL       -  CONTAINS NGM MODEL FIELDS
         26 |C     BITSEA    -  CONTAINS UNFROZEN WATER LOGICAL MAP
         27 |C     BITSNO    -  CONTAINS SNOW COVER LOGICAL MAP
         28 |C     BITWVL    -  CONTAINS WATER VS. LAND LOGICAL MAP
         29 |C
         30 |C   SUBROUTINES CALLED:
         31 |C     UNIQUE    -  WRTDAT
         32 |C
         33 |C ATTRIBUTES:
         34 |C   LANGUAGE: FORTRAN
         35 |C   MACHINE:  CRAY Y-MP
         36 |C
         37 |C$$$
         38 |C
         39 |C
         40 |      INCLUDE 'parmodel'
  1       1 |C     PARAMETER FILE
  1       2 |C
  1       3 |C     SETUP FOR IBM SINGLE PRECISION REPRESENTATION
  1       4 |C     ON OUTPUT
  1       5 |C                      THIS VERSION 1 May 1991 (DiMego)
  1       6 |C
  1       7 |      PARAMETER (
  1       8 |     *      IBPW      =      4,
  1       9 |     *      IBPOR     = 120000,
  1      10 |     *      INUMREC   = IBPOR / IBPW,
  1      11 |     *      INSPECS   =    700,
  1      12 |     *      ILATG2    =    120,
  1      13 |     *      ILEVH     =     12,
  1      14 |     *      ILEVS     =     16,
  1      15 |     *      ILONF     =    360,
  1      16 |     *      INLATSH   =      5,
  1      17 |     *      IIJMAX    =  23668,
  1      18 |     *      IIJMAXB   =  25536,
  1      19 |     *      IIJRAD    =    892,
  1      20 |     *      IJCAP     =     80,
  1      21 |     *      IWPCPL    =     80,
  1      22 |     *      IKM       =     16 )
  1      23 |      PARAMETER (
  1      24 |     *      ILBCH     =   7*(147+111-7),
  1      25 |     *      ILBHI     =    (147+111-26),
  1      26 |     *      ILBUI     =    (2*147+111-39),
  1      27 |     *      ILBUO     =   (8*147+7*111-56),
  1      28 |     *      ILBVI     =    (147+2*111-39),
  1      29 |     *      ILBVO     =   (7*147+8*111-56),
  1      30 |     *      ILMOUT    =     20 )
  1      31 |      PARAMETER (
  1      32 |     *      IMAXOUTF  =     17,
  1      33 |     *      IMAXOUTT  =      5,
  1      34 |     *      IMAXOUTV  =    100,
  1      35 |     *      IMAXPROF  =      6,
  1      36 |     *      INFILLER  =2500000,
  1      37 |     *      INH       =     65,
  1      38 |     *      INGRDUSE  =      2,
  1      39 |     *      INGRDUS1  =      1,
  1      40 |     *      INIADDRS  =     44,
  1      41 |     *      INSCR     =     23,
  1      42 |     *      INSCRGEO  =     12,
  1      43 |     *      INSCR3    =     10,
  1      44 |     *      INSOIL    =      2,
  1      45 |     *      INSUMG    =  42164,
  1      46 |     *      INSIGSTP  = ILONF * (3 * ILEVS + ILEVH + 2),
  1      47 |     *      INLAT     = ILATG2 + INLATSH + 1,
  1      48 |     *      INSHEM    = 13 * INH + 23,
  1      49 |     *      IIJKMAX   = IKM * IIJMAX )
  1      50 |      PARAMETER (
  1      51 |     *  IIWORDSB  = IIJMAXB / 64,
  1      52 |     *  IKMP1     = IKM + 1,
  1      53 |     *  ILVLTOT   = 7 * IKM + 37,
  1      54 |     *  ILONFP1   = ILONF + 1,
  1      55 |     *  INVBL     = ILVLTOT * INSUMG,
  1      56 |     *  IMAXSCR   = IIJMAX*(INSCR+INSCRGEO) + IIJKMAX*INSCR3+INFILLER,
  1      57 |     *  IRADWRK   = IWPCPL * IIJRAD * IKM,
  1      58 |     *  IBLK1D    =  2 * IKM + 14,
  1      59 |     *  IBLK2D    = 16 * IKM + 36,
  1      60 |     *  IBLK1WRK  = IIJMAX * IBLK1D,
  1      61 |     *  IBLK2WRK  = IIJRAD * IBLK2D )
  1      62 |C
         41 |C
         42 |C
         43 |      DIMENSION VBL    ( INVBL ),
         44 |     *          SPECS  ( INSPECS ),
         45 |     *          BITSEA ( IIJMAX * INGRDUSE ),
         46 |     *          BITSNO ( IIJMAX * INGRDUSE ),
         47 |     *          BITWVL ( IIJMAX * INGRDUSE )
         48 |C
         49 |      LOGICAL BITSEA, BITSNO, BITWVL
         50 |C
         51 |      CHARACTER*6 DATANAME
         52 |C
         53 |C================================================================
         54 |C
         55 |      GMT0     = SPECS(2)
         56 |      DAY0     = SPECS(3)
         57 |      AMONTH0  = SPECS(4)
         58 |      CENYEAR0 = SPECS(5)
         59 |C     IRUNMARK = NINT(SPECS(235))
         60 |C     IOUTUPRT = NINT(SPECS(271))
         61 |C     IVERSION = NINT(SPECS(231))
         62 |      IRUNMARK = 2
         63 |      IOUTUPRT = 6
         64 |      IVERSION = 1
         65 |      ITIME    = NINT(SPECS(233))
         66 |      NSTEPS   = NINT(SPECS(234))
         67 |C
         68 |      CALL WRTDAT(DATANAME, CENYEAR0, AMONTH0, DAY0, GMT0, IRUNMARK,
         69 |     *             IVERSION, IOUTUPRT, IOUTURST, ITIME, NSTEPS)
         70 |C
         71 |      ILEFT = MOD ( INSPECS, INUMREC )
         72 |      ILOOP = INSPECS / INUMREC
         73 |      IST   = 1
         74 |      ISP   = INUMREC
         75 |      DO 1 II = 1, ILOOP
         76 |         WRITE ( IOUTURST ) (SPECS(I),I=IST,ISP)
         77 |         IST = ISP + 1
         78 |         ISP = ISP + INUMREC
         79 |1     CONTINUE
         80 |      IF ( ILEFT .GT. 0 ) THEN
         81 |         ISP = INSPECS
         82 |         WRITE ( IOUTURST ) (SPECS(I),I=IST,ISP)
         83 |      END IF
         84 |C
         85 |      ILEFT = MOD ( INVBL, INUMREC )
         86 |      ILOOP = INVBL / INUMREC
         87 |      IST   =     1
         88 |      ISP   = INUMREC
         89 |      DO 2 II = 1, ILOOP
         90 |        WRITE ( IOUTURST ) (VBL(I), I=IST, ISP)
         91 |        IST = ISP + 1
         92 |        ISP = ISP + INUMREC
         93 |2     CONTINUE
         94 |      IF( ILEFT .NE. 0) THEN
         95 |        ISP = INVBL
         96 |        WRITE ( IOUTURST ) (VBL(I), I=IST, ISP)
         97 |      END IF
         98 |C
         99 |C     SAME VALUES FOR ALL LOGICAL MASKS
        100 |C
        101 |      ILEN = IIJMAX * INGRDUSE
        102 |      ILEFT = MOD ( ILEN, INUMREC )
        103 |      ILOOP = ILEN / INUMREC
        104 |C
        105 |C
        106 |      IST   =     1
        107 |      ISP   = INUMREC
        108 |      DO 3 II = 1, ILOOP
        109 |        WRITE ( IOUTURST ) (BITSEA(I), I=IST, ISP)
        110 |        IST = ISP + 1
        111 |        ISP = ISP + INUMREC
        112 |3     CONTINUE
        113 |      IF( ILEFT .NE. 0) THEN
        114 |        ISP = ILEN
        115 |        WRITE ( IOUTURST ) (BITSEA(I), I=IST, ISP)
        116 |      END IF
        117 |C
        118 |      IST   =     1
        119 |      ISP   = INUMREC
        120 |      DO 4 II = 1, ILOOP
        121 |        WRITE ( IOUTURST ) (BITSNO(I), I=IST, ISP)
        122 |        IST = ISP + 1
        123 |        ISP = ISP + INUMREC
        124 |4     CONTINUE
        125 |      IF( ILEFT .NE. 0) THEN
        126 |        ISP = ILEN
        127 |        WRITE ( IOUTURST ) (BITSNO(I), I=IST, ISP)
        128 |      END IF
        129 |C
        130 |      IST   =     1
        131 |      ISP   = INUMREC
        132 |      DO 5 II = 1, ILOOP
        133 |        WRITE ( IOUTURST ) (BITWVL(I), I=IST, ISP)
        134 |        IST = ISP + 1
        135 |        ISP = ISP + INUMREC
        136 |5     CONTINUE
        137 |      IF( ILEFT .NE. 0) THEN
        138 |        ISP = ILEN
        139 |        WRITE ( IOUTURST ) (BITWVL(I), I=IST, ISP)
        140 |      END IF
        141 |C
        142 |C
        143 |      RETURN
        144 |      END
 
